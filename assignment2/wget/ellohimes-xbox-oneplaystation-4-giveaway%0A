<script>
//<![CDATA[
var tag = document.createElement('script');

tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

function onYouTubeIframeAPIReady() {
    $(function () {
        var players = [];

        $('.cover-video.youtube').each(function () {
            var player;
            var width = $('.popup-blocks-container').width();
            var height = (width * 9) / 16;
            $(this).parent().height(height);

            var playerVars = { controls: 1, disablekb: 0, rel: 0, modestbranding: 1, showinfo: 1, autoplay: 0 };

            var scope = angular.element(this).scope();

            var videoId, callback, postInit;

            if(scope.entry_method) {

                scope.entry_method.onExpand = function () {
                    try {
                        if (!navigator.userAgent.match(/iPod|iPhone/) && ((scope.entry_method.config4 + '').match(/autoplay=1/) || player.getCurrentTime() > 0)) {
                            player.playVideo();
                        }
                    } catch (e) {
                    }
                };

                scope.entry_method.onUnexpand = function () {
                    try {
                        player.pauseVideo();
                    } catch (e) {
                    }
                };

                var pairs = (scope.entry_method.config4+'').split(/&/);
                var pair;

                for(var i = 0; i < pairs.length; i++) {
                    pair = pairs[i].split('=');
                    if(pair.length == 2) {
                        playerVars[pair[0]] = pair[1];
                    }
                }
                videoId = scope.entry_method.config3;
                callback  = function (event) {
                    scope.youtubeVideoStateChange(scope.entry_method, event);
                    scope.$apply();
                };

                postInit = function () {
                    var grace = 5;
                    var secondsOpen;
                    var interval = setInterval(function () {
                        secondsOpen = scope.entry_method.secondsOpen || 0;

                        if(player && player.getDuration && player.getDuration() >  1 && (player.getDuration() > grace || secondsOpen > player.getDuration())) {
                            if(player.getCurrentTime() > (player.getDuration() - grace) || secondsOpen > player.getDuration()) {

                                scope.$apply(function () {
                                    clearInterval(interval);
                                    setTimeout(function () {
                                        scope.entry_method.watched = true
                                    }, (grace-1) * 1000);
                                });
                            }
                        }
                    }, 1000);
                }
            } else {
                videoId = scope.incentives[0].youtube_video_id;
                callback = postInit = function() {}
            }

            player = new YT.Player(this.id, {
                height: height,
                width: width,
                videoId: videoId,
                playerVars: playerVars,
                events: { onStateChange: callback }
            });

            postInit();

            window.player = player;
        });
    })
}

//]]>
</script><script>
//<![CDATA[

    angular.element(document).ready(function () { setTimeout(function () {
window.twttr = (function (d,s,id) {
var t, js, fjs = d.getElementsByTagName(s)[0];
if (d.getElementById(id)) return; js=d.createElement(s); js.id=id;
js.src="https://platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs);
return window.twttr || (t = { _e: [], ready: function(f){ t._e.push(f) } });
}(document, "script", "twitter-wjs"));

twttr.ready(function (twttr) {
    var linkMatcher = /(https?:\/\/[^ ]+[^ ,.)\]])/gi;
    var endLinkMatcher = /(https?:\/\/[^ ]+[^ ,.)\]])\s*$/gi;
    var endHastagMatcher = /((?:^|\s)+#[a-z0-9_]+\s*)+$/ig;
    var manualLinkMatcher = /\[LINK\]/gi;

    var lastClick = {};

    function getEndHashtags(str) {
        var m = str.replace(endLinkMatcher, '').match(/(\s*#[a-z0-9_]+\s*)+$/ig);

        return m ? m[0].replace(/\s*#/, '').split(/\s*#/g) : [];
    }

    function deconstructTweet(tweet, defaultLink) {
        var data = {};
        var replaced = tweet.replace(manualLinkMatcher, defaultLink);

        data.text = replaced.replace(endHastagMatcher, '').replace(endLinkMatcher, '').replace(endHastagMatcher, '');
        data.hashtags = getEndHashtags(replaced).join(',');
        var middleLinkMatch = replaced.replace(endHastagMatcher, '').replace(endLinkMatcher, '').match(linkMatcher);

        if(!tweet.match(manualLinkMatcher) && middleLinkMatch) {
            data.link = 'N'+Math.random(); // No link.
        } else {
            var endLinkMatch = replaced.match(endLinkMatcher);
            if(endLinkMatch) {
                data.link = endLinkMatch[0];
            } else {
                data.link = defaultLink;
            }
        }

        return data;
    }

    function parseTwitterElements() {
        $('.tweet-box').each(function () {
            var $t = $(this);
            if(!$t.attr('twitter-parsed')) {
                $t.attr('twitter-parsed', true);
                var scope = angular.element(this).scope();
                var entry_method = scope.entry_method;
                var tweet = deconstructTweet($t.data('tweet'), $t.data('tweet-url'));

                twttr.widgets.createShareButton(
                        tweet.link, this, function (iframe) {scope.resize()}, {
                            text: tweet.text,
                            hashtags: tweet.hashtags,
                            count: 'none',
                            size: 'large'
                        }
                );
            }
        });

        $('.hashtag-box').each(function () {
            var $t = $(this);
            if(!$t.attr('twitter-parsed')) {
                $t.attr('twitter-parsed', true);
                var scope = angular.element(this).scope();
                var entry_method = scope.entry_method;

                var tweet = deconstructTweet($t.data('tweet'), $t.data('tweet-url'));

                twttr.widgets.createShareButton(
                        tweet.link, this, function (iframe) { scope.resize() }, {
                            text: tweet.text,
                            hashtags: $t.data('hashtags'),
                            size: 'large'
                        }
                );
            }
        });


        $('.follow-box').each(function () {
            var $t = $(this);
            if(!$t.attr('twitter-parsed')) {
                $t.attr('twitter-parsed', true);
                var scope = angular.element(this).scope();
                var entry_method = scope.entry_method;
                var user = $t.data('user');

                twttr.widgets.createFollowButton(
                        user, this, function (iframe) { scope.resize() }, {
                            size: 'large',
                            showCount: false
                        }
                );
            }
        });

        $('.retweet-box').each(function () {
            var $t = $(this);
            if(!$t.attr('twitter-parsed')) {
                $t.attr('twitter-parsed', true);

                var scope = angular.element(this).scope();
                var entry_method = scope.entry_method;
                var tweetUrl = $t.data('tweet-url');
                var tweetId = tweetUrl.match(/\/status\/(\d+)/i)[1];
                twttr.widgets.createTweet(
                        tweetId, this, function (iframe) { scope.resize() }, {
                            conversation: 'none',
                            cards: 'none'
                        }
                );
            }
        });

    }

    $('body').bind('widget:thirdpartyparse', function () {
        setTimeout(parseTwitterElements, 500);
    });

    setTimeout(parseTwitterElements, 500);

    twttr.events.bind('tweet', function (event) {
        var scope = angular.element(lastClick['tweet']).scope();
        scope.handleTweet(scope.entry_method);
    });

    twttr.events.bind('follow', function (event) {
        setTimeout(function () {
            angular.element('[ng-controller=EnterController]').scope().handleFollow(event.data.screen_name);
        }, 1000)
    });

    $(window).bind("message", function(event) {
        if(event.originalEvent.data && event.originalEvent.data.match && event.originalEvent.data.match(/retweet/)) {
            var tweetId = event.originalEvent.data.match(/source_tweet_id.:.(\d+)/)[1];
            angular.element('[ng-controller=EnterController]').scope().handleRetweet(tweetId);
        }
    });

    twttr.events.bind('click', function (event) {
      lastClick[event.region] = $('.expanded').find('.follow-box:visible, .hashtag-box:visible, .retweet-box:visible, .tweet-box:visible');
    });
});
}, 1); });

//]]>
</script>
