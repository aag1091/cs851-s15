    //mmaSocialShare: leedan Jan. 2013
    function share(type) {
        var url = "http://www.metmuseum.org" + window.location.pathname;
        var title = escape(window.document.title);
        var windowFeatures = GetCenterDimensions();
        switch (type) {
            case 'facebook':
                window.open('http://www.facebook.com/sharer.php?u=' + url + '&t=' + title, 'myWindow', "'" + windowFeatures + "'");
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Facebook']);
                break;
            case 'twitter':
                title = title + ":&via=metmuseum";
                window.open("https://twitter.com/share?url=" + url + "&text=" + title, 'sharer', "'" + windowFeatures + "'");
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Twitter']);
                break;
            case 'pinterest':
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Pinterest']);
                selectPinImage();
                break;
            case 'tumblr':
                window.open("http://www.tumblr.com/share/link?url=" + encodeURIComponent(url) + "&name=" + encodeURIComponent(title) + "&description='&nbsp;'", 'sharer', "'" + windowFeatures + "'");
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Tumblr']);
                break;
        }
    }

    //Viewport smaller with height smaller than 675px move top of popup modal to '0'
    //Note - mma social share popup supports smallest viewport hieght of 600px; so small tablet size.
    function GetTopVal(){
        if ($(window).height() < 675)
            return 0;
        else
            return 50;
    }

    function GetCenterDimensions() {
        var windowFeatures;
        var leftPosition, topPosition;
        //Allow for borders.
        leftPosition = (window.screen.width / 2) - ((604 / 2) + 10);
        //Allow for title and status bars.
        topPosition = (window.screen.height / 2) - ((604 / 2) + 50);
        var windowFeatures = "status=no,height=520,width=604,resizable=yes,left=" + leftPosition + ",top=" + topPosition + ",screenX=" + leftPosition + ",screenY=" + topPosition + ",toolbar=no,menubar=no,scrollbars=no,location=no,directories=no,dependent=yes,fullscreen=no";
        return windowFeatures;
    }
