<script type='text/javascript'>
// <![CDATA[
        $('.rating span').each(function(i, o){
            $(this).click(function(){
                $('.rating').toggle();
                $('#rating .spinner').toggle();
                $.post($(this).data('href'), function(data, status){
                    $('.rating').toggle();
                    $('#rating .spinner').toggle();
                    $('.rating').html(data);
                    $('.rating').append('<p>Thanks!</p>')
                });
            })
        });

        $('.rating span').tooltip({'placement':'bottom'})

// ]]>
</script>
