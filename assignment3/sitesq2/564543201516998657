        $(function() {
          var $sidebar   = $(".scrollable"),
              $window    = $(window),
              offset     = $sidebar.offset(),
              topPadding = 15;
              diff = 1200;

          $window.scroll(function() {
              if ($window.scrollTop() > (offset.top + diff)) {
                  $sidebar.css('position','fixed');
                  $sidebar.css('top', topPadding);
                } else {
                  $sidebar.css('position','static');
