			        if (typeof (showExitPopup) != 'undefined') {
			            showExitPopup = false;
			        }			    
				    if (result == "AUTOGEN") {
				        Won.ICom.Code.Services.SiteAjaxService.GetSecureURL(onAutoGenComplete);
				        return;
				    }
				    else if (result == 'SOK') {
				        var url = window.location.href.toString();
				        if (url.toLowerCase().indexOf("myibd/forgotpassword.aspx") > -1) {
				            window.location = "/default.aspx";
				        }
				        else if (url.toLowerCase().indexOf("default.htm") > -1) {
				            window.location = "/default.aspx";
				        }
				        else {
				            window.location.reload(true);
				        }
					}
					else {
						personal.loginDisplayGenericError();
					}
				}
				
				function onAutoGenComplete(result) {
				    var url = result + "Secure/MyIBD/UpdatePassword.aspx?src=AUTOGEN";
				    window.location = url;
				}
