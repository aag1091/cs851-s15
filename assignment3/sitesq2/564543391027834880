                function DisplayInterstitial(response) {
                    // show overlay

                    $("#interstitial-overlay").jqm({ modal: true, zIndex: 3000000 }).jqmShow();

                    // set close timer
                    if (response.TimerInterval > 0) {
                        setTimeout(function() { $("#interstitial-overlay").jqm().jqmHide(); }, response.TimerInterval);
