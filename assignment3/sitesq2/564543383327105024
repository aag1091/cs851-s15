    <script>
$(function() {
  // Mobile navigation toggle
  $('#mobileToggle').toggleClick(function() {
    $(this).addClass('active');
    $('nav').show();
  }, function() {
    $(this).removeClass('active');
    $('nav').hide();
  });

  //Slice job dropdowns and add View all button
  $("#byCat ul li").slice(0, 7).css('display', 'block');   
  $("#byLoc ul li").slice(0, 7).css('display', 'block');   
  $("#byGroup ul li").slice(0, 6).css('display', 'block');        
  $(".jobDD p.all").click(function () { 
    $(this).parent().addClass("all"); 
    $(this).hide(); 
    $(this).prev("ul").children("li").css('display', 'block');
  });

  // toggle dropdowns
  function toggleTabs(trigger, content) {
    function toggleOff() {
      $(trigger).removeClass('active');
      $(trigger).parent().find(content).hide();
    }

    $(trigger).click(function() {
      if(!$(this).hasClass('active')) {
        toggleOff();
        $(this).addClass('active');
        $(this).parent().find(content).show();
      }
      else {
        toggleOff();
      }
    });
  }

  toggleTabs(".ddTrigger", ".hide");
  toggleTabs(".toggleText", ".hide");
});
document.getElementById('cwuc_jobalertsignupusercontrol_1_jobalert-btnSignUp').setAttribute('value','Subscribe');
</script>

	

