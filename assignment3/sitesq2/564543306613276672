		<script type="text/javascript">
            // Cache references to the button
            var delayModal,
                loginBtn = document.getElementById("modalLoginBtn"),
                loginBtnText = loginBtn.innerHTML,
                loginBtnLink = loginBtn.href,
                loginBtnClass = loginBtn.className,
                timeInterval = 1000,
                timeTaken = 0,
                timeLimit = 20000;
            
            // Function to check if the login panel function exists
            var isModalReady = function() {
                // Return false appears to be erratic
                loginBtn.href = "#";
                delayModal = setInterval(lookForFunction, timeInterval);
                return false;
            };
            
            var lookForFunction = function(){
                timeTaken += timeInterval;
                if (typeof loginPanelInit !== "undefined") {
                    clearInterval(delayModal);
                    isLoaded();
                } else if (timeTaken == timeLimit) {
                    clearInterval(delayModal);
                    window.location = loginBtnLink;
                } else {
                    notLoaded();
                }
            };
            
            // If loginPanelInit exists, reset the button and trigger the initial modal opening
            var isLoaded = function() {
                loginBtn.href = loginBtnLink;
                loginBtn.innerHTML = loginBtnText;
                loginBtn.className = loginBtnClass;
                $("a.loginpanel").trigger("click");
            };
            
            // Add the loading icon & remove hover colour. Function only runs
            var notLoaded = function() {
                notLoaded = Function("");
                loginBtn.innerHTML = "<img src='/assets/images/btn-loader.gif' />";
                loginBtn.className+=' loading';
            };  
        </script>
