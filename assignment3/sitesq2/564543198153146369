SCORES:  <a shape="rect" id="showScoresLink" href="javascript:showScoreboard()">ON</a>  <a shape="rect" id="hideScoresLink" href="javascript:hideScoreboard()">OFF</a></div><div id="gcDateNaviWrap"><div id="gcDateNavi"><a shape="rect" style="width: 90px; margin-left: 10px;" onclick="loadPrevious()" id="lastDay" href="javascript:void(0);"></a><a shape="rect" id="selectedDay"></a><a shape="rect" style="width: 90px; margin-right: 10px;" onclick="loadNext()" id="nextDay" href="javascript:void(0);"></a></div></div><div id="gcCal"><div class="currDay">8</div><div style="background-color: #000;" class="calendarPicker"><input id="date-picker" type="hidden" /></div></div><script type="text/javascript">
		
			var startDateOverride = jQuery.datepicker.parseDate('mm/dd/yy', '02/08/2015');
		
		
		var gameLabels = ['games', 'game', 'games', 'games', 'games'];
		
		jQuery(document).ready(function() {
			var dayContentJson = {};
			 jQuery('#date-picker').datepicker({
			 	prevText: 'Prev',
				nextText: 'Next',
				monthNames: ['January','February','March','April','May','June', 'July','August','September','October','November','December'],
				dayNamesShort: ['Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat'],
				dayNames: ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'],
				firstDay: 0,
				showOn: 'button',
				buttonImage: 'http://2.cdn.nhle.com/nhl/images/calendar@2x.png',
				buttonImageOnly: true,
				
					defaultDate: startDateOverride,
				
				onSelect: function(dateText, inst) {
					
						loadGameDay(dateText)
						
					
				},
				beforeShow: function(input, inst) {
					jQuery.ajax({ url: 'http://www.nhl.com/gamecenter/en/ajax/gamecalendarjson.htm', async:false, dataType:'json', data:{month: startDateOverride.getMonth() + 1, year: startDateOverride.getFullYear()}, success: function(data){ jQuery.datepicker.gameDates = data.gameDates;}});
				},
				beforeShowDay: function(date) {	
					var numberOfGames = 0;
					jQuery.each(jQuery.datepicker.gameDates, function(index, value) { 
						var day = jQuery.datepicker.parseDate('mm/dd/yy', value.gd).getDate();
						if(day == date.getDate()) {
							 numberOfGames = value.n;
						} 
					});
	
					var cellClass = numberOfGames == 0 ? 'cellNoGames' : 'cellGames';
					var numberClass = numberOfGames == 0 ? 'noGames' : 'gamesNumber';
					var gameLabel = numberOfGames < gameLabels.length ? gameLabels[numberOfGames] : 'games';

					var cellContent = '<div class="' + cellClass   + '"><div class="calDD">' + date.getDate() + '</div><div class="' + numberClass + '">' + numberOfGames + '</div><span class="gmLabel">' + gameLabel + '</span></div>';
					var key = jQuery.datepicker.formatDate('mm/dd/yy', date);
					dayContentJson[key] = {cellContent: cellContent};
					return [true, "", key, numberOfGames];
				},
				onChangeMonthYear: function(year, month, inst) {
					for (prop in dayContentJson) delete dayContentJson.prop;
					jQuery.ajax({ url: 'http://www.nhl.com/gamecenter/en/ajax/gamecalendarjson.htm', async:false, dataType:'json', data:{month: month, year: year}, success: function(data){ jQuery.datepicker.gameDates = data.gameDates;}});
				},
				dayContentJson : dayContentJson
			});
		});
		
