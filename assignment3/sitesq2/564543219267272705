<script type="text/javascript">
function captionMatchImage() {
	myWidth = $('#article-main-image img').width();
	$('#article-main-caption').width(myWidth);
}
jQuery(document).ready(function() {
	captionMatchImage();
});
function indexcard_initCallback(carousel) {
	jQuery('#indexcard-next').bind('click', function() {
		carousel.next();
		return false;
	});
	jQuery('#indexcard-prev').bind('click', function() {
		carousel.prev();
		return false;
	});
};
jQuery(document).ready(function() {
	jQuery('#indexcard-carousel').jcarousel({
		scroll: 1,
		auto: 5,
		wrap: 'circular',
		initCallback: indexcard_initCallback,
		buttonNextHTML: null,
		buttonPrevHTML: null
	});
});
</script>



