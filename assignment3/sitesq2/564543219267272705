		  function () {
			$("#hidden-1").show();

		  },
		  function () {
			$("#hidden-1").hide();
		  }
		);
		$("#hidden-2").hover(
		  function () {
			$("#hidden-2").show();
		  },
		  function () {
			$("#hidden-2").hide();
		  }
		);
		$("#hidden-3").hover(
		  function () {
			$("#hidden-3").show();
		  },
		  function () {
			$("#hidden-3").hide();
		  }
		);
		$("#hidden-4").hover(
		  function () {
			$("#hidden-4").show();
		  },
		  function () {
			$("#hidden-4").hide();
		  }
		);
		$("#hidden-5").hover(
		  function () {
			$("#hidden-5").show();
		  },
		  function () {
			$("#hidden-5").hide();
		  }
		);
		$("#hidden-6").hover(
		  function () {
			$("#hidden-6").show();
		  },
		  function () {
			$("#hidden-6").hide();
		  }
		);
	  $("#hidden-7").hover(
		  function () {
			$("#hidden-7").show();
		  },
		  function () {
			$("#hidden-7").hide();
		  }
		);
		$("#hidden-8").hover(
		  function () {
			$("#hidden-8").show();
		  },
		  function () {
			$("#hidden-8").hide();
		  }
		);
	});

</script>

