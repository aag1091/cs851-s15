			$("#hidden-4").delay(delayAmount).fadeIn();
			$("#menu-1").addClass("on");
		  },
		  function () {
			$("#hidden-4").hide();
			$("#hidden-4").clearQueue();
			$("#menu-1").removeClass("on");
