						function scrollNextPage () {
							if ($(window).scrollTop() >= $(document).height()-$(window).height()-300) {
								if (currentPage == totalPages) {
									$(window).unbind("scroll");
									$("#BtnScrollPage").hide();
								}
								else {
									$("#DynamicPageLoadingSpinner").stop(true,true).fadeIn(800);
									getNextPage(currentPage,totalPages,productUrl,displayMemberPrice,catalogPath,productSort);
									currentPage++;
									$("#DynamicPageLoadingSpinner").fadeOut(800);
								}
							}
						}

						$(window).bind("scroll", function(event) {
							scrollNextPage();
						});
						
						$("#BtnScrollPage").click(function() {
							scrollNextPage();
						});
				
			});


