<script>
    $(function() {
	    $(document).scrollTop( $("#game_img").offset().top );
	    $(window).on('beforeunload', function() {
		    $(document).scrollTop($("#game_img").offset().top);
	    });
        $('#game_ambience').prependTo("#page_wrapper").show();
    });

    var start = 0;
    var imageCount = 5;
    var perPage = 3;
    var baseUrl = '/en/game/gameNews/' + '57/' ;
    var photoUrl = '/en/game/carousel/57';

    $(document).ready(function() {
	    // get photo carousel for games multimedia
	    setCarouselConf();

	    var items = $('.grid_block');
	    var handleClick = function(e) {
		    var elm = $(e.target).closest('a');
		    var title = elm.closest('.grid_block').attr('data-section');

		    ga('send', {
			    'hitType': 'event',
			    'hitCallback': function() {window.location.href = elm.attr('href');},
			    'eventCategory': 'News',
			    'eventAction': title
		    });
		    e.preventDefault();
	    };

	    items.children('a').click(handleClick);
	    items.find('.news_header').click(handleClick);

	    $("#show-more").click(function() {
		    start += perPage;
		    var url = baseUrl + start;

		    $.get(url, function(html) {
			    $("#news_block").append(html);
			    if ($('<div>' + html + '</div>').children().length < perPage) {
				    $("#news_more").remove();
			    }
			    setTimeout(function() {
				    FB.XFBML.parse();
				    $.ajax({ url: 'http://platform.twitter.com/widgets.js', dataType: 'script', cache:true});
			    }, 100);
		    })
	    });
		//TODO: display | hide platforms
		if (imageCount <= 5) {
	    	$('.showhide_store').hide();
	    } else {
		    $('.showhide_store').click(function() {
			    $('.downloads-line-last').slideToggle(600);
			    $('#hide_store_all').toggle();
			    $('#show_store_all').toggle();
		    });
	    }
    });


    function setCarouselConf() {
    	$('#carousel').carouFredSel({
            width: '100%',
            auto: false,
            items: {
                visible: 3,
                start: -1
            },
            scroll: {
                items: 1,
                duration: 1000,
                timeoutDuration: 3000
            },
            prev: '#prev',
            next: '#next'
        });
    }
</script>
