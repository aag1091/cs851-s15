<script type="text/javascript">
				idx(document).ready(function() {
					// idxFooterJsReady
					
										// page load tracking
					idx.post('/idx/ajax/loadtracking.php',{"action":"postLoadTime", "script":"search", "buildStart":"1428095013.2368",	"buildEnd":"1428095013.5388"});
									});
								idx('#IDX-showFilterOptions').click(function() { idx('#IDX-filterFields').toggle(350,'easeInOutExpo'); });var fieldID, idxID, mlsPtID;
var timer = null;

if (window.location.pathname.match(/idx/))
    ajaxURL = '/idx/ajax/search.php';

var idxSearchForm; // cache the search form
var cczRadios; // cache the radios
var subTypeContainer, propStatusContainer;
var loadDefaults = false;
var middleware = false;
var statuses, subTypes, defaultPrices;
var advancedFields = false;
var editingSavedSearch = false;
var hashPt = 0;

var formSerializeOptions = {
    invalidClass: 'IDX-noSubmit',
    forceClass: 'no-disable',
    fieldsToBeginning: ['idxID','pt','lp','hp','bd','tb','sqft','acres']
};

idx(document).ready(function() {

    idxSearchForm = idx('#IDX-searchForm');
    middleware = idxSearchForm.data('middleware');
    advancedFields = idxSearchForm.data('advfields');
    loadDefaults = needToLoadDefaults();

    // when form submit button is clicked
    idx('#IDX-formSubmit, #IDX-submitButton, #IDX-formSubmit-bottom').click(handleSearchForm); // end click

    /* special stuff when form is reset */
    idx('#IDX-searchForm .IDX-formReset').click(searchReset);

    // set the pt set in the hash
    var hash = window.location.hash;
    if (hash.match(/#pt=[0-9]+/))
    {
        hashPt = hash.substring(4);
        idx("#IDX-pt").find("option[value="+hashPt+"]").attr('selected','selected');
    }

    /**
     * Store the prop statuses on load
     */
    statuses = idx.parseJSON(idx('#IDX-allStatues').text());
    subTypes = idx.parseJSON(idx('#IDX-allSubTypes').text());
    defaultPrices = idx.parseJSON(idx('#IDX-defaultPriceData').text());

    /**
     * The prop type has changed, switch the advanced fields
     */
    idxSearchForm.find("select#IDX-pt").change(loadAdvancedFields);
    idxSearchForm.find("select#IDX-pt").change(changeCoreFields);

    // initialize the core fields
    changeCoreFields(true);

    // if we load adv fields with ajax initially
    loadAdvancedFields(true);

    /**
     * ajax list fields to load their data dynamically
     */
    idxID = idx('#IDX-idxID').val(); // 'a001';
    mlsPtID = idx('#IDX-pt option:selected').val(); // 1;

    // initialize the select2's
    initSelect2();

    // CCZ toggling
    cczRadios = idx('.IDX-ccz-radio'); // cache these
    cczRadios.data('e-change',true).change(cczToggle); // store that there is an event handler, and handle the event
    cczToggle(true); // initialize it

    // select Most expensive to least by default in sort by field
    idx("#IDX-srt option[value='prd']").attr('selected', 'selected');

    // if there's data to be used to populate the form then do it
    attemptToPopulateForm();

    // Functions to run once the search form is populated
    searchFormReady();

    // if the template is scroll checkboxes, remove the bedrooms, bathrooms, and square feet options from sort by field
    if (idx('#IDX-pt').hasClass('IDX-selectableList')) {
        idx("#IDX-srt option[value='bda'], option[value='bdd'], option[value='tba'], option[value='tbd'], option[value='sqfta'], option[value='sqftd']").remove();
    }

}); // end idx ready

/**
 * change the core fields because the prop type has change
 */
function changeCoreFields(init) {

    var pt = null;

    if (idx("#IDX-pt").size())
        pt = idx("#IDX-pt").val();

    mlsPtID = pt;

    // remove the IDX-activept-* class
    idx(".IDX-searchForm").removeClass (function (index, css) {
         return (css.match (/\bIDX-activept-\S+/g) || []).join(' ');
    });
    // add IDX-activept-* class
    idxSearchForm.addClass('IDX-activept-'+pt);

    // Set all the core fields to be hidden and not submit
    idxSearchForm.find('.IDX-coreField-pt').addClass('IDX-hide');
    idxSearchForm.find('.IDX-coreField-pt :input').addClass('IDX-noSubmit');

    // Set this prop types's core fields to show and submit
    idxSearchForm.find('.IDX-coreField-'+pt).removeClass('IDX-hide');
    idxSearchForm.find('.IDX-coreField-'+pt+' :input').removeClass('IDX-noSubmit');

    if (init === true)
    {
        // set initial no-data class
        if (statuses && !statuses[pt])
            idx('#IDX-propStatus-group').addClass('IDX-noData');
        if (subTypes && !subTypes[pt])
            idx('#IDX-propSubType-group').addClass('IDX-noData');
    }
    // not the initial load
    else
    {
        // if frontend then we want to track the pt change via the hash
        if (!middleware)
        {
            window.location.hash = 'pt='+pt;
        }
    }

    // populate status with pt statuses
    if (idx(statuses).size() > 0)
        changeOptions('#IDX-propStatus',statuses[pt]);

    if (idx(subTypes).size() > 0)
        changeOptions('#IDX-propSubType', subTypes[pt]);

    // if first loading a form we're editing don't prepopulate it so exit the function now
    if (init && idx('#searchFormInput').hasClass('Edit'))
        return true;

    // set the price for this prop type
    if (defaultPrices)
    {
        idx('#IDX-lp').val(defaultPrices.min[pt]);
        idx('#IDX-hp').val(defaultPrices.max[pt]);
    }

    // decide whether to remove bedrooms, bathrooms or square feet options from sort by field
    var sortFields = new Array('bd', 'tb', 'sqft');
    idx.each(sortFields, function(key, value) {
        if (idx('#IDX-'+value+'-group').hasClass('IDX-hide')) {
            idx("#IDX-srt option[value='"+value+"a']").hide();
            idx("#IDX-srt option[value='"+value+"d']").hide();
        }
        else {
            idx("#IDX-srt option[value='"+value+"a']").show();
            idx("#IDX-srt option[value='"+value+"d']").show();
        }
    });
}

/**
 * Change Prop Type specific core field select options
 */
function changeOptions(selector, options) {

    var element = idx(selector);
    var html = '';

    // check that there even is a prop sub type element
    if (element.size())
    {
        if (idx(options).size() > 0)
        {
            var multipleSelect = (element.prop('type'));

            idx.each(options, function(key,value) {
                // multiple select option html
                if (multipleSelect)
                    html += '<option value="'+value+'">'+value+'</option>';
                // scroll checkbox html
                else
                    html += '<label class="IDX-selectableListLabel"><input type="checkbox" value="'+value+'" name="a_propSubType[]" class="IDX-selectableListCheckbox ">'+value+'</label>';
            });
            element.html(html);

            element.closest('.IDX-control-group').removeClass('IDX-noData');
        }
        else
        {
            element.children().remove();
            element.closest('.IDX-control-group').addClass('IDX-noData');
        }
    }
    return true;
}

/**
 * gather and load the advanced fields
 */
function loadAdvancedFields(initialAjaxLoad) {

    if (!advancedFields)
        return false;

    // the prop type select element
    var ptSelector = idx('select#IDX-pt');

    // only for single searches
    if (!ptSelector.hasClass('IDX-searchType-s') || idx('#IDX-advancedSearchFields').size() == 0)
        return false;

    // no address searches
    if (idx('#IDX-main').hasClass('IDX-type-address'))
        return false;

    // get the prop type to load
    var pt = ptSelector.val();

    // remove advanced fields
    idx('#IDX-advancedSearchFields .IDX-searchAdvancedFields').remove();

    // show the loader
    var advFieldLoadingDiv = idx('#IDX-advancedFieldsLoading');
    advFieldLoadingDiv.fadeIn('slow');

    // load the advanced fields
    var options = {'action':'loadAdvFields', 'idxID':idx('#IDX-idxID').val(), 'pt':pt, 'pageID':idx('#IDX-searchForm').attr('data-pageid'), 'version':idx('#IDX-advancedSearchFields').data('version').toFixed(3)};

    // mobilize the fields if a mobile device is using standard wrapper
    var mainEl = idx('#IDX-main');
    if (mainEl.hasClass('IDX-mobilizeList'))
        options.mobilizeList = true;

    idx.get(ajaxURL, options, function(html) {

        // show the loader
        advFieldLoadingDiv.hide();

        // append the return html to the adv field container
        idx('#IDX-advancedSearchFields').append(html);
        idx('#IDX-advancedFieldsMod-'+pt).fadeIn('fast');

        initSelect2();

        mobilizeAdvancedFields();

        // if initial ajax load, attempt to populate the advanced fields
        if (initialAjaxLoad === true && loadDefaults)
            attemptToPopulateForm();

    },'html'); // end getJSON
}

/**
 * When the submit form is submitted, we want to remove any of the empty fields from the get/post
 * When submit button is clicked, go through all inputs and disable them if they are empty
 */
function handleSearchForm(e) {

    // Prevent it from submitting right away
    e.preventDefault();

    // if we need to limit any fields, do so now
    handleLimitTo();

    // disable custom city/county/zip hidden inputs
    if (idx('.IDX-ccz-radio:checked').val() == 'city')
    {
        idx('#coID').prop('disabled', true);
        idx('#pcID').prop('disabled', true);
    }
    else if (idx('.IDX-ccz-radio:checked').val() == 'county')
    {
        idx('#ciID').prop('disabled', true);
        idx('#pcID').prop('disabled', true);
    }
    else if (idx('.IDX-ccz-radio:checked').val() == 'zipcode')
    {
        idx('#ciID').prop('disabled', true);
        idx('#coID').prop('disabled', true);
    }

    // Email Functionality
    if (idxSearchForm.hasClass('IDX-emailUpdateSignupForm'))
    {
        var loggedIn = idx('#IDX-main').hasClass('IDX-loggedIn');

        if (loggedIn)
        {
            idx('#IDX-emailUpdateSignupStep2Container').html('<div id="IDX-emailUpdateSignupStep2" class="IDX-emailUpdateSignupText">Step 2: Saving search...</div>');
        }
        else
        {
            // not logged in so move the registration content into step 2 stuff
            idx('#IDX-registrationContent').prepend('<div id="IDX-emailUpdateSignupStep2" class="IDX-emailUpdateSignupText">Step 2: Create a personalized Listing Manager account.</div>');
            idx('#IDX-registration').hide().prependTo('#IDX-emailUpdateSignupStep2Container').removeClass('ui-dialog-content ui-widget-content');
        }

        idxSearchForm.fadeOut(600, function() {

            // deterimine the results page the search is going to
            var action = idxSearchForm.attr('action');
            var matches = action.match(/\/results\/(.*)/);
            var queryString = 'page=' + matches[1] + '&' + idxSearchForm.idxSearchSerialize( formSerializeOptions );

            // Logged In
            if (loggedIn)
            {
                if (editingSavedSearch) {
                    submitEditSaveSearch(queryString);
                } else {
                    /* handle if the user is logged in already */
                    idx('#IDX-saveSearchForm .IDX-queryString').val(queryString);

                    // they are logged in, submit the form
                    idx('#IDX-saveSearchForm').ajaxSubmit({
                        dataType:'json',
                        success: formSuccess
                    });
                }
            }
            // Not Logged In
            else
            {
                idx('#IDX-registration .IDX-saveParams').attr('disabled','disabled');
                idx('#IDX-registration .IDX-saveWhat').val('emailUpdatesSearch').removeAttr('disabled');

                // set the searchID
                var searchPageID = idxSearchForm.data('pageid');

                idx('#IDX-registration .IDX-queryString').val(queryString).removeAttr('disabled');
                idx('#IDX-registration .IDX-searchPageID').val(searchPageID).removeAttr('disabled');

                idx('#IDX-registration').fadeIn(400);
            }

        });
    }
    else
    {
        var queryString = idx(idxSearchForm).idxSearchSerialize( formSerializeOptions );
        var url = idxSearchForm.attr('action') + '?' + queryString;


        // middleware: we want to open the form's results in a new window
        if (middleware)
        {
            // open the window of results
            window.open(url, '_blank');
            window.focus();
        } else if(editingSavedSearch) {
            submitEditSaveSearch(queryString);
        } else {
            window.location.href = url;
        }
    }
}

function initSelect2() {
    // only do this if select2 exists
    if (idx().select2) {
        idx('.IDX-quickList').select2({
            allowClear: true,
            multiple: true,
            initSelection : function (element, callback) {

                var data = [];
                idx.each(idx(element).val().split("!%"), function (key,value) {
                    data.push({id: value, text: value});
                });

                callback(data);
            },
            formatSearching : function () {
                return 'Loading values...<img style="float:right" src="/images/spinner.gif" />';
            },
            query: function(query) {

                var fieldID = idx(this)[0].element.attr('data-fieldid');

                if (!timer)
                {
                    loadFieldData(query,fieldID);
                    timer = 1;
                }
                else
                {
                    // if term, make sure it's longer than 2
                    if (query.term.length > 0 && query.term.length < 3)
                        return false;

                    clearTimeout(timer);
                    timer = setTimeout(function() {
                        loadFieldData(query,fieldID);
                    },400);
                }
            }
        });
    }
}

function loadFieldData(query,fieldID) {

    var options = {'action':'loadFieldData', 'idxID':idxID, 'mlsPtID':mlsPtID, 'fieldID':fieldID, 'page':query.page, 'term':query.term};
    idx.getJSON(ajaxURL, options, function(response) {
        query.callback(response);
    }); // end getJSON
}

function disableFields() {

    // clear any persisting hidden quick list fields
    idx('.IDX-quickListHiddenField').remove();

    // Go through each input
    idx('#IDX-searchForm :input').each(function() {

        var disable = false;

        var el = idx(this);

        // if has the class no-disable, then skip
        if (el.hasClass('no-disable'))
          return; // basically like continue, but were in a function

        // Disable and inputs that don't need to be submitted
        if (el.hasClass('IDX-noSubmit') || el.attr('name') == undefined)
        {
            disable = true;
        }
        // If the field has nothing set or no data, disable it
        else if (el.val() == '' || el.val() == null)
        {
            disable = true;
        }
        // handle any select2 inputs
        else if (el.hasClass('IDX-quickList') && el.attr('type') == 'hidden')
        {
            var currentElement = idx(this);
            idx.each(idx(this).select2("data"), function(key,value) {
                currentElement.after('<input class="IDX-quickListHiddenField" type="hidden" name="'+currentElement.attr('name')+'[]" value="'+value.id+'" />');
            });
            disable = true;
        }

        /**
         * Disable it
         */
        if (disable)
        {
            el.attr('disabled','disabled');
        }
    }); // end each

    return true;
}

/**
 * if we are modifying a search or editing a
 * search in the middleware we need to populate it
 * with default values, which means we need to clear the
 * initial for setup first
 */
function needToLoadDefaults()
{
    // frontend modifying a search
    if (location.search.match(/modifySearch=true/))
        return true;

    // frontend editing a saved search
    if (location.search.match(/edit=true/))
    {
        verifySearchEditing();
        return true;
    }

    // editing a saved link
    if (middleware && location.pathname.match('addeditsavedlink') && location.search.match(/id=\d+/))
        return true;

    // editing a widget
    if (middleware && location.pathname.match('addeditwidget') && location.search.match(/id=\d+/))
        return true;

    // editing a lead saved search
    if (middleware && location.pathname.match('addeditsavedsearch') && location.search.match(/ssid=\d+/))
        return true;

    return false;
}

/**
 * loop through possible source elements and populate the form if they exist with
 */
function attemptToPopulateForm()
{
    // the sources to check in order, if data is found we break out of the loop and use the first data found
    var dataSourceChecked = ['#IDX-editSavedSearchData','#IDX-modifySearchData'];
    idx.each(dataSourceChecked, function (k, elementID) {
        var e = idx(elementID);
        if (e.size())
        {
            var data = idx.parseJSON(e.text());

            if (data.queryArray)
            {
                populateForm(idxSearchForm, data.queryArray);
                return false; // break out of the each loop
            }
        }
    });
    return true;
}

/**
 * populates and idx form based on an object pass of field names and values
 * @param  {obj} data Object with keys of field names and values of those inputs
 */
function populateForm(theForm, data)
{
    // start fresh if populating with modify search data
    if (loadDefaults)
        searchClear();

    // go through the params
    idx.each(data, function(name, value) {

        if (name == 'pt' && hashPt)
            value = hashPt;

        var e;

        // Look for the start of the name match
        e = theForm.find('[name^='+name+']');

        /**
         * If we couldn't find it based on the start of the name
         * try some different ways
         */
        if ( ! e.size())
        {
            /**
             * advanced fields can have different names
             * based on their type, ie aw_, a_, ak_, etc
             * if we detect it's an advanced field, it needs to be
             * able to work for any.  so if the name is aw_region and the
             * the field is now a_region, it should match that element
             * amin and amax fields are unique
             */
            if (name.indexOf('_') >= 0 && name.substr(0,4) !== 'amin' && name.substr(0,4) !== 'amax') {
                var fieldNameSplit = name.split('_');
                name = fieldNameSplit[1];
                e = theForm.find('.IDX-field-'+name);
            }
        }

        // radios are special
        if (e.is(':radio'))
        {
            // more than one radio, set the value and cache the element
            var e1 = theForm.find('[name$='+name+'][value='+value+']').prop('checked',true);

            // if there's an event handler trigger it
            if (e1.data('e-change'))
                e1.change();

            // mobile
            if (mobile)
                e.checkboxradio("refresh"); // refresh all elements
        }
        else
        {
            // if a quick list values separated by !%
            if (e.hasClass('IDX-quickList') && idx.isArray(value)) {
                var select2ValueString = value.join('!%');
                e.filter('.IDX-quickList').val(select2ValueString);
            }

            // if checkbox and not an array, make it one
            if (e.is(':checkbox') && !idx.isArray(value))
                value = value.split(',');


            // set the value
            e.filter(':not(.IDX-quickList)').val(value);

            // mobile handling
            if (mobile && e.is('.IDX-select'))
               e.selectmenu("refresh");
        }
    });

    initSelect2();

    idx(document).trigger('idxsearchform.populated', idxSearchForm);
}

function scrollCheckboxShowSelected()
{
    // if selectable checkbox ccz, then animate to the first selected value (if there is one)
    if (idxSearchForm.find('.IDX-cczSelectableList').size())
    {
        var firstCheckedCCZ = idx('#IDX-cczContainer .IDX-cczList.IDX-active').find('.IDX-selectableListCheckbox:checked:first').parent();

        if (firstCheckedCCZ.size()) {
            var cczContainer = firstCheckedCCZ.parent();
            cczContainer.animate(
                {scrollTop: cczContainer.scrollTop() + firstCheckedCCZ.position().top},
                575,
                'easeOutExpo'
            );
        }
    }
}

function searchFormReady()
{
    // methods to run once field fields have been populated
    scrollCheckboxShowSelected();
    mobilizeLists();
}

function mobilizeAdvancedFields()
{
    mobilizeGroupOfSelects( idx('#IDX-advancedSearchFields select[multiple]') );
}

function mobilizeLists()
{
    mobilizeGroupOfSelects( idx('#IDX-search-row-content select[multiple]') );
}

function mobilizeGroupOfSelects( selects )
{
    // mobile list cleanup, if none are selected, then we need the first ones to be selected by default
    if (idx('#IDX-main').hasClass('IDX-mobilizeList'))
    {
        selects.each(function(k,v) {
            var el = idx(this);

            // don't allow the display option to be selected
            var attr = { disabled: 'disabled'};

            // be selected if none are
            if ((el.val() || []).length === 0)
                attr.selected = 'selected';

            // build the display option object
            var displayOption = idx('<option>').val('').attr(attr).text(this.length + ' Available');

            // add it
            el.prepend(displayOption);
        });

        /**
         * ios 7.03 multi select bug hack
         * http://stackoverflow.com/questions/20039194/multiple-select-in-safari-ios-7
         * https://discussions.apple.com/message/23745665#23745665
         */
        // hack for iPhone 7.0.3 multiselects bug
        if(navigator.userAgent.match(/iPhone/i)) {
            selects.each(function(){
                var select = idx(this).on({
                    "focusout": function(){
                        var values = select.val() || [];
                        setTimeout(function(){
                            select.val(values.length ? values : ['']).change();
                        }, 1000);
                    }
                });
            });
        }
    }
}

// function to clean up field values
function cleanupValue(value) {
    return value.replace(/&/g, '&');
}

/**
 * Used in the middleware for changing prop types
 * @param type (string) either 'mls' or 'pt' the type of input changed
 */
function mlsPtChange (type) {

    var pageID = idx('#IDX-searchForm').attr('data-pageid');

    // set the idxID based on the Platinum or legacy v1 markup
    var idxID = (idx('#IDX-idxID').size()) ? idx('#IDX-idxID').val() : idx('#goToidxID').val();
    var mlsName = (idx(this).attr('id') == 'IDX-idxID') ? idx('#IDX-idxID option:selected').text() : idx('#goToidxID option:selected').text();

    var options = {'action':'getMethodHtml', 'pageID':pageID, 'idxID':idxID, 'type':'search'};

    // if v1 legacy markup we need the pt with it
    if (type == 'pt' && idx('#IDX-searchForm').hasClass('IDX-v1searchMarkup'))
        options.pt = idx('#propTypeSelect').val();

    idx('#IDX-searchPageWrapper').html('<div class="alert text-center">Loading Search HTML for '+mlsName+'<br />'+ajaxLoadBar+'</div>');
    var loader = instanceWorkingGraphic();

    idx.get(ajaxURL, options, function(html) {

        // do global things
        clearWorkingGraphic(loader);

        idx('#IDX-searchPageWrapper').fadeOut('fast',function() {
            idx(this).remove();
            idx('#middlewareSearch').html(html);
        });

    },'html'); // end
}

function reorderParams() {
    /**
     *  Form Variables Rearrangement
     *  We do this since browser can't take more than a finite # of GET variables
     *  The ones at front will take precedence over those in the very end
     */

    //set up specific variables we want at the front of our search
    //lp = low price, hp = high price, bd = bedrooms, tb = bathrooms, pt = property type
    //sqft = square feet, acres = acres, idxID = MLS ID
    var keys = ['lp','hp','bd','tb','pt','sqft','acres','idxID'];

    // remove any existing ones that have been added
    idx('.IDX-reorderedHiddenField').remove();

    //go through each input (which includes selects for jquery
    idx('#IDX-searchForm :input').each(function(){
        //if the input name is from "keys" array
        if(idx.inArray(this.name,keys) != -1 && !idx(this).is('disabled') && idx(this).val() !== '')
        {
            //prepend to the front of the form
            // idx(this).closest("form").prepend(this) ;
            idx(this).attr('disabled','disabled');
            idx(this).closest("form").prepend('<input class="IDX-reorderedHiddenField" type="hidden" name="'+this.name+'" value="'+this.value+'" />');
        }
    });
}

function searchReset(e) {
    e.preventDefault();
    idxSearchForm.resetForm();
    select2Clear();
    cczToggle('reset');
}

function select2Clear()
{
    // reset the select2 fields as well
    if (idx().select2)
    {
        idx('.IDX-quickList').each(function(k,v) {
            idx(this).select2("val","");
        });
    }
}

function searchClear()
{
    // start fresh
    idxSearchForm.clearForm();
    select2Clear();

    /**
     * Make single select inputs be defaulted
     * to the first option at least
     * since per (results per page) field can not be cached as it invalidates cache everytime it is changed
     * we should not select the first option, instead just select the pre-selected one by default (which is 10)
     */
    idxSearchForm.find('select').each(function(k, v) {
       if ( this.type === 'select-one' && this.name !== 'per' ) {
            idx(this).find('option:first').attr('selected','selected');
       } else if (this.name === 'per') {
            /**
             * firefox fails to select the pre-selected option by default on page reload (twig already set the default to 10 when creating the field)
             * this line of code causes the 10 (default value) to be selected
             */
           idx('#IDX-per option[value="10"]').attr('selected', 'selected');
       }
    });
}

/**
 * when the ccz (city/county/zip) list display changes
 * things happening in terms of hiding and displaying elements
 */
function cczToggle(init) {

    if (!idx('.IDX-cczList').size())
        return false;

    var ccz;

    if (init === true)
    {
        var match = idx('.IDX-cczList.IDX-active').attr('id').match(/IDX-(city|county|zipcode)-group/);
        ccz = match[1];
        idx('.IDX-ccz-radio[value="'+ccz+'"]').prop('checked', true);
    }
    else if (init == 'reset')
    {
        ccz = idx('.IDX-ccz-radio:checked').val();
    }
    else
        ccz = idx(this).val();

    var cczContainer = idx('#IDX-cczContainer');

    if (idx('#IDX-ccz-group').hasClass('IDX-cczListController'))
    {
        cczContainer.find('.IDX-cczListChange, .IDX-cczList').removeClass('IDX-active');
        idx('#IDX-'+ccz+'Change, #IDX-'+ccz+'-group').addClass('IDX-active');
        cczContainer.find('.IDX-selectableListCheckbox:checked').prop('checked',false);
    }
    else
    {
        cczContainer.find('.IDX-cczList').removeClass('IDX-active');
        idx('#IDX-'+ccz+'-group').addClass('IDX-active');
        cczContainer.find('.IDX-cczList select').addClass('IDX-noSubmit');
        idx('#IDX-'+ccz).removeClass('IDX-noSubmit');
    }

    idx('#IDX-ccz-group .IDX-radio-inline').removeClass('IDX-active');
    idx('#IDX-ccz-'+ccz).addClass('IDX-active');
    return true;
}

/**
 * limitTo functionality
 * if none of these inputs are selected, we want on submission to limit to only these values
 * this function adds the class to inputs that should be limited to
 * @param string inputName The name of the input field that needs the limit to class added
 */
function setLimitTo(inputName)
{
    var inputs = idxSearchForm.find(':input[name="'+inputName+'"]');
    inputs.addClass('IDX-limitTo');
    return true;
}

/**
 * removes the limit to class from inputs
 * @param string inputName The name of the input field that needs the limit to class removed
 */
function clearLimitTo(inputName)
{
    var inputs = idxSearchForm.find(':input[name="'+inputName+'"]');
    inputs.removeClass('IDX-limitTo');
    return true;
}

/**
 * when the form is submitted, if a limitTo field has none selected or checked, this
 * function selects or checks them all to limit the search to the values available
 */
function handleLimitTo()
{
    var limitToInputs = idxSearchForm.find('.IDX-limitTo');
    var limitToInputsByName = {};

    idx.each(limitToInputs, function(k, value) {

        /**
         * mobile elements may just be a span or something,
         * so make sure that it's an element by verifying the type
         */
        if (!limitToInputsByName[this.name] && this.type)
            limitToInputsByName[this.name] = this.type;
    });

    idx.each(limitToInputsByName, function(name, type) {

        var elements;
        var elementProp;

        // checkboxes test if checked
        if (type == 'checkbox')
        {
            elements = idxSearchForm.find(':input[name="'+name+'"]');
            elementProp = 'checked';
        }
        // select test if any selected
        else if (type == 'select-multiple')
        {
            elements = idxSearchForm.find(':input[name="'+name+'"] option');
            elementProp = 'selected';
        }

        if (elements.is(':'+elementProp))
        {
            // at least 1 is selected... do nothing
        }
        else
        {
            // none are selected... select them all
            elements.prop(elementProp,true);
        }
    });

    return true;
}

function verifySearchEditing()
{
    var matches = location.search.match(/ssid=(\d+)/);
    if (matches) {
        // save the id for the saved search that we are editing
        editingSavedSearch = matches[1];
        idxSearchForm.find('.IDX-formSubmit').text('Save Search');
    }
}


function submitEditSaveSearch(queryString)
{
    var options = {
        "action": "editSaveSearch",
        "id": editingSavedSearch,
        "queryString": queryString
    };

    idx('.IDX-editSearchLoader').remove();
    idxSearchForm.find('.IDX-formSubmit').after("<div class='IDX-editSearchLoader'><img  src='/images/ajaxLoadSmall.gif' style='display:inline-block; margin: 0 auto;' /> Saving...</div>");

    idx.post(ajaxURL, options, function (data) {

        if (data.error) {
            idx('.IDX-editSearchLoader').html('Could not save search. '+data.message);
            return false;
        }

        idx('.IDX-editSearchLoader').remove();
        window.location.href = data.url;
    },'json');

}

/**
 * IDX Specific Plugin for Submitting Search forms
 */
(function ( idx ) {

    var settings;

    idx.fn.idxSearchSerialize = function ( options ) {

        settings = idx.extend({
            decode: true, // return param string URI decoded
            invalidClass: null, // auto invalidate field with class
            forceSubmit: null, // regardless of value, it needs to be submitted,
            fieldsToBeginning: [] // array of fields to move to the beginning of the form
        }, options);

        var form = this,
            clone = form.clone( true, true ),
            serialized;

        // copy the select and textarea values to the cloned form
        idx.each(form.find('select'), function(i) {
            clone.find('select').eq(i).val( idx(this).val() );
        });
        idx.each(form.find('textarea'), function(i) {
            clone.find('textarea').eq(i).val( idx(this).val() );
        });

        // filter out the empty fields and fields that aren't wanted
        clone.find('input:text, select, textarea').filter(invalidField).remove();

        // Make hidden quick list inputs
        clone.find('.IDX-quickList[type=hidden]').each(function(k, element) {

            // only if there is data
            if (this.value) {
                var el = idx(element);

                // each value gets a hidden input
                idx.each(el.select2("data"), function(key, value) {
                    clone.prepend('<input class="IDX-quickListHiddenField" type="hidden" name="'+el.attr('name')+'[]" value="'+value.id+'" />');
                });
            }

            // remove the original quick list element from the clone
            idx(this).remove();
        });

        // do we need to reorder some fields?
        if (settings.fieldsToBeginning.length)
        {
            // put the fields to the beggining of the form
            idx.each( settings.fieldsToBeginning.reverse(), function(k, name) {
                clone.find(':input[name="'+name+'"]').prependTo(clone);
            });
        }

        // serialize it
        serialized = clone.serialize();

        // return it
        if ( settings.decode )
            return decodeURI( serialized );
        else
            return serialized;
    }

    /**
     * determine if the field is invalid for submission or not
     * @return bool true = is invalid so it wont be submitted, false = not invalid so it will be submited
     */
    function invalidField() {

        // regardless of value, we want to submit a field with the following class
        if (settings.forceSubmit && idx(this).hasClass( settings.forceSubmit ))
            return false;

        // input has to have a value
        if ( ! this.value )
            return true;

        // if there's an invalid class to check for an this has it, invalidate it
        if (settings.invalidClass && idx(this).hasClass( settings.invalidClass ))
            return true;

        return false;
    }

})( idx );
/* global form js *//* global form js */
			</script>

