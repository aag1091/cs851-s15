		<title></title>
								<meta http-equiv="refresh" content="2;url=https://itunes.apple.com/us/album/nobody-love-single/id961842586?ls=1&amp;at=11lc99&amp;ct=8a0utj"/>	
			<script>
				var destinationUrl = "https://itunes.apple.com/us/album/nobody-love-single/id961842586?ls=1&at=11lc99&ct=8a0utj";
		var numRedirects = 0;
		function LinkDecorator() {};
		LinkDecorator.prototype.decorate = function(url) {
						return url;
		}
		var linkDecorator = new LinkDecorator();
				var numAsyncTasks = 2;
					  function onAsyncTaskComplete() {
		numAsyncTasks--;
			    if (numAsyncTasks <= 0) {
	    	onAllAsyncTasksComplete();
	    }
	   }
		function makePixel(url) {
						var img = document.createElement("img");
			img.onload = function() {
								onAsyncTaskComplete();
			}
			img.src = url;
		}
	   		function onAllAsyncTasksComplete() {
			if (numRedirects == 0) {
								numRedirects++;
								window.location = linkDecorator.decorate(destinationUrl);
							}
		}
	</script>		
	</head>
	<body>
		<script type="text/javascript">
						makePixel("\/\/googleads.g.doubleclick.net\/pagead\/viewthroughconversion\/987848193\/?value=1.00\x26currency_code=USD\x26label=uAtYCLaQrVkQgbyF1wM\x26guid=ON\x26script=0");
									makePixel("https:\/\/www.facebook.com\/tr?id=1387974504779085\x26ev=smartURLClick\x26cd[uuid]=bc05562a\-65a0\-4eee\-8b24\-98d00252af66");
												window.setTimeout(onAllAsyncTasksComplete, 2000);
					</script>
