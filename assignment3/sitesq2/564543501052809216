	var is_mobile = 0;
	
	var dart_sitename = 'espn.cricinfo.news';

	var sz = '970x250,970x66,924x50,728x90,1x1';

	var kvpos = '';
	var sp = 'cricinfo';

	for(i=0; i < devicenames.length; i++){
		var device_str = devicenames[i].toLowerCase();
		if(user_agent.indexOf(device_str) > -1){
			is_mobile = 1;
		
			dart_sitename = 'espn.cricinfo.news';
			sz = '320x50';
			kvpos = 'top';
			sp = 'cricinfomobile';
			break;
		}
	}
	
