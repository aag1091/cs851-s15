				</div><div class="ad-slot show-mobile">
<script type="text/javascript" language="JavaScript">
	var user_agent = navigator.userAgent.toLowerCase();
	var is_mobile = 0;
	
	var dart_sitename = '';
	var sz = '';
	var kvpos = '';
	var sp = '';

	for(i=0; i < devicenames.length; i++){
		var device_str = devicenames[i].toLowerCase();
		if(user_agent.indexOf(device_str) > -1){
			is_mobile = 1;
		
			dart_sitename = 'espn.cricinfo.news';
			sz = '300x250';
			kvpos = 'top';
			sp = 'cricinfomobile';
			break;
		}
	}
	if(is_mobile === 1){
	
