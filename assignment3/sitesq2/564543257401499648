var glb_ActiveSection='';
var tickerRefreshInterval = '30000'
function setMasterPageActiveSection(elementid)
{
 // 
 if(glb_ActiveSection=='')
 {
    glb_ActiveSection=elementid;
 }   
    //$("#"+ elid).
   // $("#subnav").find("li").find("a").removeClass("currentSection");
    $("#"+elementid).addClass("currentSection");
}
function ResetMasterPageActiveSection(elementid)
{
   
    if(glb_ActiveSection!=elementid)
    { 
        $("#"+elementid).removeClass("currentSection");
    }
}

/* This is for Mac machine */
var mactest=navigator.userAgent.indexOf("Mac")!=-1
if(mactest==true)
{
    document.write('<link rel="stylesheet" type="text/css" href="../css/macStyles.css" />');
}    


function SuggestionClick()
{
  
     window.location.href='../suggestions.aspx?ret='+  replacecharacter(replacecharacter(window.location.href,'#','%23'),'&','%26');
}
function CallDummy()
{}

sfHover = function() {
	var sfEls = document.getElementById("internal-top-nav").getElementsByTagName("LI");
	for (var i=0; i<sfEls.length; i++) {
		sfEls[i].onmouseover=function() {
			this.className+=" sfhover";
		}
		sfEls[i].onmouseout=function() {
			this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
		}
	}
}
if (window.attachEvent) window.attachEvent("onload", sfHover);


//START Function to get elements by class name - iterates through all tag elements on the page
            function getElementsByClassName( strClassName, obj ) {
                var ar = arguments[2] || new Array();
                var re = new RegExp("\\b" + strClassName + "\\b", "g");

                if ( re.test(obj.className) ) {
                    ar.push( obj );
                }
                for ( var aa = 0; aa < obj.childNodes.length; aa++ )
                    getElementsByClassName( strClassName, obj.childNodes[aa], ar );
                
                return ar;
            }
            //END Function to get elements by class name - iterates through all tag elements on the page

function updateurl(anchor_obj) //Mantis Issue 4153
{   
    if($.browser.msie)
    {
        if(window.location.href!=String(anchor_obj))
        {
            var arr_href=window.location.href.split("#");
            var arr_anchor=String(anchor_obj).split("#");
            if(arr_href[0]==arr_anchor[0])
            {
                location.reload(true);
            }
        }
    }
}

function redirect()
{
   var url="http://"+window.location.href.split("/")[2] + "/"; 
   self.parent.location= url + "schedules/realtimedepartures.aspx#enable=stopidlookup";
   window.document.url=url + "schedules/realtimedepartures.aspx#enable=stopidlookup";
   //location.reload(true);
    document.form2.action =url + "schedules/realtimedepartures.aspx#enable=stopidlookup"; // July 13th 2010
}
