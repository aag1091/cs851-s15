         <script language="javascript" type="text/javascript">

            //START Set the main nav elements to get the active treatment if the URLs match
            url = document.location.href;
            //alert(url);
            var aryClassElements = getElementsByClassName( 'hilite', document.body );
            for ( var bb = 0; bb < aryClassElements.length; bb++ ) {
                if ((url.indexOf('departuretimestexting') > -1) || (url.indexOf('realtimedepartures') > -1) || (url.indexOf('stopidlookup') > -1) || (url.indexOf('DepartureTimesFAQ') > -1)){
                //if (url.indexOf('realtimedepartures') > -1) {
		            aryClassElements[2].className = 'active';
		            break;
	            }
	            if (url.indexOf('schedules') > -1) {
		            aryClassElements[3].className = 'active';
	            }
	            if (url.indexOf('destinations') > -1) {
		            aryClassElements[4].className = 'active';
		        }
                /* Mantis:6361 - Start */ 
	            if (url.indexOf('announcements') > -1) {
		            aryClassElements[4].className = 'active';
		         }
		        /* Mantis:6361 - End */
	            if (url.indexOf('disable') > -1) {
		            aryClassElements[4].className = 'active';
	            }
            	
            }
            //END Set the main nav elements to get the active treatment if the URLs match

            
        </script>
