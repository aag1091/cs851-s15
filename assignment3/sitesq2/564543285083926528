            type:"GET",
            dataType:"jsonp",
            context:$("#iframe-block"),
            success:function(data){ 
            	            		var showFrame = 1;
            	
                if(data.code=='200' && data.isGuest == '0')
                { 	
                	$(this).attr('src',"/blogtoolbar.php?username="+data.username+"&logo="+data.logo);
