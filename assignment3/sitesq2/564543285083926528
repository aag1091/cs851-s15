<script type="text/javascript">
	jQuery(function($) {
		$("#blogShareLink").click(function(){
			$("#blogBox").toggle();
		});

		var isLogin = $("body").data("isLogin");
							
		if(typeof(isLogin) == "undefined")
		{
			$.ajax({
	            url:"http://www.podbean.com/site/user/loginInfo",
	            type:"GET",
	            dataType:"jsonp",
	            success:function(data){ 
                if(data.code=="200" && data.isGuest == "0")
                { 	
                	bindFollow();
                	$("body").data("isLogin",1);
                }
                else
                	$("body").data("isLogin",0);
	            }
	        });
		}
		else if(isLogin =="1")
		{
			bindFollow();
		}

		function bindFollow()
		{
			$.ajax({
		     url:"http://www.podbean.com/site/userCenter/getFollowStatus",
		     type:"GET",
		     data:"id=656928&type=pbblog",
		     dataType:"jsonp",
		     context:$("#follow"),
		     success:function(data){  
		      
		      if(data.code=="200"&&data.followStatus=="2")
		      {
		      	 $(this).click(function(event) {
		            alert("You don't need to follow yourself.");
		            return false;
		         });
			  } 
		      else if(data.code=="200")
		      {
		       if(data.followStatus==1)
		       {
		        $(this).addClass("followed");
		        $(this).text("Following");
		       } 

		       $(this).click(function(){
		        var node = $(this);
		        if(node.hasClass("followed"))
		              { 
		                $.ajax({
		                    url: "http://www.podbean.com/site/userCenter/follow",
		                    type:"GET",
		                    data:"id=656928&type=pbblog&action=unfollow",
		                    context:node,
		                    dataType:"jsonp",
		                    success: function(data){
		                      if(data.code == 200)
		                      { 
		                       $(this).removeClass("followed");
		                          $(this).addClass("unfollow");
		                          $(this).text("Follow");
		                      }
		                    }
		                });
		            } 
		             else
		             {
		                $.ajax({
		                    url: "http://www.podbean.com/site/userCenter/follow",
		                    type:"GET",
		                    data:"id=656928&type=pbblog&action=follow",
		                    context:node,
		                    dataType:"jsonp",
		                    success: function(data){
		                      if(data.code == 200)
		                      {
		                       $(this).removeClass("unfollow");
		                          $(this).addClass("followed");
		                          $(this).text("Following");
		                      }
		                    }
		                });
		              }
		                return false;
		       });
		       
		      }
		     }
            });
		}
		
	});
</script>


