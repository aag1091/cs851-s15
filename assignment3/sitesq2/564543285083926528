<script>

	var isMobile = false;
	$(document).ready(function() {
		
		playerLayout();
		$('.aplayer-panel').on("click",showIframe);

		if(navigator.userAgent.search(/iphone/i)!=-1||navigator.userAgent.search(/ipad/i)!=-1
			||navigator.userAgent.search(/version\/([\d.]+).*safari/i)!=-1
