		<script type="text/javascript">
			jQuery(function($) {
				
				$.ajax({
		            url:"http://www.podbean.com/site/userCenter/getEpisodeLikeStatus",
		            type:"GET",
					data:{"episodes":["5539490","5512927","5481447","5465196","5446164","5435586","5403525","5392858","5376421","5374031"]},       
		            dataType:"jsonp",
		            success:function(response){
					    $.each(response.data, function(i, n){
						  var linkText = "";
						  var idText = "episode-like-" +n.id;
						  
						  if(n.status == 1)
						  {
						  	linkText  = "Liked";
						  	likeClass = "liked";
						  }
						  else
						  {
						  	linkText  = "Like";
						  	likeClass = "unlike";
						  }	
							if(n.count > 0){linkText += "(" + n.count + ")";}

						  var content = "<a href=\"http://www.podbean.com/site/user/login?return=http%3A%2F%2Ftototv.podbean.com%2F\" id=" + idText + "  class=\"post_toolbar_like "+ likeClass +"\"  data=" + n.count + " >"+ linkText +"</a><span class=divider> | </span>";

						  $("#postbar_"+n.id).prepend(content);
						
						  if(!response.isGuest)
						  {
						  	$("#episode-like-"+ n.id).click(function(){
			          		  if($(this).hasClass("liked"))
					          { 
					              $.ajax({
					                  url: "http://www.podbean.com/site/userCenter/like",
					                  type:"GET",
					                  data:"id="+ n.id +"&type=pb&action=dislike",
					                  context:$(this),
					                  dataType:"jsonp",
					                  success: function(data){
					                    if(data.code == 200)
					                    { 
					                        $(this).removeClass("liked");
					                        $(this).addClass("unlike");
											var count = parseInt($(this).attr("data"))-1;

											if(count == 0)
												$(this).text("Like");
											else
												$(this).text("Like("+count+")");
											
											$(this).attr("data",count);
					                    }
					                  }
					              });
					          }   
					          else
					          {
					              $.ajax({
					                  url: "http://www.podbean.com/site/userCenter/like",
					                  type:"GET",
					                  data:"id="+ n.id +"&type=pb&action=like",
					                  context:$(this),
					                  dataType:"jsonp",
					                  success: function(data){
					                    if(data.code == 200)
					                    {
					                        $(this).removeClass("unlike");
					                        $(this).addClass("liked");
					                        var count = parseInt($(this).attr("data"))+1;
					                        $(this).text("Liked("+count+")");
					                    	$(this).attr("data",count);
					                    	
										}
					                  }
					              });
					          }
							  
							  return false;

					        });
						  }
						});
		            }
			    });

				$.ajax({
					url: "http://www.podbean.com/api2/public/filesPlays",
					type:"GET",
					data:{"blogId":"656928","query":[{"file":"AGalAGayEp65.m4a","w":"a160da58"},{"file":"AGalAGayEp64-2_27_15707PM.m4a","w":"9bb211da"},{"file":"AGalAGayEp63-2_6_15914PM.m4a","w":"7d8774c6"},{"file":"AGalGayEp62-1_26_15757PM.m4a","w":"a85f1583"},{"file":"AGalAGayEp61-1_12_15905PM.m4a","w":"721d6a4e"},{"file":"AGalAGayEp60-1_4_15612PM.m4a","w":"c2f125de"},{"file":"AGalAGayEp59.m4a","w":"08fb77e3"},{"file":"AGalAGayEp58.mp3","w":"88f2cc8f"},{"file":"agalandagayep57.mp3","w":"e5c6f9e7"},{"file":"AGalAGayEp56.mp3","w":"c3771eca"}]}, 
					dataType:"json",      
		            success: function(result) {
						if(result.code =="200")
						{
							$.each(result.data, function(id,count){
								id = id.replace(/\./g,"_");
								if(count >0)
								{
									$(".hits_"+id).text(count);	
								}
								else
								{
									$(".hits_"+id).parent("a").prev("span").remove();
									$(".hits_"+id).parent("a").remove();	
								}	
							});							
						}
					}
				});
			});
		</script>
