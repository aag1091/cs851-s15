			function streamTimeoutProgress(progVal){
			
				var thisRem = parseInt((disconnectCounter-progVal)/1000);
				if (thisRem >= 0) {
					$("#sTO_remTime").html((thisRem==0?', please wait...':' in '+thisRem+' second'+(thisRem==1?'':'s')));
