	function videoEmbed (url, auto, height) {
		if (typeof height == "undefined") height = 303;
		$("#vidembed").css('height', height+'px');
		var params = auto ? '?autoplay=1' : '';
		var embed = '<iframe src="'+url+params+'" width="480" height="'+height+'" frameborder="0" allowfullscreen></iframe>';
		$("#vidembed").html(embed);
		return false;
	}
