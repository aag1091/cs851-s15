<script type="text/javascript" language="JavaScript">
function exLink(l){
    //If the link is not passed Inline, then l must be an event...
    //If so, let's find the link that fired the event
    if(typeof(l.href)=='undefined'){
        l = l.srcElement ? l.srcElement : l.target;
    }
    var linkText = l.innerHTML;
    l.target='_blank';
    var link = "http://www.careerbuilder.com/applyonline/externallinkapply.aspx?useframes=False&Job_DID=J3F45W6BQK9T5V53Z1N&companyname=Holiday+Inn-+Independently+Owned+%26+Operated&aourl="+escape(l.href);
    l.innerHTML=linkText;
    l.href=link;
    return false;
}
function openInNewWindow(url) 
{ 
    var openedWin = window.open(url, 'exLinkWindow'); 
    openedWin.focus(); 
    return false;
}
</script>





            
            
