			function myOrientResizeFunction(){
				jQuery(".panel .items").css("width", ((jQuery(".panel").width() / 4 ) - (jQuery(".items").outerWidth(true) - jQuery(".items").width())) + "px");
			}
			//bind to resize
			jQuery(window).resize(function() { myOrientResizeFunction() });
			//if you need to call it at page load to resize elements etc.
			jQuery(window).load(function() { myOrientResizeFunction() });
			//check for the orientation event and bind accordingly
			if (window.DeviceOrientationEvent) { window.addEventListener('orientationchange', myOrientResizeFunction, false);}		
		});
