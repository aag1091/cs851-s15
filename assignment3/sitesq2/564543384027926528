			function getLangEncoding() {
				var langEncoding;
				if (navigator.userLanguage) {
					langEncoding = navigator.userLanguage.toLowerCase();
				} else if (navigator.browserLanguage) {
					langEncoding = navigator.browserLanguage.toLowerCase();
				} else if (navigator.language) {
					langEncoding = navigator.language.toLowerCase();
				} else if (navigator.systemLanguage) {
					langEncoding = navigator.systemLanguage.toLowerCase();
				}
	
				return langEncoding;
			}
		</script><script type="text/javascript">
