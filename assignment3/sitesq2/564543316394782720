<script type="text/javascript">
   
      $(function() {
              $(window).scroll(function() {
                      if($(this).scrollTop() != 0) {
                              $('#toTop').fadeIn();
                      } else {
                              $('#toTop').fadeOut();
                      }
              });

              $('#toTop').click(function() {
                      $('body,html').animate({scrollTop:0},800);
              });
              /*btn visitor*/
			  $('#btn-visitor').on('click', function (event) {
					window.location = $(this).find('a').attr('href');
					event.preventDefault();
				});
			  /*Video Js Player*/
			 var myPlayer = videojs('example_video_1',{"techOrder":["youtube","html5"], "src":"http://www.youtube.com/watch?v=IXmV9JR8ocQ&authuser=0&showinfo=0&controls=0&vq=hd720", "ytcontrols":false});
				
             $("#btn-learn").click(function(){
				
				$("#videoContainer").height($(window).height()-30);
				
				$("#close").css("left",$(window).width()- 50);
				$("#close").css("top",-1 * ($(window).height())+ 100);			
				myPlayer.ready(function(){
				   myPlayer.play();
				   /*myPlayer.on("ended", function(){
						myPlayer.bigPlayButton.hide();
				   });*/
				   //return false;
				});			
						
				$("#videoContainer").show();
				return false;
			});
			$("#close").click(function(){				 
				myPlayer.pause();	
				myPlayer.currentTime(0);
				myPlayer.load();		
				myPlayer = undefined;
				$("#videoContainer").hide();				
			});
      });
     
      var ta;
      var tb;
      var delaya = 2500;
      var delayb = 2500;
      var vscrlla = 'stop';
      var vscrllb = 'stop';
      var numCardsDownloaded = 10;
      var numCardsToShow = 5;
      var nextToShowa = numCardsDownloaded - 1;
      var nextToShowb = numCardsDownloaded - 1;
      var fromfile='';
      var load_images = 0;

      function animateFeeda() {
                toHidea = (nextToShowa + numCardsToShow) % numCardsDownloaded;
        $('#activitya'+nextToShowa).remove().prependTo('#activity-list-a').slideDown(1000);
        $('#activitya'+toHidea).slideUp(1000);
                nextToShowa = (nextToShowa + numCardsDownloaded - 1) % numCardsDownloaded;
        ta=setTimeout('animateFeeda()', (delaya*(1+Math.random())));
      }

      function animateFeedb() {
                toHideb = (nextToShowb + numCardsToShow) % numCardsDownloaded;
        $('#activityb'+nextToShowb).remove().prependTo('#activity-list-b').slideDown(1000);
        $('#activityb'+toHideb).slideUp(1000);
                nextToShowb = (nextToShowb + numCardsDownloaded - 1) % numCardsDownloaded;
        tb=setTimeout('animateFeedb()', (delayb*(1+Math.random())));
      }

      function ssscrl(str)
      {
        if (str=='a')
        {
          if(vscrlla=='stop')
          {
           clearTimeout(ta);
           $("#scrlha").html('<img src="images/play.png" border="0"/>');
           vscrlla='start'
          }
          else
          {
           ta=setTimeout('animateFeeda()', (delaya*(1+Math.random())));
           $("#scrlha").html('<img src="images/stop.png" border="0"/>');
           vscrlla='stop'
          }
        }
        else
        {
          if(vscrllb=='stop')
          {
           clearTimeout(tb);
           $("#scrlhb").html('<img src="images/play.png" border="0"/>');
           vscrllb='start'
          }
          else
          {
           tb=setTimeout('animateFeedb()', (delayb*(1+Math.random())));
           $("#scrlhb").html('<img src="images/stop.png" border="0"/>');
           vscrllb='stop'
          }
        }
      }

      function wresize(){
          var w = $(document).width() / 2;
          w += 490;
          $('#toTop').css({'left':w+'px'});
      }

      $(document).ready(function() {
         
        $('#remote-bt').click(function () {
                fromfile=$(this).attr('name');
                dWindow(false, {
                        ajaxUrl: $(this).attr('href'),
                        cacheAjaxResult: true,
                        wrapperId: 'log-in-window',
                        minHeight: 405,
                        minWidth: 500
                });
                return false;
        });
/*        ta=setTimeout('animateFeeda()', (delaya*(1+Math.random())));
        tb=setTimeout('animateFeedb()', (delayb*(1+Math.random())));

        $('.pic_frame').load(function(){
           load_images=load_images+1;
           if (load_images==9){
              ta=setTimeout('animateFeeda()', (delaya*(1+Math.random())));
              tb=setTimeout('animateFeedb()', (delayb*(1+Math.random())));
          }
        });
		
		 
*/
		
	

        wresize();
      });
function startframes()
{
              ta=setTimeout('animateFeeda()', (delaya*(1+Math.random())));
              tb=setTimeout('animateFeedb()', (delayb*(1+Math.random())));
}
function guidedtour(step)
{
    var nstep=step+1;
    $.ajax({type:"POST",
      dataType:"json",
      url:"fileaction.php",
      async:false,
      data: {opt:"guidedtour",
             step:step
            },
          success:function(data){
            $("#tdresult").html(data.vform);
            tb_show( '<b> Guided Tour </b>', '#TB_inline?height=570&amp;width=800&amp;inlineId=viewresu', null);
          }
    });

    if(nstep==8)
    {
    $('#toTop2').html('<b>Close</b>');
    $('#toTop2').click(function() {
            tb_remove();
    });
    } else {
    $('#toTop2').click(function() {
            guidedtour(nstep);
    });
    }
}
function popitup(url){
              newwindow=window.open(url,'name','height=400,width=500');
              if (window.focus) {newwindow.focus()}
              return false;
      }
