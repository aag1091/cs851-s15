window.fbAsyncInit = function() { 
	FB.init({appId: '34613858448',
			status: true, 
			cookie: true, 
			xfbml: true, 
			channelUrl:'//www.bitstrips.com/channel.php',
			oauth:true}); 
	if (typeof fb_auth_change !== 'undefined') {
		FB.Event.subscribe('auth.authResponseChange', fb_auth_change);
	}
	if (typeof fb_login !== 'undefined') {
		FB.Event.subscribe('auth.login', fb_login);
	}
	if (typeof fb_logout !== 'undefined') {
		FB.Event.subscribe('auth.logout', fb_logout);
	}
	if (typeof fb_like!== 'undefined') {
		FB.Event.subscribe('edge.create', fb_like);
	}
	FB.Canvas.setAutoGrow();
	if (typeof resized !== 'undefined') resized();
	if (typeof permissions_callback !== 'undefined') {
		FB.getLoginStatus(function () {
			$(document).ready(fb_logged_in);
			scroll_to(0,0);
		});
	}
	
		
};
