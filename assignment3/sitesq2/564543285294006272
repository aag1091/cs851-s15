function makeavoice_jwplayer() {
	var mav_jwplayer=document.getElementById("mav_jwplayer");
	if (mav_jwplayer) {
		jwplayer('mav_jwplayer').setup({
		'id': 'playerID',
		'flashplayer': 'http://recradio.gr/stream-codes/jw-player-2013/player.swf',
		'duration': '99999',
		'file': 'http://livestream2.recradio.gr:8100/;stream.mp3',
		'type': 'sound',
		'width': '100%',
		'height': '38',
		'autostart': 'true',
		'image': 'http://recradio.gr/pictures/REC-fm-1040.png',
		'controlbar': 'bottom',
		'skin': 'http://recradio.gr/stream-codes/jw-player-2013/chrome.zip'
		});
	}
}
makeavoice_jwplayer();
