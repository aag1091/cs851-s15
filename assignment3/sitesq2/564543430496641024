document.write('<div class="mergetoc_blk"><h2>' + $tfs.CurrentSite.CurrentPromotion.Promo1Text.replace(/@CurrentSite\.Institution\.Name/g, institution).replace(/@CurrentSite\.Name/g, $tfs.CurrentSite.Name) + '</h2><a class="btn skn3" href="https://secure.247sports.com/UserSubscription/New?promo=FSTOC&skey=' + $tfs.CurrentSite.Key + '">' + $tfs.CurrentSite.CurrentPromotion.ButtonText + '</a></div>');
}
