            <li data-ng-if="!$first" data-ng-repeat="bulletin in bulletins">
                <h2 class="wx-alert-text {{getAlertCss(bulletin)}}" data-ng-attr-style="z-index: {{bulletins.length - $index}}">
                    <a data-ng-href="{{bulletin.url}}" class="wx-alert-link" data-from-str="{{modulefromstring}}_link_{{$index+1}}_{{bulletin.fromStr}}" data-ng-click="toggleShowMenu(false)">
                        <span class="wx-alert-description" data-ng-bind="bulletin.description"></span>
                        <span class="wx-alert-time" data-ng-show="bulletin.validStartTime"
