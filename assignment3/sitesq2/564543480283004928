  <script type="text/javascript">

	$(document).ready(function() {
		var delayAmount=100;
		$("#menu-1").hover(
		  function () {
			$("#hidden-4").delay(delayAmount).fadeIn();
			$("#menu-1").addClass("on");
		  },
		  function () {
			$("#hidden-4").hide();
			$("#hidden-4").clearQueue();
			$("#menu-1").removeClass("on");
		  }
		);

		$("#menu-2").hover(
		  function () {
			$("#hidden-5").delay(delayAmount).fadeIn();
			$("#menu-2").addClass("on");
		  },
		  function () {
			$("#hidden-5").hide();
			$("#hidden-5").clearQueue();
			$("#menu-2").removeClass("on");
		  }
		);

		$("#menu-3").hover(
		  function () {
			$("#hidden-6").delay(delayAmount).fadeIn();
			$("#menu-3").addClass("on");
		  },
		  function () {
			$("#hidden-6").hide();
			$("#hidden-6").clearQueue();
			$("#menu-3").removeClass("on");
		  }
		);

		$("#menu-4").hover(
		  function () {
			$("#hidden-7").delay(delayAmount).fadeIn();
			$("#menu-4").addClass("on");
		  },
		  function () {
			$("#hidden-7").hide();
			$("#hidden-7").clearQueue();
			$("#menu-4").removeClass("on");
		  }
		);
		$("#menu-5").hover(
		  function () {
			$("#hidden-1").delay(delayAmount).fadeIn();
			$("#menu-5").addClass("on");
		  },
		  function () {
			$("#hidden-1").hide();
			$("#hidden-1").clearQueue();
			$("#menu-5").removeClass("on");
		  }
		);

		$("#menu-6").hover(
		  function () {
			$("#hidden-2").delay(delayAmount).fadeIn();
			$("#menu-6").addClass("on");
		  },
		  function () {
			$("#hidden-2").hide();
			$("#hidden-2").clearQueue();
			$("#menu-6").removeClass("on");
		  }
		);

		$("#menu-7").hover(
		  function () {
			$("#hidden-3").delay(delayAmount).fadeIn();
			$("#menu-7").addClass("on");
		  },
		  function () {
			$("#hidden-3").hide();
			$("#hidden-3").clearQueue();
			$("#menu-7").removeClass("on");
		  }
		);

		$("#menu-8").hover(
		  function () {
			$("#hidden-3").delay(delayAmount).fadeIn();
			$("#menu-8").addClass("on");
		  },
		  function () {
			$("#hidden-3").hide();
			$("#hidden-3").clearQueue();
			$("#menu-8").removeClass("on");
		  }
		);

		$("#hidden-1").hover(
		  function () {
			$("#hidden-1").show();

		  },
		  function () {
			$("#hidden-1").hide();
		  }
		);
		$("#hidden-2").hover(
		  function () {
			$("#hidden-2").show();
		  },
		  function () {
			$("#hidden-2").hide();
		  }
		);
		$("#hidden-3").hover(
		  function () {
			$("#hidden-3").show();
		  },
		  function () {
			$("#hidden-3").hide();
		  }
		);
		$("#hidden-4").hover(
		  function () {
			$("#hidden-4").show();
		  },
		  function () {
			$("#hidden-4").hide();
		  }
		);
		$("#hidden-5").hover(
		  function () {
			$("#hidden-5").show();
		  },
		  function () {
			$("#hidden-5").hide();
		  }
		);
		$("#hidden-6").hover(
		  function () {
			$("#hidden-6").show();
		  },
		  function () {
			$("#hidden-6").hide();
		  }
		);
	  $("#hidden-7").hover(
		  function () {
			$("#hidden-7").show();
		  },
		  function () {
			$("#hidden-7").hide();
		  }
		);
		$("#hidden-8").hover(
		  function () {
			$("#hidden-8").show();
		  },
		  function () {
			$("#hidden-8").hide();
		  }
		);
	});

</script>

