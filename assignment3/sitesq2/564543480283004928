<script type="text/javascript">
	$(document).ready(function() {
		$("#login-link, #login-box").mouseover(function() {
		$("#login-box").show();
		$("#search-box").hide();
		$("#login-link").css('color', '#0A5485');
		return false;
	});
	$("#login-box").mouseout( function() {
		$("#login-box").hide();
		$("#search-box").show();
		$("#login-link").css('color', '#FFFFFF');
		return false;
	});

	});
</script>

