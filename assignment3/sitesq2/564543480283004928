		if (document.cookie.indexOf('upsell_seen') == -1 && document.cookie.indexOf('fp_welcome_yes') != -1 && !showInterstitial) {
			var expire = new Date();
			expire.setTime(1428707906000);
			document.cookie = "upsell_seen=1; expires="+expire.toGMTString()+"; path=/; domain=.financial-planning.com;";
			showInterstitial = "Print Upsell";
			$(document).ready(function() {
