        <script type="text/javascript">

        $(document).ready(function() {

        $('#most-read').click(function() {

				$('#most-read').removeClass("tab-off");
				$('#most-emailed').removeClass("tab-on");

				$('#most-read').addClass("tab-on");
				$('#most-emailed').addClass("tab-off");

				$('#most-read-content').show();
				$('#most-emailed-content').hide();

        });

        $('#most-emailed').click(function() {

				$('#most-read').removeClass("tab-on");
				$('#most-emailed').removeClass("tab-off");

				$('#most-read').addClass("tab-off");
				$('#most-emailed').addClass("tab-on");

				$('#most-read-content').hide();
				$('#most-emailed-content').show();
        });
       });

    </script>

