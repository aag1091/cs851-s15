<script type="text/javascript">
  jQuery(function($) {
    var request, player,
      $song_container = $('.song_spotlight_container')


      var handleSongChanged = function(ev, current){
        var sid = current ? current.id : null, $child
        if(sid && ($child = $('.artist_backpage_songs_container [data-song-id='+sid+']')) && $child.length ){
          var $parent = $child.closest('.artist_backpage_songs_row[data-url]')
          if($parent.length){
            request = $.ajax({
              url: $parent.attr('data-url'),
              type: 'get',
              success: function(html) {
                $song_container.html(html);
                $song_container.effect('highlight');
                $('.artist_backpage_songs_row.active').removeClass('active');
                $parent.addClass('active');
                //$parent.removeClass('loading');
              },
              beforeSend: function() {
                request && request.abort()
              },
              complete: function() {
              }
            }).always(function(){
                request = null
              })
          }
        }
      }

      Reverb.playerReady(function(){
        player = MusicPlayer.getInstance()
        player.bind('song_changed', handleSongChanged)
        Reverb.onPageChanged(function(){
          player.unbind('song_changed', handleSongChanged)
        })
      })



    $('.artist_backpage_songs_container').delegate('.artist_backpage_songs_row', 'click', function(e){

      var $tag = $(e.target.tagName), $self = $(this),
        $play_btn = $self.find("a[data-song-id]")

      if(!$tag.is("li,span,div,.standard_play_button") || $(e.target).closest(".btn_group").length){
        return;
      }

      if($tag.is("li") && $play_btn.length > 0){
        playSongNow( $play_btn.attr("data-song-id") )
      }

      //$self.siblings().removeClass('loading');
      //$self.addClass('loading');

    }).delegate(".btn_group .btn_toggle", "click", function(e) {

        var $parent = $(this).closest(".btn_group")

        if(!$parent.hasClass("expanded")){
          $parent.addClass("expanded")
          elem_to_show = $parent.find("ul:first")
          if ($parent.parents(".btn_group").size() > 0)
            top_menu_elem = $parent.parents(".btn_group").first()
          else
            top_menu_elem = $parent
          side_space = ($j('body').width() - top_menu_elem.offset().left - top_menu_elem.width())
          if (top_menu_elem == $parent){
            if(side_space < 200){
              align_type = "tl-tr"
              align_offset = -4
            }else{
              align_type = "tr-tl"
              align_offset = 4
            }
          }else{
            //nest sub-menus with a slight offset
            if(side_space < 200){
              align_type = "tr-tr"
              align_offset = -40
            }else{
              align_type = "tl-tl"
              align_offset = 40
            }
          }
          elem_to_show.alignTo($parent,align_type,{left:align_offset})

          window.setTimeout(function(){
            $(document.body).bind("click", function handler(e){
              var $target = $(e.target)
              if($target.is(".btn_toggle") && ($target.parents(".btn_group")).toArray().indexOf($parent[0]) != -1 ){
                //
              }else{
                $(this).unbind("click", handler)
                $parent.removeClass("expanded")
              }

            })
          },0)

        }else{
          $parent.removeClass("expanded")
        }


      })

  });
</script>
