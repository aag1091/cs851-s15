                                cb_data_votes = { action: 'cb_vote_counter', nonce: nonce, postid: '5295' },
                                cb_data_score = { action: 'cb_add_user_score', nonce: nonce, cbCurrentVotes: parseInt(cbVoteAmount), cbNewScore: cbFinalX, postid: '5295' };

                            cbVoteOverlay.off('mousemove click mouseleave mouseenter');

                            $.post('http://remedytip.com/wp-admin/admin-ajax.php', cb_data_votes, function(cb_votes) {
