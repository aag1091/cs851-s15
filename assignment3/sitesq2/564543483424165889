                                    var cb_checker = cookie.get('cb_user_rating');

                                    if (!cb_checker) {
                                        var cb_rating_c = '5295';
                                    } else {
                                        var cb_rating_c = cb_checker + ',5295';
                                    }
                                    if ( cookie.enabled() ) {
                                        cookie.set('cb_user_rating', cb_rating_c, { expires: 28, });
                                    }
                                }
                            });

