<script type="text/javascript">
    var playerLocation = "/javascript/jwplayer/player.swf";
    var livePlayerLocation = "/javascript/jwplayer/player.swf"; // "/javascript/jwplayer/ChapelPlayer.swf";
    var skin = "/javascript/jwplayer/Skins/COF.zip";
    var playerImg = "/images/frontend/BroadcastPending.png";
    var delayedPlayerImg = "/images/frontend/BroadcastDelayed.png";
</script>


    <script type="text/javascript">
        var hasPhotos = false;
        var videoUploaded = false;
        var canShowVideo = false;
        var videoInterval;

        $(function () {
            CheckPhotos();
            CheckVideo();

            setInterval(function(){
                $.getJSON("/guests/CeremonyMobile.aspx?PlanID=1098645", {Action: "CanShowVideo"}, function(result){
                    canShowVideo = result;
                });
            }, 30000);

            setInterval(CheckPhotos, 30000);
            videoInterval = setInterval(CheckVideo, 30000);
        });

        function CheckPhotos() {
            if (hasPhotos)
                return;

            $.getJSON("/guests/CeremonyMobile.aspx?PlanID=1098645", { Action: "HasPhotos" }, function (result) {
                if (result.IsSmugMug){
                    $("#smugMugPhotoLink").fadeIn("slow");
                    hasPhotos = true;
                }
                else if(result.HasPhotos) {
                    $("#photoLink").fadeIn("slow");
                    hasPhotos = true;
                }
            });
        }

        function CheckVideo() {
            if (videoUploaded && canShowVideo)
                return;


            $.getJSON("/guests/CeremonyMobile.aspx?PlanID=1098645", { Action: "IsVideoUploaded" }, function (result) {                
                videoUploaded = result.IsUploaded;

                if (videoUploaded && canShowVideo) {                    
                    ShowVideo();
                }
                else
                {
                    HideVideo();
                }
            });
        }

        function ShowVideo() {
            $("#videoBlock").show();
            $("#noVideoPermission").hide();
            $("#purchaseTime").hide();

            var videoURL = "http://cof-wedding-videos.s3.amazonaws.com/wedding/1098645.mp4"

            
            jwplayer('FlashContent').setup({
                'flashplayer': playerLocation,
                'id': 'playerID',                
                'controlbar.position': 'bottom',
                'autostart': 'true',
                'file': videoURL,
                'skin': skin,
                'image': '/images/frontend/Video_Image.png',
                'viral.functions': 'info',
                'viral.onpause': 'false'
            });
        
        }

        function HideVideo() {
            $("#videoBlock").hide();
            var hasPermission = true;
            if (hasPermission && videoUploaded)
            {
                $("#noVideoPermission").hide();
                $("#purchaseTime").show();
            }
            else
            {
                $("#noVideoPermission").show();
                $("#purchaseTime").hide();
            }
        }

        function CheckCode() {
            var code = $("#videoCode").val();
            if (code == "" || isNaN(code))
                alert("You have entered an invalid code, please try again.");
            else
            {
                $.getJSON("/guests/CeremonyMobile.aspx?PlanID=1098645", { Action: "CheckVideoCode", Code: code }, function (result) {

                if (result) {
                    //don't check video any more for this session
                    clearInterval(videoInterval);
                    ShowVideo();
                }
                else
                {
                    HideVideo();
                    alert("You have entered an invalid code, please try again or contact a wedding planner at the number below.");
                }
                });
            }
        }

        function purchaseCode() {
            var salesOrderID = $("#SalesOrderID").val();
            var videoProduct = $("#videoProduct").val();
            
            if (videoProduct == 40)
                alert("You will be redirected to the checkout page.\r\nAfter completing the checkout process you will be given a code to view the wedding.");
            else
                alert("You will be redirected to the checkout page.\r\nAfter completing the checkout process you can return to this page to view the wedding.");
                      

            $.post("/Guests/Orders/Cart.aspx", {
                Action: "AddVideoCode",
                SalesOrderID: salesOrderID,
                PlanID: 1098645,
                VideoProduct: videoProduct
            }, function (result) {

                if (!isNaN(result)) {
                    $("#SalesOrderID").val(result);
                    $("#checkOutLink").attr("href","Orders/CheckOut.aspx?SalesOrderID=" + result);
                    $("#checkOutLink").show();
                    window.location = "http://" + window.location.host + "/Guests/Orders/CheckOut.aspx?SalesOrderID=" + result;
                }
            });
            
        }
    </script>
    
