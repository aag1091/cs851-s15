<script type="text/javascript">
    if (top != self) {
        var isSafeTopUrl = false;
        try {
            var topUrl = top.location.href;
            var topUrlQsIdx = topUrl.indexOf('?');
            if (topUrlQsIdx > 0) {
                topUrl = topUrl.substring(0, topUrlQsIdx);
            }
            topUrl = topUrl.toLowerCase();

            var safeTopUrls = new Array(
                'https://communities.vmware.com/skin-basic.jspa',
                'https://communities.vmware.com/skin-advanced.jspa',
                'https://communities.vmware.com/skin-advanced!UploadSkinZip.jspa',
                'https://communities.vmware.com/skin-palette.jspa',
                'https://communities.vmware.com/skin-palette!start.jspa'
            );
            for (var i = 0; i < safeTopUrls.length; i++) {
                if (safeTopUrls[i].toLowerCase() == topUrl) {
                    isSafeTopUrl = true;
                    break;
                }
            }
        } catch (err) {
        }
        if (!isSafeTopUrl) top.location = location;
    }
</script>

