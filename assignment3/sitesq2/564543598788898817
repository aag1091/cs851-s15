(function(Etsy){
  Etsy.Monitoring = {page_group:'view_listing',BW:{enabled:false}};
  Etsy.RequestUUID = 'm8Y2LVT65bGNQ2zDCTyI0w5sD-XY';

  BOOMR = window.BOOMR || {};
