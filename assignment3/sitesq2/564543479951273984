											function goTeamVideoSite(o)
											{
												var v = o.value;
												if (v!=null && v.length>0)
												{
													var url = "http://";
													switch(v)
													{
													case "video":
														url += "video.nhl.com/videocenter";
														break;
													case "avalanche":
													case "blackhawks":
													case "bluejackets":
													case "blues":
