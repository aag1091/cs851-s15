				g_firstVideoPlayed=true;
			
			processDeepLinking();
			
				_console.playVideo("fvod","747131","http://nhl.cdnllnwnl.neulion.net/u/nhlmobile/vod/nhl/2015/02/08/784/pc/2_784_chi_stl_1415_h_quickpick5_1_1600K_16x9.mp4?eid=746592&pid=747131&gid=3007&pt=1","Hossa blasts a power-play goal under the bar","0",false,null,true,"107","4","");
			
			//document.getElementById("console").playVideo("fvodl",48053,"TEST","rtmp://208.78.8.97/neulion_test/s_test_live_game_800_1",1,true,true);
			//document.getElementById("console").playVideo("fvod",48065,"TEST","rtmp://cdncon.fcod.llnwd.net/a277/e4/mp4:u/nhl/svod/flv/2009/2_73_det_buf_0910_20091013_FINAL.mp4",1,true,false);
			//document.getElementById("console").playVideo("fvod",48070,"TEST","http://10.10.90.22/s/nhl/vod/flv/feature4_shawn_horcoff_nhlnet.flv",1,true,false);
		}
		function processDeepLinking()
		{
			if(g_deepLinkHighlight.length>0)
			{
				var td=getMenuItem(Console.MENU_HIGHLIGHTS);
				if (td)
				{
					g_firstVideoPlayed=true;
					onClickMenu(window.event, td);
					setTimeout("_highlight.displayGame('"+g_deepLinkHighlight+"','"+g_deepLinkHighlightEvent+"')",1000);
				}
			}
			else
			{
				var oParams=_util.getURLParams();
				if (oParams.hl!=null)
				{
					var td=getMenuItem(Console.MENU_HIGHLIGHTS);
					if (td)
						onClickMenu(window.event, td);
				}
				else
				{
					if (oParams.hlg!=null)
					{
						var td=getMenuItem(Console.MENU_HIGHLIGHTS);
						if (td)
						{
							g_firstVideoPlayed=true;
							onClickMenu(window.event, td);
							setTimeout("_highlight.displayGame('"+oParams.hlg+"','"+oParams.event+"')",1000);
						}
					}
					else
					{
						if (oParams.hlp!=null)
						{
							var td=getMenuItem(Console.MENU_HIGHLIGHTS);
							if (td)
							{
								g_firstVideoPlayed=true;
								onClickMenu(window.event, td);
								setTimeout("_highlight.displayPlayer('"+oParams.hlp+"','"+oParams.event+"')",1000);
							}
						}
						else
						{
							deepLink();
						}
					}
				}
			}
		}
		
		function deepLink()
		{
			var defaultId = "747131";
			
				
				
				
					var defaultCatId = "107";
				
			
			var td=getMenuItem(defaultCatId, defaultId);
			if (td)
				onClickMenu(window.event, td);
			else
			{	// Default to first menu category
				var td=getMenuItem("");
				if (td)
					onClickMenu(window.event, td);
			}
		}	
		function iptvVideoComplete()   // Called when video finishes playing
		{
			_console.playNextVideo();
		}
		function unloadPage()
		{
			var tbl=document.getElementById("mainTbl");
			_util.clearMouseEvents(tbl);
		}
		function listenAdEvent(adEvent)
		{
			if(adEvent.type=="adstart")
			{
				setAdMode(true);
			}
			else if(adEvent.type=="adcomplete")
			{
				setAdMode(false);
			}
		}
	</script>
	
</head>
<body onload="init()" onunload="unloadPage()">

	
	
