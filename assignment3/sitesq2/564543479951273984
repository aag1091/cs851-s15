		var _util, _console, _search, _browse, _highlight, _whatsnew, _whatshot, _details, _liveevent, _podcast, _channel, _gameday;
		var g_lang="en", g_site="blackhawks", g_instanceId="1";
		
		
		function listenUnsupported()
		{
			alert("We're sorry, the format of this content is incompatible with your device.");
		}
		
		function init()
		{
			_util = new Util();
			_console = new Console();
			_search = new Search();
			_browse = new Browse("Browse");
			_whatshot = new Browse("WhatsHot",'hotvideos_1');
			_whatsnew = new Browse("WhatsNew",'newvideos',true);
			_details = new Details();
			_highlight = new Highlight();
			_liveevent = new LiveEvent();
			_podcast = new Podcast();
			_channel = new Channel();
			_gameday = new GameDay();
			
			iptvComplete();
		}
		
		var g_vcPlayerInitialized = false;
		function videoCenterPlay(videoId)
		{
			var video = {playlist:[videoId]};
			if(!g_vcPlayerInitialized)
			{
				g_vcPlayerInitialized = true;
				var settings = {instanceId:g_instanceId,containerId:"playerContainer"+g_instanceId,unsupportedCallback:listenUnsupported,endCallback:iptvVideoComplete,adEventCallback:"listenAdEvent"};
				settings.site = g_site;
				settings.lang = g_lang;
				settings.playerType = "n1";
				settings.hasTitleBar = false;
				settings.pageName = "nhl:"+g_lang+":video center:video center landing:section landing page";
				settings.section = "video center";
				settings.section2 = "video center landing";
				settings.hierarchy = "video center,video center landing";
				
				VideoCenter.setup(settings);
			}
			var fwSectionId = null;
			if(g_currentTabType=="maintab")
			{
				if(g_lastChannelIndex.length==0 && document.getElementById("div_Search").style.display!="none")
				{
					video.videoChannel = "search";
					fwSectionId = "search";
				}
				else
				{
					var td = document.getElementById("menuitem_"+g_menuList["m"+g_lastChannelIndex].statusIndex).getElementsByTagName("TD")[0];
					var currentChannelName = (td.innerText||td.textContent);
					currentChannelName = currentChannelName.toLowerCase();
					video.videoChannel = currentChannelName.replace(/[^A-Za-z0-9 ]/g,"").replace(/\s+/g,"_");
					fwSectionId = currentChannelName.replace(/[^A-Za-z0-9]/g,"");
				}
			}
			else
			{
				video.videoChannel = g_currentTabType;
				fwSectionId = g_currentTabType;
			}
			video.fwSectionId = "nhl_";
			if(g_isCanada)
				video.fwSectionId += "ca";
			else if(g_isUSA)
				video.fwSectionId += "us";
			else
				video.fwSectionId += "int";
			video.fwSectionId += "_" + g_site;
			if(g_lang!="en")
				video.fwSectionId += "_" + g_lang;
			video.fwSectionId += "_videocenter_" + fwSectionId;
			if(g_lastChannelId.length>0 && g_lastChannelId.indexOf("-")!=0)
				video.catid = g_lastChannelId;
			var ua = navigator.userAgent.toLowerCase();
			if(ua.indexOf("ipad")!=-1)
				ua = "_tab";
			else if(ua.indexOf("iphone")!=-1)
				ua = "_hh";
			else if(ua.indexOf("android")!=-1)
			{
				if(ua.indexOf("mobile")!=-1)
					ua = "_hh";
				else
					ua = "_tab";
			}
			else if(ua.indexOf("bb10")!=-1)
				ua = "_hh";
			else
				ua = "";
			video.fwSectionId += ua;
			video.forceAutoPlay = true;
			video.autostart = true;
			//video.startTime = Number(document.getElementById("startTime").value);
			VideoCenter.play(video, g_instanceId);
		}
		
