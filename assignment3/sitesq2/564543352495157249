        if (!($.cookie('supress_bline')) && userAgent.indexOf('bline') == -1  && (userAgent.indexOf('iPhone') >= 0 || userAgent.indexOf('iPod') >= 0)) {
            $('#bline_notice').show();
        }
        $('#dismiss_bline_notice').click(function(e) {
            e.preventDefault();
            $('#bline_notice').hide();
