        }
    });

  function log_presence() {
    $.ajax({
      url: '/contest/log_presence/melissa_sucre/',
      type: 'GET',
      complete: schedule_log_presence,
      timeout: 60000,
      dataType: 'json',
      success: function(data) {
        $(".password_warning").html(data.password_warning);
        if ($('#still_video').length >= 1) {
          $('#still_video img:first').attr('src', data.image_url);
        }
        if (!data.can_access) {
          window.location.reload();
        }
        if (data.has_warnings) {
          window.onbeforeunload = function() {};
          window.location = '/accounts/warnings/?next=/melissa_sucre/';
        }
        if (data.is_banned) {
          window.onbeforeunload = function() {};
          window.location = '/';
        }
        var text = "FOLLOWED";
        if (data.total_followed){
           text += " ("+data.online_followed+"/"+data.total_followed+")";  
        }
        $("a.followed").text(text);
        if (!flash_handler.initializer.is_on_private_room) {
            $.mydefchatconn('update_viewer_count', data.total_viewers);
        }
      }
    });
  }
