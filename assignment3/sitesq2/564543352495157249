    <script type="text/javascript">

$.show_login_box = function(){
    $("#login-overlay").show();
    $("#login-box").show();
    $("#id_username").focus();
    $("#id_username").select();
    if ($.mydefchatconn) {
        $.mydefchatconn('disable_html5_video_controls');
    }
}

$.hide_login_box = function(){
    $("#login-overlay").hide();
    $("#login-box").hide();
    if ($.mydefchatconn) {
        $.mydefchatconn('enable_html5_video_controls');
    }
}

$(document).ready(function(){
    $.hide_login_box();
    $("a.login-link").click( function(){
	$.show_login_box();
	return false;
    });
    $("#login-overlay").click(function(){
	$.hide_login_box();
	return false;
    });

    $("#login-box .close").click(function(){
	$.hide_login_box();
    });
    
});
</script>

  

