        <script>
            $(function(){
                var bExitModalShown = false;
                $(document).mousemove(function(e) {
                    if(e.pageY <= 5 && !bExitModalShown)
                    {
                        bExitModalShown = true;
                        $('#exitModal').modal("show");
                    }
                });
                $(".main").onepage_scroll({
                    loop: false,
                    pagination: false,
                    afterMove: onResize
                });
                $(window).resize(onResize);
                $(".scrollDown").click(function(){
                    $('.main').moveDown(1); 
                    //jwplayer().pause(true);
                    return false;
                });
                $(".scrollUp").click(function(){
                    $('.main').moveUp(1); 
                    return false;
                });
                function onResize(){
                    var windowHeight = $(window).height(),
                        windowWidth = $(window).width(),
                        bottomMargin = windowWidth<910 ? 360 : 260,
                        $element = $("#myElement").length > 0 ? $("#myElement") : $(".video-wrap > div"),
                        videoHeight = $element.height(),
                        videoWidth = $element.width(),
                        videoRatio = 9/16,
                        containerWidth = $(".container:first").innerWidth();
                    
                    if(typeof originalVideoHeight == "undefined" || videoHeight>originalVideoHeight ){
                        window.originalVideoHeight = videoHeight;
                        window.originalVideoWidth = videoWidth;
                    }
                    if(windowHeight-videoHeight<bottomMargin){
                        $element.css({
                            height: ""+parseInt(windowHeight-bottomMargin)+"px",
                            width: ""+parseInt((windowHeight-200)/videoRatio)+"px"
                        });
                    }else{
                        if(videoHeight<originalVideoHeight && parseInt((windowHeight-bottomMargin)/videoRatio)<containerWidth){
                            $element.css({
                                height: ""+parseInt(windowHeight-bottomMargin)+"px",
                                width: ""+parseInt((windowHeight-bottomMargin)/videoRatio)+"px"
                            });
                        }else
                            $element.css({
                                height: "auto",
                                width: "100%"
                            });
                    }
                }
                $('[data-toggle="tooltip"]').tooltip();
                $(document).unbind("mousewheel DOMMouseScroll MozMousePixelScroll");
                $('.main').swipeEvents().unbind("swipeDown swipeUp");
                jwplayer().onReady(function(){
                    $(".under-video").fadeIn(1000,onResize);
                });
                $(".rating-wrap a").on("mouseover",function(){
                    var index = $(".rating-wrap a").index(this)+1;
                    $(".rating-wrap a:lt("+index+")").find(".fa").removeClass("fa-star-o").addClass("fa-star");
                });
                $(".rating-wrap a").on("mouseout",function(){
                    $(".rating-wrap a").find(".fa").removeClass("fa-star").addClass("fa-star-o");
                });
            });
        </script>
