<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title></title>
								<meta http-equiv="refresh" content="2;url=https://itunes.apple.com/us/album/trampoline/id950788804?i=950788806&amp;ls=1&amp;at=11lc6Z&amp;ct=aiseh7"/>	
			<script>
				var destinationUrl = "https://itunes.apple.com/us/album/trampoline/id950788804?i=950788806&ls=1&at=11lc6Z&ct=aiseh7";
		var numRedirects = 0;
		function LinkDecorator() {};
		LinkDecorator.prototype.decorate = function(url) {
						return url;
		}
		var linkDecorator = new LinkDecorator();
				var numAsyncTasks = 1;
					  function onAsyncTaskComplete() {
		numAsyncTasks--;
			    if (numAsyncTasks <= 0) {
	    	onAllAsyncTasksComplete();
	    }
	   }
		function makePixel(url) {
						var img = document.createElement("img");
			img.onload = function() {
								onAsyncTaskComplete();
			}
			img.src = url;
		}
	   		function onAllAsyncTasksComplete() {
			if (numRedirects == 0) {
								numRedirects++;
								window.location = linkDecorator.decorate(destinationUrl);
							}
		}
	</script>		
	</head>
	<body>
		<script type="text/javascript">
						makePixel("https:\/\/www.facebook.com\/tr?id=249208988\x26ev=smartURLClick\x26cd[uuid]=034aea5c\-2acf\-4c5b\-bf80\-073a081ede0c");
												window.setTimeout(onAllAsyncTasksComplete, 2000);
					</script>
				<noscript>
					<img height="1" width="1" style="border-style:none;" alt="" src="https://www.facebook.com/tr?id=249208988&amp;ev=smartURLClick&amp;cd[uuid]=034aea5c-2acf-4c5b-bf80-073a081ede0c"/>
				</noscript>
