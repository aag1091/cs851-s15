<script>jQuery(document).ready(function(){floater={floatContainer:null,floatWrapper:null,floatingLike:null,isLikeSafe:null,likeURL:null,isWide:null,firstLikeView:true,stickyStopElement:jQuery('#aol-comments'),init:function(id){if(jQuery('.re_wide_format')[0]){this.isWide=true;}
