if(floater.floatWrapper.height()==0){floater.floatWrapper.height(floater.floatContainer.innerHeight());}
var stickyStop=floater.stickyStopElement.offset().top-floater.floatContainer.height();var stickyStart=floater.floatWrapper.offset().top+floater.floatWrapper.height()+70;var topper=jQuery(window).scrollTop();if(stickyStart<topper){if(stickyStop<topper){floater.belowScreen();}else{floater.offScreen();}}else{floater.onScreen();}});},onScreen:function(){if(this.floatContainer.hasClass('floatingShare')){this.floatContainer.removeClass('floatingShare');}},belowScreen:function(){if(this.floatContainer.hasClass('floatingShare')){this.floatContainer.fadeOut();}},offScreen:function(){if(!this.floatContainer.hasClass('floatingShare')){this.floatContainer.hide().fadeIn().addClass('floatingShare');this.setOffset();}else{this.floatContainer.fadeIn();}
jQuery(window).resize(function(){floater.setOffset();});},setOffset:function(){if(this.isWide==true){var offset=this.wrapperOffset=jQuery('#main').offset().left-138;}else{var offset=this.wrapperOffset=jQuery('#main').offset().left-128;}
if(offset<0){if(this.floatContainer.hasClass('floatingShare')){this.floatContainer.removeClass('floatingShare');}}else{this.floatContainer.css({left:offset});}}}
floater.init("#fltShrBar");});</script>
