<!-- required fields: 'hair':new ValidateSome([{'name':'hair-products','label':'Hair Products'}]),'skin':new ValidateSome([{'name':'skin-products','label':'Face Products'},{'name':'body-products','label':'Body Products'}]),'makeup':new ValidateSome([{'name':'makeup-products','label':'Makeup Products'},{'name':'nail-products','label':'Nail Products'}]) -->
<script type="text/javascript">
    var allureProductFinder=null;
    var productFinderConfig={
        "cookieName":"allure-product-finder",
        "tabControlId":"product-finder-tabs",
        "documentCountId":"-totalDocuments",
        "tabPathComponent":1,
        "solrConfig":{
            "pageSize":0,
            "proxyPath":"/ajax/cached/search-proxy-ajax",
            "searchFieldPrefix":"tag_",
            "facetSearchResults":false,
            "facetFieldPrefix":"facet_",
            "returnZeroCounts":false,
            "fieldInclusionOperator":"+",
            "termBooleanOperator":"OR",
            "allowMultipleRequests":false
        },
        "defaultTabNum":1
    };
    jQuery(document).ready(function(){
        productFinderConfig.validators={'hair':new ValidateSome([{'name':'hair-products','label':'Hair Products'}]),'skin':new ValidateSome([{'name':'skin-products','label':'Face Products'},{'name':'body-products','label':'Body Products'}]),'makeup':new ValidateSome([{'name':'makeup-products','label':'Makeup Products'},{'name':'nail-products','label':'Nail Products'}])};
        allureProductFinder=ProductFinderBuilder(productFinderConfig);
    });
</script>
