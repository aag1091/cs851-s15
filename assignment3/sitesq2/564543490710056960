<script type="text/javascript">
CN.hearstQue.register(function(pageAds){
    var div = null;

    if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_ALL_GLOBAL_EMBEDDEDFORM !== 'undefined') && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById("AMS_ALL_GLOBAL_EMBEDDEDFORM");
        div.innerHTML = "";

        
        jQuery("#AMS_ALL_GLOBAL_EMBEDDEDFORM").html(pageAds.AMS_ALL_GLOBAL_EMBEDDEDFORM.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
        jQuery(div).css({
            visibility: 'visible'
        });
    }

    
});

</script>



        </div>
    


        

