	}

        ad_server.networks.mylikes.get_candidate_ads = function(ad_server) {
            try {
                var chosen_slot, rtb_slot;
                for (var i=0; i < ad_server.js_rtb_slots.length; i++) {
		    rtb_slot = ad_server.js_rtb_slots[i];
                    if (rtb_slot && rtb_slot.network == 'mylikes' && rtb_slot.hasOwnProperty('cpm')) {
			if (rtb_slot.hasOwnProperty('ml_slot') && ad_server.networks.mylikes.house_tracking_map[rtb_slot.position] != rtb_slot.ml_slot) {
			    continue;
			}

			chosen_slot = ad_server.ads_by_slot[rtb_slot.position];
			chosen_slot.push({'network': 'mylikes',
				    'cpm': rtb_slot.cpm,
				    'render': ad_server.networks.mylikes.get_render_func(rtb_slot),
				    });
                    }
                }
            } catch(e) {/*ignore*/ }
        };

        ad_server.networks.amazon = {};
        ad_server.networks.amazon.get_cpm = function(key_num) {
            if (key_num == 13) {
                return 0.3;
            } else  {
                return 6 - (key_num -1) * 0.5;
            }
        };
        ad_server.networks.amazon.get_render_func = function(key, key_width, key_height) {
            var func = function(ad_div_id, position, cpm) {
                try {
                    var ifrm = likes_ad_passbacks.create_iframe(key_width, key_height);
                    var ad_div = document.getElementById(ad_div_id);
                    ad_div.appendChild(ifrm);
                    try {
                        ifrm.onload = function() {
                            ifrm.contentDocument.body.style.margin = "0px";
                            ifrm.contentDocument.body.style.padding = "0px";
                        };
                    } catch(e) {/* ignore */ }
                    window.amznads.renderAd(ifrm.contentDocument, key);
                    if (window.top.likes_tracking) {
                        window.top.likes_tracking.track_event({'type': 'ad_slot_rendered', 'network': 'az_' + key, 'position':position, 'cpm':cpm});
                    }
                } catch(e) {/*ignore*/
                }
            };
            return func;
        };

        ad_server.networks.amazon.process_candidate_ads = function (ad_server) {
            if (window.amznads && window.amznads.hasAds()) {
                var amzn_key_re = /a([0-9]*)x([0-9]*)p([0-9]*)/;
                var available_keys = window.amznads.getKeys();
                var key, matches, key_width, key_height, key_val, chosen_slot, pos;
                for (var i = 0; i < available_keys.length; i = i + 1) {
                    chosen_slot = undefined;
                    pos = undefined;
                    key = available_keys[i];
                    matches = key.match(amzn_key_re);
                    key_width = parseInt(matches[1]);
                    key_height = parseInt(matches[2]);
                    key_val = parseInt(matches[3]);
                    if (key_val == 13) {
                        continue;
                    }
                    if (key_width == 728 && key_height == 90 && !ad_server.ads_by_slot.hasOwnProperty('bottomtall')) {
                        chosen_slot = ad_server.ads_by_slot.top;
                        pos = 'top';
                    }
                    if (key_width == 300 && key_height == 250) {
                        if (ad_server.ads_by_slot.hasOwnProperty('bottomtall')) {
                            chosen_slot = ad_server.ads_by_slot.bottomtall;
                            pos = 'bottomtall';
                        } else {
                            chosen_slot = ad_server.ads_by_slot.right;
                            pos = 'right';
                        }
                    }
                    if (key_width == 320 && key_height == 50) {
                        if (ad_server.ads_by_slot.hasOwnProperty('top') && ad_server.ads_by_slot.hasOwnProperty('bottomtall')) {
                            chosen_slot = ad_server.ads_by_slot.top;
                            pos = 'top';
                        }
                    }
                    if (chosen_slot) {
                        chosen_slot.push({
                            'network': 'amazon',
                            'cpm': ad_server.networks.amazon.get_cpm(key_val),
                            'render': ad_server.networks.amazon.get_render_func(key, key_width, key_height)
                        });
                    }
                }
            }
        };
        ad_server.networks.amazon.get_candidate_ads = function(ad_server) {
            ad_server.network_calls.initiate('amazon', 'top');
            ad_server.network_calls.initiate('amazon', 'right');
            ad_server.network_calls.initiate('amazon', 'bottomtall');
            setTimeout(function() {
                try {
                    ad_server.networks.amazon.ready_task = function() {
                        window.likes_ad_server.networks.amazon.process_candidate_ads(window.likes_ad_server);
                        if (window.top.likes_tracking) {
                            window.top.likes_tracking.track_event({'type': 'amazon_ready', 'keys': amznads.getKeys()});
                        }
                        if (!(window.likes_ad_server.networks.amazon.ready)) {
                            window.likes_ad_server.networks.amazon.ready = true;
                            window.likes_ad_server.network_calls.complete('amazon', 'top');
                            window.likes_ad_server.network_calls.complete('amazon', 'right');
                            window.likes_ad_server.network_calls.complete('amazon', 'bottomtall');
                        }
                    };
                    var scr = document.createElement("script");
                    scr.type = "text/javascript";
                    scr.src = 'http://c.amazon-adsystem.com/aax2/amzn_ads.js';
                    var f = document.getElementsByTagName("script")[0];
                    var call_amzn_api = function() {
                        try {
                            window.amznads.tasks.push(ad_server.networks.amazon.ready_task);
                            window.amznads.getAds('3039', undefined, undefined, 1);
                        }catch(e){
                        }
                    };
                    scr.onload = function() {
                        if (window.amznads) {
                            call_amzn_api();
                        } else {
                            setTimeout(call_amzn_api, 200);
                        }
                    };
                    f.parentNode.insertBefore(scr, f);
                    setTimeout(function() {
                        if (!(window.likes_ad_server.networks.amazon.ready)) {
                            window.likes_ad_server.networks.amazon.ready = true;
                            window.likes_ad_server.network_calls.complete('amazon', 'top');
                        }
                    }, 600);
                } catch (e) { /*ignore*/  }
            }, 0);
        };
        ad_server.networks.pubmatic = {};
        ad_server.networks.pubmatic.ad_blacklist = [{"value": "itunes.apple.com"}, {"value": "glugames.com"}, {"value": "king.com"}, {"value": "lp.metanetwork.com"}, {"value": "seanmlese.com"}, {"value": "basebanner.com"}, {"value": "kongyunbao.com"}, {"value": "merchanta.com"}, {"value": "lowestprices.us"}, {"value": "lowerprices.us"}, {"value": "yoox.com"}, {"value": "carid.com"}, {"value": "groupon.com"}, {"value": "taggify.net"}, {"value": "comcast.com"}, {"value": "hotels.com"}, {"value": "ozonemedia.com"}, {"value": "http://memorablec.com/"}, {"value": "rosettastone.com"}, {"value": "emerse.com"}, {"value": "jnadoso"}, {"value": "lovoo.com"}, {"value": "tiptonhonda.com"}, {"value": "gossipolls.com"}, {"value": "asbank.pw"}, {"value": "wechat"}, {"value": "yatra.base"}, {"value": "wildtangent.it"}, {"value": "mobogenie.com"}, {"value": "kargo.com"}, {"value": "www.moneymakeslifebetter.com"}, {"value": "advanceamerica"}, {"value": "quickrete.com"}, {"value": "newsearchtrack.com"}, {"value": "lyft.com"}, {"value": "autopartswarehouse"}, {"value": "somoaudience.com"}, {"value": "9c5y.com"}, {"value": "adform.com"}, {"value": "iams.com"}, {"value": "mars.com"}, {"value": "zipcar.com"}, {"value": "goodrx.com"}, {"value": "intenzetattooink.com"}, {"value": "ultrasabers.com"}, {"value": "cvs.com"}, {"value": "abercrombie.com"}, {"value": "excedrin.com"}, {"value": "calottery.com"}, {"value": "callwith.me"}];
        ad_server.networks.pubmatic.is_url_blacklisted = function(url, creative, position, call_num, cpm) {
            var blacklisted = false;
            for (var i = 0; i < ad_server.networks.pubmatic.ad_blacklist.length; i++) {
                if (url.match(RegExp(".*" + ad_server.networks.pubmatic.ad_blacklist[i]['value'] + ".*"))) {
                    blacklisted = true;
                        if (window.top.likes_tracking) {
                            window.top.likes_tracking.track_event({'type': 'ad_slot_rendered', 'network': 'pm' + call_num + '_' + ad_server.networks.pubmatic.ad_blacklist[i]['value'], 'position': position, 'cpm': cpm});
                        }
                }
            }
            return blacklisted;
        };
        ad_server.networks.pubmatic.ad_whitelist = [{"value": "att.com"}, {"value": "abdata.com"}, {"value": "www.nazar.se"}, {"value": "www.fidelity.com"}, {"value": "www.safeway.com"}, {"value": "www.homedepot.com"}, {"value": "nexgardfordogs.com"}, {"value": "alot.com"}, {"value": "www.michelin.co.uk"}, {"value": "atvhayward.com"}, {"value": "freshguardclean.com"}, {"value": "www.mcdonalds.com"}, {"value": "walmart.com"}, {"value": "target.com"}, {"value": "bestbuy.com"}, {"value": "betfair.com"}, {"value": "botoxcosmetic.com"}, {"value": "zillow.com"}, {"value": "bausch.com"}, {"value": "stopdenguenow.com"}, {"value": "samsclub.com"}, {"value": "chevrolet.com"}, {"value": "nazar"}, {"value": "newbalance"}, {"value": "dreamfieldssettlement.com"}, {"value": "mediaforce.com"}, {"value": "connectionsfromhallmark.com"}, {"value": "www.coupons.com"}, {"value": "playphone.com"}, {"value": "alot"}, {"value": "att.net"}, {"value": "sears.com"}, {"value": "discovercard.com"}, {"value": "daimlerchrysler.com"}, {"value": "nimaxtheatres"}, {"value": "frontline"}, {"value": "dollargeneral.com"}, {"value": "lowes.com"}, {"value": "nordstromrack"}, {"value": "softcoin.com"}];
        ad_server.networks.pubmatic.is_ad_whitelisted = function(url) {
            var whitelisted = false;
            for (var i = 0; i < ad_server.networks.pubmatic.ad_whitelist.length; i++) {
                if (url.match(RegExp(".*" + ad_server.networks.pubmatic.ad_whitelist[i]['value'] + ".*"))) {
                    whitelisted = true;
                }
            }
            return whitelisted;
        };
        ad_server.networks.pubmatic.rendered_ads = [];
        ad_server.networks.pubmatic.get_candidate_ads = function(ad_server) {
            var helpers = {};
            helpers.get_time = function(d) {
                var t = d ? d : new Date();
                var y = t.getFullYear();
                var m = t.getMonth() + 1;
                var d = t.getDate();
                var h = t.getHours();
                var min = t.getMinutes();
                var s = t.getSeconds();
                var pad = function(i) {
                    return i <= 9 ? '0' + i : '' + i;
                }
                return escape(y + '-' + pad(m) + '-' + pad(d) + ' ' + pad(h) + ':' + pad(min) + ':' + pad(s) );
            };
            helpers.get_timezone_offset = function() {
                var t = new Date();
                return -1 * t.getTimezoneOffset()/60;
            };

            helpers.get_ad_position = function(ad_div_id) {
                try {
                    ad_div = document.getElementById(ad_div_id);
                    if (!ad_div) {
                        return "-1x-1";
                    }
                    var curleft = curtop = 0;
                    var elem = ad_div;
                    if (elem && elem.offsetParent) {
                        do {
                            curleft += elem.offsetLeft;
                            curtop += elem.offsetTop;
                        } while ( elem = elem.offsetParent);
                    }
                    return curtop + 'x' + curleft;
                } catch (e) {
                    return "-1x-1";
                }
            };
            ad_server.networks.pubmatic.helpers = helpers;

            var call_mgr = {};
            call_mgr.construct_position_map = function() {
                call_mgr.position_map = {};
                var slot_info;
                for (var i=0; i < ad_server.js_rtb_slots.length; i++) {
                    slot_info = ad_server.js_rtb_slots[i];
                    if (slot_info.network == "pubm_api") {
                        call_mgr.position_map[slot_info.position] = slot_info;
                    }
                }
            };
            call_mgr.construct_position_map();
            call_mgr.construct_base_rid_map = function() {
                call_mgr.base_rid_map = {};
                var slot_info;
                for (var i=0; i < ad_server.js_rtb_slots.length; i++) {
                    slot_info = ad_server.js_rtb_slots[i];
                    if (slot_info.network == "pubm_api") {
                        call_mgr.base_rid_map[slot_info.base_rid] = { 'position': slot_info.position, 'width': slot_info.width, 'height': slot_info.height, 'cpm_seq': slot_info.cpm_seq };
                    }
                }
            };
            call_mgr.construct_base_rid_map();
            call_mgr.get_call_info = function (rid) {
                var call_info = {};
                call_info.base_rid = Math.floor(rid/10);
                call_info.pos_info = call_mgr.base_rid_map[call_info.base_rid];
                call_info.call_num = rid % 10;
                return call_info;
            };
            call_mgr.get_rid = function ( position, call_num) {
                return call_mgr.position_map[position].base_rid*10 + call_num;
            };
            call_mgr.get_next_call_cpm_2 = function(call_num, base_rid) {
                var cpm_seq = call_mgr.base_rid_map[base_rid].cpm_seq;
                if (call_num < cpm_seq.length) {
                    return cpm_seq[call_num];
                }
            };
            call_mgr.call_status = {};
            call_mgr.initiate_call = function(rid) {
                call_mgr.call_status[rid] = {'complete': 0};
            }
            call_mgr.complete_call = function(rid) {
                call_mgr.call_status[rid].complete = 1;
            }
            call_mgr.is_position_ready = function(rid) {
                var call_info = call_mgr.get_call_info(rid);
                var base_rid = call_info.base_rid;
                var num_calls = call_mgr.base_rid_map[base_rid].cpm_seq.length;
                var wait_for = 1;
                wait_for = 4;
                var complete = true;
                for (var i=0;i<wait_for;i++) {
                    var call_num = i + 1;
                    var call_rid = call_mgr.get_rid(call_info.pos_info.position, call_num);
                    if (call_mgr.call_status[call_rid] && !(call_mgr.call_status[call_rid].complete == 1)) {
                        complete = false;
                    }
                }
                if (complete && window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'declaring_position_ready', 'position': call_info.pos_info.position});
                }
                return complete;
            };
            ad_server.networks.pubmatic.call_mgr = call_mgr;

            if (!ad_server.networks.pubmatic.synced_up_pixels) {
                var ifrm = likes_ad_passbacks.create_iframe(1,1);
                ifrm.src = "http://ads.pubmatic.com/AdServer/js/syncuppixels.html";
                if (ifrm.style) {
                    ifrm.style.float = "left";
                }
                if (document.body) {
                    document.body.appendChild(ifrm);
                } else {
                    setTimeout(function() {
                        try {
                            document.body.appendChild(ifrm);
                        } catch(e) {
                        }
                    }, 350);
                }
                ad_server.networks.pubmatic.synced_up_pixels = 1;
            }

            window.processPubMaticBid = function(response) {
                var rid_map = call_mgr.base_rid_map;
                var rid = response.meta_data.rid;
                var call_info = call_mgr.get_call_info(rid);
                var position = call_info.pos_info.position;
                var cpm = 0;
                if (response.PubMatic_Bid) {
                    cpm = response.PubMatic_Bid.ecpm;
                }
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'pubmatic_api_response', 'response': response, 'position': position, 'call_num': call_info.call_num, 'network': 'c_pm' + call_info.call_num, 'cpm': (cpm || 0.0) * 0.85});
                }
                var chosen_slot = ad_server.ads_by_slot[position];
                var ad_width  = call_info.pos_info.width;
                var ad_height = call_info.pos_info.height;
                if (response.PubMatic_Bid.error_code || response.PubMatic_Bid.creative_tag.match(/.*likes_ad_passbacks.render_passback.*/) || response.PubMatic_Bid.ecpm == 0 || (response.PubMatic_Bid.landing_page && ad_server.networks.pubmatic.is_url_blacklisted(response.PubMatic_Bid.landing_page, response.PubMatic_Bid.creative_tag, position, call_info.call_num, cpm))) {
                    call_mgr.complete_call(rid);
                    if (call_mgr.is_position_ready(rid)) {
                        ad_server.network_calls.complete('pubmatic', position);
                    }
                    return;
                }
                chosen_slot.push({
                    'network': 'pubmatic',
                    'cpm': response.PubMatic_Bid.ecpm * 0.85,
                    'render': function(ad_div_id, position, cpm) {
                        try {
                            if (window.top.likes_tracking) {
                                window.top.likes_tracking.track_event({'type': 'begin_pubmatic_render', 'position': position, 'api_response': response});
                                ad_server.networks.pubmatic.rendered_ads.push({'url': response.PubMatic_Bid.landing_page,
                                                                              'tag': response.PubMatic_Bid.creative_tag,
                                                                              'cpm': response.PubMatic_Bid.ecpm,
                                                                              'pos': position,
                                                                              'loc': 'US' });
                                if (!ad_server.networks.pubmatic.is_ad_whitelisted(response.PubMatic_Bid.landing_page)) {
                                    if (window.top.likes_tracking && window.top.likes_tracking.report_pubm_ads()) {
                                        ad_server.networks.pubmatic.report_rendered_ads();
                                    }
                                }
                            }
                            ad_div = document.getElementById(ad_div_id);
                            var ad_ifrm = window.likes_ad_passbacks.create_iframe(ad_width, ad_height);
                            ad_ifrm.onload = function() {
                                ad_ifrm.contentWindow.document.write(unescape("%3Chtml%3E%3Chead%3E%3Cbase%20target%3D%22_top%22%3E%3C/head%3E%3Cbody%20style%3D%22padding%3A0px%3B%20margin%3A0px%22%3E") + response.PubMatic_Bid.creative_tag + unescape("%3C/body%3E%3C/html%3E"));
                            };
                            ad_div.appendChild(ad_ifrm);
                            var tracking_ifrm = window.likes_ad_passbacks.create_iframe(0,0);
                            tracking_ifrm.onload = function() {
                                if (window.top.likes_tracking) {
                                    window.top.likes_tracking.track_event({'type': 'ad_slot_rendered', 'network':'c_pubm_api_' + call_info.call_num, 'position':position, 'cpm':cpm});
                                }
                            };
                            tracking_ifrm.src = response.PubMatic_Bid.tracking_url;
                            ad_div.appendChild(tracking_ifrm);
                        } catch(e) {/*ignore*/ }
                    }
                });
                call_mgr.complete_call(rid);
                if (call_mgr.is_position_ready(rid)) {
                    ad_server.network_calls.complete('pubmatic', position);
                }
            };

            var request_ad = function(position, cpm, call_num) {
                window.likes_ad_server.network_calls.initiate('pubmatic', position);
                var position_map = call_mgr.position_map;
                var script_tag = document.createElement('script');
                script_tag.setAttribute('type', 'text/javascript');
                var rid = call_mgr.get_rid(position, call_num);
                call_mgr.initiate_call(rid);
                var slot_info = position_map[position];
                script_tag.src = slot_info['api_endpoint']
                + 'operId=' + slot_info['operId'] + '&'
                + 'rs=2&'
                + 'pubId=41579&'
                + 'siteId=' + slot_info.siteId + '&'
                + 'adId='+ slot_info.adId + '&'
                + 'kadwidth=' + slot_info.width + '&'
                + 'kadheight='+ slot_info.height + '&'
                + 'kadfloor=' + cpm + '&'
                + 'SAVersion=2&'
                + 'js=1&'
                + 'kdntuid=1&'
                + 'pageURL=' + encodeURIComponent(window.top.location.href) + '&'
                + 'refurl=' + encodeURIComponent(window.top.location.href) + '&'
                + 'inIframe=1&'
                + 'kadpageurl=' + encodeURIComponent(window.top.location.href) + '&'
                + 'kltstamp=' + helpers.get_time() + '&'
                + 'timezone=' + helpers.get_timezone_offset() + '&'
                + 'loc=' + window.__likes_latitude + ',' + window.__likes_longitude + '&'
                + 'loc_source=2&'
                + 'screenResolution=' + screen.width + 'x' + screen.height + '&'
                + 'ranreq=' + Math.random() + '&'
                + 'pmUniAdId=0&'
                + 'kbgColor=ffffff&'
                + 'ktextColor=000000&'
                + 'klinkColor=0000EE&'
                + 'adVisibility=1&'
                + 'adPosition=' + helpers.get_ad_position(slot_info.ad_div_id) + '&'
                + 'rid=' + rid;
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'pubmatic_api_call', 'url': script_tag.src, 'position': position, 'call_num': call_num});
                }
                if (document.body) {
                    document.body.appendChild(script_tag);
                } else {
                    setTimeout(function() {
                        try {
                            document.body.appendChild(script_tag);
                        } catch(e) {
                        }
                    }, 350);
                }
            };
            ad_server.networks.pubmatic.request_ad = request_ad;
            ad_server.networks.pubmatic.report_rendered_ads = function() {
                try {
                    if (ad_server.networks.pubmatic.rendered_ads.length > 0) {
                        var report = [];
                        var i;
                        for (i = 0; i < ad_server.networks.pubmatic.rendered_ads.length; i++) {
                            report.push(ad_server.networks.pubmatic.rendered_ads.pop());
                        }
                        if ($) {
                            $.ajax({
                                url: '//' + window.location.host + '/api/record_pubmatic_ads',
                                   type: 'POST',
                                   dataType: 'json',
                                   data: {'report': JSON.stringify(report)}
                            });
                        } else {
                            if (window.XMLHttpRequest) {
                                var xmlhttp = new XMLHttpRequest;
                                var url = "/api/record_pubmatic_ads";
                                xmlhttp.open("POST", url, true);
                                var data = "report=" + escape(JSON.stringify(report));
                                xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
                                xmlhttp.send(data);
                            }
                        }
                    }
                } catch (e) {
                }
            };
            var cpm_seq_id = 0;
            var max_cpm_seq_length = 0;
            for (var i = 0; i < ad_server.js_rtb_slots.length; i++) {
                if (ad_server.js_rtb_slots[i].network == 'pubm_api' && ad_server.js_rtb_slots[i].cpm_seq.length > max_cpm_seq_length) {
                    max_cpm_seq_length = ad_server.js_rtb_slots[i].cpm_seq.length;
                }
            }
            for (cpm_seq_id=0; cpm_seq_id < max_cpm_seq_length; cpm_seq_id++) {
            for (var i=0; i < ad_server.js_rtb_slots.length; i++) {

                if (ad_server.js_rtb_slots[i].network == 'pubm_api' && ad_server.js_rtb_slots[i].cpm_seq[cpm_seq_id]) {
                    request_ad(ad_server.js_rtb_slots[i].position, ad_server.js_rtb_slots[i].cpm_seq[cpm_seq_id], cpm_seq_id + 1);
                }
            }
            }
        };
        ad_server.render_better_ad = function(position, cpm, ad_div_id) {
            try {
                var candidate_ads = window.likes_ad_server.ads_by_slot[position] || [];
                if (position == 'bottom' && ad_div_id == 'bottom_mobile_ad' && ad_server.ads_by_slot['bottomtall']) {
                    for (var i=0;i<ad_server.ads_by_slot['bottomtall'].length;i++) {
                        candidate_ads.push(ad_server.ads_by_slot['bottomtall'][i]);
                    }
                }
                if (candidate_ads && candidate_ads.length > 0) {
                    var max_cpm = cpm/ad_server.js_rtb_advantage;
                    var chosen_ad;
                    for (var i=0;i<candidate_ads.length;i++){
                        if (candidate_ads[i].cpm >= max_cpm && !candidate_ads[i].rendered && !candidate_ads[i].obsolete) {
                            chosen_ad = candidate_ads[i];
                            max_cpm = candidate_ads[i].cpm;
                        }
                    }
                    if (chosen_ad) {
                        window.likes_ad_passbacks.hide_all_in_div(ad_div_id);

			            chosen_ad.rendered = 1;
                        chosen_ad.render(ad_div_id, position, chosen_ad.cpm);

                        if (window.top.likes_tracking) {
                            window.top.likes_tracking.track_event({'type': 'js_rtb_ad_rendered', 'position': position, 'cpm': chosen_ad.cpm, 'network':chosen_ad['network']});
                        }
                        return true;
                    }
                }
                return false;
            } catch(e) { return false; }
        };
        ad_server.render_any_ad_when_available = function(position) {

            var counter = 0;
            var check_and_render = function() {

                counter = counter + 1;

                if (!ad_server.render_better_ad(position, 0, ad_server.position_div_map[position]) && counter < 30) {
                    setTimeout(check_and_render, 200);
                }

            }
            check_and_render();
        };
        ad_server.add_rtb_ready_listener = function(position, listener) {
            window.likes_ad_server.rtb_ready_listeners[position] = listener;
            window.likes_ad_server.network_calls.notify_if_ready(position);
        };
        ad_server.clear_rtb_ready_listener = function(position) {
            if (ad_server.rtb_ready_listeners[position]) {
                for (var i = 0; i < ad_server.rtb_ready_listeners[position].length; i++) {
                    var listener = ad_server.rtb_ready_listeners[position][i];
                    if (listener.rtb_timeout) {
                        clearTimeout(listener.rtb_timeout);
                    }
                }
                delete window.likes_ad_server.rtb_ready_listeners[position];
            }
        };
        ad_server.notify_rtb_ready_listeners = function(positions) {
            if (!positions) {
                positions = Object.keys(window.likes_ad_server.rtb_ready_listeners);
            }
            for (var i=0; i<positions.length; i++) {
                if (window.likes_ad_server.rtb_ready_listeners.hasOwnProperty(positions[i])) {
                    window.likes_ad_server.rtb_ready_listeners[positions[i]]();
                }
            }
        };
        ad_server.conduct_rtb = function() {
            ad_server.init_slots();
            var networks = ad_server.get_registered_networks();
            for (var i=0;i< networks.length; i++) {
                var network;
                if (networks[i] == 'pubm_api') {
                    network = 'pubmatic';
                } else {
                    network = networks[i];
                }
                if (ad_server.networks[network] && ad_server.networks[network].get_candidate_ads) {
                    ad_server.networks[network].get_candidate_ads(ad_server);
                }
            }
            window.likes_ad_server.network_calls.all_networks_registered = true;
            window.likes_ad_server.network_calls.notify_if_ready();
        };
        setTimeout(ad_server.conduct_rtb, 10);

    })();
} catch(e) {/*ignore*/ }
</script>
<script type="text/javascript">
window.likes_reload_ads = function(track_id) {
    window.likes_ad_passbacks.completed = {};
    var pending_track_events = [];
    try {
        window.top.likes_ad_server.conduct_rtb();
    } catch (e) {/* ignore */}
    try {
        (function () {
            var cpm = 0.35;
            var rtb_timeout;
            var rtb_check = function() {
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'rtb_check_executing', 'position': 'rightbottom'});
                }
                clearTimeout(rtb_timeout);
                window.likes_ad_server.clear_rtb_ready_listener('rightbottom');
                if (!window.likes_ad_server.render_better_ad('rightbottom', cpm, 'ad_slot_rightbottom_rect')) {
                    window.likes_ad_passbacks.render_ad('ad_slot_rightbottom_rect', 300, 250, undefined, "//slaiter2.me/ad/15/openx/529989/300/250/all/rightbottom");
                }
            };
            rtb_timeout = setTimeout(rtb_check, 400);
            rtb_check.rtb_timeout = rtb_timeout;
            window.likes_ad_server.add_rtb_ready_listener('rightbottom', rtb_check);
        })();
    } catch (e) {/* ignore */}
    try {
        (function () {
            var cpm = 0.35;
            var rtb_timeout;
            var rtb_check = function() {
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'rtb_check_executing', 'position': 'righttop'});
                }
                clearTimeout(rtb_timeout);
                window.likes_ad_server.clear_rtb_ready_listener('righttop');
                if (!window.likes_ad_server.render_better_ad('righttop', cpm, 'ad_slot_righttop_rect')) {
                    window.likes_ad_passbacks.render_ad('ad_slot_righttop_rect', 300, 250, undefined, "//slaiter2.me/ad/15/openx/529986/300/250/all/righttop");
                }
            };
            rtb_timeout = setTimeout(rtb_check, 400);
            rtb_check.rtb_timeout = rtb_timeout;
            window.likes_ad_server.add_rtb_ready_listener('righttop', rtb_check);
        })();
    } catch (e) {/* ignore */}
    try {
        (function () {
            var cpm = 0.3;
            var rtb_timeout;
            var rtb_check = function() {
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'rtb_check_executing', 'position': 'centerbottom'});
                }
                clearTimeout(rtb_timeout);
                window.likes_ad_server.clear_rtb_ready_listener('centerbottom');
                if (!window.likes_ad_server.render_better_ad('centerbottom', cpm, 'ad_slot_centerbottom_grid')) {
                    window.likes_ad_passbacks.render_ad('ad_slot_centerbottom_grid', 630, 500, undefined, "//slaiter2.me/ad/15/dummy/dummy_centerbottom/630/500/all/centerbottom", 500);
                }
            };
            rtb_timeout = setTimeout(rtb_check, 300);
            rtb_check.rtb_timeout = rtb_timeout;
            window.likes_ad_server.add_rtb_ready_listener('centerbottom', rtb_check);
        })();
    } catch (e) {/* ignore */}
    try {
        (function () {
            var cpm = 0.3;
            var rtb_timeout;
            var rtb_check = function() {
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'rtb_check_executing', 'position': 'top'});
                }
                clearTimeout(rtb_timeout);
                window.likes_ad_server.clear_rtb_ready_listener('top');
                if (!window.likes_ad_server.render_better_ad('top', cpm, 'top_ad_banner')) {
                    window.likes_ad_passbacks.render_ad('top_ad_banner', 728, 90, undefined, "//slaiter2.me/ad/15/openx/529988/728/90/all/top");
                }
            };
            rtb_timeout = setTimeout(rtb_check, 400);
            rtb_check.rtb_timeout = rtb_timeout;
            window.likes_ad_server.add_rtb_ready_listener('top', rtb_check);
        })();
    } catch (e) {/* ignore */}
    try {
        (function () {
            var cpm = 0.55;
            var rtb_timeout;
            var rtb_check = function() {
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'rtb_check_executing', 'position': 'right'});
                }
                clearTimeout(rtb_timeout);
                window.likes_ad_server.clear_rtb_ready_listener('right');
                if (!window.likes_ad_server.render_better_ad('right', cpm, 'ad_slot_right')) {
                }
            };
            rtb_timeout = setTimeout(rtb_check, 1);
            rtb_check.rtb_timeout = rtb_timeout;
            window.likes_ad_server.add_rtb_ready_listener('right', rtb_check);
        })();
    } catch (e) {/* ignore */}
    return pending_track_events;
};
</script>
<script type="text/javascript">
window.__likes_latitude = window.top.__likes_latitude = 36.9312;
window.__likes_longitude = window.top.__likes_longitude = -76.2397;
</script>





