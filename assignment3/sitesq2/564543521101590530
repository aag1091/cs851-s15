        window.top.likes_ad_server = ad_server;
        ad_server.report_event = function(path, label, value) {
            if (window.XMLHttpRequest) {
                var xmlhttp = new XMLHttpRequest;
                var url = "/api/track_event?path=" + path;
                if (label) {
                    url = url + "&label=" + label;
                }
                if (value) {
                    url = url + "&value=" + value;
                }
                xmlhttp.open("GET", url, true);
                xmlhttp.send();
            }
        };
        ad_server.js_rtb_slots = [{"api_endpoint": "http://showads.pubmatic.com/AdServer/AdServerServlet?", "network": "pubm_api", "width": 728, "ad_safe_domain_enabled": 1, "pubId": "41579", "ad_div_id": "top_ad_banner", "operId": 102, "height": 90, "siteId": "44147", "platform": "desktop", "adId": "72487", "location": "us", "position": "top", "base_rid": 1, "cpm_seq": [0.8, 0.6, 0.3, 0.1], "custom_domain_enabled": 1}, {"api_endpoint": "http://showads.pubmatic.com/AdServer/AdServerServlet?", "network": "pubm_api", "width": 300, "ad_safe_domain_enabled": 1, "pubId": "41579", "ad_div_id": "ad_slot_righttop_rect", "operId": 102, "height": 250, "siteId": "44147", "platform": "desktop", "adId": "72489", "location": "us", "position": "righttop", "base_rid": 2, "cpm_seq": [0.8, 0.6, 0.3, 0.1], "custom_domain_enabled": 1}, {"api_endpoint": "http://showads.pubmatic.com/AdServer/AdServerServlet?", "network": "pubm_api", "width": 300, "ad_safe_domain_enabled": 1, "pubId": "41579", "ad_div_id": "ad_slot_rightbottom_rect", "operId": 102, "height": 250, "siteId": "44147", "platform": "desktop", "adId": "72488", "location": "us", "position": "rightbottom", "base_rid": 3, "cpm_seq": [0.8, 0.6, 0.3, 0.1], "custom_domain_enabled": 1}, {"network": "mylikes", "ad_div_id": "ad_slot_centerbottom_grid", "ml_slot": "multi_small_grid_0_desktop", "platform": "desktop", "location": "all", "position": "centerbottom"}, {"network": "mylikes", "ad_div_id": "last_page_desk_ad_wrapper", "ml_slot": "ad_page_desktop", "platform": "desktop", "location": "all", "position": "last_page_desk"}];
        ad_server.retain_ads_across_pages = true;
