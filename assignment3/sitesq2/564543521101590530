<script type="text/javascript">
try {
(function () {
var cpm = 0.35;
var rtb_timeout;
var rtb_check = function() {
if (window.top.likes_tracking) {
window.top.likes_tracking.track_event({'type': 'rtb_check_executing', 'position': 'righttop'});
}
clearTimeout(rtb_timeout);
