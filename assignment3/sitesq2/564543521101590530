	ad_server.networks.mylikes.refresh_house_ad_funcs = {};
	ad_server.networks.mylikes.get_render_func = function(ml_ad_info) {
	    var house_ad_parent = ml_ad_info.platform + "_" + ml_ad_info.position + "_backfill";
	    ad_server.networks.mylikes.refresh_house_ad_funcs[house_ad_parent] = function(first_time) {
		var needs_draw = first_time || false;
		var ad_container = document.getElementById(house_ad_parent);
		var ad = document.getElementById(house_ad_parent + "_temp").firstElementChild.cloneNode(true);
		var ad_infos = [ml_ad_info];
		var ad_units = [ad];

		if (ml_ad_info.promo_list && ml_ad_info.promo_list.length){
		    ad_infos = ml_ad_info.promo_list;
		    ad_units = ad.querySelectorAll(".multi_ad") || [];
		    function shuffle(array) {
			var currentIndex = array.length, temporaryValue, randomIndex ;
			// While there remain elements to shuffle...
			while (0 !== currentIndex) {
			    // Pick a remaining element...
			    randomIndex = Math.floor(Math.random() * currentIndex);
			    currentIndex -= 1;

			    // And swap it with the current element.
			    temporaryValue = array[currentIndex];
			    array[currentIndex] = array[randomIndex];
			    array[randomIndex] = temporaryValue;
			}
			return array;
		    }
		    if (ml_ad_info.platform == "mobile") { //shuffle
			ml_ad_info.refresh_count = ml_ad_info.refresh_count || 0;
			if (ml_ad_info.refresh_count) {
			    ad_infos = shuffle(ad_infos);
			    needs_draw = true;
			}
			ml_ad_info.refresh_count++;
		    }
		} else if (ml_ad_info.rotate_list && ml_ad_info.rotate_list.length) {
		    var rotate_list = ml_ad_info.rotate_list;
		    for (var i in rotate_list) {
			if (rotate_list[i].views_remaining){
			    ad_infos = [rotate_list[i]];
                            if (ml_ad_info.active_promo.promo_id != i.promo_id) {
                                needs_draw = true;
			        ml_ad_info.active_promo = i;
                            }
			    rotate_list[i].views_remaining -= 1;
			    break;
			}
		    }
		}

		if (needs_draw) {
		    var extension = (ml_ad_info.position == "bottomtall" && ml_ad_info.slot_name == "bottom_backfill") ? ".600x" : ".180x180.crop";
     		    ad_container.innerHTML = "";

		    for (var i in ad_infos) {
			var ad_info = ad_infos[i];
			var ad_unit = ad_units[i];
			if (!ad_unit) continue;
			if (ml_ad_info.refresh_count) { //shuffle
			    if (ad_info.url.indexOf("&sp=") == ad_info.url.length - 5) {
				ad_info.url.replace(/&sp=[0-9]/i, "&sp=" + i);
			    } else {
				ad_info.url = ad_info.url + "&sp=" + i;
			    }
			}
			ad_unit.setAttribute("data-href", ad_info.url);
			ad_unit.setAttribute("href", ad_info.url);
			ad_unit.setAttribute("prid", ad_info.promo_id);
			ad_unit.setAttribute("slot_id", ml_ad_info.slot_id);
			var img = ad_unit.querySelectorAll(".image_container")[0];
			img.style.backgroundImage = "url('" + ad_info.thumb_url + extension + "')";
			img.setAttribute("src", ad_info.thumb_url + extension);
			ad_unit.querySelectorAll(".promo_name")[0].innerHTML = ad_info.name;
		    }

			ad_container.appendChild(ad);
			setup_bounce_tracking();
		}

		ad_container.style.display = "block";

		var base_slot_name;
		var promo_ids = [];
		if (ml_ad_info.tracking_infos) {
		    if (ml_ad_info.rotate_list && ml_ad_info.rotate_list.length){
			promo_ids.push(ml_ad_info.rotate_list[ml_ad_info.active_promo].promo_id);
			base_slot_name = ml_ad_info.rotate_list[0].slot_name;
		    } else {
			for (var i = 0; i <  ml_ad_info.tracking_infos.length; i++) {
			    promo_ids.push(ml_ad_info.tracking_infos[i].promo_id);
			}
			base_slot_name = ml_ad_info.tracking_infos[0].base_slot_name;
		    }
		} else {
		    promo_ids.push(ml_ad_info.promo_id);
		    base_slot_name = ml_ad_info.slot_name;
		}

		if (window.top.likes_tracking && window.top.likes_tracking.track_redis_imps == 0) {
		    var backfill_imp;
		    var imp_src = 'http://' + window.top.location.host + '/house_ad_iframe?ml_id=' + 3295388 + '&gid=' + 6327324;

		    if (window.top.likes_tracking) {
			imp_src += '&track_id=' + window.top.likes_tracking.current_track_id;
		    } else {
			imp_src += '&track_id=' + 47951;
		    }
		    imp_src += "&slot_name=" + base_slot_name + "&promo_ids=" + promo_ids.join(",");
		    var parent_url_args = window.location.href.split('?')[1];
		    if (parent_url_args) {
			imp_src += "&" + parent_url_args;
		    }
		    backfill_imp = document.createElement('iframe');
		    backfill_imp.className = 'backfill_imp';
		    backfill_imp.frameBorder = 0;
		    backfill_imp.src = imp_src + "&no_markup=1";
		    backfill_imp.style.width = "0px";
		    backfill_imp.style.height = "0px";
		    backfill_imp.style.display = "none";
		    var ad_div = document.getElementById(ml_ad_info.ad_div_id);
		    ad_div.appendChild(backfill_imp);
		}

		if (window.top.likes_tracking) {
		    if (ml_ad_info) {
			window.top.likes_tracking.track_event(ml_ad_info);
		    } else {
			throw("missing house ad backfill info");
		    }
		} else {
		    throw("missing likes tracking");
		}

		if (base_slot_name == "multi_backfill" && document.getElementById("related_list_container")) {
                    document.getElementById("related_list_container").style.display = "none";
		}
	    }

	    var func = function(ad_div_id, position, cpm) {
		try {
		    var ad_div = document.getElementById(ad_div_id);
		    var master_ad = document.getElementById(house_ad_parent);
		    if (!master_ad) {
