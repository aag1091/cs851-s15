        ad_server.init_slots = function() {
            window.amznads = undefined;
            window.amzn_ads = undefined;
            window.amzn_console = undefined;
            if (!ad_server.ads_by_slot || !ad_server.retain_ads_across_pages) {
                ad_server.ads_by_slot = {};
            }
            ad_server.position_div_map = {};
            for (var i=0;i< ad_server.js_rtb_slots.length;i++) {
                var position = ad_server.js_rtb_slots[i].position;
                ad_server.position_div_map[position] = ad_server.js_rtb_slots[i].ad_div_id;
                if (! ad_server.ads_by_slot.hasOwnProperty(position) ) {
                    ad_server.ads_by_slot[position] = [];
                } else {
                    for (var j=0;j < ad_server.ads_by_slot[position].length; j++) {
                        var ad = ad_server.ads_by_slot[position][j];
                        if (ad.network == 'amazon') {
                            ad.obsolete = 1;
                        }
                    }
                }
            }
        };
        ad_server.init_slots();

        ad_server.get_registered_networks = function() {
            var network_list = [];
            for (var i=0; i < ad_server.js_rtb_slots.length; i++) {
                if (network_list.indexOf(ad_server.js_rtb_slots[i].network) < 0) {
                    network_list.push(ad_server.js_rtb_slots[i].network);
                }
            }
            return network_list.sort();
        };
        ad_server.rtb_ready_listeners = {};
        ad_server.networks = {};
        ad_server.network_calls = {
            initiate: function(network, position) {
                if (!ad_server.network_calls[position]) {
                    ad_server.network_calls[position] = {};
                }
                ad_server.network_calls[position][network] = {'complete': 0};
            },
            complete: function(network, position) {
                ad_server.network_calls[position][network].complete = 1;
                var all_complete = true;
                for (network in ad_server.network_calls[position]) {
                    if (ad_server.network_calls[position][network].complete != 1) {
                        all_complete = false;
                    }
                }
                if (all_complete) {
                    ad_server.notify_rtb_ready_listeners([position]);
                }
            },
            notify_if_ready: function(position) {
                if (!ad_server.network_calls.all_networks_registered) {
                    return;
                }
                var positions = [];
                if (!position) {
                    positions = Object.keys(ad_server.rtb_ready_listeners);
                } else {
                    positions = [position];
                }
                for (var i=0;i<positions.length;i++) {
                    var pos = positions[i];
                    if (!ad_server.network_calls[pos]) {
                        ad_server.notify_rtb_ready_listeners([pos]);
                    } else {
                        var all_complete = true;
                        for (network in ad_server.network_calls[pos]) {
                            if (ad_server.network_calls[pos][network].complete != 1) {
                                all_complete = false;
                            }
                        }
                        if (all_complete) {
                            ad_server.notify_rtb_ready_listeners([pos]);
                        }
                    }
                }
            }
        };

        ad_server.networks.mylikes = {};
	ad_server.networks.mylikes.house_tracking_map = {'bottomtall':'bottom_backfill',
