<script>
$(function(){
		var choices= { col0: null, col1: null };
		var twoChoices = false;
		var myHashes;
		function hashCheck(){
			if(choices.col0 == null || choices.col1 == null) {
				twoChoices = false;
				$('.social-share a.twitter-share').addClass('disabled');
			} else {
				twoChoices = true;
				$('.social-share a.twitter-share').removeClass('disabled');
			}
		}
		$('.option-container ul li span').click(function(){
			var myCol = $(this).data('column');
			var myColKey = 'col' + myCol;
			var myHash = $(this).data('hash');
			if($(this).hasClass('active')) {
				choices[myColKey] = null;
				$(this).removeClass('active');
			} else {
				$('.column[data-column="'+myCol+'"]').find('.active').removeClass('active');
				choices[myColKey] = myHash;
				$(this).toggleClass('active');
			}
			hashCheck();
		});
		$('a.twitter-share').click(function(e){
			e.preventDefault();
			if(twoChoices) {
				var url;
				var params;
				var name;
				url = 'https://twitter.com/intent/tweet?';
				params = 'text=' + 'I chose ';
				params += '&hashtags='+ choices.col0 + ',' + choices.col1 +',EasyBracketParty';
				url += params;
				name = 'EasyBracketParty';
				window.open(url, name, 'width=640,height=480,top='+($(window).height()/2 - 240) +',left='+($(window).width()/2 - 320) +'location=0,titlebar=0,toolbar=0,location=0,menubar=0,directories=0,scrollbars=0');
			}
		});
	});
</script></div>
