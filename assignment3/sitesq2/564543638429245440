            // <![CDATA[
            $(document).ready(function() {
                // only do dropdown menu for > ie6
                if (typeof document.body.style.maxHeight != "undefined") {
                    $('#menu>li').hoverIntent({
                        over: function() {
                            $(this).children('ul').slideDown('fast', function() {
                                $(this).children('li').fadeIn('fast');
                            });
                        },
                        timeout: 0,
                        interval: 400,
                        out: function() {
                            $(this).children('ul').children('li').fadeOut('fast', function() {
                                $(this).parent('ul').slideUp('fast');
                            });
                        }
                    });
                }
                $('#menu li li a, #more li a').hover(function() {
                    $(this).find('div').fadeIn('fast');
                }, function() {
                    $(this).find('div').fadeOut('fast');
                });

                $('#paginate img.on').hover(function() {
                    $(this).attr('src', 'http://media.lintvnews.com/linapps/css/photomojo/images/pagination-' + $(this).attr('id') + '-over.png');
                }, function() {
                    $(this).attr('src', 'http://media.lintvnews.com/linapps/css/photomojo/images/pagination-' + $(this).attr('id') + '-on.png');
                });

                $('#menu ul li:last-child').css('margin-right', 0);

            });
