function showhideinfo(ob)
{
	if(ob.title=='more')
	{
		showDiv('video-tags');showDiv('video-description');
		hideDiv('video-short-description');
	}
	else
	{
		hideDiv('video-tags');hideDiv('video-description');
		showDiv('video-short-description');
	}
}
