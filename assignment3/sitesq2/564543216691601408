<script language="JavaScript">
var loc = getcookie("RLOC") ;
if (loc == null){locFlag = 1 ;}
else
{
	if(loc.indexOf("usa") != -1){locFlag = 0 ;}
	else{locFlag = 1 ;}
}
</script>



