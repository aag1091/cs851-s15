function metriccall_img(sldcounter)
{
	var querystring	= "";
	var path	= "";
	var Rkey_data	= "";
	var Rkey	= Math.floor(Math.random() * 1000000);
	querystring	= window.location.search;
	path		= window.location.pathname;


	if (querystring == "")
		Rkey_data = "?rkey="+Rkey;
	else
		Rkey_data = "&rkey="+Rkey;

	document.getElementById("metric_img").src = 'http://metric.us.rediff.com'+path+"/"+sldcounter+querystring+Rkey_data;
}
