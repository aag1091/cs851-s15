  $(function(){
    $('.js-eventdate-picker input').datepicker().on('changeDate', function(){
      WWAnalytics.ping('contact_us_date_change', "8dc482a8a29eb1f9");
    });
    $('.js-eventdate-picker .fa-calendar').off('click').on('click', function(){
      $('.js-eventdate-picker input').datepicker('show');
    });
    $('.js-contact-type .selectpicker').selectpicker();
  
    $('.js-contact-type select').on('change', function(){
  
      $('.js-contact-phone, .js-contact-question').addClass('hidden');
      var selected = $(this).find('option:selected').val();
  
      if (selected == 2)  {
        $('.js-contact-phone').removeClass('hidden');
      } else if( selected == 3) {
        $('.js-contact-question').removeClass('hidden');
      }
    });
    $('.js-contact-type select').trigger('change');
  });
</script>

