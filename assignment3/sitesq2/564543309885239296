<script type="text/javascript">
  // 1 minute is 60 seconds is 60 000 miliseconds
  var minute = 60000;
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-11303224-1']);

  // On click on subscribe
  var subscribeBanner = document.getElementById('top_banner_1');
  if (subscribeBanner) {
    subscribeBanner.addEventListener('click', function() {
      _gaq.push(['_trackEvent', 'Subscribe', 'Start', 'Header']);
    }, false);
  }

  // On click on "support" banner
  var supportBanner = document.getElementById('top_banner_3');
  if (supportBanner) {
    supportBanner.addEventListener('click', function() {
      _gaq.push(['_trackEvent', 'Support', 'Start', 'Header']);
    }, false);
  };

  // One Pay Online
  var startArticle = document.getElementById('pay_online');
  startArticle.addEventListener('click', function() {
    _gaq.push(['_trackEvent', 'Subscribe', 'Start', 'Article']);
  }, false);

  // Click on 'search' input
  var searchInput = document.getElementsByClassName('search-query');
  if (searchInput) {
    searchInput[0].onfocus = function() {
      _gaq.push(['_trackEvent', 'Search', 'Click']);
    };
  };

  var isArticlePage = false;

  try {
    // article or list of articles
    if (document.URL.indexOf('content') > -1) {
      var contentName = document.getElementsByClassName('submenu')[0].getElementsByTagName('strong')[0].innerHTML;
      if (document.URL.indexOf('.html') > -1) {

        //
        // .html ending with content - article page
        //
        isArticlePage = true;
        _gaq.push(['_setPageGroup', 3, contentName]);
        // click on tag
        var tags = document.getElementsByClassName('article__tag');
        for (var i = 0; i < tags.length; i++) {
          tags[i].addEventListener('click', function() {
            _gaq.push(['_trackEvent', 'Article Page', 'Tag', 'Click']);
          });
        };
        // click on "read more in this section" link
        var readMoreLinks = document.getElementsByClassName('read_more__header_link');
        for (var i = 0; i < readMoreLinks.length; i++) {
          readMoreLinks[i].addEventListener('click', function() {
            _gaq.push(['_trackEvent', 'More', 'Read More in Section', 'Click']);
          });
        };
        // Most read logic
        var mostRead = document.getElementById('most-read-dropdown-read');
        mostRead.addEventListener('click', function() {
          _gaq.push(['_trackEvent', 'More', 'Filter', 'Read']);
        }, false);

        var dayFilter = document.getElementById('most-read-24hours');
        dayFilter.addEventListener('click', function() {
          _gaq.push(['_trackEvent', 'More', 'Filter', '24 hours']);
        }, false);

        var mostReadArticleLinks = document.getElementsByClassName('most-read-article-link');
        for (var i=0; i < mostReadArticleLinks.length; i++) {
          var link = mostReadArticleLinks[i];
          link.addEventListener('click', function() {
            _gaq.push(['_trackEvent', 'More', 'Goto', 'Article']);
          }, false);
        };

        var mostReadShowMore = document.getElementById('most-read-show-more');
        mostReadShowMore.addEventListener('click', function() {
          console.log('Tracked click on most read show more');
          _gaq.push(['_trackEvent', 'More', 'Goto', 'Show more']);
        }, false);
        // paid article logic here

        var paidArticle = document.getElementsByClassName('pay').length > 0;
        if (paidArticle) {
          _gaq.push(['_setPageGroup', 1, 'Closed Article']);
        } else {
          _gaq.push(['_setPageGroup', 1, 'Open Article']);
        };
      } else {
        //
        // content page but without .html ending - articles list page
        //
        _gaq.push(['_setPageGroup', 2, contentName]);
      };
    };
  } catch(err) {};

  var links = document.getElementsByTagName('a');
  // On click on "Subscribe now"
  for (var i=0; i<links.length; i++) {
    if (links[i].innerHTML.toLowerCase() == 'subscribe now') {
      links[i].addEventListener('click', function() {
        _gaq.push(['_trackEvent', 'Subscribe', 'Start', 'Another Pages']);
      }, false);
    };

    // On click on Make a Payment
    if (links[i].innerHTML.toLowerCase() == 'make a payment') {
      if (document.URL == 'http://www.kyivpost.com/payment/') {
        links[i].addEventListener('click', function() {
          _gaq.push(['_trackEvent', 'Subscribe', 'Step 2']);
        }, false);
      }

      if (document.URL == 'http://www.kyivpost.com/payment/?section=donate') {
        links[i].addEventListener('click', function() {
          _gaq.push(['_trackEvent', 'Support', 'Step 2']);
        }, false);
      };
    };

    // Click on "Download" a newspaper
    if (links[i].innerHTML.toLowerCase() == 'download') {
      links[i].addEventListener('click', function() {
        _gaq.push(['_trackEvent', 'Download', 'Newspaper']);
      }, false);
    };

    // Click on "Download" on ad prices
    if (document.URL == 'http://www.kyivpost.com/newspaper/advert/') {
      if (links[i].innerHTML.toLowerCase().indexOf('price') > -1) {};
        links[i].addEventListener('click', function() {
          _gaq.push(['_trackEvent', 'Download', 'Prices']);
        });
    };
  };

  // 2 minutes on page
  var bounceRate = function() {
    setTimeout(function() {
        _gaq.push(['_trackEvent', 'Bounce Rate', '2 min on page']);
    }, minute * 2);
  };

  var circlesTrack = function() {
    if (isArticlePage == true) {
      // click on "read more in this section" circles
      var circles = document.getElementsByClassName('choice_title')[0].parentNode.getElementsByClassName('carousel-pagination')[0].getElementsByTagName('li');
      for (var i = 0; i < circles.length; i++) {
        circles[i].addEventListener('click', function() {
          _gaq.push(['_trackEvent', 'More', 'Read More in Section', 'Slider']);
        });
      };
    }
  }

  $(function() {
    circlesTrack();
  });

  bounceRate();
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>

