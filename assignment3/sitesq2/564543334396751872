<script><!--

var delay_time=1560000; //1560 seconds // 1 sec = 1000;
var cookie_time=60; //60 days //cookies will kept in browser for 60 years
var buylinkid="buylink";
buylinkid="#"+buylinkid;

//some styling //fading on mouse over //you can delete it if you want
$(buylinkid).mouseover(function() {$(buylinkid+" img").css("opacity","0.75");});
$(buylinkid).mouseout(function() {$(buylinkid+" img").css("opacity","1");});

function Get_Cookie(check_name){var a_all_cookies=document.cookie.split(';');var a_temp_cookie='';var cookie_name='';var cookie_value='';var b_cookie_found=false;var i='';for(i=0; i < a_all_cookies.length; i++){a_temp_cookie=a_all_cookies[i].split('=');cookie_name=a_temp_cookie[0].replace(/^\s+|\s+$/g, '');if(cookie_name==check_name){b_cookie_found=true;if(a_temp_cookie.length > 1){cookie_value=unescape(a_temp_cookie[1].replace(/^\s+|\s+$/g, ''));}return cookie_value;break;}a_temp_cookie=null;cookie_name='';}if(!b_cookie_found){return null;}}function Set_Cookie(name, value, expires, path, domain, secure){var today=new Date();today.setTime(today.getTime());if(expires){expires=expires * 1000 * 60 * 60 * 24;}var expires_date=new Date(today.getTime()+(expires));document.cookie=name+"="+escape(value)+((expires)? ";expires="+expires_date.toGMTString():"")+((path)? ";path="+path:"")+((domain)? ";domain="+domain:"")+((secure)? ";secure":"");}var runtime=0;var checktimeout=function(){if(delay_time<runtime){$(buylinkid).show();}else{setTimeout(checktimeout,1000);}runtime=runtime+1000;}
//get cookie
var repeatVisitor=Get_Cookie("watched");
if(repeatVisitor>=1) $(buylinkid).show();
else setTimeout(checktimeout,1000);
//set cookie
repeatVisitor++;
Set_Cookie("watched", repeatVisitor, cookie_time, '/');
//--></script>
