function overrideEss(){
   if (location.href.indexOf("donorPledge.asp") > 0) {
      //if team link found on personal page
      if ($('a b:contains(My Team Page)').length > 0) {
         //change team button
         if ($('a.teamPageLink.smallButton').length > 0) {
            $('a.teamPageLink.smallButton').html('My Team Page');
         } else {
            setTimeout(overrideEss,3);
         }
      }
   }
}
function overrideEssEdit(){
   if (location.href.indexOf("teamPageEdit.asp") > 0) {
      //if team link found on personal page
      if ($('.editppHeaderText').length > 0) {
        $('#newEditPP').closest('td').addClass('newForm');
        $('.newForm').append($('form[name=form1]'));
        $('form[name=form1]').html($('#newEditPP, #oldEditPP,#newEditPPSub')).show();
      } else {
         setTimeout(overrideEssEdit,3);
      }
   }
}

function overrideEssDonateBtn(){
   if (location.href.indexOf("donorPledge.asp") > 0) {
      //if donation button found on personal page
      if ($('#giveNowButton .bigButton').length > 0) {
         setTimeout(function(){$('#giveNowButton .bigButton').colorbox({ href: "#givingOverlay", width: "600px", maxWidth: "100%", height: "410px", maxHeight: "700px", inline: true, opacity: 0.75 });},100);
      } else {
         setTimeout(overrideEssDonateBtn,3);
      }
   }
}                


function overrideEssWidth(){
   if (location.href.indexOf("donorPledge.asp") > 0) {
      //if donation button found on personal page
      if ($('#contentTertiary div.gutter').length > 0) {
         if ($('#contentTertiary').height() == 0) {
            $('#contentTertiary').hide();
            $('#contentPrimary').css('width', ' 700px');
         }
      } else {
         setTimeout(overrideEssWidth,3);
      }
   }
}                

overrideEss();
overrideEssEdit();
overrideEssDonateBtn();
//overrideEssWidth();


