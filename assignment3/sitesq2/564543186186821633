		imagesCache = new Array()

		//Cache the images here
		for (i=0;i<sponsorSeries.length;i++){	
			imagesCache[i] = new Image
			imagesCache[i].src = "/AccountTempFiles/"+sponsorSeries[i]
			imagesCache[i].alt = sponsorCompanyName[i] + " Logo"
		}
		// theCount will keep track of the current image */
		theCount = 0
	}

	// advance to the next image 
	function turnPage(){
		if (theCount == imagesCache.length-1) 
		{theCount = 0}
		else { theCount++ }

		// (display a frame of the animation) 
		document.dance.src = imagesCache[theCount].src
		document.dance.alt = imagesCache[theCount].alt
		// repeat the turnPage function set the Time 
		setTimeout("turnPage()", 2000)
	}
	
	//Return value of the querystring key parameter
	function checkUrlString(urlString){
		if(urlString != "" && typeof urlString != "undefined" && urlString != "1"){
			var qs = urlString.substring(urlString.indexOf("?") + 1)
			return qs;
		}else{
			return false;
		}
	}
	
	// pass querystring key, return value
	// parameters: queryStringKey= keyName, urlString=either a url string or flag 1; if 1 get url from queryString
	function checkQueryString(queryStringKey, urlString){
		var query = "";
		if(urlString == "1"){
			query = location.search.substring(1);
		}else{
			query = checkUrlString(urlString);
		}
		
		if(query == false){
			return false;
		}
		
		var pairs = query.split("&");
		var qsKeyLength = 0;
		if(queryStringKey != null){
			for(var i=0; i<pairs.length; i++){
				qsKeyLength = queryStringKey.length;
				if(pairs[i].substring(0,qsKeyLength) == queryStringKey){
					if(pairs[i].indexOf("=") != -1){
						return pairs[i].substring(qsKeyLength + 1);
					}
				}
			}
		}
		return false
	}

	function OpenWindow(){
		if(checkQueryString("linkid",sponsorURL[theCount]) != false){
	      	msg=window.open(sponsorURL[theCount],"DisplayWindow","toolbar=no,directories=no,scrollbars=1,resizable=1;menubar=yes");
		}
	}
		
	// check to see if the image is clickable
	function onClickImg() {
		var flag;
		if (sponsorRealUrl[theCount].toLowerCase() != "http://" && sponsorRealUrl[theCount].toLowerCase() != "https://" && sponsorRealUrl[theCount] != "") {
			flag = true;
		}
		else
			flag = false;
		
		if(flag==true){
			OpenWindow();
		}
	}
	// -->
	</script>
	
