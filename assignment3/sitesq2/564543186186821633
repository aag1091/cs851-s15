//  Post 9.4.1 Hotfix Scripts
$(function() {
  //Fixes page blowout issue on Committee view.
    if ( document.location.href.indexOf('committeeMenu.asp') > -1 ) {
        $('td [width=600]').attr('style','width: 507px;'); 
    }
    //Fixes page blowout issue on registration form.
    if ( document.location.href.indexOf('register.asp') > -1 ) {
        $('td [nowrap]').attr('style','white-space: normal;'); 
    }
 //Fixes page blowout issue on ACH donor billing form.
    if ( document.location.href.indexOf('donorbilling.asp') > -1 ) {
        $('td [nowrap]').attr('style','white-space: normal;'); 
    }
    //Removes the unnecessary h4 tag around event goal that causes line break in IE
    var eventGoalContents =  $(".fafHomeBodyContent h4:first").html();
    if ($.browser.msie && parseInt($.browser.version) > 8) {
      $(eventGoalContents).appendTo(".hp_header");
    }
});
</script>
