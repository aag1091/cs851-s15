<script language="javascript">
function redirectLoginMenu(flag){
        if(flag == "email"){
                window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu&area=email";
        }else if(flag == "quiz"){
                window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu&area=game";
        }else if(flag == "personal"){
                window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu&area=edit";
        }else if(flag == "reports"){
                window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu&area=reports";
        }else if(flag == "tools"){
                window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu&area=updateprofile";
        }else if(flag == "hq"){
                window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu";
        }else{
               window.location.href="https://www.kintera.org/faf/login/loginParticipant.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157&login=lmenu";
        }
}  
function openLogoutWin(){
	window.name = "partmenu"
	window.open('../help/helpLogoutWin.asp?ievent=1107419&lis=1&kntae1107419=EF540A0AC3CC4A7082D8D84D0D071157','firstlogin','menubar=no,scrollbars=yes,resizable=yes,width=420,height=200');
	
}
function doSubmit(type1){
	if(type1 == 1){
		window.location.target = "_top";
		window.location.href = "&linkid=";
	}
	if(type1 == 2){
		window.location.href = "&linkid=";
	}
}
