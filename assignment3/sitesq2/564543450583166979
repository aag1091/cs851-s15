                $(document).ready(function() {
                    $('a#aq_menu_link_themes').click(function() {
                        /* Getting the variable's value from a link */
                        var langBox = $(this).attr('href');

                        /* Fade in the Popup */
                        $(langBox).fadeIn(300);

                        /* Set the center alignment padding + border see css style */
                        var popMargTop = ($(langBox).height() + 24) / 2;
                        var popMargLeft = ($(langBox).width() + 24) / 2;

                        $(langBox).css({
                            'margin-top': -popMargTop,
                            'margin-left': -popMargLeft
                        });

                        /* Add the mask to body*/
                        $('body').append('<div id="mask"><'+'/div>');
                        $('#mask').fadeIn(300);

                        return false;
                    });

                    $('a#aq_lang_selector').click(function() {
                        /* Getting the variable's value from a link */
                        var langBox = $(this).attr('href');

                        /* Fade in the Popup */
                        $(langBox).fadeIn(300);

                        /* Set the center alignment padding + border see css style */
                        var popMargTop = ($(langBox).height() + 24) / 2;
                        var popMargLeft = ($(langBox).width() + 24) / 2;

                        $(langBox).css({
                            'margin-top': -popMargTop,
                            'margin-left': -popMargLeft
                        });

                        /* Add the mask to body*/
                        $('body').append('<div id="mask"><'+'/div>');
                        $('#mask').fadeIn(300);

                        return false;
                    });

                    /* When clicking on the button close or the mask layer the popup closed */
                    $('a.close, #mask').live('click', function() {
                        $('#mask , .modal-popup').fadeOut(300, function() {
                            $('#mask').remove();
                        });
                        return false;
                    });
                });
            </script>
            

            
