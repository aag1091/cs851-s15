    var Blogs_GIBlotPost_AjaxEndpoint = '/Utility/ContentFragments/GameInformer/Post/PostFeedback.asmx';

    $(document).ready(function () {
        $('.ReportBlogButton').click(function (e) {
            var button = $(e.target);
            var id = button.attr('data-id');
            var data = '{"postID":"' + id + '"}';

            $.ajax({
                type: "POST",
                url: Blogs_GIBlotPost_AjaxEndpoint + "/MarkCommentAsSpam",
                contentType: "application/json; charset=utf-8",
                data: data,
                dataType: "json",
                beforeSend: function (xhr) {
                    TelligentUtility.WriteAuthorizationHeader(xhr);
                },
                success: function () {
                    if (window.ShowFixedMessageBox) {
                        window.ShowFixedMessageBox("This post has been submitted for review. Thank you.");
                    }
                    button.text("Submitted");
                    button.css({ "background": "#888822" });
                },
                error: function (xhr, desc, ex) {
                    var err = eval("(" + xhr.responseText + ")");
                    var msg = "An error occurred while processing your request\n";
                    if (err && err.Message)
                        msg += err.Message;
                    alert(msg);
                }
            });

            button.fadeOut(1000, function () { button.remove(); });
        })
    })
</script>
