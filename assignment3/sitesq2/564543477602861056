jQuery(document).ready(function() {
setTimeout (function() {
if (typeof ptchat != 'undefined') {
var LiveChatStatus = ptchat.chatStatus();
if (LiveChatStatus == 'enabled') {
// Live Chat is available
jQuery('.livechat-available-sidebar').html("<div class='livechat-bar topbar'><span onclick='ptchat_openChat(); return false;' class='livechat-icon'></span><span onclick='ptchat_openChat(); return false;' class='support-title'>Live Chat Available</span><span class='support-subtitle'>Support at your <a class='help-sidebar-link' onclick='ptchat_openChat(); return false;' href='#'>finger tips</a></span></div>");
