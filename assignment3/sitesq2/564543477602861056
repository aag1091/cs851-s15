}
else {
// Live Chat is NOT available
jQuery('.telephone-bar').attr('class', 'telephone-bar topbar');
}
}
else {
// Live Chat is NOT available (admin fallback)
jQuery('.telephone-bar').attr('class', 'telephone-bar topbar');
}
//alert(LiveChatStatus);
}, 2000);
});
