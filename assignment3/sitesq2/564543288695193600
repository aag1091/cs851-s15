    <script type="text/javascript">
    // This code generates a "Raw Searcher" to handle search queries. The Raw
    // Searcher requires you to handle and draw the search results manually.
    google.load('search', '1');

    var newsSearch;
    function searchComplete() {

      // Check that we got results
      document.getElementById('content').innerHTML = '';
      if (newsSearch.results && newsSearch.results.length > 0) {
        for (var i = 0; i < 1; i++) {

          // Create HTML elements for search results
          var p = document.createElement('a');
          var a = document.createElement('a');

         // a.href = newsSearch.results[i].url;
         // a.innerHTML = newsSearch.results[i].title;
          a.innerHTML = newsSearch.results[i].content;

          // Append search results to the HTML nodes
          p.appendChild(a);
          //document.body.appendChild(p);
            document.getElementById("content").appendChild(p);
        }
      }

      //2
            document.getElementById('content2').innerHTML = '';
      if (newsSearch.results && newsSearch.results.length > 0) {
        for (var i = 1; i < 2; i++) {

          // Create HTML elements for search results
          var p = document.createElement('a');
          var a = document.createElement('a');
          // a.href = newsSearch.results[i].url;

          a.innerHTML = newsSearch.results[i].content;

          // Append search results to the HTML nodes
          p.appendChild(a);
          //document.body.appendChild(p);
            document.getElementById("content2").appendChild(p);


        }
      }
     //

             //3
            document.getElementById('content3').innerHTML = '';
      if (newsSearch.results && newsSearch.results.length > 0) {
        for (var i = 3; i < 4; i++) {

          // Create HTML elements for search results
          var p = document.createElement('a');
          var a = document.createElement('a');
         a.innerHTML = newsSearch.results[i].content;
          // Append search results to the HTML nodes
          p.appendChild(a);
          //document.body.appendChild(p);
            document.getElementById("content3").appendChild(p);

        }
      }
     //


        //content_small1

      document.getElementById('content_small1').innerHTML = '';
      if (newsSearch.results && newsSearch.results.length > 0) {
        for (var i = 0; i < 1; i++) {
          // Create HTML elements for search results
          var p = document.createElement('a');
          var a = document.createElement('a');
          // a.href = newsSearch.results[i].url;
          a.innerHTML = newsSearch.results[i].title;
          // Append search results to the HTML nodes
          p.appendChild(a);
          //document.body.appendChild(p);
            document.getElementById("content_small1").appendChild(p);
        }
      }
     //


             //content_small2

      document.getElementById('content_small2').innerHTML = '';
      if (newsSearch.results && newsSearch.results.length > 0) {
        for (var i = 1; i < 2; i++) {
          // Create HTML elements for search results
          var p = document.createElement('a');
          var a = document.createElement('a');
          // a.href = newsSearch.results[i].url;
          a.innerHTML = newsSearch.results[i].title;
          // Append search results to the HTML nodes
          p.appendChild(a);
          //document.body.appendChild(p);
            document.getElementById("content_small2").appendChild(p);
        }
      }
     //



    }


    function onLoad() {
      newsSearch = new google.search.NewsSearch();
      google.search.Search.getBranding('branding');
      google.search.Search.getBranding('branding2');
      google.search.Search.getBranding('branding3');

      var  extendedArgs = google.search.Search.RESTRICT_EXTENDED_ARGS;
      newsSearch.setRestriction(extendedArgs, {'ned':'us'});
      newsSearch.setSearchCompleteCallback(this, searchComplete, null);
      newsSearch.execute("");
    }


