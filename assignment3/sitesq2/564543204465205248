<script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-21104474-1']);
  _gaq.push(['_setDomainName', 'radiko.jp']);
  _gaq.push(['_trackPageview']);
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    function is_mobile() {
      var useragents = ['DoCoMo','KDDI','DDIPOKET','UP.Browser','J-PHONE','Vodafone','SoftBank'];
      var pattern = new RegExp(useragents.join('|'), 'i');
      return pattern.test(navigator.userAgent);
    }
    function is_wp() {
      var useragents = ['IS12T'];
      var pattern = new RegExp(useragents.join('|'), 'i');
      return pattern.test(navigator.userAgent);
        
    }
    if (is_wp()) {
      location.href="http://radiko.jp/mobile/";
    }
    else if (is_mobile()) {
      location.href="http://radiko.jp/mobile/feature/";
    }
  })();
</script>
