<script type="text/javascript">

var isSharethroughBlank = false; //declare window global

function adCallBack(obj) {

    if (obj.slot == XFP_sharethrough_top) {
        
       
        if (obj.isEmpty || isSharethroughBlank) { // ad is blank

            if ($('.featureComponent .coverItem').length == 6) {
                $('.featureComponent .coverItem').eq(4).show();
            }
            if ($('.featureComponent .coverItem').length == 3) {
                $('.featureComponent .coverItem').eq(1).show();
            }
            if ($('.featureComponent .coverItem').length == 9) {
                $('.featureComponent .coverItem').eq(7).show();
            }
            if ('blogs_eye_entry' == 'blogs_eye_entry') {
                var length = $('#vidComponent table').length;
                $('#vidComponent table').eq(length - 1).show();
                //$('#sharethrough_top').removeAttr('style');
            }
            isSharethroughBlank = false; // reset to false for next time ad is refreshed
        }
        else {
    
            // ad is not blank
            //video component
            if ('blogs_eye_entry' == 'blogs_eye_entry') {
                 
                var length = $('#vidComponent table').length;
                $('#vidComponent table').eq(length - 1).hide();
                //$('#sharethrough_top').removeAttr('style');
            }
            //homepage
            //Six pack
            if ($('.featureComponent .coverItem').length == 6) {
                $('.featureComponent .coverItem').eq(4).hide();
                $('#sharethrough_top').show();
            }
            //Three pack
            if ($('.featureComponent .coverItem').length == 3) {
                $('.featureComponent .coverItem').eq(1).hide();
                $('#sharethrough_top').show();
            }
            //Nine pack
            if ($('.featureComponent .coverItem').length == 9) {
                $('.featureComponent .coverItem').eq(7).hide();
                $('#sharethrough_top').show();
            }
        }
    }
}

function blankAdCallback(width, height, pos) {
  
    //test for height/width
    if (width == 11 && height == 11 && pos == 'top') {
        isSharethroughBlank = true; // set global
        
    }
}


</script>
