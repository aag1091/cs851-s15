        document.location.href = 'http://www.cbssports.com/login?xurl=' + encodeURIComponent('http://www.cbssports.com/nfl/eye-on-football/25062890/ron-rivera-sends-letter-team-gear-to-panthers-fan-after-house-fire');
    }

