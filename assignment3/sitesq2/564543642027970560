<script type="text/javascript">
    $(document).ready(function () {
        $('ul.top-menu').superfish();
        $('ul.left-menu').superfish({
            animation: { height: 'show' },   // slide-down effect without fade-in 
            delay: 1               // 1.2 second delay on mouseout 
