        trlCookie = getPolusaCookie('polllortsi2');
        if(data.tr == 'true' || trlCookie.indexOf('true') >= 0){
            setPolusaCookie('polllortsi2', 'true',(365*60));
            jQuery('#comment-container').remove();
            jQuery('#sb-comments').remove();
        }
    });
</script>

