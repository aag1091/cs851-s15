var DW_ptid_string = DW_ptid.toString();
var onidFinal_string = onidFinal.toString();
var page_ptid_nodeid = DW_ptid_string + ":" + onidFinal_string;


var interstitial = (((($.inArray(DW_ptid_string, interstitial_ptids) == -1 && $.inArray(page_ptid_nodeid, interstitial_ptids_nodeids) == -1) || ($.inArray(page_ptid_nodeid, interstitial_exceptions) != -1)) && interstitial_id != "")) ? true : false;
var skin_top = ((($.inArray(DW_ptid_string, skin_top_ptids) != -1 || $.inArray(page_ptid_nodeid, skin_top_ptids_nodeids) != -1) && skin_top_id != "")) ? true : false;
var magnet_bottom = ((($.inArray(DW_ptid_string, magnet_bottom_ptids) != -1 || $.inArray(page_ptid_nodeid, magnet_bottom_ptids_nodeids) != -1) && magnet_bottom_id != "")) ? true : false;
var leader_top = (((($.inArray(DW_ptid_string, leader_top_ptids) == -1 && $.inArray(page_ptid_nodeid, leader_top_ptids_nodeids) == -1) || ($.inArray(page_ptid_nodeid, leader_top_exceptions) != -1)) && leader_top_id != "")) ? true : false;
var leader_bottom = (($.inArray(DW_ptid_string, leader_bottom_ptids) != -1 || $.inArray(page_ptid_nodeid, leader_bottom_ptids_nodeids) != -1) && ($.inArray(page_ptid_nodeid, leader_bottom_exclusions) == -1) && leader_bottom_id != "") ? true : false;
var leader_middle = (($.inArray(DW_ptid_string, leader_middle_ptids) != -1 || $.inArray(page_ptid_nodeid, leader_middle_ptids_nodeids) != -1) && leader_middle_id != "") ? true : false;
var marquee_top = (((($.inArray(DW_ptid_string, marquee_top_ptids) == -1 && $.inArray(page_ptid_nodeid, marquee_top_ptids_nodeids) == -1) || ($.inArray(page_ptid_nodeid, marquee_top_exceptions) != -1)) && marquee_top_id != "")) ? true : false;
var mpu_top = (((($.inArray(DW_ptid_string, mpu_top_ptids) == -1 && $.inArray(page_ptid_nodeid, mpu_top_ptids_nodeids) == -1) || ($.inArray(page_ptid_nodeid, mpu_top_exceptions) != -1)) && mpu_top_id != "")) ? true : false;
var mpu_bottom = (((($.inArray(DW_ptid_string, mpu_bottom_ptids) != -1 || $.inArray(page_ptid_nodeid, mpu_bottom_ptids_nodeids) != -1) && ($.inArray(page_ptid_nodeid, mpu_bottom_exceptions) == -1)) && mpu_bottom_id != "")) ? true : false;
var leader_plus_top = ((($.inArray(DW_ptid_string, leader_plus_top_ptids) != -1 || $.inArray(page_ptid_nodeid, leader_plus_top_ptids_nodeids) != -1) && leader_plus_top_id != "")) ? true : false;
var mpu_plus_top = (($.inArray(DW_ptid_string, mpu_plus_top_ptids) != -1 || $.inArray(page_ptid_nodeid, mpu_plus_top_ptids_nodeids) != -1) && mpu_plus_top_id != "") ? true : false;

