var leader_top_id = ($('#leader-top').length) ? 'leader-top' : 'ad728x90';
var leader_bottom_id = ($('#leader-bottom').length) ? 'leader-bottom' : '';
var leader_middle_id = ($('#leader-middle').length) ? 'leader-middle' : '';
var marquee_top_id = ($('#marquee-top').length) ? 'marquee-top' : 'cbs-ad-234x60';
var mpu_top_id = ($('#mpu-top').length) ? 'mpu-top' : 'cbs-ad-300x250-0';
var mpu_bottom_id = ($('#mpu-bottom').length) ? 'mpu-bottom' : 'cbs-ad-300x250';
