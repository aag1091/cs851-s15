$(document).ready(function(){
extendedForecastURL = 'http://www.birminghammail.co.uk/weather/';
iconURL = 'http:\/\/s.birminghammail.co.uk\/skins\/shared\/gfx/widgets/weather/';
ajaxURL = 'http:\/\/www.birminghammail.co.uk\/weather\/?service=ajax&item=weather';
$.ajax({
url: ajaxURL,
dataType: 'json',
success: function(forecast) {
var linkURL = 'http:\/\/www.birminghammail.co.uk\/weather\/';
$("#weather-hdr-icon-link").attr("href", linkURL);
$("#weather-hdr-icon-image").attr("src", iconURL + forecast.conditions.weatherIcon + ".png");
