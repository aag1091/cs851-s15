<script type="text/javascript">
jQuery( function($)
{
    $( '.newsletter_signup_node_id' ).val( $( 'body' ).attr( 'data-context-node-id' ) );

    // custom newsletter signup box text
    // var section = $( 'meta[ property="article:section" ]' ).attr( 'content' );
    var section = document.location.pathname.split("/")[1];

    switch( section ) {
        case "Environment":
            $( '#action-block-wrapper #action-2-msg-1' ).html( 'Subscribe to Recharge' );
            $( '#action-block-wrapper #action-2-msg-2' ).html( 'Get Monitor global energy news and analysis delivered weekly to your inbox.' );
            break;
    }

    // customize take action box
    if( $( '#ezve-right-col-2-r14.eznid-867085' ).length) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Environment' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Environment' );
    } else if( $( '#ezve-right-col-2-r14.eznid-867087' ).length ) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Education' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Education' );
    } else if( $( '#ezve-right-col-2-r14.eznid-867090' ).length ) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Human Trafficking' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Human-Trafficking' );
    } else if( $( '#ezve-right-col-2-r14.eznid-867092' ).length ) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp; Conflict Resolution' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Conflict-Resolution' );
    } else if( $( '#ezve-right-col-2-r14.eznid-867094' ).length ) {
        $( '#action-block-wrapper #action-1-btn a' ).html( 'Take Action:&nbsp;<br/>Disaster Relief &amp; Recovery' );
        $( '#action-block-wrapper #action-1-btn a' ).attr( 'href', '/Take-Action/Disaster-Relief-and-Recovery' );
    } else {
        $( '#action-block-top' ).hide();
    }
});
</script>
