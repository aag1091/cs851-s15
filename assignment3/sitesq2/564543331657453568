            function ChartbeatTitleGet(scw){
              var t= "";
              if (scw.ContentProperties)
              {
                if (scw.ContentProperties.title)
                  {
                      t = scw.ContentProperties.title;
                  }
              }
              return t;
            }
          
            function ChartbeatSectionsGet(scw)
            {
            var ret = "";
            if (scw)
            {
              if (scw.ContentProperties)
              {
                var contProp = scw.ContentProperties;
                ret += (contProp.sec) ? contProp.sec : '';
                if (ret!='')
                {
									ret += (contProp.subsec1) ? '-' + contProp.subsec1 : '';
									ret += (contProp.subsec2) ? '-' + contProp.subsec2 : '';
									ret += (contProp.subsec3) ? '-' + contProp.subsec3 : '';
									ret += (contProp.subsec4) ? '-' + contProp.subsec4 : '';
									if(contProp.type.indexOf('list:') !== -1){
										ret += '-list';
									}
									if(contProp.type !== 'article:video'){
										if(contProp.type.indexOf('article') !== -1){
											if(ret !== contProp.sec + '-news'){
												ret += ',' + contProp.sec + '-news';
											}
											ret += ',news';
										}
									}
                }
              }
            }
            return ret;
            }
           
