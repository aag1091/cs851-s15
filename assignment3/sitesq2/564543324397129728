  function initEmailArticleForm1745892() {
    var validation = {
      rules: {
        fromEmailAddr1745892: {
            required: true,
            email: true
        },
        toEmailAddr1745892: {
            required: true,
            multiemail: true
        },
        senderName1745892: {
            required: true
        },
        message1745892: {
            required: true
        }
      },
      messages: {
        fromEmailAddr1745892: {
            required: "Sender email is required",
            email: "A valid email is required"
        },
        toEmailAddr1745892: {
            required: "Receiver email is required",
            multiemail: "A valid email is required"
        },
        senderName1745892: {
            required: "Sender name is required"
        },
        message1745892: {
            required: "Email message is required",
            email: "Email message is required"
        }
      },
      submitHandler: function(form) {
          $('#submitEmail1745892').css("background-color", "#ccc");
          $('#submitEmail1745892').attr("disabled", true);

          var siteId = '2.2934';
		  var pageId = '2.2934';
          var articleId = '1.1745892';
		  
          emailArticle('1745892', siteId, articleId, pageId);
      }
    };
    var validator = jQuery("#emailArticleForm1745892").validate(validation);
  }
  
  function resetEmailArticleForm(elementId) {
    $('#fromEmailAddr' + elementId).val('');
    if (polopoly.user.isLoggedIn() && polopoly.user.screenName().length > 0) {
        $('#senderName' + elementId).val(polopoly.user.screenName());
    } else {
        $('#senderName' + elementId).val('');
    }
    $('#toEmailAddr' + elementId).val('');
    $('#message' + elementId).val('');
    $('div.emailFriend div.toprow').each(function(){
      $(this).find('label.error').remove();
    });
    $('#submitEmail' + elementId).css("background-color", "#FFF");
    $('#submitEmail' + elementId).removeAttr("disabled");
    $('#emailArticleError' + elementId).html('');
    
    // Reset individual errors
    $("#emailArticleForm1745892 input.error").removeClass("error");
    $("#emailArticleForm1745892 label label.error").remove();
    
    characterCounter(elementId);
  }
  
  function emailArticle(elementId, siteId, articleId, pageId) {
    var fromEmailAddr = $('#fromEmailAddr' + elementId).val();
    var senderName = $('#senderName' + elementId).val();
    var toEmailAddr = $('#toEmailAddr' + elementId).val();
    var message = $('#message' + elementId).val(); 
    var scrubbedMessage = message.replace(/(<([^>]+)>)/ig,"");
    
    if(articleId != '' && fromEmailAddr != '' && senderName != '' && toEmailAddr != '' && scrubbedMessage != '' && siteId != '') {
      jQuery.ajax({
        type: 'POST',
        dataType: 'json',
        url: '/emailarticle?articleId=' + articleId + '&fromEmailAddr=' + fromEmailAddr + 
        '&senderName=' + senderName + '&toEmailAddr=' + toEmailAddr + '&message=' + scrubbedMessage + '&siteId=' + siteId + '&pageId=' + pageId,
        success: function(response) {
          if (response.error) {
            $('#emailArticleError' + elementId).html(response.error);
          } else {
            $('#emailArticleMask, #emailArticleContainer .window').hide();
          }
          
          $('#submitEmail' + elementId).removeAttr("disabled");
        }
      });
    } else {
        $('#submitEmail' + elementId).removeAttr("disabled");
    }
  }

  function characterCounter(elementId)  {
    var charLimit = 200;
    
    $("#emailCharacters").append("<strong>"+ charLimit +"</strong> characters remaining");
    
    $("#message" + elementId).keyup(function(){
        if($(this).val().length > charLimit){
            $(this).val($(this).val().substr(0, charLimit));
        }
        
        var remainingChars = charLimit -  $(this).val().length;
    
        $("#emailCharacters").html("<strong>"+ remainingChars + "</strong> characters remaining");
    
        if(remainingChars <= 20) {
            $("#emailCharacters").css("color","#C11B17");
        }
        else {
            $("#emailCharacters").css("color","#666");
        }
    });
  }
  

  jQuery.validator.addMethod("multiemail", function(value, element) {
	if(this.optional(element)) // return true on optional element
      return true;
        
    var emails = value.split(';');
    valid = true;
      
    for(var index in emails) {
      value = emails[index].replace(/(^\s+|\s+$)/g,"");
      valid = valid && jQuery.validator.methods.email.call(this, value, element);
    }
          
    return valid;
  }, 'The format for one or more emails is incorrect.');

</script>  <div id="sendStatHolder"></div>
  
                
        
