<script>
var modal = (function(){
    var 
    method = {},
    $overlay,
    $modal,
    $content,
    $close;

    // Append the HTML
	$overlay = $('<div id="overlay"></div>');
	$modal = $('<div id="modal"></div>');
	$content = $('<div id="content"></div>');
	$close = $('<a id="close" href="#">close</a>');
	
	$modal.hide();
	$overlay.hide();
	$modal.append($content, $close);

	$(document).ready(function(){
    	$('body').append($overlay, $modal);
	});

    // Center the modal in the viewport
    method.center = function () {
	    var top, left;
	
	    top = Math.max($(window).height() - $modal.outerHeight(), 0) / 2;
	    left = Math.max($(window).width() - $modal.outerWidth(), 0) / 2;
	
	    $modal.css({
	        top:top + $(window).scrollTop(), 
	        left:left + $(window).scrollLeft()
	    });
	};

    // Open the modal
    method.open = function (settings) {
	    $content.empty().append(settings.content);
	
	    $modal.css({
	        width: settings.width || 'auto', 
	        height: settings.height || 'auto'
	    })
	
	    method.center();
	
	    $(window).bind('resize.modal', method.center);
	
	    $modal.show();
	    $overlay.show();
	    clearTimeout(modalPop);
	    $("#modal #emailSignupForm").validate({submitHandler:function(form){
															$("#email_signup_modal #enewsLoading").css('visibility','visible');
															$.post($(form).attr("action"),$(form).serialize(),function(data){
																	var respObj = jQuery.parseJSON( data );
													  				if (respObj.status == "fail"){
													  					$('#email_signup_modal #modalMain').empty().append('<p>We are very sorry, but there was a problem signing up for the newsletter.<br/>  Please let our webmaster know.</p>');	
													  				}
													  				else{ // Success
													  					$.cookie('emailnewsletterpopup', '1',{ expires: 365 });
													  					$('#email_signup_modal #modalMain').empty().append('<p>You have successfully signed up for our newsletter.<br/>  Thank you for subscribing!</p>');
													  				}
													  		});
													  		return false;				
															},
	    									  rules:{
	    									  	'lid[]': {required: true}
	    									  },
	    									  errorPlacement: function(error, element) {
		    									  switch (element) {
											        case element.attr("type") == 'checkbox':
											            error.insertAfter($("#modal #emailSignupForm fieldset"));
											            break;
											        default:
	    												error.insertAfter(element); // <- the default
	    										  }
											  }
	    									  });
	};

    // Close the modal
    method.close = function () {
	    $modal.hide();
	    $overlay.hide();
	    $content.empty();
	    $(window).unbind('resize.modal');
	    $.cookie('emailnewsletterpopup', '1',{ expires: 365 });
	};

	$close.click(function(e){
	    e.preventDefault();
	    method.close();
	});

    return method;
}());

// Wait until the DOM has loaded before querying the document
$(document).ready(function(){
	var emailModalContent = $("#emailSignupModalContainer").html();
	
	/*$('body').delegate('#emailSignupForm', 'submit', function () {
	    $("#email_signup_modal #enewsLoading").css('visibility','visible');
		$.post($(this).attr("action"),$(this).serialize(),function(data){
				var respObj = jQuery.parseJSON( data );
  				if (respObj.status == "fail"){
  					$('#email_signup_modal #modalMain').empty().append('<p>We are very sorry, but there was a problem signing up for the newsletter.<br/>  Please let our webmaster know.</p>');	
  				}
  				else{ // Success
  					$.cookie('emailnewsletterpopup', '1',{ expires: 365 });
  					$('#email_signup_modal #modalMain').empty().append('<p>You have successfully signed up for our newsletter.<br/>  Thank you for subscribing!</p>');
  				}
  		});
  		return false;
  		
	})*/
	
	modalPop = setTimeout(function() {
	        var showPopup = $.cookie('emailnewsletterpopup');
	        if (showPopup != "1" && "false" == "true" ){
	    		modal.open({content: emailModalContent});
	    	}
		}, 1000);
		
	$("body").on('click','.closeEmailPopup',function(){
		modal.close();
	});
	
	$(".emailPopupLink").on('click',function(){
		modal.open({content: emailModalContent});
		return false;
	});
	
});

</script>
	        </section>

                       
