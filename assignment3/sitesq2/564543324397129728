<script>

// Wait until the DOM has loaded before querying the document
$(document).ready(function(){
    $( "#rail_email_form" ).submit(function( event ) {
        var emailFromRR = $("#rrEmail").val();
        var emailModalContent = $("#emailSignupModalContainer").html();
        modal.open({content: emailModalContent});
        $(".popupEmail").val(emailFromRR);
		return false;     
    });   
});

</script>
                                    
