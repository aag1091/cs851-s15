                      <script type='text/javascript'>
          jQuery("nav[role='navigation'] ul.container li.home").addClass("active");
        </script>
            
        
    
		<script>
		  
			var navProcessed = false;
			var seeMoreText = "See More";
			var navOffset = 100;
			
			if ("See More".length > 1) {
				seeMoreText = "See More";
			}
			
			try {
				if (typeof 100 === 'number') {
					navOffset = 100;
				} else {
					navOffset = parseInt(100,10);
				}
			} catch(e) {
				console.log("Unable to set navOffset",e);
				navOffset = 100;
			}

			function fixNav(hideOnly) {
			
		      if (navProcessed) {
		         jQuery("#seeMoreSubNav").remove();
		         jQuery("nav[role='navigation'] > ul.navcontainer > li.seeMore-hidden").show();
		      }  
		      
		      var ulWidth = jQuery("nav[role='navigation'] > ul.navcontainer").width();  
		      var liWidth = 0;
		      var liWidthArr = [];
		      var liCount = 0;
		      var currLi = 0;
		    
		      jQuery("nav[role='navigation'] > ul.navcontainer > li").each(function(index){
		        liWidth = liWidth + jQuery(this).width();
		        liWidthArr[index] = liWidth;
		        liCount = index;
		      });
		        
		      var excpt = false;
		    
		      if(liWidth > ulWidth) {
			  
				  navProcessed = true;	  
		          currLi = liCount;
		          while((liWidthArr[currLi] + navOffset) > ulWidth) {
		            jQuery("nav[role='navigation'] > ul.navcontainer > li:nth-child("+(currLi+1)+")").addClass("seeMore-hidden").hide();
		            currLi = currLi - 1;
		            if (currLi < 0) {
		                excpt = true;
		                break;
		            }
		          
		          }
		          
		          if (hideOnly) return;
		        
				  var myHtml = '';
				  myHtml += '<li class="dropdown" role="menuitem" id="seeMoreSubNav">';
				  myHtml += '<a href="javascript:void(0)" data-target="#" data-toggle="dropdown" role="button" id="btnMoreNav" class="dropdown-toggle">'+seeMoreText+'<b class="caret"></b></a>';
				  myHtml += ' <ul role="menu" class="moreNav dropdown-menu">';
				  
				  var seeMoreActive = false;
			  
				  for(var x=(currLi+2);x <= (liCount+1);x++) {
					 var thisLi = jQuery("nav[role='navigation'] > ul.navcontainer > li:nth-child("+x+")");
					 var thisHref = jQuery("nav[role='navigation'] > ul.navcontainer > li:nth-child("+x+") > a").attr("href");
					 var thisLinkTitle = jQuery("nav[role='navigation'] > ul.navcontainer > li:nth-child("+x+") > a").html();
					 var thisLiActive = jQuery("nav[role='navigation'] > ul.navcontainer > li:nth-child("+x+")").hasClass("active");
					 if (thisLiActive) {
					 	myHtml += '<li role="menuitem" class="sub-active">';
					 	seeMoreActive = true;
					 } else {
					 	myHtml += '<li role="menuitem">';
					 }
					 myHtml += '<a href="'+thisHref+'">'+thisLinkTitle+'</a>';
					 myHtml += '</li>';
				  }
			  
				  myHtml += '</ul></li>';
				  
				  jQuery("nav[role='navigation'] > ul.navcontainer").append(myHtml);
				  
				  var navBgColor = jQuery("nav[role='navigation']").css("background-color");
				  var navLinkColor = jQuery("nav[role='navigation'] > ul > li:nth-child(3) > a").css("color");
				  var navHeight = jQuery("#btnMoreNav").height();
				  var caretTop = 13;
				  
				  try {
					caretTop = Math.floor(( navHeight / 2) - 2);
				  } catch(e) {
					console.log("More Nav Dropdown: error calculating height");
					caretTop = 13;
				  }
				  
				  
				  jQuery("ul.moreNav.dropdown-menu").css("background-color",navBgColor);
				  jQuery("ul.moreNav li a").css("color",navLinkColor);
				  jQuery("#btnMoreNav .caret").css({
					"margin-top": caretTop,
					"margin-left" : "4px",
					"border-top-color" : navLinkColor,
					"border-bottom-color" : navLinkColor
				  });
				  if (seeMoreActive) {
				  	jQuery("#seeMoreSubNav").addClass("sub-active");
					jQuery("#seeMoreSubNav").prevAll(".remove-separator:first").addClass("seeMore");
				  }
			  }
			}
		  	
		  	fixNav(true);
		  	jQuery(document).ready(function(){
		  		fixNav(false);
		  	});
		    jQuery("nav[role='navigation'] > ul.navcontainer").resize(function(){
		      fixNav();
		    });
		  
		</script>
              
      
  
                </section>

              
                  
    
  
    
  
    
    
