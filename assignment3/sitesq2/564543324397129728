     // When the window is resized, it can wind up floating to the bottom of the page.  This puts it back at the top,
     // but it should be avoided on mobile devices as resize seems to be frequently fired just by scrolling 
     // (i.e. dragging) the page, resulting in the form chasing the scroll position.
     $(window).on('resize',function() {
        //Only manipulate the form if it's actually visible and not on a mobile device
        if ($("#emailArticle1745892").css("display") != "none" && !jQuery.browser.mobile) {
           reposition1745892();
        }
     });

    });
    
  // Action when a href called 'modal' is clicked
  popEmailArticleForm = function() {
    $("#emailCharacters").html('');
    resetEmailArticleForm('1745892');

    var id = $('.social .email a[name=modal]').attr('id');

    // Fade in the mask background
    $('#emailArticleMask').fadeTo("slow",0.8); 

    // Fade in popup window
    $(id).fadeIn(2000);
    
    reposition1745892();
	
	$('#emailArticle1745892').css('display', 'block');
  }
    
  reposition1745892 = function() {
