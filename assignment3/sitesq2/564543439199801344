                }
                clearTimeout(rtb_timeout);
                window.likes_ad_server.clear_rtb_ready_listener('rightbottom');
                if (!window.likes_ad_server.render_better_ad('rightbottom', cpm, 'ad_slot_rightbottom_rect')) {
                    window.likes_ad_passbacks.render_ad('ad_slot_rightbottom_rect', 300, 250, undefined, "//nvdesonthegif.com/ad/15/openx/529989/300/250/all/rightbottom");
                }
            };
