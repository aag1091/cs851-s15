 data-has-parent-tweet="true" 


data-expanded-footer="&lt;div class=&quot;js-tweet-details-fixer tweet-details-fixer&quot;&gt;&#10;&#10;&#10;&#10;      &lt;div class=&quot;entities-media-container js-media-container&quot; style=&quot;min-height:0px&quot;&gt;&#10;      &lt;/div&gt;&#10;&#10;  &lt;de": "att.net"}, {"value": "sears.com"}, {"value": "discovercard.com"}, {"value": "daimlerchrysler.com"}, {"value": "nimaxtheatres"}, {"value": "frontline"}, {"value": "dollargeneral.com"}, {"value": "lowes.com"}, {"value": "nordstromrack"}, {"value": "softcoin.com"}];
        ad_server.networks.pubmatic.is_ad_whitelisted = function(url) {
            var whitelisted = false;
            for (var i = 0; i < ad_server.networks.pubmatic.ad_whitelist.length; i++) {
                if (url.match(RegExp(".*" + ad_server.networks.pubmatic.ad_whitelist[i]['value'] + ".*"))) {
                    whitelisted = true;
                }
            }
            return whitelisted;
        };
        ad_server.networks.pubmatic.rendered_ads = [];
        ad_server.networks.pubmatic.get_candidate_ads = function(ad_server) {
            var helpers = {};
            helpers.get_time = function(d) {
                var t = d ? d : new Date();
                var y = t.getFullYear();
                var m = t.getMonth() + 1;
                var d = t.getDate();
                var h = t.getHours();
                var min = t.getMinutes();
                var s = t.getSeconds();
                var pad = function(i) {
                    return i <= 9 ? '0' + i : '' + i;
                }
                return escape(y + '-' + pad(m) + '-' + pad(d) + ' ' + pad(h) + ':' + pad(min) + ':' + pad(s) );
            };
            helpers.get_timezone_offset = function() {
                var t = new Date();
                return -1 * t.getTimezoneOffset()/60;
            };

            helpers.get_ad_position = function(ad_div_id) {
                try {
                    ad_div = document.getElementById(ad_div_id);
                    if (!ad_div) {
                        return "-1x-1";
                    }
                    var curleft = curtop = 0;
                    var elem = ad_div;
                    if (elem && elem.offsetParent) {
                        do {
                            curleft += elem.offsetLeft;
                            curtop += elem.offsetTop;
                        } while ( elem = elem.offsetParent);
                    }
                    return curtop + 'x' + curleft;
                } catch (e) {
                    return "-1x-1";
                }
            };
            ad_server.networks.pubmatic.helpers = helpers;

            var call_mgr = {};
            call_mgr.construct_position_map = function() {
                call_mgr.position_map = {};
                var slot_info;
                for (var i=0; i < ad_server.js_rtb_slots.length; i++) {
                    slot_info = ad_server.js_rtb_slots[i];
                    if (slot_info.network == "pubm_api") {
                        call_mgr.position_map[slot_info.position] = slot_info;
                    }
                }
            };
            call_mgr.construct_position_map();
            call_mgr.construct_base_rid_map = function() {
                call_mgr.base_rid_map = {};
                var slot_info;
                for (var i=0; i < ad_server.js_rtb_slots.length; i++) {
                    slot_info = ad_server.js_rtb_slots[i];
                    if (slot_info.network == "pubm_api") {
                        call_mgr.base_rid_map[slot_info.base_rid] = { 'position': slot_info.position, 'width': slot_info.width, 'height': slot_info.height, 'cpm_seq': slot_info.cpm_seq };
                    }
                }
            };
            call_mgr.construct_base_rid_map();
            call_mgr.get_call_info = function (rid) {
                var call_info = {};
                call_info.base_rid = Math.floor(rid/10);
                call_info.pos_info = call_mgr.base_rid_map[call_info.base_rid];
                call_info.call_num = rid % 10;
                return call_info;
            };
            call_mgr.get_rid = function ( position, call_num) {
                return call_mgr.position_map[position].base_rid*10 + call_num;
            };
            call_mgr.get_next_call_cpm_2 = function(call_num, base_rid) {
                var cpm_seq = call_mgr.base_rid_map[base_rid].cpm_seq;
                if (call_num < cpm_seq.length) {
                    return cpm_seq[call_num];
                }
            };
            call_mgr.call_status = {};
            call_mgr.initiate_call = function(rid) {
                call_mgr.call_status[rid] = {'complete': 0};
            }
            call_mgr.complete_call = function(rid) {
                call_mgr.call_status[rid].complete = 1;
            }
            call_mgr.is_position_ready = function(rid) {
                var call_info = call_mgr.get_call_info(rid);
                var base_rid = call_info.base_rid;
                var num_calls = call_mgr.base_rid_map[base_rid].cpm_seq.length;
                var wait_for = 1;
                wait_for = 4;
                var complete = true;
                for (var i=0;i<wait_for;i++) {
                    var call_num = i + 1;
                    var call_rid = call_mgr.get_rid(call_info.pos_info.position, call_num);
                    if (call_mgr.call_status[call_rid] && !(call_mgr.call_status[call_rid].complete == 1)) {
                        complete = false;
                    }
                }
                if (complete && window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'declaring_position_ready', 'position': call_info.pos_info.position});
                }
                return complete;
            };
            ad_server.networks.pubmatic.call_mgr = call_mgr;

            if (!ad_server.networks.pubmatic.synced_up_pixels) {
                var ifrm = likes_ad_passbacks.create_iframe(1,1);
                ifrm.src = "http://ads.pubmatic.com/AdServer/js/syncuppixels.html";
                if (ifrm.style) {
                    ifrm.style.float = "left";
                }
                if (document.body) {
                    document.body.appendChild(ifrm);
                } else {
                    setTimeout(function() {
                        try {
                            document.body.appendChild(ifrm);
                        } catch(e) {
                        }
                    }, 350);
                }
                ad_server.networks.pubmatic.synced_up_pixels = 1;
            }

            window.processPubMaticBid = function(response) {
                var rid_map = call_mgr.base_rid_map;
                var rid = response.meta_data.rid;
                var call_info = call_mgr.get_call_info(rid);
                var position = call_info.pos_info.position;
                var cpm = 0;
                if (response.PubMatic_Bid) {
                    cpm = response.PubMatic_Bid.ecpm;
                }
                if (window.top.likes_tracking) {
                    window.top.likes_tracking.track_event({'type': 'pubmatic_api_response', 'response': response, 'position': position, 'call_num': call_info.call_num, 'network': 'c_pm' + call_info.call_num, 'cpm': (cpm || 0.0) * 0.85});
                }
                var chosen_slot = ad_server.ads_by_slot[position];
                var ad_width  = call_info.pos_info.width;
                var ad_height = call_info.pos_info.height;
                if (response.PubMatic_Bid.error_code || response.PubMatic_Bid.creative_tag.match(/.*likes_ad_passbacks.render_passback.*/) || response.PubMatic_Bid.ecpm == 0 || (response.PubMatic_Bid.landing_page && ad_server.networks.pubmatic.is_url_blacklisted(response.PubMatic_Bid.landing_page, response.PubMatic_Bid.creative_tag, position, call_info.call_num, cpm))) {
                    call_mgr.complete_call(rid);
                    if (call_mgr.is_position_ready(rid)) {
                        ad_server.network_calls.complete('pubmatic', position);
                    }
                    return;
                }
                chosen_slot.push({
                    'network': 'pubmatic',
                    'cpm': response.PubMatic_Bid.ecpm * 0.85,
                    'render': function(ad_div_id, position, cpm) {
                        try {
                            if (window.top.likes_tracking) {
                                window.top.likes_tracking.track_event({'type': 'begin_pubmatic_render', 'position': position, 'api_response': response});
                                ad_server.networks.pubmatic.rendered_ads.push({'url': response.PubMatic_Bid.landing_page,
                                                                              'tag': response.PubMatic_Bid.creative_tag,
                                                                              'cpm': response.PubMatic_Bid.ecpm,
                                                                              'pos': position,
                                                                              'loc': 'US' });
                                if (!ad_server.networks.pubmatic.is_ad_whitelisted(response.PubMatic_Bid.landing_page)) {
                                    if (window.top.likes_tracking && window.top.likes_tracking.report_pubm_ads()) {
                                        ad_server.networks.pubmatic.report_rendered_ads();
                                    }
                                }
                            }
                            ad_div = document.getElementById(ad_div_id);
                            var ad_ifrm = window.likes_ad_passbacks.create_iframe(ad_width, ad_height);
                            ad_ifrm.onload = function() {
                                ad_ifrm.contentWindow.document.write(unescape("%3Chtml%3E%3Chead%3E%3Cbase%20target%3D%22_top%22%3E%3C/head%3E%3Cbody%20style%3D%22padding%3A0px%3B%20margin%3A0px%22%3E") + response.PubMatic_Bid.creative_tag + unescape("%3C/body%3E%3C/html%3E"));
                            };
                            ad_div.appendChild(ad_ifrm);
                            var tracking_ifrm = window.likes_ad_passbacks.create_iframe(0,0);
                            tracking_ifrm.onload = function() {
                                if (window.top.likes_tracking) {
                                    window.top.likes_tracking.track_event({'type': 'ad_slot_rendered', 'network':'c_pubm_api_' + call_info.call_num, 'position':position, 'cpm':cpm});
                                }
                            };
                            tracking_ifrm.src = response.PubMatic_Bid.tracking_url;
                            ad_div.appendChild(tracking_ifrm);
                        } catch(e) {/*ignore*/ }
                    }
                });
                call_mgr.complete_call(rid);
                if (call_mgr.is_position_ready(rid)) {
                    ad_server.network_calls.complete('pubmatic', position);
                }
            };

            var request_ad = function(position, cpm, call_num) {
                window.likes_ad_server.network_calls.initiate('pubmatic', position);
                var position_map = call_mgr.position_map;
                var script_tag = document.createElement('script');
                script_tag.setAttribute('type', 'text/javascript');
                var rid = call_mgr.get_rid(position, call_num);
                call_mgr.initiate_call(rid);
                var slot_info = position_map[position];
                script_tag.src = slot_info['api_endpoint']
                + 'operId=' + slot_info['operId'] + '&'
                + 'rs=2&'
                + 'pubId=41579&'
                + 'siteId=' + slot_inCount" aria-hidden="true" data-tweet-stat-count="0">
        <span class="ProfileTweet-actionCountForAria" >0 retweets</span>
      </span>
    </span>
