            <div class="searchOptions">
				<ul class="searchOptions_list">
					<li class="first">
						<span class="searchOptions_custom_radios checked" data-value="gsa" id="search_mysa_radio" onclick="javascript:customRadioToggleAndOptions('search_mysa_radio','searchindex_key')" ></span>
						<label for="searchindex_property">mySA.com</label>
					</li>
					<li>
						<span class="searchOptions_custom_radios" data-value="businesses" id="search_local_radio" onclick="javascript:customRadioToggleAndOptions('search_local_radio','searchindex_key')"></span>
						<label for="searchindex_business">Local Directory</label>
					</li>
				</ul>

            	
				<style>
					.searchOptions .searchOptions_radios {display: none;} 
					.searchOptions span.searchOptions_custom_radios {content: " ";display: inline-block;outline: 0px solid red;width: 16px;height: 10px;}
					.searchOptions span.searchOptions_custom_radios {background: url('/img/modules/mysa_redesign_assets/mySA_asset_global_icons_1x.png');background-position: 0px -98px;}
					.searchOptions span.checked {background-position: 0px -114px;}
				</style>
				<script type="text/javascript">
					function resetRadios(except){
						// need oject assoc here 
						var radios = [ 
							{ radio: "searchindex_business", custom: 'search_local_radio' },
							{ radio: 'searchindex_yahoo', custom: 'search_yahoo_radio' },
							{radio: 'searchindex_property', custom: 'search_mysa_radio'}];
						// reset all 
						for (var i = 0; i < radios.length; i++ ){
							if (radios[i].custom == except){
								continue;
							}
							var radio_custom = $("#"+radios[i].custom+"");
							if (radio_custom){
								radio_custom.removeClass("checked");
							}
						}
					}
					function customRadioToggleAndOptions(cid,rid){
						if (cid && rid){
							var span_id_obj = $("#"+cid+"");
							var search_obj = $("#"+rid+"");
							// Reset 
							resetRadios(cid);
							// Uncheck and update hidden radio input  if this was checked 
							if (span_id_obj.hasClass("checked")){
								span_id_obj.removeClass("checked");
								radio_obj.checked = false;
							} else {
								span_id_obj.addClass("checked");
								if (span_id_obj && span_id_obj.attr("data-value")){
									var data_val = span_id_obj.attr("data-value");
									// update hidden input for search
									search_obj.attr("value",""+data_val+""); 
								}
							}
						}
					}
				</script>
            	
