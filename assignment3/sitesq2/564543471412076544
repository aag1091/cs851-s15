        window.onload = function () {
            // show any error messages
            if (document.getElementById("_hiddenErrorMessage") != null) {
                if (document.getElementById("_hiddenErrorMessage").value.length > 0) {
                    if (document.getElementById("_labelErrorMessage") != null) {
                        document.getElementById("_labelErrorMessage").innerHTML = document.getElementById("_hiddenErrorMessage").value;
                    }
                }
            }

            // show any status messages
            if (document.getElementById("_hiddenStatusMessage") != null) {
                if (document.getElementById("_hiddenStatusMessage").value.length > 0) {
                    if (document.getElementById("_labelStatusMessage") != null) {
                        document.getElementById("_labelStatusMessage").innerHTML = document.getElementById("_hiddenStatusMessage").value;
                    }
                }
            }
            if (typeof showGraphs == 'function') {
                showGraphs();
            }
            if (typeof updateTweetDate == 'function') {
                updateTweetDate();
            }
            if (typeof lookup_location == 'function') {
                lookup_location();
            }
            if (typeof initAnimation == 'function') {
                initAnimation();
            }
        }

        function confirmNavigation(message) {
            var isConfirmed = confirm(message);
            if (isConfirmed) {
                showPleaseWait();
            }
            return isConfirmed;
        }

        function showPleaseWait() {
            document.getElementById('PleaseWait').style.display = 'block';
        }
        window.onunload = function dummy() { }; 

        function hideDialog() {
            if (document.getElementById("DialogBoxOuter") != null) {
                var divstyle = new String(); 
                divstyle = document.getElementById("DialogBoxOuter").style.visibility; 
                if (divstyle.toLowerCase() == "visible" || divstyle == "") { 
                    document.getElementById("DialogBoxOuter").style.visibility = "hidden"; 
                } 
                else { 
                    document.getElementById("DialogBoxOuter").style.visibility = "visible"; 
                }
            }
        }
    </script>
