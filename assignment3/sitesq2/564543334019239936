            document.getElementById("lblmail").innerHTML = "sales@alamy.com";
            document.getElementById("lblmail").href = "mailto:sales@alamy.com";
            document.getElementById("lblCntry1").innerHTML = "We think you're in the US / Canada.<br/>If we've got it wrong <a class='white h6' onclick=" + "showCntryOpt('US')" + ">Click Here</a>";
            document.getElementById("lblTel1").innerHTML = '866 671 7305';
