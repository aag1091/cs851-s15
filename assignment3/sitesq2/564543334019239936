                $("#txtemailid").addClass('ie-place-holder').val($("#txtemailid").attr('placeholder'));
            $(".ie-place-holder").focus(function () {
                if ($(this).val() == $(this).attr('placeholder'))
                    $(this).val('');
            });
            $(".ie-place-holder").blur(function () {
                if ($(this).val() == '')
                    $(this).val($(this).attr('placeholder'));
            });

            
