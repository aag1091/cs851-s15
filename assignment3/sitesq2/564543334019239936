            document.getElementById("lblTel1").innerHTML = '0800 589 2680';
            document.getElementById("lblmail1").innerHTML = "sales@alamy.com";
            document.getElementById("lblmail1").href = "mailto:sales@alamy.com";
            if ($('#lblTelHeadr').length != 0) {               
                document.getElementById("lblTelHeadr").innerHTML = "0800 589 2680";
