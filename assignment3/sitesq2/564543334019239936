    function windowListener(targetLink) {
        if ('False') {
             window.opener.location.href = targetLink;
             window.close();
             return false;
         }
     }
    function SetGeoIP(GeoIPCntryCode) {
        
        document.getElementById("DE").innerHTML = "<a onclick=" + "chngCntry('DE')" + ">Germany</a> | ";
        if (GeoIPCntryCode == "US" || GeoIPCntryCode == "CA") {
