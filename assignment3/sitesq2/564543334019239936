<script type="text/javascript" src="/script/alamy-ref-words-min.js?v=9.15"></script>

<script type="text/javascript">
    var http = 'http://www.alamy.com';
    var https = 'https://secure.alamy.com';
    var IsIntranet = '0';
    $(document).ready(function () {
        if ($('input[name=media_type]:checked').val()) {
            $("input[name=media_type]").filter('[value=' + $('input[name=media_type]:checked').val() + ']').prop('checked', false);
        }
        /*search image type drop down */
        $("#dropdown_searchoptions").removeAttr('style');
        $(".search-bar .form-horizontal .dropdown-trigger").bind('click', ' .dropdown-trigger', function (e) {
            $(".dropdown-wrapper").toggleClass("on");
            e.stopPropagation();
        });
        $(".search-bar .form-horizontal .dropdown-menu li").bind('click', function () {
            $("#select_searchoptions").text($(this).find('label').text());
            $(this).find('input').attr('checked', 'checked');
            $(".dropdown-wrapper").removeClass("on");
        });
        $('*').on('click', 'body', function () {
            if ($(this).attr('class') != 'dropdown-trigger')
                $(".dropdown-wrapper").removeClass("on");
        });
        $("#submitsearch").click(function () { SubmitSearch(); return false; });
        $("#search").keypress(function (e) {
            if (e.which || e.keyCode) {
                if ((e.which == 13) || (e.keyCode == 13)) {
                    SubmitSearch();
                    return false;
                }
            } else { return true; }
        });
    });       
    var SubmitSearch = function (e) {
        var HTTP_Path = 'http://www.alamy.com';
          var NewURLPart = ''
          var id = '';
          /*if (navigator.appVersion.indexOf("MSIE 7.") != -1 || navigator.appVersion.indexOf("MSIE 8.") != -1 || navigator.appVersion.indexOf("MSIE 9.") != -1){
              if ($(".ie-place-holder-sr") && $(".ie-place-holder-sr").val() == $(".ie-place-holder-sr").attr('placeholder'))
              inputTB.val('');
          }*/
          if ($('input[name=media_type]:checked').val())
              id = $('input[name=media_type]:checked').val();
          else id = "0";

          /* Verify if search term is Alamy ref starts */
          var searchval = $('#search').val();
          if (searchval != "" && (searchval.indexOf(" ") == 0 || searchval.indexOf(" ") == searchval.length - 1)) {
              searchval = searchval.replace(" ", "");
          }
          if (searchval.length > 0) {
              if (id == '0' && isAlamyVideoRef(searchval)) {
                  id = 'videos';
              }
              if (id == 'videos' && isAlamyRef(searchval)) {
                  id = '0';
              }
          }
          /* Verify if search term is Alamy ref ends */
          var searchpage, searchpageSegmant;
          if (id.toLowerCase() == 'videos') {

              searchpage = HTTP_Path + "/search/" + NewURLPart + "Videoresults.aspx?";
              var x = searchpage + buildQuerystring(searchval, id);
              window.location.href = x
          }
          else if (id.toLowerCase() == 'livenews') {
              searchpage = HTTP_Path + "/news/newsresults.aspx?#BHM=foo%3Dbar%26qt%3D" + encodeURIComponent(searchval) + "%26st%3D0%26bb%3D4";
              if (searchval != "")
                  window.location.href = searchpage;
              else
                  window.location.href = searchpage + buildQuerystring(searchval, id);
          }
          else {
              //945 - Unified Search

              searchpageSegmant = searchval ? "search.html?" : "Imageresults.aspx?";
              searchpage = HTTP_Path + ((!searchval || NewURLPart) ? '/search/' : '/') + NewURLPart + searchpageSegmant;
              window.location.href = searchpage + buildQuerystring(searchval, id);
          } 


      };
      var buildQuerystring = function (searchval, id) {
          var qs, dtfr, dtto, hc, selectdate, aqt, epqt, nqt, oqt, contributorname, size, archive, elem = "", customgeoip;
          var dialogopen = 0;
          //elem = $('#customgeoip').val();
          customgeoip = (elem == '') ? '' : elem;
          qs = "CreativeOn=1&adv=1&ag=0&all=1&creative=&et=0x000000000000000000000&vp=0&loc=0";
          if (dialogopen == 0)
              qs += "&qt=" + encodeURIComponent($('#search').val()) + "&qn=";
          else
              qs += "&qt=&qn=";
          if ('0' == "1" || '0' == 'true') {
            qs += "&mode=1";
            qs += "&customGeoIP=" + customgeoip;
        }
        qs += "&lic=6&lic=1"
        if (id == "1")
            qs += "&imgt=1"
        else if (id == "2")
            qs += "&imgt=2"
        else if (id == "Cutouts")
            qs += "&imgt=4"
        var arr = $('#search').val().split(" ");
        var v_imgt = 0;
        for (var i = 0; i < arr.length; i++) {
            if (arr[i].toLowerCase() == "vectors" || arr[i].toLowerCase() == "vector")
                v_imgt = 1;
        }
        if (id == "8" || v_imgt == 1)
            qs += "&imgt=8"
        qs += "&archive=" + "1";
        qs += "&dtfr=" + "" +
                "&dtto=" + "" +
                "&hc=" + "" +
                "&selectdate=" + "" +
                "&size=" + "0xFF" +
                "&aqt=" + "" +
                "&epqt=" + "" +
                "&oqt=" + "" +
                "&nqt=" + "";
        qs += "&gtype=0";
        return qs;
    };
    function isAlamyRef(aref) {
        var isImageRefWord = 1;
        for (var i = 0; i < ImageRefWords.length; i++) {
            if (ImageRefWords[i] == aref)
                isImageRefWord = 0;
            break;
        }
        return (new RegExp("^[A-HJKMNPRTXWXY]([0-9A-FO]{5}|[GHJKMNPR-TW-Y][0-9A-FO]{4}|[0-9A-HJKMNPRSTXWYO][GHJKMNPR-TW-Y][0-9A-FO]{3}|[0-9A-HJKMNPRSTXWYO]{2}[GHJKMNPR-TW-Y][0-9A-FO]{2}|[0-9A-HJKMNPRSTXWYO]{3}[GHJKMNPR-TW-Y][0-9A-FO]{1}|[0-9A-HJKMNPRSTXWYO]{4}[GHJKMNPR-TW-Y])$", "i").test(aref) && isImageRefWord);
    }

    function isAlamyVideoRef(vref) {
        var isVideoRefWord = 1;
        for (var i = 0; i < VideoRefWords.length; i++) {
            if (VideoRefWords[i] == vref)
                isVideoRefWord = 0;
            break;
        }
        return (new RegExp("V[0123456789ABCDEFGHJKMNPRTWXY]{6}", "i").test(vref) && isVideoRefWord);
    }
    </script>

