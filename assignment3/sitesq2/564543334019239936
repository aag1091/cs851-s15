    function switchLanguage() {
        var intranetmode = "0";
        if (IsIntranet == "true" || IsIntranet == "1") intranetmode = "1";
        if ((intranetmode == "1" && window.self == window.top) || intranetmode == "0") {
            var exp = '';
            var now = new Date();
            var then = now.getTime() + (30 * 24 * 60 * 60 * 1000);
            now.setTime(then);
            exp = '; expires=' + now.toGMTString();
            document.cookie = "userLanguage=de;path= /;domain= .alamy.com" + exp;
            
            window.location.href = window.location.href.replace('http://www.alamy.com', 'http://de.alamy.com');
            
        }
    }
   
    function chngCntry(SelCntry) {
        SetGeoIP(SelCntry);
        $("#cntryOpt span").show();
        $("#cntryOpt1 span").show();
        var GeoIPCookie;
        document.cookie;
        GeoIPCookie = SelCntry;
        document.cookie = "FooterGeoIP=" + GeoIPCookie + ";path= /;domain= .alamy.com";
    }   
    function showCntryOpt(cntry) {
        $("#cntryOpt").toggle();
        $("#cntryOpt1").toggle();
        $("#" + cntry).toggle();
        $("#" + cntry+"_lowscreen").toggle();
    }   
    function showLangSelect() {

        $("#langlist").toggle();
        $("#langlist1").toggle();
    }   
    var EmailSubscriptionSuccess = function (res) {
        isloggdin = res.d.LoggedIn;
        email = res.d.Email;
        if (isloggdin == 1) {
            if (res.d.Isemailsuscribed == "True") {
                $("ul.remove-bullet.ft-bullet").removeClass("pb-77");
                $("#signup,#emaildetails,#SignUpHeading,#sendme").hide();                
                $("#ContainerDiv").removeClass("col-md-9").addClass("col-md-12");
            }

            else {
                Iscustomer_value = res.d.Iscustomertypeuser == "True" ? true : false
                IsContributor_value = res.d.Iscontributortypeuser == "True" ? true : false

                if (Iscustomer_value == true && IsContributor_value == true) {
                    $('#sendme').show();
                    $("ul.remove-bullet.ft-bullet").addClass("pb-77");
                } else {
                    $('#sendme').hide();
                    $("ul.remove-bullet.ft-bullet").css('padding-bottom', '124px');
                }
                $("#signup,#emaildetails,#SignUpHeading").show();              
                $("#ContainerDiv").removeClass("col-md-12").addClass("col-md-9");
               
            }

        }

        else {
            $("#signup,#emaildetails,#SignUpHeading,#sendme").show();           
            $("#ContainerDiv").removeClass("col-md-12").addClass("col-md-9");
        }

    }
    var emailexp = /^[a-zA-Z_0-9][a-zA-Z_0-9\+\.\-\!\'&]{0,}@[a-zA-Z_0-9\.\-\!]+\.[a-zA-Z]{1,}$/i;


    function validateEmail() {
        //Check that the email entry is valid   Footer_txtemailid         
        var ischeckboxclicked = true;
        if (isloggdin == 1) {
            if (Iscustomer_value && IsContributor_value) {
                if (!($('#chkbuyer').is(':checked')) && !($('#chkseller').is(':checked'))) {
                    ischeckboxclicked = false;
                }
            }
        } else {
            if (!($('#chkbuyer').is(':checked')) && !($('#chkseller').is(':checked'))) {
                ischeckboxclicked = false;
            }
        }
        if (ischeckboxclicked) {
            if ($('#txtemailid').val() != '') {
                if (!emailexp.test($('#txtemailid').val())) {
                    $('#lblErrormsg').text("Oops! This email address isn’t valid");
                    $('#lblErrormsg').removeClass("hidden").addClass("help-block").addClass("show");
                    $('#txtemailid').focus();
                    $('#signup').addClass("has-error");
                    setTimeout(function () { $('#lblErrormsg').removeClass("show").addClass("hidden"); }, 5000);
                    return false;
                }

                return true;
            } else {
                $('#lblErrormsg').text("Oops! This email address isn’t valid");
                $('#lblErrormsg').removeClass("hidden").removeClass("green").addClass("help-block").addClass("show");
                $('#txtemailid').focus();
                $('#signup').addClass("has-error");
                setTimeout(function () { $('#lblErrormsg').removeClass("show").addClass("hidden"); }, 5000);
                return false;
            }
        } else {
            $('#lblErrormsg').text("Please select at least one of the options");
            $('#lblErrormsg').removeClass("hidden").removeClass("green").addClass("help-block").addClass("show");
            $('#txtemailid').focus();
            $('#signup').removeClass("has-error");
            $('#errordiv').addClass("has-error");
            setTimeout(function () { $('#lblErrormsg').removeClass("show").addClass("hidden"); }, 5000);
            return false;
        }


    }
    var Emailsuccesmessage = function (data) {
        if (data.d) {
            $('#lblErrormsg').text("Thanks for signing up to our emails");
            $('#lblErrormsg').removeClass("hidden").removeClass("help-block").addClass("green").addClass("show");
            $('#txtemailid').val('');
            $('#signup').removeClass("has-error");
            if (emailflag == 1) {
                $("#signup,#emaildetails,#SignUpHeading,#sendme").hide();              
                $("#ContainerDiv").removeClass("col-md-9").addClass("col-md-12");
            }
            setTimeout(function () { $('#lblErrormsg').removeClass("show").addClass("hidden"); }, 5000);
        } else
            EmailErrormessage();

    }
    var EmailErrormessage = function () {
        $('#lblErrormsg').text("You’re already signed up to our emails ");
        $('#lblErrormsg').removeClass("hidden").removeClass("green").addClass("help-block").addClass("show");        
        $('#signup').addClass("has-error");
        setTimeout(function () { $('#lblErrormsg').removeClass("show").addClass("hidden"); $('#signup').removeClass("has-error"); }, 5000);

    }
   
</script>
      
        
<div class="modal" id="login" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" data-backdrop="static">
  <div class="modal-dialog custom-class" id="passwordDiv"> 
    <!-- .pwd popup-content -->
    <div class="modal-content modal-tab-content">
      <div class="modal-header modal-tab-header"> 
        <div class="pull-right mt-8"><i class="icon-close modal-tab-close h4" id="close" aria-hidden="true"></i></div>
        <div class="modal-title modal-tab-title col-md-4  col-sm-4 col-xs-6" id="myModalLabel">Password confirmation</div>
      </div>
      <i class="icon-arrow-down modal-tab-arrow  col-md-4  col-sm-4 col-xs-6 h6"></i>
      <div class="modal-body">
        <form class="form-horizontal" role="form">
          <div class="form-group">
            <label for="inputPassword3" class="col-sm-5 control-label light-navy modal-tab-label">Please enter your password</label>
            <div class="col-sm-7" id="errMsgParentPass">
              <input type="password" class="form-control" id="txtPwd" maxlength="50" />
              <small data-bs-validator="notEmpty" id="errMsgPass"  style=""></small>
                
              <div><a href="/forgotten-pwd.asp" class="h6 light-navy">Forgotten your password?</a></div>
                
            </div>
          </div>
        </form>
      </div>
      <div class="modal-footer modal-tab-footer">
        <button type="button" id="passwordCon" class="btn btn-cyan pull-right btn-inline-navbar">Continue</button>
      </div>
    </div>
    <!-- /.pwd popup-content --> 
  </div>
</div>

<script type="text/javascript">

    (function ($) {

        $.fn.ConfirmPassword = function (settings) {
            //setting default values
            var settings = $.extend({
                successfunction: '',
                userid: ''
            }, settings);
            //show password confirmation popup window
            $('#login').removeClass("hidden").addClass("show").on('shown.bs.modal', function () {
                $("#txtPwd").focus();
            }).modal('show');

            //registering clicks and other events, after unbinding old
            $("#close").unbind("click").click(function () {
                $('#login').modal('hide').removeClass("show").addClass("hidden"); // function close pop up  
                $("#errMsgParentPass").removeClass("has-error");
                $('#errMsgPass').removeClass("show help-block").addClass("hidden");
                $('#txtPwd').val("");
            });

            $("#passwordDiv").unbind("keypress").keypress(function (e) { if (e.which || e.keyCode) { if ((e.which == 13) || (e.keyCode == 13)) { confirmPasswordAction(); return false; } } else { return true; } });

            $("#passwordCon").unbind("click").click(function () {
                confirmPasswordAction();
            });

            //function used for password confirmation 
            var confirmPasswordAction = function () {
                $("#errMsgParentPass").removeClass("has-error");
                $('#errMsgPass').removeClass("show help-block").addClass("hidden");
                var passwd = $('#txtPwd').val();
                if (passwd.length > 5 && passwd.length < 51) {
                    var passwordConfirmData = { "UserId": userid, "Password": passwd };
                    $.post("/myalamycustomer.asmx/ConfirmPassword", passwordConfirmData, passwordConfirmSuccess);
                }
                else {
                    $("#errMsgParentPass").addClass("has-error");
                    $('#errMsgPass').html("Sorry, your password's incorrect. Please try again.").removeClass("hidden").addClass("show help-block");
                    $('#txtPwd').val("").focus();
                }
            };

            //Function call on the success of confirmpassword ajax function
            var passwordConfirmSuccess = function (data, status) {
                if (data.status.code == 200) {
                    $('#login').modal('hide').removeClass("show").addClass("hidden");
                    successfunction();
                    //MakeEditable();
                }
                else {
                    $("#errMsgParentPass").addClass("has-error");
                    $('#errMsgPass').html("Sorry, your password's incorrect. Please try again.").removeClass("hidden").addClass("show help-block");
                    $('#txtPwd').val("").focus();
                }
            };

        };
    }(jQuery)); 
</script>

    
