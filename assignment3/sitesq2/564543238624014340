								<meta http-equiv="refresh" content="2;url=https://itunes.apple.com/gb/album/hole-in-my-heart-single/id960799523?ls=1&amp;at=10lsY3&amp;ct=xu2yml"/>	
			<script>
				var destinationUrl = "https://itunes.apple.com/gb/album/hole-in-my-heart-single/id960799523?ls=1&at=10lsY3&ct=xu2yml";
		var numRedirects = 0;
		function LinkDecorator() {};
		LinkDecorator.prototype.decorate = function(url) {
						return url;
		}
		var linkDecorator = new LinkDecorator();
				var numAsyncTasks = 2;
					  function onAsyncTaskComplete() {
		numAsyncTasks--;
			    if (numAsyncTasks <= 0) {
	    	onAllAsyncTasksComplete();
	    }
	   }
		function makePixel(url) {
						var img = document.createElement("img");
			img.onload = function() {
								onAsyncTaskComplete();
			}
			img.src = url;
		}
	   		function onAllAsyncTasksComplete() {
			if (numRedirects == 0) {
								numRedirects++;
								window.location = linkDecorator.decorate(destinationUrl);
							}
		}
	</script>		
	</head>
	<body>
		<script type="text/javascript">
						makePixel("https:\/\/www.facebook.com\/tr?id=51266945\x26ev=smartURLClick\x26cd[uuid]=e1579485\-0f4e\-41ec\-aaf5\-b82e122ca3d8");
									makePixel("\/\/googleads.g.doubleclick.net\/pagead\/viewthroughconversion\/999313052\/?value=1.00\x26currency_code=USD\x26label=rY5fCPD_pFkQnJ3B3AM\x26guid=ON\x26script=0");
												window.setTimeout(onAllAsyncTasksComplete, 2000);
					</script>
