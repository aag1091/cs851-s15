<script type="text/javascript">
var narrowAdsLayout=false;
$(document).ready(function() {
var testmwElement = document.getElementById("modalWindow");
//console.log("test height="+testmwElement.offsetHeight);
doResponsive(); //run when page loads
$(window).resize(function() { doResponsive(); }); // and bind to resize events

//randomize nominee list
$(".leftcol").randomize("div.promo_item");
});

function doResponsive() {
if ($("#responsiveSkyscraperAd").css("display") != "none" && !narrowAdsLayout) {
//move ads into divs
var ad1= $("div.skyscraper_ad").detach();
$("#responsiveSkyscraperAd").append(ad1);
narrowAdsLayout = true;
//console.log("moved to narrow view");
} else if ($("#responsiveSkyscraperAd").css("display") == "none" && narrowAdsLayout) {
//move ads back where they were 
var ad1= $("#responsiveSkyscraperAd div.skyscraper_ad").detach();
$("div#bodycontainer").prepend(ad1);
narrowAdsLayout = false;
//console.log("moved to wide view");
}

}
</script>

