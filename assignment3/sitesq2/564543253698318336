	    interestsTemplate.backgroundImageDesktop = interestsTemplate.backgroundImageDesktop !== '' ? interestsTemplate.backgroundImageDesktop : null;
	  },
	  adjustBackgroundImage: function() {
	    if (interestsTemplate.backgroundImageDesktop && componentGlobalVars.windowSize == 'desktop') {
	      $('.interestsTemplate_BackgroundWrapper').css('background-image', 'url(' + interestsTemplate.backgroundImageDesktop +')');
	    } else if (interestsTemplate.backgroundImageTablet && componentGlobalVars.windowSize == 'tablet') {
	      $('.interestsTemplate_BackgroundWrapper').css('background-image', 'url(' + interestsTemplate.backgroundImageTablet +')');
	    } else if (interestsTemplate.backgroundImageMobile && componentGlobalVars.windowSize == 'mobile') {
	      $('.interestsTemplate_BackgroundWrapper').css('background-image', 'url(' + interestsTemplate.backgroundImageMobile +')');
	    } else {
	      if (interestsTemplate.backgroundImageDesktop) {
	        $('.interestsTemplate_BackgroundWrapper').css('background-image', 'url(' + interestsTemplate.backgroundImageDesktop +')');
	      } else if (interestsTemplate.backgroundImageTablet) {
	        $('.interestsTemplate_BackgroundWrapper').css('background-image', 'url(' + interestsTemplate.backgroundImageTablet +')');
	      } else if (interestsTemplate.backgroundImageMobile) {
	        $('.interestsTemplate_BackgroundWrapper').css('background-image', 'url(' + interestsTemplate.backgroundImageMobile +')');
	      }
	    }    
	  }
	}
	$(document).ready(function() {
	  interestsTemplate.configureImageVars();
	  if(GLOBAL.pageWidth < 714) {
	      componentGlobalVars.windowSize = 'mobile';
	      interestsTemplate.adjustBackgroundImage();
		}
	  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
	      componentGlobalVars.windowSize = 'tablet';
	      interestsTemplate.adjustBackgroundImage();
	  }
	  if(GLOBAL.pageWidth > 1065) {
	      componentGlobalVars.windowSize = 'desktop';
	      interestsTemplate.adjustBackgroundImage();
	  }
	});
	$(window).resize(function() {
		if(GLOBAL.pageWidth < 714) {
		  if (componentGlobalVars.windowSize != 'mobile') {
		      componentGlobalVars.windowSize = 'mobile';
		      interestsTemplate.adjustBackgroundImage();
		  }
		}
	  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
	    if (componentGlobalVars.windowSize != 'tablet') {
	      componentGlobalVars.windowSize = 'tablet';
	      interestsTemplate.adjustBackgroundImage();
	    }
	  }
	  if(GLOBAL.pageWidth > 1065) {
	    if (componentGlobalVars.windowSize != 'desktop') {
	      componentGlobalVars.windowSize = 'desktop';
	      interestsTemplate.adjustBackgroundImage();
	    }
	  }
	});			
</script>



</body>
</html>