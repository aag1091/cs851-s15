    <div id="top_clarifying_facebook_share_button" title="Share on Facebook"></div>
    <div id="top_clarifying_tweet_share_button" title="Share on Twitter"></div>
    <div id="top_clarifying_google_share_button" title="Share on Google Plus"></div>
    <div id="top_clarifying_email_share_button" title="Share via Email" style="cursor: pointer; margin-top: -1px;">
