				if ( comm_par ) {
					frame.src = "http://jetpack.wordpress.com/jetpack-comment/?blogid=77713817&postid=134111&comment_registration=0&require_name_email=1&stc_enabled=1&stb_enabled=1&show_avatars=1&avatar_default=mystery&greeting=Leave+a+Reply&greeting_reply=Leave+a+Reply+to+%25s&color_scheme=light&lang=en-US&jetpack_version=3.4.1&sig=a7137e4887f56f18a80dd2220cc039afa47c047a#parent=http%3A%2F%2Flinktogiving.com%2Fchampion-easybird-auto-feed-trap-thrower-with-free-trap-taxi-3%2F" + '&replytocom=' + parseInt( comm_par, 10 ).toString();
