                        <header>
                            <h1 itemprop="name" class="entry-title">21 Bad Makeup Habits You Should Stop Doing</h1>

                            

                            <div class="meta-info">

                                                                <div class="td-post-date"><time  itemprop="dateCreated" class="entry-date updated td-module-date" datetime="2015-02-03T05:42:57+00:00" >Feb 3, 2015</time><meta itemprop="interactionCount" content="UserComments:0"/></div>                                                                                            </div>
                        </header>
                    </div>
                </div>
            </div> <!-- /.td-pb-row -->

            <div class="td-pb-row">
                                            <div class="td-pb-span8 td-main-content" role="main">
                                <div class="td-ss-main-content">
                                    
        

        <div class="td-post-content td-pb-padding-side">
            
            
 <!-- A generated by theme --> 

