<script>
 function setActive(i) {
		var gsDiv = $('.galleria-thumbnails-container .galleria-image').eq(i);

		$('.galleria-thumbnails-container .galleria-image').removeClass('active');
		gsDiv.addClass('active');
		$('.galleria-image').removeClass('active');
		$('.galleria-image').eq(i).addClass('active');
		//alert(i+'\r\n'+window.name);
	}
function activeGalleriaf(){
	var current = $('.galleria-thumbnails-container .active').index(),
	maxIndex = $('.galleria-thumbnails-container .galleria-image').length - 1,
	next = (current + 1) >  maxIndex ? 0 : (current + 1); 
	//next; 
	//alert(next);
	SetCookie('galleriaIndex', next, exp);
	//alert(next);
	//$('.galleria-thumbnails-container .galleria-image').eq(next).trigger('click');
}

function activeGalleriaf1(){
var current = $('.galleria-thumbnails-container .active').index(),
	maxIndex = $('.galleria-thumbnails-container .galleria-image').length - 1,
	next = (current + 1) >  maxIndex ? 0 : (current + 1);
window.name=next;
setActive(parseInt(window.name)); 	
}

