<script type="text/javascript">
    (function () {
        <!-- begin AdWerx code -->
        if (!window.fixBodyText) {
            window.fixBodyText = function() {
                $('body').css({ visibility: 'visible' });
            };
        }
        if (!window.pollSnapEngageAgentOnlineStatus) {
            window.pollSnapEngageAgentOnlineStatus = function() {
                if (window.SnapEngage) {
                    SnapEngage.getAgentStatusAsync(function(agentOnline) {
                        if (agentOnline && true) {
                            console.log('SnapEngage.agentOnline at ' + (new Date));
                            $(document).trigger('SnapEngage.agentOnline');
                        } else {
                            console.log('SnapEngage.agentOffline at ' + (new Date));
                            $(document).trigger('SnapEngage.agentOffline');
                        }
                    });
                } else {
                    console.log('SnapEngage.agentOffline at ' + (new Date));
                    $(document).trigger('SnapEngage.agentOffline');
                }
                setTimeout(window.pollSnapEngageAgentOnlineStatus, 120000);
            };
        }
        // end AdWerx code
        var se = document.createElement('script');
        se.type = 'text/javascript';
        se.async = true;
        se.src = '//commondatastorage.googleapis.com/code.snapengage.com/js/c18af344-199d-4258-b4c1-ae23016eeb0b.js';
        var done = false;
        se.onload = se.onreadystatechange = function () {
            if (!done && (!this.readyState || this.readyState === 'loaded' || this.readyState === 'complete')) {
                done = true;
                // Place your SnapEngage JS API code below
                // SnapEngage.allowChatSound(true); // Example JS API: Enable sounds for Visitors.
                // --- begin AdWerx code ---
                window.fixBodyText();
                window.pollSnapEngageAgentOnlineStatus();
                $(document).trigger('SnapEngage.loaded');
                // --- end AdWerx code ---
            }
        };
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(se, s);

        $(document).on('SnapEngage.loaded', function(evt) {
            $('.snapengage-toggle-link').each(function(index) {
                var $this = $(this);
                $this.click(window.SnapEngage.startLink);
            });
            $('.snapengage-parent').fadeIn();
        })
        $(document).on('SnapEngage.agentOnline', function(evt) {
            $('.snapengage-toggle-link').each(function(index) {
                var $this = $(this);
                var $data = $this.data();
                var label = 'Live Chat';
                var iconClass = 'icon-comments';
                if ($data && $data.online) {
                    if ($data.online.label) label = $data.online.label;
                    if ($data.online.icon) iconClass = $data.online.icon;
                }
                $this.empty().append('<i class="' + iconClass + '"></li>').append(label);
            });
            $('.snapengage-chat-link').each(function(index) {
                var $this = $(this);
                $this.on('click', window.SnapEngage.startLink);
                $this.fadeIn();
            });
        });
        $(document).on('SnapEngage.agentOffline', function(evt) {
            $('.snapengage-toggle-link').each(function(index) {
                var $this = $(this);
                var $data = $this.data();
                var label = 'Email Us';
                var iconClass = 'icon-envelope';
                if ($data && $data.offline) {
                    if ($data.offline.label) label = $data.offline.label;
                    if ($data.offline.icon) iconClass = $data.offline.icon;
                }
                $this.empty().append('<i class="' + iconClass + '"></li>').append(label);
            });
            $('.snapengage-chat-link').each(function(index) {
                var $this = $(this);
                $this.fadeOut();
                $this.off('click');
            });
        });
    })();
</script>
<!-- end SnapEngage code -->
