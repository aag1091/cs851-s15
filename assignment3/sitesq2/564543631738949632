<script type="text/javascript">
$(document).ready(function() {
var $wrapper = $('.coupon-wrapper'),
            stuck = [];
    $wrapper.each(function(i, couponWrapper) {
        var $couponWrapper = $(couponWrapper),
                $snapToElem = $('.footer'); // default
        stuck[i] = true;
        if ($couponWrapper.data() && $couponWrapper.data().snapTo) {
            $snapToElem = $($couponWrapper.data().snapTo);
        }
        if ($snapToElem && $snapToElem.length) {
            var $snapTo = $($snapToElem[0]);
            var snapToTop = $snapTo.position().top;
            $couponWrapper.css({ zIndex: parseInt($snapTo.css('z-index')) - 10 });
            //console.debug($couponWrapper.css('z-index'));
            $(window)
              .on('resize', _.debounce(function() {
                  $('html').css('height', 'auto').css('height', $(this).height() >= $(document.body).height() ? '' : 'auto');
                  $(this).trigger('scroll');
              }, 300))
              .on('scroll', function(e) {
                  var scrollTop = $(window).scrollTop(),
                          windowHeight = $(window).height(),
                          docHeight = $(document).height(),
                          snapToHeight = $snapTo.height();
                  //console.debug({ windowHeight: windowHeight, docHeight: docHeight, scrollTop: scrollTop, snapToTop: snapToTop, stuck: stuck[i], snapToHeight: snapToHeight });
                  // make the coupon stick to view port bottom if $snapTo element is not yet in view
                  if (!stuck[i] && windowHeight <= snapToTop && (docHeight - windowHeight - scrollTop) >= snapToHeight ) {
                      $couponWrapper.addClass('sticky');
                      stuck[i] = true;
                  }
                  else if (stuck[i] && (windowHeight > snapToTop || (docHeight - windowHeight - scrollTop) < snapToHeight)) {
                      $couponWrapper.removeClass('sticky');
                      stuck[i] = false;
                  }
              })
              .trigger('resize');
        }
    });
});
</script>


