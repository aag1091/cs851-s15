<script type="text/javascript">
  function load_retargeting_pixels() {
    if (AdWerx.include_pixel) {
      drop_retargeting_pixels();
    } else {
      $.getScript("/javascripts/adwerx/tracking/retargeting_pixels.js").done(drop_retargeting_pixels);
    }

    function drop_retargeting_pixels() {
      // console.log("Dropping anx pixels (anx_key): 758616, 841759, 1618793, 1618893")

      setTimeout(function () {
        AdWerx.include_pixel("", "758616", "&lt;!-- Segment Pixel - adwerx_run_of_site - DO NOT MODIFY --&gt;&lt;script src=&quot;https://secure.adnxs.com/seg?add=758616&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");
      }, 10);
      setTimeout(function () {
        AdWerx.include_pixel("", "841759", "&lt;!-- Segment Pixel - adwerx_run_of_site - DO NOT MODIFY --&gt;&lt;script src=&quot;https://secure.adnxs.com/seg?add=841759&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");
      }, 20);
      setTimeout(function () {
        AdWerx.include_pixel("", "1618793", "&lt;!-- Segment Pixel - adwerx_run_of_site - DO NOT MODIFY --&gt;&lt;script src=&quot;https://secure.adnxs.com/seg?add=1618793&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");
      }, 30);
      setTimeout(function () {
        AdWerx.include_pixel("", "1618893", "&lt;!-- Segment Pixel - adwerx_run_of_site - DO NOT MODIFY --&gt;&lt;script src=&quot;https://secure.adnxs.com/seg?add=1618893&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");
      }, 40);
    };
  }
  ;

  $(load_retargeting_pixels);
</script>
