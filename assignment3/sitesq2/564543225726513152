<script type='text/javascript'>
	//<![CDATA[
	function bp_thumbnail_resize(image_url,post_title) {
		var image_w=250;
        var image_h=200;
		var show_default_thumbnail=true;
		var default_thumbnail="http://1.bp.blogspot.com/-4fqrt2MACHQ/UADPlJjibiI/AAAAAAAAAcc/FBN03q3jT2A/s1600/blank_post_thumbnail.png";
		if(show_default_thumbnail == true && image_url == "") image_url= default_thumbnail;
      image_tag='<div class="block-image"><img src="'+image_url.replace('/s72-c/','/s'+image_w+'-h'+image_h+'-c/')+'" class="postimage" alt="'+post_title+'"/></div>';
      yimage_tag = '<div class="block-image"><img src="' + image_url.replace('default', '0') + '" alt="' + post_title + '" class="postimage"/></div>';
		if (image_url.indexOf("img.youtube.com")!= -1){
			return yimage_tag;
		}
		else if (image_url != "") {
			return image_tag;
		}
		else
		return "";
	}
	//]]>
</script>
<a href='http://www.tricksroad.com/2015/02/link-building-tools.html'>
<script type='text/javascript'>
                          document.write(bp_thumbnail_resize("http://1.bp.blogspot.com/-AbgJXh4-c0U/VNdIwkudqZI/AAAAAAAAC9g/0lTdcwW0-fU/s72-c/Link-building-tools.jpg","31 SEO Experts Reveal 3 Favorite Link Building Tools"));
                        </script>
</a>
