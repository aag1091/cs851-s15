            }
            else
                googletag.pubads().setTargeting("RT_MUTV3", "False");

            if ($.cookie('MUTV4')) {
                googletag.pubads().setTargeting("RT_MUTV4", "True");
            }
            else
                googletag.pubads().setTargeting("RT_MUTV4", "False");
            //--------------------------------------------------------------------------------------   
        });
        
  </script>  
  
</body>
</html>
