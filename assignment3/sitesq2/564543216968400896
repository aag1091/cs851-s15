			<script>
				var destinationUrl = "https://itunes.apple.com/us/album/handwritten-deluxe/id963404245?ls=1&at=10lc2w&ct=hqy2q4";
		var numRedirects = 0;
		function LinkDecorator() {};
		LinkDecorator.prototype.decorate = function(url) {
						return url;
		}
		var linkDecorator = new LinkDecorator();
				var numAsyncTasks = 3;
					  function onAsyncTaskComplete() {
		numAsyncTasks--;
			    if (numAsyncTasks <= 0) {
	    	onAllAsyncTasksComplete();
	    }
	   }
		function makePixel(url) {
						var img = document.createElement("img");
			img.onload = function() {
								onAsyncTaskComplete();
			}
			img.src = url;
		}
	   		function onAllAsyncTasksComplete() {
			if (numRedirects == 0) {
								numRedirects++;
								window.location = linkDecorator.decorate(destinationUrl);
							}
		}
	</script>		
	</head>
	<body>
		<script type="text/javascript">
						makePixel("\/\/googleads.g.doubleclick.net\/pagead\/viewthroughconversion\/953926008\/?value=1.00\x26currency_code=USD\x26label=wTc1CILrpVkQ\-ILvxgM\x26guid=ON\x26script=0");
									makePixel("https:\/\/www.facebook.com\/tr?id=1378688109013161\x26ev=smartURLClick\x26cd[uuid]=251765f9\-6710\-4da1\-a737\-737f08ee19c3");
									makePixel("https:\/\/analytics.twitter.com\/i\/adsct?txn_id=l5kp5\x26p_id=Twitter");
												window.setTimeout(onAllAsyncTasksComplete, 2000);
					</script>
