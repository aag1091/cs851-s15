$(function(){
    var navs = $('.nav li');
    var dropdowns = $('.dropdown');

    navs.hover(function(){var o = $(this); var idx=o.index(); dropdowns.hide(); dropdowns.eq(idx).show();});
    dropdowns.mouseleave(function(){dropdowns.hide(); });
});
