<script>
var btns = $('.title-slide a');
var slider = $('.photo-slide').bxSlider({
    auto: true,
    pager: false,
    controls: false,
    onSlideBefore: function(){
        var i = slider.getCurrentSlide();
        btns.removeClass('on');
        btns.eq(i).addClass('on');
    }
});
$('.arr.prev').click(function(){
    slider.goToPrevSlide();
})
$('.arr.next').click(function(){
    slider.goToNextSlide();
})

btns.eq(0).click(function(){
    btns.eq(0).addClass('on');
    btns.eq(1).removeClass('on');
    btns.eq(2).removeClass('on');
    btns.eq(3).removeClass('on');
    slider.goToSlide(0);
});
btns.eq(1).click(function(){
    btns.eq(1).addClass('on');
    btns.eq(0).removeClass('on');
    btns.eq(2).removeClass('on');
    btns.eq(3).removeClass('on');
    slider.goToSlide(1);
});
btns.eq(2).click(function(){
    btns.eq(2).addClass('on');
    btns.eq(1).removeClass('on');
    btns.eq(0).removeClass('on');
    btns.eq(3).removeClass('on');
    slider.goToSlide(2);
});
btns.eq(3).click(function(){
    btns.eq(3).addClass('on');
    btns.eq(1).removeClass('on');
    btns.eq(2).removeClass('on');
    btns.eq(0).removeClass('on');
    slider.goToSlide(3);
});

</script>

