<script>
$(document).ready(function(){
    var repword=$('#info');
    $('#main_submit').click(function(){
        $.ajax(
        {
            url:'/home/news/newsletter/mailchimp_insert.php',
            type:'post',
            data:{'nl':'3','email':$("#email_main").val()},
            success:function(res){
                repword.css('display','block');
                repword.html("<span class='arr'></span>"+res);
            }
        }
    );
});
});

$(".btn-close").click(function(){ $('#element-subscribe').slideToggle(500); })
$('#subscription').click(function(){ $('#element-subscribe').slideToggle(500); });

$('html').click(function() { $('#element-subscribe').slideUp(500); });
$('#element-subscribe').click(function(event){ event.stopPropagation(); });
$('#subscription').click(function(event){ event.stopPropagation(); });
$(window).scroll(function(){ $('#element-subscribe').slideUp(500); })


$(window).bind('scroll', function() {
    if ($(window).scrollTop() > 100) {
        $("#topheader").hide();
        $("#topsubsection").attr('class', 'fixation fixed');
        $('.dropdown').css('top', '40px');
        $('.dropdown').css('position', 'fixed');
        $('#element-subscribe').css('top', '40px');
        $('#element-subscribe').css('position', 'fixed');
    }
    else {
        $("#topheader").show();
        $("#topsubsection").attr('class', 'fixation unfixed');
        $('.dropdown').css('top', '110px');
        $('.dropdown').css('position', 'absolute');
        $('#element-subscribe').css('top', '110px');
        $('#element-subscribe').css('position', 'absolute');
    }
});
</script>
