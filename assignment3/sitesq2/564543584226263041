var btnsj = $('.dot-wp a');
var junkslider = $('.junkslide').bxSlider({
    auto: true,
    pager: false,
    controls: false,
    onSlideBefore: function(){
    	var j = junkslider.getCurrentSlide();
    	btnsj.removeClass('on');
    	btnsj.eq(j).addClass('on');
    }
});
btnsj.click(function(){var o = $(this); var idx=o.index(); btnsj.removeClass('on'); o.addClass('on'); junkslider.goToSlide(idx);});
