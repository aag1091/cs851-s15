$(document).ready(function() {
	if( /Android|webOS|iPhone|iPod|BlackBerry|windows phone/i.test(navigator.userAgent) ) {
 		
		config.FastCart = 0;
	} else {
		
	}
	
	// attach fast cart event to all 'add to cart' link
