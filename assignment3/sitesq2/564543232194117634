sh before moving on    holders.done(function(result){        result = parseInt(result);        if (result > 0) {            jQuery.post(settings.ajax, data, function(response) {                 console.log('Got this from the server: ' + response);                response = response.trim();                if (response.substring(0,2)  == "ok") {                    //support added just fine into mysql table and everything, you might aswell change the button etc here                    // OVDE DODAJ MAGIJU DA SE DESI                     RemoveProceedCookie('371');                    jQuery('#FacebookStartSupport').modal('hide');                    jQuery('#TwitterStartSupport').modal('hide');                    jQuery('#TumblrStartSupport').modal('hide');                    jQuery('#LinkedinStartSupport').modal('hide');                    jQuery('#ShareModal').modal('show');                    var addedfriends = response.split("_-_");                    jQuery('.num').text(addedfriends[1]);                    jQuery('#ShareModal').on('hidden', function(){                        window.setTimeout('location.reload()', 300);                    });                } else {                    //what ever happened it's not ok so might aswell throw an error with that response or something                    console.log('Got this from the server: ' + response);                }            });        } else {            Message = CustomMessage;            social_startlogin(provider, true); //if provider we are trying to add support for is not logged in, log it in        }    });}function social_addsupport(project_id, provider) {    //this one will call function add support and pass required data from it    var data = {        action: 'social_addsupport_function',        'campaign_id': project_id,        'provider' : provider    };    var holder = social_isproviderloggedin(provider); // wait for the ajax call to finish before moving on  