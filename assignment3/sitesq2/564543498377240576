<script type="text/javascript">

/* Scripts for ALL Pages */

jQuery(window).load(function(){ // Make sure everything is loaded first

	// Enable Automatic scrolling between internal page (hashtag) links
	
	jQuery('a.scrollme').click(function() { 
		  var target = jQuery(this.hash);
		  if (target.length == 0) target = jQuery('a[name="' + this.hash.substr(1) + '"]');
		  if (target.length == 0) target = jQuery('html');
		  jQuery('html, body').animate({ scrollTop: target.offset().top - 0 }, 500);
		  return false;
	  });
	
		
	// Run Masonry 
	
	jQuery('.recipes-category-list').masonry({
	   columnWidth: ".grid-sizer",
	  itemSelector: 'li',
	  gutter: ".gutter-sizer"
	}).masonry('bindResize');
	
	jQuery('.recipes-tax-list').masonry({
	  columnWidth: ".grid-sizer",
	  itemSelector: 'li',
	  gutter: ".gutter-sizer"
	}).masonry('bindResize');
	
	jQuery('.video-tax-list').masonry({
	  columnWidth: ".grid-sizer",
	  itemSelector: 'li',
	  gutter: ".gutter-sizer"
	}).masonry('bindResize');
			
}); // End Window Load


jQuery(document).ready(function($) { 

		//Run a conditional check to see if we're on custom post types and then do stuff.
		
				                	 //If we're on general or Health posts //>
					 $('body').addClass('health');
                
	
	// Pageslide - Mobile Nav
		
		$(".nav-button").click(function () { // Set up toggle class for Top Navigation
			$("#site-wrapper").toggleClass("is-offset");
		});
		
	
	//Enable any mobile-specific functions
	
	if( /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) )
		{
		   //alert('You are using a mobile device!');
		   //Run any mobile-specific functions here
		}
	else
		{
		   //alert('You are not using a mobile device!'); 
		   //Run any functions that should only be run on desktops
		   
		   
		   //Animate the secondary menu in the persistent header when scrolling
			$(window).scroll(function() 
				{   
					var bannerHeight = 50;
					if ($(window).scrollTop()>= bannerHeight )    // If we've scrolled past the height of the top banner image
						{
						 $('#header').addClass('scrolled'); // Add the class "floater" to the header
							if ($('.header-btm').is(":visible") ) {
								setTimeout(function() { // Set a timeout function then fade in the new header
								 //$('.header-btm').css('height', '10px');
								}, 400);
							}
							
						}
						else
						{
							$('#header').removeClass('scrolled');
								if ($('.header-btm').is(":visible") ) {
									setTimeout(function() { // Set a timeout function then fade in the new header
									 //$('.header-btm').css('height', '54px');
									}, 400);
								}						
						 
						 }
				});
		   
		}
	
	// Set up the automatic Scrolling link
		  
		jQuery(window).scroll(function(){ // When the screen scrolls 100 pixels from the top, reveal the "scrollup" icon/link. When it goes back to the top, hide the link.
			if (jQuery(this).scrollTop() > 600) {
				jQuery('.scrollup').fadeIn();
			} else {
				jQuery('.scrollup').fadeOut();
			}
		}); 
	  
	  // Set up Cross-browser placeholders
	  if ( !("placeholder" in document.createElement("input")) ) {
		$("input[placeholder], textarea[placeholder]").each(function() {
			var val = $(this).attr("placeholder");
			if ( this.value == "" ) {
				this.value = val;
			}
			$(this).focus(function() {
				if ( this.value == val ) {
					this.value = "";
				}
			}).blur(function() {
				if ( $.trim(this.value) == "" ) {
					this.value = val;
				}
			})
		});

		// Clear default placeholder values on form submit
		$('form').submit(function() {
            $(this).find("input[placeholder], textarea[placeholder]").each(function() {
                if ( this.value == $(this).attr("placeholder") ) {
                    this.value = "";
                }
            });
        });
	}
	
	// Set up Fancy Select
	$('select').fancySelect();
	
	
	//Hide P tags with no content
	
	//$( "p:empty" ).css( "display", "none" );
		
	//Set the width of the Facebook Comments box based on the width of the container at page load
	
	var FBwidth = jQuery('#comments').width(); // Get the width of the containing Comments Box
	
	$('.fb-comments').attr('data-width', FBwidth); // Attach the width of the Comments Box to the 'data-width' attribute of the fb-comments div
	$( window ).resize(function() {
	  var FBwidth = jQuery('#comments').width(); // Get the width of the containing Comments Box
	  $('.fb-comments').attr('data-width', FBwidth); // Change the data-width attribute on resize
	});
	
	
	//* Utilize the jquery.resize plugin to update any iframe size (e.g. videos)
	var iFrames = $('.iframe');

	function iResize() {
	
		for (var i = 0, j = iFrames.length; i < j; i++) {
		  iFrames[i].style.height = iFrames[i].contentWindow.document.body.offsetHeight + 'px';}
	}
	
	if ($.support.safari || $.support.opera) { 
	
	   iFrames.load(function(){
		   setTimeout(iResize, 0);
	   });
	
	   for (var i = 0, j = iFrames.length; i < j; i++) {
			var iSource = iFrames[i].src;
			iFrames[i].src = '';
			iFrames[i].src = iSource;
	   }
	
	} else {
	   iFrames.load(function() { 
		   this.style.height = this.contentWindow.document.body.offsetHeight + 'px';
	   });
	}
	
	setInterval(iResize, 1000);
	
	/* Set Up Tabs */
	
	$('.tabs .tab-links a').on('click', function(e)  {
        var currentAttrValue = $(this).attr('href');
 
        // Show/Hide Tabs
        $('.tabs ' + currentAttrValue).siblings().slideUp(400);
		$('.tabs ' + currentAttrValue).delay(400).slideDown(400);
 
        // Change/remove current tab to active
        $(this).parent('li').addClass('active').siblings().removeClass('active');
 
        e.preventDefault();
    });
	
	jQuery('.toggle-view h3').click(function () { // Enable the toggle view for toggle-able items
 
        var text = jQuery(this).closest('li').children('div.panel');
 
        if (text.is(':hidden')) {
            text.slideDown('200');
            jQuery(this).find('span').html('-');     
        } else {
            text.slideUp('200');
            jQuery(this).find('span').html('+');     
        }
         
    });
	
	jQuery('#menu-mobile li.menu-item-has-children').children('a').each(function() {
			
			$(this).click(function (event) { // Enable the toggle view for toggle-able items
			
			event.preventDefault();
			var text = jQuery(this).closest('li').children('ul.sub-menu');
	 
			if (text.is(':hidden')) {
				text.slideDown('200');
				jQuery(this).closest('li').toggleClass('active');     
			} else {
				text.slideUp('200');
				jQuery(this).closest('li').toggleClass('active');      
			}
			 
		});
	});
	
	// Sharrre Social Sharing Script
	
	$('.googlePlus').sharrre({
	  share: {
		googlePlus: true
	  },
	  urlCurl: 'http://draxe.com/wp-content/themes/draxe2014/inc/sharrre.php',
	  enableHover: false,
	  enableTracking: true,
	  buttons: {  },	  
	  click: function(api, options){
		api.simulateClick();
		api.openPopup('googlePlus');
	  }
	});
	$('.twitter').sharrre({
		  share: {
			twitter: true
		  },
		  urlCurl: 'http://draxe.com/wp-content/themes/draxe2014/inc/sharrre.php',
		  enableHover: false,
		  enableTracking: true,
		  buttons: {  twitter: { via: 'drjoshaxe' } },
		  click: function(api, options){
			api.simulateClick();
			api.openPopup('twitter');
		  }
		});
	$('.facebook').sharrre({
		  share: {
			facebook: true
		  },
		  urlCurl: 'http://draxe.com/wp-content/themes/draxe2014/inc/sharrre.php',
		  enableHover: false,
		  enableTracking: true,
		  buttons: {  },
		  click: function(api, options){
			api.simulateClick();
			api.openPopup('facebook');
		  }
		});
	$('.pinterest').sharrre({
		  share: {
			pinterest: true
		  },
		  urlCurl: 'http://draxe.com/wp-content/themes/draxe2014/inc/sharrre.php',
		  enableHover: false,
		  enableTracking: true,
		  buttons: {		  	
		  	pinterest: { description: $('.pinterest').attr('data-text'), media: $('.pinterest').attr('data-media')  }
		  },
		  click: function(api, options){
			api.simulateClick();
			api.openPopup('pinterest');
		  }
		});
	$('.stumbleupon').sharrre({
		  share: {
			stumbleupon: true
		  },
		  urlCurl: 'http://draxe.com/wp-content/themes/draxe2014/inc/sharrre.php',
		  enableHover: false,
		  enableTracking: true,
		  buttons: {  },
		  click: function(api, options){
			api.simulateClick();
			api.openPopup('stumbleupon');
		  }
		});

	//Add even/odd classes to menu-blocks for styling
        jQuery('ul.natrem-category-list li:even').addClass ('even');
        jQuery('ul.natrem-category-list li:odd').addClass ('odd');
		
		jQuery('ul.video-tax-list li:even').addClass ('even');
        jQuery('ul.video-tax-list li:odd').addClass ('odd');
	
	
	//Adjust the height of the Topic video div based on the container #content div, keeping it at a 16:9 ratio
	
	$("iframe[src*='youtube.com']​​​​​​​​​​​​​​​​").addClass("video-player");
	
	var contentWidth = $('#content').width();
	var videoHeight = Math.round((contentWidth/16)*9)
	$(".video-player, iframe[src*='youtube.com']").css('height', videoHeight);
	$( window ).resize(function() {
	  var contentWidth = $('#content').width();
		var videoHeight = Math.round((contentWidth/16)*9)
	  $(".video-player, iframe[src*='youtube.com']").css('height', videoHeight);
	});
	
	
}); //End document ready

</script><script>
