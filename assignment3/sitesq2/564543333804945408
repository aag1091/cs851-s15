    function create_DFM_ad(d){
        if($('#'+d).html()==''){
            var tagOrd=Math.random()*10000000000000000;
            switch(d){
                case 'DFM-adPos-nav-1':       DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dnav_1&sz=300x250&c="+tagOrd+"'></iframe>"; break;
                case 'DFM-adPos-nav-2':       DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dnav_2&sz=300x250&c="+tagOrd+"'></iframe>"; break;
                case 'DFM-adPos-nav-3':       DFM_temp_Ad_string="<iframe width='300px' height='250px' frameborder=0 scrolling=no seamless src='http://pubads.g.doubleclick.net/gampad/adx?iu="+DFM_ad_loc+"&t=pos%3Dnav_3&sz=300x250&c="+tagOrd+"'></iframe>"; break;
