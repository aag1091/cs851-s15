    /* fill widget data */
    // data format => title~!~link~!~imagelink~#!title~!~link~!~imagelink
    var widgetdata = [];
    for (var i=0;i<most_pop_list.length;i++) {
        /* return for empty itme */
        if(most_pop_list[i].length < 1) continue;
        widgetdata[i] = [];
        var story_item = most_pop_list[i];
        story_item = story_item.split("~!~");
        /* 3rd item is an imagelink, if any */
        if(typeof story_item[2] != 'undefined') {
            widgetdata[i]['image'] = story_item[2];    
        }
        /* 2nd item is a story link */
        if(typeof story_item[1] != 'undefined') {
            widgetdata[i]['link'] = story_item[1];    
        }
        /* 1st item is a story title */
        if(typeof story_item[0] != 'undefined') {
            widgetdata[i]['title'] = story_item[0];
        }
    }
    // site debug information
    if(siteDebug) { console.log('widget data:');console.log(widgetdata);}
    /* move stories with no image to last positions */
    var copytable = $.extend({}, widgetdata);    
    var datasize = widgetdata.length-1;
    var nextinc = 1;
    for(var i=0; i < datasize;i++) {
        if(typeof widgetdata[i]['image'] == 'undefined') {
            copytable[datasize+nextinc] = widgetdata[i];
            copytable[i] = ""; nextinc++;
        }
    }
    //renumbering array elements, if any
    var i=0;
    var widgetdata = new Array();
    for(key in copytable) {    
        if($.isEmptyObject(copytable[key])) { continue;} 
        /* no image story gets default image */
        if($.isEmptyObject(copytable[key]['image'])) { copytable[key]['image'] = DEFAULT_IMAGE;} 
            widgetdata[i] = new Array();
            widgetdata[i]['image'] = copytable[key]['image'];
            widgetdata[i]['link'] = copytable[key]['link'] + '?source=topstoriesrot';
            widgetdata[i]['title'] = copytable[key]['title'];
            i++;        
    }    
    /* start generating widget markup */
    var output = '<li class="item"><div class="slide">';  
    var size = widgetdata.length; var rem = size % 3; var setsize = size-rem; var i=0;
    if(siteDebug) {
        console.log('size: ');console.log(size); 
        console.log(setsize);console.log(rem);
        console.log('widget data  (after sort):');console.log(widgetdata);
    }
    for(key in widgetdata) {    
        if( i >= setsize) break;
        if ((key%3 === 0) && key != 0) { output += '</div></li><li class="item"><div class="slide">'; }
        output += '<div class="was-figure">';
        output += '	  <a href="' + widgetdata[key]['link'] + '" title="' + widgetdata[key]['title'] + '">';
        output += '      <div style="width:151px; "></div>';
        if(typeof widgetdata[key]['image'] != 'undefined') {
            output += '      <img src="'+ widgetdata[key]['image'] + '" alt="' + widgetdata[key]['title'] + '" style="width:151px;" onError="this.onerror=null;this.src=\'http://local.sbsun.com/assets/images/defaultBlog.png\';">';
        }
        output += '	  </a>';
        output += '   <h3>';
        output += '      <a href="' + widgetdata[key]['link'] + '" title="' + widgetdata[key]['title'] + '">' + widgetdata[key]['title'] + '</a>';
        output += '   </h3>';
        output += '</div>'; i++;
    }
    output += '</div></li>';
    $('#popular-widget-items').html(output);   
});
