                <!-- TODO: confirm this interpretation of requirements, i.e. add params always                    --> 
                if (sessionStorage['desktop-wtEvent'] === 'HRock_Check_AddressResearchRequest_Submit') { 
					if (typeof sessionStorage['desktop-wtAddrRequestOpt'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtAddrRequestOpt', content: sessionStorage['desktop-wtAddrRequestOpt']}).appendTo('head'); 
						sessionStorage.removeItem('desktop-wtAddrRequestOpt'); 
					} 
					if (typeof sessionStorage['desktop-wtEmailAddr'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtEmailAddr', content: sessionStorage['desktop-wtEmailAddr']}).appendTo('head'); 
						sessionStorage.removeItem('desktop-wtEmailAddr'); 
					} 
					if (typeof sessionStorage['desktop-wtComments'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtComments', content: sessionStorage['desktop-wtComments']}).appendTo('head'); 
						sessionStorage.removeItem('desktop-wtComments'); 
					} 
				} else if (sessionStorage['desktop-wtEvent'] === 'HRock_Check_NotifyMe_Submit') { 
					if (typeof sessionStorage['desktop-wtNotifyMeOpt'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtNotifyMeOpt', content: sessionStorage['desktop-wtNotifyMeOpt']}).appendTo('head'); 
						sessionStorage.removeItem('desktop-wtNotifyMeOpt'); 
					} 
					if (typeof sessionStorage['desktop-wtEmailAddr'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtEmailAddr', content: sessionStorage['desktop-wtEmailAddr']}).appendTo('head'); 
						sessionStorage.removeItem('desktop-wtEmailAddr'); 
					} 
				} else if (sessionStorage['desktop-wtEvent'] === 'HRock_Check_PendingOrder_Continue') { 
					if (typeof sessionStorage['desktop-wtPendingOrderOpt'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtPendingOrderOpt', content: sessionStorage['desktop-wtPendingOrderOpt']}).appendTo('head'); 
						sessionStorage.removeItem('desktop-wtPendingOrderOpt'); 
					} 
