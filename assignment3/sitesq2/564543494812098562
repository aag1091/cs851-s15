    var v_pathName = window.location.pathname; 
    if (v_pathName.indexOf('set-cookie') == -1) { 
        if ((v_pathName.indexOf('availability') > 0) || (v_pathName.indexOf('checkreferralmap') > 0)) { 
            var  wtEligResult = '', v_dcsua = (navigator) ? navigator.userAgent : ''; 
            if ((/unsuccessful/).test(window.location.href)) { 
                wtEligResult = 'Red Address/WTN';  
            } else if ((/pending-order/).test(window.location.href)) { 
                wtEligResult = 'Pending Uverse Order';  
            } else if ((/existing-service-found/).test(window.location.href)) { 
                wtEligResult = 'Existing Uverse Service';  
            } else if ((/connected-community/).test(window.location.href)) { 
                wtEligResult = 'Connected Community';  
            } else if ((/close-match/).test(window.location.href)) { 
                wtEligResult = 'Close Match';  
            } else if ((/mdu-address/).test(window.location.href)) { 
                wtEligResult = 'MDU Close Match';  
            } else if ((/map-referral/).test(window.location.href) || (/checkreferralmap/).test(window.location.href)) { 
                wtEligResult = 'Map Referral';  
            } else if ((/ranged-addresses/).test(window.location.href)) { 
                wtEligResult = 'Ranged Addresses';  
            } else if ((/success/).test(window.location.href)) { 
                wtEligResult = 'Green Address/WTN';  
            } else if ((/out-of-footprint/).test(window.location.href)) { 
                wtEligResult = 'Out Of Footprint';                
            } else if ((/address-not-found/).test(window.location.href)) { 
                wtEligResult = 'Address Not Found';  
            } 
            jQuery('<meta/>', {name: 'DCSext.dcsua', content: v_dcsua}).appendTo('head'); 
            if (sessionStorage['desktop-CAEvent'] == 'true') { 
