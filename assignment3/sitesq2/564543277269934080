<script>
/**
* This file contains JS config values
* that must be different per page,
* and therefore cannot be cached in /javascripts/Uverse.config.js
*/
Uverse.config.dvr = {
dvrMode: 'dvrItems'
};
Uverse.config.requestPath = '/ng/tv/show/grammy-awards' ;
Uverse.config.cgate = {
url: 'http://uverse.com/cprodx?appID=m83879&isPassive=false&returnURL=http://uverse.com/tv/show/grammy-awards?1&amp;source=ECNG000000000000S'
};
</script><script type="text/javascript">
	(function(win) {
		var location = win.location;

		var authCookie = 'TATS-US-TokenID';
		var pathCheck = 'WATokenService';

		var cgate = Cookie.read(authCookie);
		var nxtgn = Cookie.read('NEXTGEN_NAME');
		var shortCode = Cookie.read('NEXTGEN_SITE_AREA');

		var href = location.href;
		var hash = location.hash;

		if(hash) {
			var hashString = hash.substr(1);
			href = location.protocol + "//" + location.host;

			if (hash == '#/') {
				href += location.pathname;
			} else if (hashString.charAt(0) == '/') {
				href += hashString;
			} else {
				href += location.pathname + '?' + hashString;
			}
		}


		if (!nxtgn && cgate && location.href.indexOf(pathCheck) == -1) {
			win.location = 'http://uverse.com/login.html?return=' + href;
		}

		var prefix = '';
		if (shortCode && shortCode !== '' && shortCode != '""') {
			prefix = '/' + shortCode;
		}

		if (prefix != '') {
			win.location = 'http://uverse.com/login.html?return=' + href;
		}
		

	})(window);
</script>
