					<h2 
						class="list-card-title ellipsis singleLine" 
						data-bind="
							visible: allowsOverlayTitle
							, text: categoryTitle
							, title: categoryTitle
						"></h2>
						<div class="list-card-metadata-wrapper">
							<span class="list-card-metadata ellipsis singleLine" data-bind="html: videoTitle, visible: allowsOverlayTitle"></span>
							<span class="list-card-metadata" data-bind="visible: duration, text: messageProperty(formattedDuration(), Uverse.config.cards.assetDuration)"></span>
						</div>
				</header>

				<article class="list-card-article" data-bind="css: { 'show-more': !ellipsifyDescription() }, ellipsifyText: {
						html: description
						, truncateTo: 'letter'
						, setTitle: true
					}"></article>
		
			</div>
			
	</li>

