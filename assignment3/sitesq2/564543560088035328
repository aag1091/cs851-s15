            {
                // Facebook link is a bit messier - need to bounce through popup
                link = "http://www.thejournal.ie/utils/login/facebook/?url=https%3A%2F%2Fwww.facebook.com%2Fdialog%2Foauth%3Fstate%3Dhttp%3A%2F%2Fwww.the42.ie%2Futils%2Flogin%2Ffacebook%2Ffront%2F%26display%3Dpopup%26client_id%3D116141121768215%26scope%3D" + get_fb_perm_string() + "%26redirect_uri%3Dhttp%3A%2F%2Fwww.thejournal.ie%2Futils%2Flogin%2Ffacebook%2F";
