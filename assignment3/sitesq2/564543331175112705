<script type="text/javascript">
 // <![CDATA[
  function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');

    for(var i=0;i < ca.length;i++) {
      var c = ca[i];

      while (c.charAt(0)==' ') {
        c = c.substring(1,c.length);
      }

      if (c.indexOf(nameEQ) == 0) {
        return c.substring(nameEQ.length,c.length);
      }
    }
    return null;
  }

  var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
  if (is_chrome && getCookie("HD") != null) {
    document.getElementById('fbrdllogin').style.display = 'block';
    document.getElementById('fbrdllogin').style.visibility = 'visible';

    document.getElementById('casyeslogin').style.display = 'none';
    document.getElementById('casyeslogin').style.visibility = 'hidden';

    document.getElementById('casnologin').style.display = 'none';
    document.getElementById('casnologin').style.visibility = 'hidden';
  }
  // ]]>
</script>
