       /** Encode special characters in a string */
        var htmlEncode = (function(str) {
            var map = {"&":"amp", "<":"lt", ">":"gt", '"':"quot", "'":"#39"};
            return str.replace(/[&<>"']/g, function(match) { return "&" + map[match] + ";"; });
        });

        mboxCreate('gq_ed_global',
            'entity.brand=GQ',
            'entity.id=2000000003043396',
