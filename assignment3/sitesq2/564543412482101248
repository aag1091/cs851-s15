                    <a href="#slide=10" title="Previous slide">Previous</a>
                </div>
                <div class="count">1 of 10</div>
                <div class="next">
                    <a href="#slide=2" title="Next slide">Next</a>
                </div>
            </div>

            <script>
              PARSELY = {
                // after PARSELY tracking code loads asynchronously
                onload: function() {
                  // bind pageview tracking for all "next" links
                  jQuery(".control-bar .next a").bind("click", function() {
                    var url = this.href,
                    urlref = location.href;

                    PARSELY.beacon.trackPageView({
                      url: url,
                      urlref: urlref,
                      js: 1,
                      action_name: "Next"
                    });
                    return true;
                  });
                }
              };
            </script>
        
        

        
        
