        jQuery("#AMS_GQM_GLOBAL_NAVBAR_ROLLOVER").html(pageAds.AMS_GQM_GLOBAL_NAVBAR_ROLLOVER.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
        jQuery(div).css({
            visibility: 'visible'
        });
    }

    
});

</script>



        
    </div>



    

