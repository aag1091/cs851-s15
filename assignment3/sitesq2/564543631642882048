<script type="text/javascript">

var debug = '';
function log(s){
	if(typeof console != 'undefined') console.log(s);
	debug += s + '\n';
}

if(typeof JSON != 'undefined' && typeof JSON.stringify != 'undefined'){
	var usurv_logs = JSON.parse(decodeURIComponent(get_cookie('surveyLogs2')));
	if(!usurv_logs) usurv_logs = new Array();
	
	var paywallSurveys = {
		maxLogLength: 10, // limit the size of the log to prevent it getting huge

		// Number of skips allowed before long-term quarantine period defined by quarantinePeriods.skipLimit

		skipsBeforeQuarantine: 
parseInt('$m.global.archantvariables.usurvSkipsBeforeQuarantine') ? 
parseInt('$m.global.archantvariables.usurvSkipsBeforeQuarantine') : 2, 	
		// Number of days over which to count the number of skips allowed before long-term quarantine

		skipsTimeFrame: 
parseInt('$m.global.archantvariables.usurvSkipsTimeFrame') ? 
parseInt('$m.global.archantvariables.usurvSkipsTimeFrame') : 30,

		// Length of quarantine for... 
		quarantinePeriods : { 	
			// Passive skip (refresh, back button, close) 	

			'passiveSkip': 
parseFloat('$m.global.archantvariables.usurvPassiveSkipQuarantine') ? 
parseFloat('$m.global.archantvariables.usurvPassiveSkipQuarantine') : 1,
        
			// Active Skip (skip button pressed)	

			'activeSkip':
parseFloat('$m.global.archantvariables.usurvActiveSkipQuarantine') ? 
parseFloat('$m.global.archantvariables.usurvActiveSkipQuarantine') : 1, 
         
			// Completion (survey completed)		

			'completed':
parseFloat('$m.global.archantvariables.usurvCompletedQuarantine') ? 
parseFloat('$m.global.archantvariables.usurvCompletedQuarantine') : 7,
         
			// Skip limit reached		

			'skipLimit':
parseFloat('$m.global.archantvariables.usurvSkipLimitQuarantine') ? 
parseFloat('$m.global.archantvariables.usurvSkipLimitQuarantine') : 120  
         
		},
		log: function(){
			return usurv_logs ? usurv_logs : new Array();
		},
		isQuarantined: function(){
			log('Log length: ' + this.log().length);
			if(this.log().length == 0) return false;
			
			// count number of skips
			var skips = 0;
			var timeFrame = 86400000 * this.skipsTimeFrame;
			for(var i=this.log().length-1; i>=0; i--){
				var elapsed = new Date().getTime() - parseInt(this.log()[i].date);
				if(this.log()[i].type == 'completed' || elapsed > timeFrame) break;
				skips++; 
				if(skips >= this.skipsBeforeQuarantine && elapsed < timeFrame) break;
			}
			log('Skips: ' + skips);
			log('Elapsed: ' + parseFloat(elapsed / 86400000).toFixed(5) + ' days');
			
			// figure out quarantine period
			var lastLog = this.log()[this.log().length-1];
			log('Last log: '+lastLog.type);
			if(skips >= this.skipsBeforeQuarantine) var qp = this.quarantinePeriods['skipLimit'];
			else var qp = this.quarantinePeriods[lastLog.type];
			
			// get difference in days
			var diff = new Date().getTime() - parseInt(lastLog.date); // now - last surveyed
			diff /= 86400000;
			log('Quarantine period: ' + qp + ' days');
			log('Time since last survey: ' + parseFloat(diff).toFixed(5) + ' days');
			
			// Are we still in quarantine period?
			if(diff >= qp) { 
				log('Not quarantined\n');
				return false;
			} else {
				log('Quarantined\n');
				return true;
			}
			
		},
		add: function(type, date){
			if(!date) date = new Date().getTime();
			log('Add: '+type);
			if(type == 'activeSkip' || type == 'completed') {
				this.removeLast();
				log('Last log removed');
			}
			this.log().push({'date': date , 'type': type});	
			this.updateCookie();
		},
		updateCookie: function(){
			if(this.log().length > this.maxLogLength) this.log().splice(this.log().length - this.maxLogLength, this.maxLogLength);
			setCookie('surveyLogs2', JSON.stringify(this.log()), 365, true);
		},
		removeLast: function(){
			this.log().pop();
			this.updateCookie();
		}
	};
	
}

function is_article(){
	if(typeof is_article_page != 'undefined'){
		return is_article_page;
	} else return false;
}

function get_cookie(cookie_name) {
	var results = document.cookie.match ('(^|;) ?' + cookie_name + '=([^;]*)(;|$)');
	if (results) return (results[2]);
	else return null;
}


function setCookie(c_name,value,exdays,path) {
	if(path) path = ' path=/';
		else path = '';
	var exdate=new Date();
	exdate.setDate(exdate.getDate() + exdays);
	var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString() + "; " + path);
	document.cookie = c_name + '=' + c_value;
}
function deleteCookie(c_name,path){
	if(!path) path = false;
	setCookie(c_name, '', -1, path);
}  
function get_usurv_user_id() {
	if(get_cookie("usurvCookieUserId2")){
		deleteCookie('usurvCookieUserId');
		deleteCookie('usurvCookieUserId', true);
		var usurvId = get_cookie("usurvCookieUserId2");
	} else if(get_cookie("usurvCookieUserId")){
		setCookie('usurvCookieUserId2', get_cookie("usurvCookieUserId"), 730, true);
		var usurvId = get_cookie("usurvCookieUserId");
		deleteCookie('usurvCookieUserId');
		deleteCookie('usurvCookieUserId', true);
	} else{
		var theDate = new Date();
		var randomNumber = Math.floor(Math.random()*100001);
		var date = theDate.getDate().toString();
		var month = (theDate.getMonth()+ 1).toString();
		var year = theDate.getFullYear().toString();
		var hours = theDate.getHours().toString();
		var minutes = theDate.getMinutes().toString();
		var usurvId = date + month + year + hours + minutes + randomNumber;
	
		setCookie('usurvCookieUserId2', usurvId, 365, true);
	}
	return usurvId;
}

function runUsurvSurvey(){

	var usurvOptions = {
		PartnerId:		236,
		UserId:			get_usurv_user_id(),
		WebsiteId:		usurvWebsiteId,
		MobWebsiteId:	usurvWebsiteId,
      	OffForMobile:	false,
      	UseMobileRedirect:	false,
		UrlContinue:	'http://www.wisbechstandard.co.uk:80/news/ukip_candidate_says_party_might_consider_repeal_of_same_sex_marriages_tory_candidate_says_she_probably_wouldn_t_have_put_it_on_the_statute_book_in_the_first_place_1_3948066?usurv=completed',
		UrlSkip:		'http://www.wisbechstandard.co.uk:80/news/ukip_candidate_says_party_might_consider_repeal_of_same_sex_marriages_tory_candidate_says_she_probably_wouldn_t_have_put_it_on_the_statute_book_in_the_first_place_1_3948066?usurv=skip',
     	HeaderHtml: '<div class="exclaim"><div class="text"><h6>Help us keep our site free</h6><p>To read this article please answer one short, anonymous survey every seven days. <strong>In this small way you help us keep our site free.</strong></p></div></div>'
	}
  
  	//if(typeof ButtonSpriteName != 'undefined'){
   		//usurvOptions.ButtonSpriteName = ButtonSpriteName;
 	//}
  	usurvOptions.ButtonSpriteName = 'Sprite1';

	if(!paywallSurveys.isQuarantined() && is_article()) {
	
		//if($('#usurvHeader')[0]) usurvOptions.HeaderHtmlContainerId = 'usurvHeader';
		//else usurvOptions.HeaderHtml = '<div>&nbsp;</div>';
	
		Usurv.SurveyGateway.AddNoSurveyHandler(function(){
			paywallSurveys.removeLast();
			log('no survey - show google survey');
		});
	
		log('do survey');

		paywallSurveys.add('passiveSkip'); 
		Usurv.SurveyGateway.GetSurvey(usurvOptions);
	
	} else if('$m.global.archantvariables.usurvRegisterCall' == 'true') {
		log('Quarantined or not article page'); 
		Usurv.SurveyGateway.Register({
			PartnerId: 236,
			WebsiteId: usurvWebsiteId,
			MobWebsiteId: usurvWebsiteId,
			UserId: get_usurv_user_id()
		});
	}
}



</script>
<script src="http://www.usurv.com/Scripts/Integration/v1/SurveyGataway.min.js" type="text/javascript"></script>
<link href=" http://www.usurv.com/Scripts/Integration/v1/Style.css" rel="stylesheet" type="text/css" />
<script type="text/javascript">

	
	var
 usurvWebsiteId = parseInt('84') ?
 parseInt('84 ') : null;
	var
 useOutsideUkCookie = 
parseInt('$m.global.archantvariables.useOutsideUkCookie') ? 
parseInt('$m.global.archantvariables.useOutsideUkCookie ') : 0;
	
	$(function(){
		if(typeof JSON != 'undefined' && get_cookie('outside_uk') == 'true' && useOutsideUkCookie && usurvWebsiteId) runUsurvSurvey();
		else if( typeof JSON != 'undefined' && (!useOutsideUkCookie) && usurvWebsiteId) runUsurvSurvey();
