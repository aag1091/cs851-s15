<script type="text/javascript">
$(function() {
  var launch_job_alert_popup = function(e) {
  var ajax_params = {'ajax': 1, 'modal': 1};
  if ($(this).hasClass('control')) {
    ajax_params['variation'] = 'control';
  } else if ($(this).hasClass('emotional')) {
    ajax_params['variation'] = 'emotional';
  } else if ($(this).hasClass('testimonial_bottom')) {
    ajax_params['variation'] = 'testimonial_bottom';
  } else if ($(this).hasClass('modal_dropdown')) {
    ajax_params['variation'] = 'modal_dropdown';
  } else if ($(this).hasClass('modal_bigarrowright')) {
    ajax_params['variation'] = 'modal_bigarrowright';
  } else if ($(this).hasClass('modal_horizontal_orange')) {
    ajax_params['variation'] = 'modal_horizontal_orange';
  } else if ($(this).hasClass('modal_topteaser')) {
    ajax_params['variation'] = 'modal_topteaser';
  } else if ($(this).hasClass('modal_horizontal_bottom')) {
    ajax_params['variation'] = 'modal_horizontal_bottom';
  } else if ($(this).hasClass('modal_vertical')) {
    ajax_params['variation'] = 'modal_vertical';
  }
  $.get($(this).attr('href'), ajax_params, function(raw_response) {
    parseAjaxResponse(raw_response);
  });
  return false;
};
$('#createAlerteLink').on('click', launch_job_alert_popup);

  

      //Jump link for inlineApply form
  $("#applyAboveLink").on('click',function(e){
    e.preventDefault();
    var target = $('#contact-main-form');
    $('html,document').animate({
      scrollTop: target.offset().top - 140
    }, 400);
    setTimeout(function() {
      $("#contact-main-form input:first").focus();
    }, 800);
    return false;
  });
    $('.upload_resume_slick_apply').on('click', function() {
    $('#resume').trigger('click');
  });

  

  


});
</script><script type="text/javascript">
$(function() {
    /* Trigger tooltip */
  $('[rel=tooltip]').tooltip({placement:"bottom",delay: { show: 300, hide: 100 }});
  var offset = 220;
  var duration = 500;
  $(window).scroll(function() {
      if (jQuery(this).scrollTop() > offset) {
          jQuery('.back-to-top').fadeIn(duration);
      } else {
          jQuery('.back-to-top').fadeOut(duration);
      }
  });
  $('.back-to-top').on('click',function(e) {
      e.preventDefault();
      $('html, body').animate({scrollTop: 0}, duration);
      return false;
  })
});
</script>
