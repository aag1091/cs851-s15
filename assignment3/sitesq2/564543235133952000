<script>
  $(function() {
    var $tos_alert = $('#tos-alert')
      .addClass('loaded')
      ;

    
      $tos_alert.find('button')
        .bind('click', function(e) {
          e.preventDefault();

          $tos_alert.removeClass('loaded');

          document.cookie = "closed_tos_update=1; expires=Fri, 8 May 2015 23:59:59 GMT";

          setTimeout(function() {
            $tos_alert.detach();
          }, 1000);
        })
        ;
      
  });
