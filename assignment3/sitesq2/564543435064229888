			<script>
				var destinationUrl = "https://itunes.apple.com/us/album/reflection-deluxe/id909767577?ls=1&at=11l79x&ct=p1hp1q";
		var numRedirects = 0;
		function LinkDecorator() {};
		LinkDecorator.prototype.decorate = function(url) {
						return url;
		}
		var linkDecorator = new LinkDecorator();
				var numAsyncTasks = 3;
					  function onAsyncTaskComplete() {
		numAsyncTasks--;
			    if (numAsyncTasks <= 0) {
	    	onAllAsyncTasksComplete();
	    }
	   }
		function makePixel(url) {
						var img = document.createElement("img");
			img.onload = function() {
								onAsyncTaskComplete();
			}
			img.src = url;
		}
	   		function onAllAsyncTasksComplete() {
			if (numRedirects == 0) {
								numRedirects++;
								window.location = linkDecorator.decorate(destinationUrl);
							}
		}
	</script>		
	</head>
	<body>
		<script type="text/javascript">
						makePixel("\/\/googleads.g.doubleclick.net\/pagead\/viewthroughconversion\/1007017769\/?value=1.00\x26label=DpMXCJHj_lUQqb6X4AM\x26guid=ON\x26script=0");
