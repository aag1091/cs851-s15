      //<![CDATA[
  jQuery(document).ready(function() {
      function sharedCountSingle(url){
        jQuery.sharedCount(url, function(data){
          var twitterCount = data.Twitter;
          var facebookCount = data.Facebook.total_count;
          var googleCount = data.GooglePlusOne;
          var linkedinCount = data.LinkedIn;
          var totalCount = twitterCount+facebookCount+linkedinCount+linkedinCount;
          var offset = 25;
          var twitterHeight = Math.ceil(twitterCount/totalCount*50)+offset;
          var facebookHeight = Math.ceil(facebookCount/totalCount*50)+offset;
          var googleHeight = Math.ceil(googleCount/totalCount*50)+offset;
          var linkedinHeight = Math.ceil(linkedinCount/totalCount*50+offset);
          jQuery(".socialShare .count").text(totalCount);
          jQuery(".socialShare .icon.twitter").css('height',twitterHeight+'%');
          jQuery(".socialShare .icon.facebook").css('height',facebookHeight+'%');
          jQuery(".socialShare .icon.google").css('height',googleHeight+'%');
          /* jQuery(".socialShare .icon.linkedin").css('height',linkedinHeight+'%'); */
          jQuery(".socialShare").addClass('loaded');
        });	
      }
      jQuery.sharedCount = function(url, fn) {
        url = location.href.split('?')[0];
        //url = encodeURIComponent(url || location.href);
        url = encodeURIComponent(url);
        //console.log(url);
        var arg = { 
          data: {
            url : url,
            apikey : "76637e7b3fbac0d8a1855a6555ed6b3e013dbcfc" //API KEY HERE
          },
          url: "//" + (location.protocol == "https:" ? "sharedcount.appspot" : "free.sharedcount") + ".com/",
          cache: true,
          dataType: "json"
        };
        if ('withCredentials' in new XMLHttpRequest) {
          arg.success = fn;
        }
        else {
          url.split('?')[0];
          var cb = "sc_" + url.replace(/\W/g, '');
          window[cb] = fn;
          arg.jsonpCallback = cb;
          arg.dataType += "p";
        }
        return jQuery.ajax(arg);
      };
sharedCountSingle("");
});
