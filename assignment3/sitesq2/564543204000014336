var labelthumby2 = function(json) {
var topics = [];
var title_feed = json.feed.title.$t;
var pagetype_long='';
pagetype=title_feed.split("|")[0];
pagetype=pagetype.split(" ")[0].toLowerCase();
console.log(pagetype);

if(pagetype=='iphone'){pagetype_long='iPhone Apps Gone Free';}
if(pagetype=='ipad'){pagetype_long='iPad Apps Gone Free';}
if(pagetype=='mac'){pagetype_long='Mac Apps Gone Free';}
if(pagetype=='android'){pagetype_long='Android Apps Gone Free';}
if(pagetype=='ebooks'){pagetype_long='Free eBooks';}
if(pagetype=='collections'){pagetype_long='Top Free EDU Content';}
if(pagetype=='best-selling'){pagetype_long='Best-selling eBooks';} 
if(pagetype=='episodes'){pagetype_long='Best TV Episodes';} 
if(pagetype=='movies'){pagetype_long='Top Movies & Blockbuster';}
if(pagetype=='audiobooks'){pagetype_long='Top Audiobooks';}
if(pagetype=='songs'){pagetype_long='Top Music Downloads';} 





var html = '<h2  style="background: none;text-align: left;font: 18px Oswald;padding: 3px;color: #999999;text-transform: uppercase;margin: -0.3em 0 0.1em;">'+pagetype_long+'</h2><ul id="label_with_thumbs">';
for(var i=0;i<json.feed.entry.length;i++)
{var entry=json.feed.entry[i];var posttitle=entry.title.$t;var posturl;if(i==json.feed.entry.length)break;
for(var k=0;k<entry.link.length;k++){
if(entry.link[k].rel=='replies'&&entry.link[k].type=='text/html'){
var commenttext=entry.link[k].title;var commenturl=entry.link[k].href;}
if(entry.link[k].rel=='alternate'){posturl=entry.link[k].href;break;}}
var thumburl;try{thumburl=entry.media$thumbnail.url;}catch(error)
{s=entry.content.$t;a=s.indexOf("<img");b=s.indexOf("src=\"",a);c=s.indexOf("\"",b+5);d=s.substr(b+5,c-b-5);if((a!=-1)&&(b!=-1)&&(c!=-1)&&(d!="")){thumburl=d;}else thumburl='http://3.bp.blogspot.com/-zP87C2q9yog/UVopoHY30SI/AAAAAAAAE5k/AIyPvrpGLn8/s1600/picture_not_available.png';}
html+= '<li class="clearfix">';
if(showpostthumbnails==true)
html+= '<a href="'+posturl+'" target ="_top"><img class="label_thumb" style="border-radius:0px" src="'+thumburl+'"/></a>';
posttitle_arr = posttitle.split('|');
posttitle_arr2 = posttitle_arr[0].split('-');
if(pagetype=='iphone' || pagetype=='ipad' || pagetype=='mac' || pagetype=='android')
{var num = posttitle_arr[4].match(/\d*\.?\d+/);}

var post_url_arr = posttitle.split("|");
if(pagetype=='iphone' || pagetype=='ipad' || pagetype=='mac' || pagetype=='android'){
