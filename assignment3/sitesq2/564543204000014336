        function imageComplete() {
          if (imageSearch.results && imageSearch.results.length > 0) {
            var contentDiv = document.getElementById('image_search');
            // contentDiv.html = '';
            var results = imageSearch.results;
            for (var i = 0; i < 1; i++) {
              var result = results[i];
              var imgContainer = document.createElement('div');
              /*
         var title = document.createElement('div');
         //title.setAttribute("style","font-size:14px");
         document.getElementById("image_search").appendChild(title);
         title.innerHTML = result.titleNoFormatting;
           */
              var newImg = document.createElement('img');
              newImg.setAttribute("class","rounded shadow");
              newImg.setAttribute("border","2");
              newImg.setAttribute("border-color","#ddd");
         newImg.setAttribute("alt",term);
             newImg.setAttribute("title",term);
              newImg.setAttribute("z-index","1");
              document.getElementById("image_search").appendChild(newImg);
              newImg.src = result.tbUrl;
              //newImg.setAttribute("alt",result.titleNoFormatting);
              //newImg.setAttribute("title",result.titleNoFormatting);
              // imgContainer.appendChild(title);
              imgContainer.appendChild(newImg);
              contentDiv.appendChild(imgContainer);
            }
            // Now add the paging links so the user can see more results.
          //  addPaginationLinks(imageSearch);
          }}
