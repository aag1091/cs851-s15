}
if("content"in entry){var postcontent=entry.content.$t;}
else
if("summary"in entry){var postcontent=entry.summary.$t;}
else var postcontent="";var re=/<\S[^>]*>/g;postcontent=postcontent.replace(re,"");
var towrite='';
html+= towrite;html+= '</li>';
if(displayseparator==true)
if(i!=(numposts-1))
html+='';}html+='</ul>';
	$('img').error(function() {$(this).hide();}); 
document.getElementById(pagetype).innerHTML = html;  
	
};


var customizeForDevice = function(){
    var ua = navigator.userAgent;
    var checker = {
      iphone: ua.match(/(iPhone|iPod)/),
      ipad: ua.match(/(iPod|iPad)/),
	  mac: ua.match(/(Mac|iPod)/),
	  blackberry: ua.match(/BlackBerry/),
      android: ua.match(/Android/)
    };
    if (checker.android){}
    else if (checker.iphone){
	label_thumb_iphone();
console.log("iphone");
}
    else if (checker.ipad){
	label_thumb_ipad();
console.log("ipad");
}

    else if (checker.mac){
	label_thumb_mac();
console.log("Mac");
}
    else if (checker.android){
	label_thumb_android();
console.log("Android");
}
    /*else if (checker.blackberry){}*/
    else { 
//	label_thumb_iphone();
//	label_thumb_ipad();
//	label_thumb_mac();
//	label_thumb_android();
var ratemal = Math.floor((Math.random() * 7) + 1);
switch (ratemal) {
    case 1:        label_thumb_freeebooks();        break;
    case 2:        label_thumb_collections();        break;
    case 3:        label_thumb_bestselling();        break;
    case 4:        label_thumb_episodes();        break;
    case 5:        label_thumb_movies();        break;
    case 6:        label_thumb_audiobooks();        break;
	case 7:        label_thumb_songs();        break;
}
