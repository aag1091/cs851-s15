                <script type="text/javascript">
	                $(document).ready(function(){
	                    // ê³µì§€ ë¶ˆëŸ¬ì˜¤ê¸°
	                    $.getJSON('http://json.dcinside.com/notice.php?jsoncallback=?', function(data) {
	                        var str = "";
	                        $.each(data, function(i, e) {
	                            str += "<dd><span class=\"txt_gall_notice\"><a href=\""+e['link']+"\">"+e['title']+"</a></span></dd>";
	                        });
	                        $('#notice_rolling').html(str)

	                        // ê³µì§€ ë¡¤ë§
	                        if ($('#notice_rolling').children().length>1) {
	        	                setInterval('notice_rolling()',3000);
	        	            }
	                    });

	                });

	                function notice_rolling () {
	                    $('#notice_rolling').find('dd').each(function(){
	                        if ($(this).is(':first-child')) {
	                            $(this).slideUp(function(){$(this).appendTo($(this).parent()).slideDown();});
	                        }
	                    });
	                }
                </script>
