                    { event: "viewList", item: [query + (Math.floor(Math.random()*6)+1), query + (Math.floor(Math.random()*6)+1), query + (Math.floor(Math.random()*6)+1)] },
                    { event: "viewItem", item: query + (Math.floor(Math.random()*6)+1) });
        
    
</script>

