<script language="javascript" type="text/javascript">
    $(".more-link").showHeaderMenu({
        id: '#dd_container_more'
                ,onBeforeShow: function() {
            $('.more-link').prevAll('li').first().children('.divider').hide();
        },
        onBeforeHide: function() {
            if ($('.more-link').prevAll('li').first().css('background-image') =='none') {
                $('.more-link').prevAll('li').first().find('.divider').show();
            }
        }
            });

		$('li.show_nav_btn').each(function(index){
			$(this).mouseover( function(){ $('img#_' + index).hide(); $('img#_' + (index + 1)).hide(); });
			$(this).mouseout( function(){ if ( (!$(this).hasClass('more-link')) && (!$(this).hasClass('li_dd'))) {$('img#_' + index).show(); if (!$(this).next().hasClass('nav_on_more')) $('img#_' + (index + 1)).show()}});
		});

		

    $('.drop_down_3').showHeaderMenu({
        id: '#dd_container_3',
        onBeforeShow: function() {
            $('.drop_down_3').addClass('nav_on_more');
            $('.drop_down_3').prevAll('li').first().children('.divider').hide();
        },
        onBeforeHide: function() {
            $('.drop_down_3').removeClass('nav_on_more');
            if ($('.drop_down_3').prevAll('li').first().css('background-image') =='none') {
                $('.drop_down_3').prevAll('li').first().find('.divider').show();
            }
            if ($('.drop_down_3').nextAll('li').first().css('background-image') =='none') {
                $('.drop_down_3 img.divider').show();
            }

        }
    });

    </script>                <div id="cbs--loader--master"></div>

                
