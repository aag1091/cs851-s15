<script>
    CBS.V2.SiteWideTealium = function() {

        this.initTealiumSiteWide = function () {
            if (typeof Tealium === 'undefined' && typeof siteWideTealium === 'object') {
                setTimeout(function(){siteWideTealium.initTealiumSiteWide()}, 100);
            } else {
                Tealium.attachClickEvents("site_wide/1");
            }
            $('#footerLinksRow1 a, #footerLinksRow2 a').click(function(e){
                $('#footerLinksRow1 a, #footerLinksRow2 a').removeClass('hover');
                $(this).addClass('hover');
            });
        };

        this.getTealiumFooterData = function() {
            var clickedTop = $('#footerLinksRow1 a.hover');
            var clickedBot = $('#footerLinksRow2 a.hover');
            var posTop = $(clickedTop).index('#footerLinksRow1 a');
            var posBot = $(clickedBot).index('#footerLinksRow2 a');
            var pos = (posTop === -1)?posBot + $('#footerLinksRow1 a').length :posTop;
            var linkTxt = ($(clickedTop).length === 0)?$(clickedBot).html():$(clickedTop).html();
            return "footer|" + linkTxt + "|" + pos + "|click";
        }

        this.getTealiumGlobalNavData = function() {
            var clicked = $('.nav-menu .nav-item a.hover');
            var pos = $(clicked).parent().index();
            return "global nav|" + $(clicked).html() + "|" + pos + "|click";
        }

    };

