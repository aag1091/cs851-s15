<script>
  show_player = function() {
    var snap = new $_M.Snap({"snap":{"id":472701}}.snap);
    player_settings =  {
      container: $('.sp-video'),
      container_id: '#player',
      static_host: 'http://static.snappytv.com',
      static_secure_host: 'https://staticsnappytv-a.akamaihd.net',
      debug: '',
      geo: 'US',
      tz: 'EST',
      device: 'desktop',
      referer: 'empty',
      cname: '',
      cname_hash: '',
      context: 'view_page',
      platform: 'snappytv'
    };
    player_settings.container.STVSnapPlayer(player_settings);
    player_settings.container.STVSnapPlayer('embedPlayer', snap.attributes, true, true);
    $(player_settings.container_id).bind('SNAP_PLAYER_INTERFACE_READY', _.bind(function(e) {
      if( true ) {
        player_settings.container.STVSnapPlayer('playSnap', snap.attributes);
      } else {
        player_settings.container.STVSnapPlayer('loadSnap', snap.attributes);
      }
    }, this));
  }
</script>
<script>
  show_player();
</script>

<script src="http://static.snappytv.com/assets/lib/modernizr-1.6.min-e8db0d439bdbf55889fd2232013fb445.js" type="text/javascript"></script>
<script src="http://static.snappytv.com/assets/player_share_window-49918eb4145ea73dca2c5e5952f01dd6.js" type="text/javascript"></script>
<script>
  $(document).ready(function() {
  
    if (!Modernizr.input.placeholder) {
      $("#new_comment label").inFieldLabels({fadeDuration:100});
    } else {
      $("#new_comment label").hide();
    }
    $('.clip-here').click( function(e) {
      $('#player').html('');
      e.preventDefault();
      $.ajax({
        type: 'POST',
        data: { snap_id: "472701"
        },
        url: "/snaps/stream_data_for_snap",
        dataType: 'json',
        success: function(res) {
          conurl = res.service + escape( '?aifp=' + res.aifp + '&auth=' + res.auth );
          show_player( res.file, conurl );
        }
      });
    });
  });
</script>
