<script>
    function login_required() {
      if (!is_logged_in()) {
        $.show_login_box();
        return true;
      } else {
        return false;
      }
    }

    function registration_required() {
      if (!is_logged_in()) {
        window.location.href = "/accounts/register/";
        return true;
