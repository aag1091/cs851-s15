						<script type="text/javascript">
                        	var clicked = false;
							var refineIDs = decodeURIComponent('1138').replace(/\s/g, '+').split('+');
							function underlineNav(id) {
							    document.getElementById(id).style.textDecoration = 'underline';
							}

							function clearUnderlineNav(id) {
							    document.getElementById(id).style.textDecoration = 'none';
							}
							
							function submitMultiSelect(baseRefine,isNew,isBrand) {
   							    var newRefineValue = baseRefine;
							    var newDesc = "";
							   	$('.popupCb').each(function(index, elem) {
									for (j=0; j < refineIDs.length; j++) {
										if (refineIDs[j] === elem.value) {
											refineIDs.splice(j,1);
									  	}
									}
									// add back to refineIDs if checked  
									if (elem.checked) {
										refineIDs.push(elem.value);
										newRefineValue=newRefineValue+"Z"+elem.value;
							            if(isBrand){
							            	newDesc += document.getElementById("cbPopupLabel"+(index+1)).value + "/";
							            }
							        }
							    });
								if(isNew){
									if(newDesc != ""){
										newDesc = newDesc.toLocaleLowerCase();
									}
							   		var actionUrl = document.getElementById('facetValuesForm').action;
							   		actionUrl=actionUrl.replace("_/"+baseRefine,newDesc+"_/"+newRefineValue);
							   		var seoNvalDesc = document.getElementById("seoNvalDesc");
							   		if(seoNvalDesc != null){
							   			seoNvalDesc.value=seoNvalDesc.value+newRefineValue;
							   		}
							   		document.getElementById('facetValuesForm').action=actionUrl;
							    }else{
							    	 document.getElementById('refine').value = refineIDs.join(' ');
							    }
							    document.getElementById('facetValuesForm').submit();
							}
							
							jQuery(function () {
							    addLeftNavRefineEvents('http://www.qvc.com/fashion/cee-bee-cheryl-burke/_/N-lgltZ1z138mv/c.html?pageSize=96')
							});
						</script>
                                                                                           
