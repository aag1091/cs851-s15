	var relateds = $('div.divNavCustom a');
	for (var i = 0; i < relateds.length; i++) {
		relateds[i].onclick = function() {
			
		}
	}
}


function addOnClickEvent4LRF() {
	var linkLabs = $('ul li a.RefineLink label');
	for (var i = 0; i < linkLabs.length; i++) {
		linkLabs[i].parentNode.onclick = function() {
			cmCreateConversionEventTag("Cartridge","2","FacetedNavigationCustom","0","NAVIGATION - Fashion \x3E REFINE Brand\x3Ecee bee CHERYL BURKE-_-ARC\x3A Brand, Neckline, Fabric-_-null-_-"+formatLRFAtt4(this.parentNode.parentNode.previousElementSibling.innerHTML)+"-_-"+this.parentNode.parentNode.previousElementSibling.innerHTML+"");
		}
	}
	
	var noCheckBoxLinks = $("ul.noCheckBox li a.RefineLink");
	for (var i = 0; i < noCheckBoxLinks.length; i++) {
		noCheckBoxLinks[i].onclick = function() {
			cmCreateConversionEventTag("Cartridge","2","FacetedNavigationCustom","0","NAVIGATION - Fashion \x3E REFINE Brand\x3Ecee bee CHERYL BURKE-_-ARC\x3A Brand, Neckline, Fabric-_-null-_-"+formatLRFAtt4(this.parentNode.parentNode.previousElementSibling.innerHTML)+"-_-"+this.parentNode.parentNode.previousElementSibling.innerHTML+"");
		}
	}
	
	var linkChooseMores = $("p.chooseMore");
	for (var i = 0; i < linkChooseMores.length; i++) {
		linkChooseMores[i].onclick = function() {
			cmCreateConversionEventTag("Cartridge","2","FacetedNavigationCustom","0","NAVIGATION - Fashion \x3E REFINE Brand\x3Ecee bee CHERYL BURKE-_-ARC\x3A Brand, Neckline, Fabric-_-null-_-"+formatLRFAtt4(this.previousElementSibling.previousElementSibling.innerHTML)+"-_-"+this.previousElementSibling.previousElementSibling.innerHTML+" | "+"Choose More"+"");
		}
	}
	
}

//for defect 19322
function addLrfConversionTag() {
	var attr5s = $('div#divNavFaceted div h3');
	
	var attr5 = "";
	for (var i = 0; i < attr5s.length; i++) {
		var attr5One = attr5s[i];
		var label = '';
		if($(attr5One).children('a')[0]) {
			label = attr5One.innerText.replace($(attr5One).children('a')[0].innerText,"");
		} else {
			label = attr5One.innerText;
		}
		
		if(i == 0){
			attr5 += label;
		}else{
			attr5 += ' | ' + label;
		}

	}
	
	var funStr = $("textarea#lrcConversionTagTextarea").eq(0).text();
	funStr = funStr.replace('ATTRIBUTE5',attr5);
	
	var fun = new Function(funStr.substring(funStr.indexOf('cmCreat'),funStr.indexOf(')\');')+1));
	
	fun();
}
// end of defect 19322

function formatLRFAtt4(link) {
	var isSort = false;
	var isBoost = false;
	var isBury = false;
	var att4 = "";
	
		if (link == "Brand") {
			isSort = true;
		}
	
		if (link == "Neckline") {
			isSort = true;
		}
	
		if (link == "Fabric") {
			isSort = true;
		}
	
	if (isBoost && isBury) {
		att4 = "BOOSTBURY | ";
	} else if (isBoost) {
		att4 = "BOOST | ";
	} else if (isBury) {
		att4 = "BURY | ";
	}
	if (isSort) {
		att4 += "SORT";
	}
	if (att4 != "" && att4.endsWith(" | ")) {
		att4 = att4.replace(" | ","");
	}
	return att4==""?null:att4;
}

