<script language="Javascript">   
var busy = false;
function nextUrl(url){
	if (!busy) {
		busy = true;
		
		//When SEO Flag is open, we should add "/" before the URL
		if(url.charAt(0)!='/'){
			url='/'+url;
		}
		
		window.location.href=url;
	}
}

$(function(){
	
	$('a.prodDetailLink').on('click', function(){
		qCookie.set('productListingProducts', 'A261534,A261546,A261549,A261543,A261539,A261536,A261554,A261535,A261552,A261553,A261550,A261541,A261540,', {path: '/'});
		var crumbURLs = '';
		var crumbLabels = '';
		$('#breadCrumbs a').each(function (index, el) {
				var labeltext = el.innerHTML;
				crumbURLs = crumbURLs + el.href + '||';
				crumbLabels = crumbLabels + labeltext + '||';
		});
		var thisPageURL = "";
		if(thisPageURL == ""){
			//crumbURLs = crumbURLs + 'http://www.qvc.com/fashion/cee-bee-cheryl-burke/_/N-lgltZ1z138mv/c.html?pageSize=96';
			var thisPageURL = 'http://www.qvc.com/fashion/cee-bee-cheryl-burke/_/N-lgltZ1z138mv/c.html?pageSize=96';
			
			var refinedUrl = '';
			var refinementId = '';
			if(thisPageURL.indexOf('&refine=')>-1)
				refinedUrl = thisPageURL.substring(thisPageURL.indexOf('&refine=')+8);
			if(thisPageURL.indexOf('?refine=')>-1)
				refinedUrl = thisPageURL.substring(thisPageURL.indexOf('?refine=')+8);
			if(refinedUrl.indexOf('&')>-1)
				refinedUrl = refinedUrl.substring(0,refinedUrl.indexOf('&'));
			
			if(refinementId != null && refinementId != ''){
				thisPageURL = thisPageURL.replace(refinedUrl,refinementId);
			}
		}
		crumbURLs = crumbURLs + thisPageURL;
		
		// Added for defect 8697
		var lastLabel = 'cee bee CHERYL BURKE'; 

		if(lastLabel.match(/[0-9]/)){                                
	                                      
		   lastLabel = 'cee bee CHERYL BURKE'; 
		}
        crumbLabels = crumbLabels + lastLabel;
		
		//crumbLabels = crumbLabels + 'Fashion';
		// End of defect 8697
		qCookie.set('prodListPathUrls', crumbURLs, {path: '/'});
		qCookie.set('prodListPathLabels', crumbLabels, {path: '/'});
	});
	
	$('a.PagingLink').on('click', function(event) {
		
		
		//qCookie.set('refinementCookie','true');
	 	var allSelectedCompareProducts = getElementsByClass( "cb" );
		var regSelectToCompare = "cbSelectToCompare";
		var appendToUrl = '';
		for (i=0; i<allSelectedCompareProducts.length; i++) {					
			var colorCode = '';
			var selectedPartNumber = allSelectedCompareProducts[i].value;
			if ( allSelectedCompareProducts[i].checked == true && regSelectToCompare.test(allSelectedCompareProducts[i].id) ){				
				if(document.getElementsByName('ColorCode_' + selectedPartNumber)[0])
					colorCode = document.getElementsByName('ColorCode_' + selectedPartNumber)[0].value;
				appendToUrl = appendToUrl + '&selectedProductId='+ allSelectedCompareProducts[i].value + '&ColorCode_' + selectedPartNumber + '=' + colorCode;
			}
		}
		
		//00125114 - Defect 19290 - Removed client-side decoding
		var aLinkUrl = this.href + appendToUrl;
		window.location.href = aLinkUrl;
		return false;
		
	});
	
	$('select.productListAjaxSelect').on('change', function(event) {
		
		//qCookie.set('refinementCookie','true');
		var aLinkUrl = this.value;
		
		//00125114 - Defect 19290 - Removed client-side decoding
       	window.location.href = aLinkUrl;
       	return false;
		
	});
});
</script>
