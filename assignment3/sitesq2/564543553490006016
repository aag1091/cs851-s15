<script type="text/javascript">

var redirectURL = location.href;

// Remove selected products
function ClearAndRedirect()  {
	var ignoreSelectedPrd = redirectURL.replace(/selectedProductId/gi, "ignoreProductId");
	location.href = ignoreSelectedPrd;
}

	// Throw Coremetrics element tag (this used to be manualLinkClick tag)
	// @param ele - a select dropbox
	function linkclicktagcall(ele) {
		var elementCategory = 'Product Listing';
		
		var currentpre=ele.id;
		var selectedText = ele.options[ele.selectedIndex].text;

		var selectName = '';
		if (currentpre == 'selProductSortTop') {
			selectName = 'Sort By:';
		} else if (currentpre == 'selItemsPerPageTop') {
			selectName = 'Items Per Page:';
		}

		cmCreateElementTag(selectName + '[' + selectedText + ']', elementCategory);
	}
</script>

