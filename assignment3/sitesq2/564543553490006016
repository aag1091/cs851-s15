<script type="text/javascript">
function deleteCookies(){
                qCookie.set('narrowCookie','',{path: '/',expires: -1});
                qCookie.set('currentPageNumber','',{path: '/',expires: -1});   
                qCookie.set('currentPageCount','',{path: '/',expires: -1});
}


var previousRefine = '';
if (previousRefine && previousRefine != '') {
	var ulObj = document.getElementById('ulSelectedFacets');
	refineLins = ulObj.getElementsByClassName('RefineLink');
	if (refineLins && refineLins.length > 0) {
		for (var i = 0; i < refineLins.length; i++) {
			var search = refineLins[i].search;
			if (search.contains('formatRefine=true')) {
				refineLins[i].search = search.replace(search.substring(search.indexOf('refine='),search.indexOf('&', search.indexOf('refine='))),'refine='.concat(previousRefine.replace(/\s/g,'+')));
			}
		}
	}
	var viewAll = jQuery('p.chooseMore a.RefineLink');
	var search = viewAll[0].search;
	if (search.contains('formatRefine=true')) {
		viewAll[0].search = search.replace(search.substring(search.indexOf('refine='),search.indexOf('&', search.indexOf('refine='))),'refine='.concat(previousRefine.replace(/\s/g,'+')));
	}
}
</script>


