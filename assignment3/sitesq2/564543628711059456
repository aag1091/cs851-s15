            <div class="newsletterFooter" align="center">
                <div class="newsletterHolder">
                <span class="join">Join our newsletter!</span>
                <span class="get">Get daily updates when you sign up to our free newsletter</span>
                <div class="newsletter fix ">
                    <div class="newsletterBoxSmall fix">
                        
                         <form action="" method="post">
                            <div class="input-border"><input name="newsletterMail" type="text" id="newsletterMail" value="Your mail" /></div>
                            <button id="newsletterSubscribe"><span>Sign in</span></button>
                        </form>
                    </div> 
                </div>
                <div id="newsletterMessage" class="newsletterMessage left"></div>
<script language="javascript" type="text/javascript">

    function isValidEmailAddress(emailAddress) {
        var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
        return pattern.test(emailAddress);
    }

    $.ajaxSetup({ cache: false });
    $(function(){
        var defaultText = 'Your mail';
        $('#newsletterMail').focus(function(){
            if ($(this).val()==defaultText) {
                $(this).val('');
            }
        });
        $('#newsletterMail').blur(function(){
            if ($(this).val()=='') {
                $(this).val(defaultText);
            }
        });
        $("#newsletterSubscribe").click(function(){
            var mail = $("#newsletterMail").val();
            if (!isValidEmailAddress(mail)) {
                $("#newsletterMessage").html('Invalid mail address');
                return false;
            }
            $("#newsletterMessage").load("/sw4i/newsletterRegistration", {'email': mail, 'lang': 'en' },
            function (responseText, textStatus, XMLHttpRequest) {
                if ((XMLHttpRequest.getResponseHeader("subscribed") == "1")) {
                    $("#newsletterMail").val("");
                    window.location = "/NewsletterThankYou.html";
                }
            }
        );
            return false;
        });
    });

</script>

