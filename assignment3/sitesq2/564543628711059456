<script>
    $('li:[data-open]').click(function(e) {
        if ($(this).attr('data-open') === '1') {
            $(this).attr('data-open', '0');
            $(this).children('#strelad').hide();
            $(this).children('#strelag').show();
//            e.preventDefault();
//            console.log('clicked 1');
            $(this).nextAll('li').each(function() {
                if ($(this).is('.depth1')) {
                    return false; // next parent reached, stop
                }
                $(this).show();
            });
        } else {
            $(this).attr('data-open', '1');
            $(this).children('#strelag').hide();
            $(this).children('#strelad').show();
//            e.preventDefault();
//            console.log('clicked 0');
            $(this).nextAll('li').each(function() {
                if ($(this).is('.depth1')) {
                    return false; // next parent reached, stop
                }
                $(this).hide();
            });            
        }
    });
</script>

