                            for(i = 0; i < data.length; i++) {
                                document.getElementById("tweetcontainer").innerHTML = document.getElementById("tweetcontainer").innerHTML + '<a href="http://twitter.com/QueenJurs/status/' + (data[i].id_str ? data[i].id_str : data[i].id) + '"><div class="content">' + data[i].text + '</div></a>';
                            }
                        }
                    </script>
                </div>
                
                
                
                    
