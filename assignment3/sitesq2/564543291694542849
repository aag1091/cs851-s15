			<script type="text/javascript">

				

				function shoptopia_user_core(ss_data) {
					this.ss_data = ss_data || {};
					this.data = {};
					this.loaded = false;
					this.defaults = {
						shoptopia_user_id : 0,
						shoptopia_user_name : '',
						// picture : 'http://images.shoptopia.com/missing/pictures/graylady.jpg',
						shoptopia_user_gender : 'Female',
						is_logged_in : false,
						is_confirmed : false,
						is_18 : false,
						shoptopia_user_language_locale : 'en_US'
					};
					this.theme = {};
				}

				shoptopia_user_core.prototype.save = function() { // save to cookies, that is.
					var date = new Date();
					date.setTime(date.getTime()+(24*60*60*1000)); //one day
					var expires = "expires="+date.toGMTString();
					var bits = document.domain.split('.');
					var tld = bits.pop();
					var cookieDomain = '.' + bits.pop() + '.' + tld;
					var temp_data = this.data;
					temp_data.bassPerms = false;
					document.cookie = "shoptopia_user=" + escape(JSON.stringify(temp_data)) + "; " + expires+"; path=/; domain=" + cookieDomain;
				}

				shoptopia_user_core.prototype.clear_cookies = function() {
					var date = new Date();
					date.setTime(date.getTime()+(1000*60*60*24* -1));
					var expires = "expires="+date.toGMTString() + ';';
					bits = document.domain.split('.');
					tld = bits.pop();
					cookieDomain = bits.pop() + '.' + tld;
					document.cookie = "shoptopia_user=;"+expires+" path=/; domain=." + cookieDomain;
				}

				shoptopia_user_core.prototype.readCookie = function(name) {
					var nameEQ = name + "=";
					var ca = document.cookie.split(';');
					for(var i=0;i < ca.length;i++) {
						var c = ca[i];
						while (c.charAt(0)==' ') c = c.substring(1,c.length);
						if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
					}
					return null;
				}

				shoptopia_user_core.prototype.refresh = function() { // get fresh data from server
					// console.log('clearing out data to force a data refresh');
					this.loaded = false;
					this.clear_cookies();
					// console.log(this);
					this.load();
				}

				shoptopia_user_core.prototype.load = function() {
					// console.log('loading');
					this.loaded = false;
					this.data = this.data || {};
					for(x in this.defaults) this.data[x] = this.defaults[x]; // assumed not logged in.
					shoptopia_user_json = readCookie('shoptopia_user');
					if(shoptopia_user_json) {
						// console.log('loading from cookie');
						this.data = JSON.parse(unescape(shoptopia_user_json));
						this.loaded = true;
						this.jog_display();
					} else {
						// no cookie, go load it.
						
						var that = this;
						$.ajax({
							url: "http://shoptopia.shopatcoloniecenter.com/webService",
							data: {
								task: 'getUserInfo'
							},
							dataType: 'jsonp',
							success: function(response){
								// console.log(response);
								if(response.data) {
									// pull everything over to the this obj
									for(x in response.data) {
										if(x == 'uid') that.data.shoptopia_user_id = response.data[x];
										else if(x == 'name') that.data.shoptopia_user_name = response.data[x];
										else if(x == 'userGender') that.data.shoptopia_user_gender = response.data[x];
										else if(x == 'picture') that.data.shoptopia_user_picture = response.data[x];
										else that.data[x] = response.data[x];
									}
									that.data.is_logged_in = that.data.shoptopia_user_id > 0; // jsut to be safe
									that.data.is_18 = that.data.bassPerms.userOver18 || false;
								} else {
									// no data could be pulled - not logged in.
									for(x in this.defaults) that.data[x] = this.defaults[x];
								}
								
								that.loaded = true;
								that.save();
								// console.log('running jog display for user no: ' + shoptopia_user.data.shoptopia_user_id);
								that.jog_display();
							}
						});
					}
				}

				shoptopia_user_core.prototype.account_activity_check = function(name) {
					var query_vars = [];
					var kvpairs = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
					for(var i = 0; i < kvpairs.length; i++) {
						pair = kvpairs[i].split('=');
						query_vars[pair[0]] = pair[1];
					}
					//console.log(query_vars);
					for(key in query_vars) {
						if(key == 'emailConfirmCode') {
							$.ajax({
								url: 'http://bass.shoptopia.com/webService',
								dataType: 'jsonp',
								data: {
									task: 'SEW_emailConfirmCode_process',
									emailConfirmCode: query_vars[key]
								},
								success: function(response){
									alert(response.userMessages.join('\n'));
								}
							});
						} // etc
					}
				}

				shoptopia_user_core.prototype.jog_display = function() { // this method needs to be smart enough to know when to just refresh the page.
					
					if(this.loaded) {

						if( ! this.data.shoptopia_user_id && window.location.href.indexOf('user/new') == -1) {
							if(window.location.search.indexOf('start=login') != -1) mBox.login();
							else if(window.location.search.indexOf('start=regist') != -1) mBox.register();
						}

						// loginBait, jailBait, etc
						baiter.jog({is_logged_in: this.data.is_logged_in, is_confirmed:this.data.is_confirmed, is_18: this.data.is_18});

						if(this.data.is_logged_in && this.data.notificationCount) {
							var shoptopia_user_id = this.data.shoptopia_user_id;
							$("#notification-new-mail").html(this.data.notificationCount).show(300).click(function(){
								window.location.href = '/user/' + shoptopia_user_id + '/feed';
							});
						} else {
							$("#notification-new-mail").html('').hide(300);
						}

					} else {
						this.load(); // will call jog_display again.
					}

				}

				
				

		/* no extensions */
		
			
			function shoptopia_user_v2() {
				
				shoptopia_user_core.call(this);
			}

			
			shoptopia_user_v2.prototype = new shoptopia_user_core();

			
			shoptopia_user_v2.prototype.constructor = shoptopia_user_core;
							
			
			shoptopia_user_v2.prototype.jog_display = function() {

				if(this.loaded) {
					if( ! this.data.shoptopia_user_id && window.location.href.indexOf('user/new') == -1) {
						if(window.location.search.indexOf('start=login') != -1) mBox.login();
						else if(window.location.search.indexOf('start=regist') != -1) mBox.register();
					}

					if(this.data.shoptopia_user_id) {
						temp = $('.provisional');

						if(temp.length) {
							temp.remove();
							mine = $('#logged_in_nav');
							// set the utility bar. for new anon sites
							$('#mGreeting strong', mine).html(this.data.shoptopia_user_name);
							$('a#edit_profile_link', mine).attr('href', '/user/' + this.data.shoptopia_user_id + '/edit');
							$('a#feed_link', mine).attr('href', '/user/' + this.data.shoptopia_user_id + '/feed');
							mine.show(100);
						}
					}

					// loginBait, jailBait, etc
					baiter.jog({is_logged_in: this.data.is_logged_in, is_confirmed:this.data.is_confirmed, is_18: this.data.is_18});

					// Following stores
					(function($, api, isLoggedIn, baiter) {
						var $followZone = $('.follow-zone');

						// Only run if the followZone widget exists on current page.
						if ($followZone.length > 0) {
							var currentStoreInstanceId = parseInt($followZone.attr('data-store-instance-id'));
							console.log('currentStoreInstanceId: ' + currentStoreInstanceId);
							var $unfollowButton = $followZone.find('.follow-zone__unfollow');
							var $followButton = $followZone.find('.follow-zone__follow');

							// If the user is logged in, check if the store is followed and attach follow/unfollow events. Otherwise, show the follow button and trigger login bait.
							if (isLoggedIn) {
								var followStore = function(storeInstanceId) {
									var endpoint = api + '/webService?task=follow&appID=10&followeeID=' + storeInstanceId + '&callback=?';

									displayStoreAsFollowed();

									$.getJSON(endpoint, function(response) {
										console.log('follow response', response);
									});
								};

								var unfollowStore = function(storeInstanceId) {
									var endpoint = api + '/webService?task=follow&appID=10&followeeID=' + storeInstanceId + '&remove=1&callback=?';

									displayStoreAsUnfollowed();

									$.getJSON(endpoint, function(response) {
										console.log('unfollow response', response);
									});
								};

								var displayStoreAsFollowed = function() {
									$followZone.addClass('follow-zone--following');
								};

								var displayStoreAsUnfollowed = function() {
									$followZone.removeClass('follow-zone--following');
								};

								var detectIfStoreIsFollowed = function(storeInstanceId) {
									var endpoint = api + '/webService?task=getUserInfo&callback=?';

									$.getJSON(endpoint, function(response) {
										if (response && response.data && response.data.bassPerms && response.data.bassPerms.aStoreFan) {
											var followedStores = response.data.bassPerms.aStoreFan;

											$.each(followedStores, function(key, value) {
												var aStoreInstanceId = parseInt(value);

												if (aStoreInstanceId === storeInstanceId) {
													displayStoreAsFollowed();
													return false;
												}
											});

											$followZone.addClass('follow-zone--loaded');
										}
									});
								};

								var init = function() {
									detectIfStoreIsFollowed(currentStoreInstanceId);

									$followButton.on('click', function(e) {
										e.preventDefault();
										followStore(currentStoreInstanceId);
									});

									$unfollowButton.on('click', function(e) {
										e.preventDefault();
										unfollowStore(currentStoreInstanceId);
									});
								};

								init();
							}

							else {
								$followZone.addClass('follow-zone--loaded');

								$followButton.on('click', function(e) {
									e.preventDefault();
									baiter.trigger();
								});
							}
						}
					})($, ingressURL, this.data.is_logged_in, baiter);
				}

				else {
					this.load(); // will call jog_display again.
				}
				
			}

			var shoptopia_user = new shoptopia_user_v2();
		
		
		$(function() {
			shoptopia_user.load(); 
			shoptopia_user.refresh();
			shoptopia_user.account_activity_check();
		});
	
				
			</script>

			
			
