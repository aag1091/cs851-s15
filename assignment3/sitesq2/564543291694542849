				var ingressURL = 'http://shoptopia.shopatcoloniecenter.com';
				function logout_13(){
					if(typeof shoptopia_user == 'object' && typeof shoptopia_user.clear_cookies == 'function') shoptopia_user.clear_cookies();
					$.ajax({
						url:ingressURL + '/logout?', 
						dataType: 'jsonp', 
						success: function(){window.top.location.href='/'}
					});
				}
