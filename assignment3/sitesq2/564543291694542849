<script>
	$('li.dropdown-js')
		.on('click', function() {
			var dropdown = $(this).find('ul');
			if (dropdown.hasClass('drop')) {
				dropdown.removeClass('drop');
			}
			else {
				$('.drop').removeClass('drop');
				dropdown.addClass('drop');
			}
		});
</script>



