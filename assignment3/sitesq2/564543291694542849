	<script type="text/javascript">
		// use like this: pw_ga.add_event({category:'test', action:'test', debug:true}); // try it!
		var _gaq = _gaq || [];
		var pw_ga = {
			version: 0.8,
			add_event: function(options) {
				options = options || 'help';
				var debug = options.debug || options == 'help' || false;
				aGAID = aGAID || []; // this is the array of account numbers we wish to track on. it is supposed to be a global defined elsewhere on the page.
				var category = options.category || '';
				var action = options.action || '';
				// category and action are required.
				if(category.length < 3 || action.length < 3) {
					if(debug) console.log('options must include a category and an action');
					return;
				}
				var label = '';
				if(typeof options.label == 'string' && options.label.length) label = options.label;
				var value = options.value || '';
				var non_interaction = options.non_interaction || false;

				if(debug) console.log(options);

				var event_arguments = ['_trackEvent', category, action]; // this is what will be passed to the _gaq
				// one can only provide later values if you also provide earlier ones. keep that in mind as calling this. you can't have a value unless you have a label.
				// for now, if an early one is empty, the rest are ignored. i suppose i could put 'undefined' for earlier values, but can't use 0. so it will still track, but it won't be the right info.
				if(label.length) {
					event_arguments.push(label);
					//value must be an integer - if supplied
					if(parseInt(value) == value) {
						event_arguments.push(parseInt(value));
						//non_interaction must be a boolean, which defaults to false anyway - so i only care if you tried to tell me 'yes'
						if((typeof non_interaction == 'Boolean' && non_interaction) || non_interaction == 'yes' || non_interaction == 'true' || non_interaction == 1) event_arguments.push(true);
					}
				}

				// currently we need/support page view tracking - which should be done already, and event tracking.
				// all we are going to do is append things to the gaq. but we are going to loop through it all by a list of established ga account numbers.
				for(x in aGAID) { // aGAID defined in google.php or on core site
					var account_number = aGAID[x];
					if(debug) {
						console.log('pw_ga debug says ga account ' + account_number + ' would track the following');
						console.log(event_arguments);
					} else {
						_gaq.push(['_setAccount', account_number]);
						_gaq.push(event_arguments);
					}
				}
			}
		}

	</script>

	
