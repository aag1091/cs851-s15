    function onFlashTemplateLoaded(id) {
        var player  = brightcove.getExperience(id);
        var exp     = player.getModule(APIModules.EXPERIENCE);
        exp.addEventListener(BCExperienceEvent.TEMPLATE_READY,
            function(){
                var video = player.getModule(APIModules.VIDEO_PLAYER);

                if (true === rightRailAuto) {
                    video.play();
                }
                setTimeout(function(){
                    if (true === rightRailAuto ||
                            true === video.isPlaying()) {
                        video.setVolume(0.1);
                    }
                }, 2000);
            });
