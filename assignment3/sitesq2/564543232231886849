      $('#kuler-showcase-module-0 .product-deal-countdown').each(function () {
        var $dealEl = $(this);

        if ($dealEl.data('isDeal')) {
          $dealEl.countdown({
            until: new Date($dealEl.data('year'), $dealEl.data('month') - 1, $dealEl.data('day')),
            compact: false
          });
        }
      });
