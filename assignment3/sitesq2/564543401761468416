    <script type="text/javascript">
    (function() {
      "use strict";

      var $container = $(".js-product-size-chart[data-product-id='2']");

      var $inchesSource = $(".js-inches-source", $container);
      var $inchesTarget = $(".js-inches-target", $container);
      var $centimetersSource = $(".js-centimeters-source", $container);
      var $centimetersTarget = $(".js-centimeters-target", $container);

      new TS.ToggleClassBetweenTargetElementsOnSourceClick({
        className: "js-active",
        elements: [{
          source: $inchesSource,
          target: $inchesTarget,
        },{
          source: $centimetersSource,
          target: $centimetersTarget
        }],
        initialTargetElement: $inchesTarget
      }).bindEvents();

      new TS.ToggleClassBetweenElementsOnClick({
        className: "js-active",
        elements: [$inchesSource, $centimetersSource],
        initialElement: $inchesSource
      }).bindEvents();
    })();
    </script>
