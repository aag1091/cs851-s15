    surveyShowOn = 47,
    surveyId = 58419,
    takeSurvey = 0,
    ipcConfigCookie = parseInt($.cookie('ipcConfig'));

if (isNaN(ipcConfigCookie)) {
    // Has never been to the site before so select them randomly
    surveyRand = Math.floor(Math.random()*surveyMaxRand);
