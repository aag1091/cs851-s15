<script src="http://www.sexysandee.com/assets/js/jquery.countdown.min.js"></script>
 
<script>
	$(function () {
		var austDay = new Date();
		austDay = new Date(2015, 03-1, 28, 12, 03);
		$('#countdown').countdown({until: austDay});
	});
	$('#countsdown').countdown({
		until: $.countdown.UTCDate(2015, 03-1, 28, 12, 03),
		format: 'HMS',
		compact: true
	});
</script>


