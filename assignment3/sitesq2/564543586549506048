	var input = jQuery(n);
	input.val(input.attr('title'));
	input.addClass('watermark');
 
	input.focus(function() {
		if (jQuery(this).hasClass('watermark'))
			jQuery(this).val('').removeClass('watermark');
	});
 
	input.blur(function() {
		if ('' == jQuery(this).val())
			jQuery(this).val(jQuery(this).attr('title')).addClass('watermark');
	});
});
