</script> <script src="/js/modernizr.min.js"></script> <!--[if IE 7]><link rel="stylesheet" href="/css/font-awesome-ie7.min.css"><![endif]--> <!--[if lte IE 8]><script src="/js/respond.min.js"></script><![endif]--> <!-- Charbeat --> <script type='text/javascript'>var _sf_startpt=(new Date()).getTime()</script> <link rel="stylesheet" type="text/css" href="/css/print.1.5.min.css?v=XFb"> <link rel="stylesheet" href="/css/responsive-misc.1.1.min.css"> <script type="text/javascript">
