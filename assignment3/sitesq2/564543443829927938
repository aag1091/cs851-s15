var saveButtonClicked = false;
$(function() {
	$('.btn-mbg-desk-save').on('click', function(e) {
		e.preventDefault();
		$.ajax({
			url: "/getchannels.action",
			traditional : true,
			cache : false,
			success: function(data) {
				var obj = data;
				if(obj.success=="true" || obj.success===true){
					if(obj.loggedIn === true || obj.loggedIn === "true"){
						var html = "";
						$(obj.channels).each(function(index,item){
							html += "<input type='checkbox' name='channels' value='" + item.id + "' label='" + item.name + "'/>" + item.name + "<br>";
						});
						if(html != ""){
							$('#channelsModal .channels .controls').html(html);
						} else {
							$('#channelsModal .channels .controls').html('You have no saved channels');
						}
						$('#channelsModal').modal();
						try{
							// _gaq.push(['_trackEvent', 'Article', 'Logged In Save Button', '17380' ]);
							ga('send', 'event', 'Article', 'Logged In Save Button', '17380');
						}catch(err){}
					}else{
						saveButtonClicked = true;
						$('.userRegister').trigger('click');
						try{
							redirectLabel = "Article Save";
							// _gaq.push(['_trackEvent', 'Article', 'Logged Out Save Button', '17380' ]);
							ga('send', 'event', 'Article', 'Logged Out Save Button', '17380');
						}catch(err){}
						return false;
					}
				}else{
					return false;
				}
			}
		});
	});
	$('.btn-mbg-mobile-save').on('click', function(e) {
		e.preventDefault();
		$.ajax({
			url: "/getchannels.action",
			traditional : true,
			cache : false,
			success: function(data) {
				var obj = data;
				if(obj.success=="true" || obj.success===true){
					if(obj.loggedIn === true || obj.loggedIn === "true"){
						var html = "";
						$(obj.channels).each(function(index,item){
							html += "<input type='checkbox' name='channels' value='" + item.id + "' label='" + item.name + "'/>" + item.name + "<br>";
						});
						if(html != ""){
							$('.mob-save-body .channels .controls').html(html);
						} else {
							$('.mob-save-body .channels .controls').html('You have no saved channels');
						}
						$('.mob-save-body').slideDown(250, function() {
							$('html,body').animate({
							   scrollTop: $("#mob-save-box").offset().top - 100
							});
						});
						try{
							// _gaq.push(['_trackEvent', 'Article', 'Logged In Save Button', '17380' ]);
							ga('send', 'event', 'Article', 'Logged In Save Button', '17380');
						}catch(err){}
					}else{
						saveButtonClicked = true;
						$('.userRegister').trigger('click');
						try{
							redirectLabel = "Article Save";
							// _gaq.push(['_trackEvent', 'Article', 'Logged Out Save Button', '17380' ]);
							ga('send', 'event', 'Article', 'Logged Out Save Button', '17380');
						}catch(err){}
						return false;
					}
				}else{
					return false;
				}
			}
		});
	});
	
	$('#saveContentButton').on('click', function(e) {
		e.preventDefault();
		if ($(this).hasClass('disabled')) return false;
		var sub = new Object();
		sub.id = '17380';
		sub.objectType = 'post';
		var newChannel = $('#newChannel').val().trim();
		var idCount = $('input[name=channels]:checked').length;
		if (newChannel == '' && idCount == 0) {
			$('#channelsMessage').attr('class','').addClass('alert alert-warning').html('Please select an existing channel or enter a new one before clicking save.').fadeIn()
			return false;
		}
		if (newChannel != '') {
			sub.newChannel = $('#newChannel').val().trim();
		}
		if (idCount > 0) {
			var channels = new Array();	
			$('#channelSave input:checked').each(function() {
				channels.push($(this).val());
			});
			sub.channelIds = channels;
		}
		
		var inputString = JSON.stringify(sub);
		$(this).addClass('disabled');
		$.ajax({
			type : 'POST',
			cache : false,
			url : '/savechannelsubscription.action',
			data: {
				subscription : inputString
			}
		}).done(function(obj) {
			if(obj.success === true || obj.success === 'true') {
				var successMessage = 'Your article has been saved!';
				if (newChannel !== '') successMessage = 'Your article and new channel have been saved!';
				$("#channelsModal").animate({ scrollTop: 0 }, 'fast');
				$('#channelsMessage').attr('class','').addClass('alert alert-success').html('').append(successMessage).fadeIn();
				setTimeout(function() {
					$('#channelsModal').modal('hide');
					$('#channelsMessage').attr('class','').html('').hide();
				}, 3000);
			} else {
				$('#channelsMessage').attr('class','').addClass('alert alert-error').html('').append("Error saving article: " + obj.message).fadeIn();
			}
		}).fail(function(jqXHR, textStatus) {
			$('#channelsMessage').attr('class','').addClass('alert alert-warning').html('').append(textStatus).fadeIn();
		});
		
		try{
			// _gaq.push(['_trackEvent', 'Article', 'Article Saved', '17380' ]);
			ga('send', 'event', 'Article', 'Article Saved', '17380');
		}catch(err){}
	});
	$('#saveContentButton-mob').on('click', function(e) {
		e.preventDefault();
		if ($(this).hasClass('disabled')) return false;
		var sub = new Object();
		sub.id = '17380';
		sub.objectType = 'post';
		var newChannel = $('#newChannel-mob').val().trim();
		var idCount = $('input[name=channels]:checked').length;
		$('html,body').animate({
		   scrollTop: $(".btn-mbg-mobile-save").offset().top - 100
		});
		if (newChannel == '' && idCount == 0) {
			$('#channelsMessage-mob').attr('class','').addClass('alert alert-warning').html('Please select an existing channel or enter a new one before clicking save.').fadeIn()
			return false;
		}
		if (newChannel != '') {
			sub.newChannel = $('#newChannel-mob').val().trim();
		}
		if (idCount > 0) {
			var channels = new Array();	
			$('#channelSave-mob input:checked').each(function() {
				channels.push($(this).val());
			});
			sub.channelIds = channels;
		}
		
		var inputString = JSON.stringify(sub);
		$(this).addClass('disabled');
		$.ajax({
			type : 'POST',
			cache : false,
			url : '/savechannelsubscription.action',
			data: {
				subscription : inputString
			}
		}).done(function(obj) {
			if(obj.success === true || obj.success === 'true') {
				var successMessage = 'Your article has been saved!';
				if (newChannel !== '') successMessage = 'Your article and new channel have been saved!';
				$(".mob-save-body").animate({ scrollTop: 0 }, 'fast');
				$('#channelsMessage-mob').attr('class','').addClass('alert alert-success').html('').append(successMessage).fadeIn();
				setTimeout(function() {
					$('.mob-save-body').slideUp(250, function() {
						$('#channelsMessage-mob').hide();
						$('#newChannel-mob').val('');
						$('#saveContentButton-mob').removeClass('disabled');
					});
					$('#channelsMessage-mob').attr('class','').html('').hide();
				}, 3000);
			} else {
				$('#channelsMessage-mob').attr('class','').addClass('alert alert-error').html('').append("Error saving article: " + obj.message).fadeIn();
			}
		}).fail(function(jqXHR, textStatus) {
			$('#channelsMessage-mob').attr('class','').addClass('alert alert-warning').html('').append(textStatus).fadeIn();
		});
		
		try{
			// _gaq.push(['_trackEvent', 'Article', 'Article Saved', '17380' ]);
			ga('send', 'event', 'Article', 'Article Saved', '17380');
		}catch(err){}
	});
	$('#channelsModal').on('hidden', function() {
		$('#channelsMessage').hide();
		$('#newChannel').val('');
		$('#saveContentButton').removeClass('disabled');
	});
	$('.mob-close-cancel').on('click', function() {
		$('.mob-save-body').slideUp(250, function() {
			$('#channelsMessage-mob').hide();
			$('#newChannel-mob').val('');
			$('#saveContentButton-mob').removeClass('disabled');
		});
	});
});
