  if (typeof this.reloadCount == 'undefined') { 
    this.reloadCount = 0;
    htmlAdWH.apply(window, args);
    return;
  } else { this.reloadCount++; }
  
  if (this.reloadCount % refreshRate == 0 && typeof adsReloadAd === 'function') { adsReloadAd(this); }
}.bind(adsDiv));
adSetInView('0');
}());
