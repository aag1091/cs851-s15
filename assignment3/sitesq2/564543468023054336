<script type="text/javascript">
	$(window).load(function(){
		$('#container').on('click', '#submitFormColorMatching', function(e){
			e.preventDefault();
		    $('#parent').slideUp();
			setTimeout(function() { $('#loading').slideDown(); }, 400);
			$.post('inc/ajax/colormatching_ajax.php',{user_from:$('#first').val(), user_to:$('#prependedInput').val(), token:$('#token').val()},
			function(data){
				if(data.success == true){
			        setTimeout(function() { $('#parent').empty(); }, 400);
					setTimeout(function() { $('#parent').append(data.contents).slideDown(); }, 400);
				    setTimeout(function() { $('#loading').slideUp(0); }, 400);
				}
			},'json'
			);
		});
		$('#container').on('click', '#submitFormFriendships', function(e){
			e.preventDefault();
		    $('#parent').slideUp();
			setTimeout(function() { $('#loading').slideDown(); }, 400);
