function chnageurl(page){
	console.log('chnageurl'+page);
var href = window.location.href;
        var slide = href.split('-').length - 1;
		href = href.split('/');		
        if(href[href.length-1] != ''){  href.push('');   }
        if(slide){
		 href.splice(-2,2,'');	
		var newHref="17843-"+parseInt(page+1)+".html";
				  
        } else {
		var newhref=href[href.length-1].split('.');
		var newHref="17843-"+parseInt(page+1)+".html";
		
        href.splice(-2,2,'');
        }
        
        if(history.pushState){
          history.pushState(
          {    'i':i      },  'slide no#'+i,

		 href.join('/')+newHref);
        } else {		
    
		window.location.hash = newHref;
        }
    hitga();
}



function slideimage(direction)
	{
		var current_page=page;
		var no_of_page=$('#slidebootom >li');
		page=parseInt(parseInt(page)+parseInt(direction));
		page2=page-1;
		if(page<0)
		{ 
			page2=0; 
			page=parseInt(parseInt(no_of_page.length)-parseInt(1));
		}
	if(page>=parseInt(no_of_page.length-3))
	{
		page2=parseInt(no_of_page.length-4)+parseFloat(.5);
	}
	if(page>=parseInt(no_of_page.length))
	{
		page2=0;
		page=0;
	}
	
		var margin=922	;
	

$('#image_'+page).html('<iframe src="http://ibnlive.in.com/photogallery/showcase/right_ads.php"  frameborder="0" scrolling="no"  width="300" height="250" ></iframe>'); 

$('#slidebootom >li').removeClass('act').removeClass('wborder').addClass('wborder');
$('#imageb_'+page).parent().removeClass('wborder').addClass('act');
$('#sl').animate({'margin-left':(parseInt(page)*-margin)},500,function(e){
				$('#image_'+current_page).html('');
				$('#sl').parent().css('height',$('#bimage_'+page).height());				
				});
				if(page>1){
				$('#slidebootom').animate({'margin-left':(parseFloat(page2)*-190)},500,function(e){
				});
				}
				if(page==0){
					$('#slidebootom').animate({'margin-left':(parseFloat(page2)*-190)},500,function(e){
				});
				}				
		chnageurl(page);
}
