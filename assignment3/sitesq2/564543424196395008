<script language="JavaScript" type="text/JavaScript">
var myFeaturedFrame = new xmlScrollView();
myFeaturedFrame.disableScroll();
myFeaturedFrame.setOutPutElement('jobframe');
myFeaturedFrame.setItemRoot('Job');
myFeaturedFrame.declareVariable('JobTitle', '%jobTitle%');
myFeaturedFrame.declareVariable('Company', '%jobEmployer%');
myFeaturedFrame.declareVariable('City', '%jobCity%');
myFeaturedFrame.declareVariable('State', '%jobState%');
myFeaturedFrame.declareVariable('Url', '%jobLink%');
myFeaturedFrame.setTemplate('<div class="jobsHolder" onmouseover="this.style.background=\'#cccccc\';" \ onmouseout="this.style.background=\'\';" style="cursor: pointer;">\ <ul><li class="title"><a href="%jobLink%&utm_source=cjonline.com&utm_medium=Widget&utm_campaign=TopekaJobs">%jobTitle%</a></li>\ <li class="employer">%jobEmployer%</li>\ <li class="location">%jobCity%, %jobState%</li>\ </ul></div>');
myFeaturedFrame.setScrollDelay(30);
myFeaturedFrame.outputFromString(xmlString);
</script>
