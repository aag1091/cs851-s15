				  function show_menu() {
					//Ensure that anything under certain URL's get the correct highlight.
					$('#superfish-inner li.sf-breadcrumb').addClass('sfHover');
					$('#superfish-inner li.sf-breadcrumb ul:first').attr('style','display:block;visibility:visible');
				  }
				  //Make navigation horizontal
				  function sf_start() {
					$('#superfish-inner ul:first').addClass("sf-navbar").superfish({ pathClass: 'active-trail'});
					sf_active();
					if ($("#superfish-inner li.sf-breadcrumb a:first").child() != 0) {
						$("#superfish-inner li.sf-breadcrumb a:first").addClass("active");
					}
					show_menu();
				  };
				</script>
			  <script type="text/javascript">
				sf_start(); //Initialize Superfish
			  </script>

						</div>
					</div>

				</div>
				<div id="wl-wrapper-leaderboard">
					<div class="wl-container">
						<div class="wl-content">
							<span class="wl-clear"></span>
							<div class="wl-ad">
								<div class="wl-ad">
	<div id='728x90_1' class='adslot'  style="z-index: 50000;position:relative;">
<script type='text/javascript'>
googletag.defineSlot(NMTdata.ads.dfp_adunit_prefix+NMTdata.ads.dfp_adunit, [[728,90]],'728x90_1')
    .addService(googletag.pubads())
    .setTargeting('pos',['728x90_1'])
      .setTargeting("g", utag_data.gender) .setTargeting("age", NMTdata.data.yearToAge(utag_data.birthdate))
       .setTargeting("Taxonomy", utag_data.taxonomy).setTargeting('pplogin', $.cookie('ppUser')? 'y': 'n')
    .setTargeting('ccc',NMTdata.ads.dfp_ccc);
googletag.pubads().enableSyncRendering();
googletag.enableServices();
googletag.display('728x90_1');
</script>
</div></div>

