<a id="btnadclose" href="javascript:void(0)" style="background: #000000; display: none; padding: 2px 7px; color: #ffffff; position: absolute; top: 0px; left: 0px; text-decoration: none; opacity: 0.8; font-weight: bold;">X</a>
</div>
<style type="text/css">
.flex_adhesivefixed{
   position:fixed;
   left:50%;
   margin-left: -360px;
   bottom:0px;
   height:90px;
   width:728px;
   background:#ffffff;
   z-index: 1000000;
   display:none;
}
#btnadclose {
background: #000000;
display: none;
padding: 2px 7px;
color: #ffffff;
position: absolute;
top: 0px;
left: 0px;
text-decoration: none;
opacity: 0.8;
font-weight: bold;
}
</style>

<script>
$(document).ready(function(){ 
	var timeout = 0;
	var timeout_set;
	var timeout_close = [];
if($.cookie('728x90_2_closed') != 'yes')
	{
	$(window).scroll(function(){
	clearTimeout(timeout_set);
	if(timeout < 999) {
	var pullscroll=$(this).scrollTop();
		if (pullscroll > 250){
			if(timeout < 1)
			{
			  	showadhesive(timeout);	
                               $("#btnadclose").css("display","block");
			       timeout_close.push(setTimeout(function () { timeout = 1;}, 15000));
			}
			
				var hT = $('#wl-wrapper-leaderboard-bottom').offset().top;
				hH = $('#wl-wrapper-leaderboard-bottom').outerHeight();
				wH = $(window).height();
				   if (pullscroll > (hT+hH-wH)){
						hideadhesive();
					} 
					else {
						showadhesive(timeout);
					} 
			}
		else
		{
			hideadhesive();
		}
	}
	else {
		hideadhesive();
	}
	
});
}	

$("#btnadclose").click(function(){
                $.cookie('728x90_2_closed', 'yes', { domain  : 'cjonline.com', path: '/'});
		for (var i = 0; i < timeout_close.length; i++) {						
			clearTimeout(timeout_close[i]);
		}
		timeout_close = [];
		timeout = 999;
		$("#btnadclose").css("display","none");
		hideadhesive();
	});
});

function hideadhesive() {
 $(".flex_adhesive").removeClass("flex_adhesivefixed");
 $(".flex_adhesive").css("position","relative");
 $(".flex_adhesive").css("left","0%");
 $(".flex_adhesive").css("margin-left","0px");   
 $("#btnadclose").css("display","none");
}

function showadhesive(timeout) {
  if(timeout < 999) {	
	  $(".flex_adhesive").addClass("flex_adhesivefixed");
	  $(".flex_adhesive").css("position","fixed");
	  $(".flex_adhesive").css("left","50%");
	  $(".flex_adhesive").css("margin-left","-360px");  
	  $(".flex_adhesive").css("bottom","0px"); 
	  $(".flex_adhesive").css("display","block");
	  $(".flex_adhesive").css("z-index","1000000");
	  if(timeout == 1) {
		  $("#btnadclose").css("display","block");
	  }
  }
}
</script>	<span class="wl-clear"></span>
