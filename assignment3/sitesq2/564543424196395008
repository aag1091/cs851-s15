  <div class="pane-content">
    <div class="panel-display panel-1col clear-block" id="mini-panel-jobs_widget">
  <div class="panel-panel panel-col">
    <div><div class="panel-pane pane-custom pane-7" >
  
  
  
  <div class="pane-content">
    <script type="text/JavaScript" src="http://slb.adicio.com/js/xmlScrollView1.2.js"></script>
<script type="text/JavaScript" src=" http://topeka-jobs.cjonline.com/jobs/search/results?IsFeatured=Y&Sort=random&rows=3&format=jsxml&location=topeka&radius=25"></script>
<style type="text/css">
  #jobs_widget_wrapper {
    width: 97%;
    margin: 10px 0 10px 0;
  }
   #jobs_widget_wrapper .jobs_widget_Featured { 
    width: 100%; padding: 4px 4px 4px 4px; background-color: #edeff1; font-family: verdana,arial,helvetica,sans-serif; color: #000;
  }
    #jobs_widget_wrapper .jobs_widget_FeaturedSearch { 
    width: 100%; padding: 4px 4px 4px 4px; background-color: #FFF; font-family: verdana,arial,helvetica,sans-serif; color: #000; height: 118px; margin-bottom: 5px;
  }
    #jobs_widget_wrapper .featuredJobs {
    color: #fff; font-weight: bold; text-align:center; padding: 5px 0 5px 0;
  }
  #jobframe {
    font-size: .85em; overflow: hidden;
  }
  #jobframe .jobsHolder ul {
    /*margin-left: -25px;*/
    padding: 7px;
    font-size: 12px;
  }
  #jobframe .jobsHolder li{
    list-style-type: none; padding-left: 4px;
  }
  #jobframe .jobsHolder .title {
    font-weight: bold;
  }
  #jobframe .jobsHolder .employer {
    font-size: 1em;
  }
  #jobframe .jobsHolder .location {
    font-style: italic;
  }
 #jobframe .job_search_widget {
    margin:10px 0;
  }
  .job_search_widget_title {
    font-weight: bold;
  }
  #kwsPrimary {
    width:290px !important;
    display:block;
  }
  #autocomplete_jobswidgetlocation {
    width: 210px !important;
  }
  #jobs_widget_wrapper .jobs_lede_img {
    width: 85%;
    border:0;
  }
</style>

