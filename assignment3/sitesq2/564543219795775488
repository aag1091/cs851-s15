PDRTJS_settings_2669765_comm_1604={"id":2669765,"unique_id":"wp-comment-1604","title":"Very%20interesting%20page.%20What%20do%20you%20think%20about%20the%20whale%20research%20centres%20at%20Friday%20Harbour%20near%20Vancouver%3F%20I%20love%20killer%20whales.%20My%20passion%20and%20interest%20for%20them%20is%20so%20precise%20and%20distinctive%20th...","permalink":"https:\/\/theorcaproject.wordpress.com\/2011\/01\/20\/keto-tilikum-express-stress-of-orca-captivity\/#comment-1604","item_id":"_comm_1604"};
