							jQuery(document).ready(function() {
					// if we are handling infinite scroll, then monitor the page length function, adding more tiles if necessary
					if (Twine.settings.infinitescroll==false || Twine.settings.embedded==false) {
						jQuery(window).scroll(function () { 
							// if we are near the bottom of the page, get more items
						   if (Twine.settings.isLoading==0 && $(window).scrollTop() >= $(document).height() - $(window).height() - 200) {
							   	if (typeof Twine.theme.scrollLoadOff =='undefined') {
									Twine.GetContent();
								}
						   } 
						});
					}

					// for an initial load, add in the initial tiles.
					Twine.settings.isLoading=0;

				});
			
