$(window).scroll(function(){ $('#element-subscribe').slideUp(500); })


$(window).bind('scroll', function() {
    if ($(window).scrollTop() > 100) {
        $("#topheader").hide();
        $("#topsubsection").attr('class', 'fixation fixed');
        $('.dropdown').css('top', '40px');
        $('.dropdown').css('position', 'fixed');
        $('#element-subscribe').css('top', '40px');
        $('#element-subscribe').css('position', 'fixed');
    }
    else {
        $("#topheader").show();
        $("#topsubsection").attr('class', 'fixation unfixed');
        $('.dropdown').css('top', '110px');
        $('.dropdown').css('position', 'absolute');
        $('#element-subscribe').css('top', '110px');
        $('#element-subscribe').css('position', 'absolute');
    }
});
</script>
