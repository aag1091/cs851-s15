<script>

  (function(){

    // get dom parent of reply button (this has reply id on which we need)
    function findCommentReply(el) {
      while ((el = el.parentNode) && !(el.getAttribute('data-fn') === 'comments-reply'));
      return el;
    }

    // function to insert cloned form inline for replies
    // parent is the parent of the reply button that contains the reply id and insert point reference
    function cloneForm(parent){

      // create local reference to form
      var thisForm = form;

      // we can't find the form, assume replying is disabled
      if(!form) {
        return;
      }

      // get insert point for cloned form
      var insertPoint = parent.querySelector('[data-role="load-into"]');

      // if we have html5 validation then insert the form inline
      // if we don't (eg IE8), scoot the user down to the main comment form instead
      if(thisForm.getAttribute('data-has-html5') === 'true'){

        // grab cloned form
        thisForm = clonedForm;

        // remove any validation classes from an early entry
        var validationAdded = thisForm.querySelectorAll('.form__input--show-validation');
        for(var i = -1;++i<validationAdded.length;){
          validationAdded[i].classList.remove('form__input--show-validation');
          validationAdded[i].value = validationAdded[i].value;
        }

        // change 'Post a comment' to 'Post a reply'
        var formTitle = thisForm.querySelector('[data-role="form-title"]');
        if(formTitle){
          formTitle.textContent = "Post a reply";
        }

        // add callfn to call our widget loader
        thisForm.classList.add('callfn');

        // insert cloned form
        insertPoint.parentNode.insertBefore(thisForm, insertPoint);

      }

      // add 'reply-to' ids to form
      thisForm.querySelector('input[name="itemId"]').value = parent.getAttribute('data-item-id');
      thisForm.querySelector('input[name="assocTypeId"]').value = 9;

      // hash down to form
      location.hash = '';
      location.hash = 'comment-form';

    }

    // get form node
    var form = document.querySelector('.form--add-article-comment');
    var formFound = form !== null;

    // get target div to load initial hit of comments
    var target = document.querySelector('.comments__group');


    // check form exists in case replying is disabled
    if (formFound) {
      // make a clone for inline replies
      var clonedForm = form.cloneNode(true);
      var feedbackDisplay = form.querySelector('[data-role="validation-feedback"]');
    }


    // add a click lister to target looking for a reply-to button
    // add listener for reply clicks
    target.addEventListener('click', function(event){
      // check form exists in case replying is disabled
      if(formFound) {
        feedbackDisplay.classList.add('is-hidden');
      }

      if(event.target.getAttribute('data-role') === 'reply-button'){

        var parent = findCommentReply(event.target);
        if(parent){
          cloneForm(parent);
        }

      }

    }, false);


    // load initial hit of comments into target div
    var xhr = new XMLHttpRequest();

    xhr.onreadystatechange = function(){
      if(xhr.readyState == 4 && xhr.status === 200){
        target.innerHTML = xhr.responseText;
      }
    }

    // open xhr request...
    xhr.open('GET', '/common/ajax/getComments/5/9706458/1/20/');

    // ...and send
    xhr.send();


  })();


</script></div><div class="grid__col site-layout-secondary__col2  callfn adaptive-content" data-fn="adaptive-content" data-lite="true">
