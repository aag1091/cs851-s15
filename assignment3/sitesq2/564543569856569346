<script>
window.from_sds_page = 0;
var sale_item_options = {};
var user_waiting_options = {};

var sale_item_option_prices = {};
var sale_item_option_retail_prices = {};
var sale_item_option_discount = {};




var representativePrice = '34';
var representativeOptionId = _.find(_.keys(sale_item_option_prices), function(optionId) {
    return sale_item_option_prices[optionId] == representativePrice;
});
if (representativeOptionId) {
	if (sale_item_options[representativeOptionId] != 'True')//if not sold out
        $('#option_id, select[name="option_id"]').val(representativeOptionId);
}



</script>

