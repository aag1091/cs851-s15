<script>
//emulate behavior of html5 textarea maxlength attribute.
jQuery(function($) {
	var check_maxlength = function(e) {
		var max = parseInt($(this).attr('maxlength'));
		var len = $(this).val().length;
		if (len > max) {
			$(this).val($(this).val().substr(0, max));
		}
		if (len >= max) {
			return false;
		}
	};
	$('textarea[maxlength]').keypress(check_maxlength).change(check_maxlength);


	$(".notify-butter a.close").click(function(e){
		e.preventDefault();
		var expire = new Date();
		var time = expire.getTime();
		time += 365 * 24 * 3600 * 1000;
		expire.setTime(time);
		document.cookie = 'ck_sales_banner' + '=true; path=/; expires='+expire.toUTCString();
		
		$(".notify-butter").hide();
		$("body").css('padding-top',0);
	})
});
</script>

