<canvas id="mycanvas" width="100%" height="100">
</canvas>
<style>
#mycanvas {
    display: block;
    width: 100%;
    height: 100px;
    top: 300px;
    left: 0;
    right: 0;
    position: absolute;
    opacity: 0.7;
}
</style>
<script>var canvas = document.getElementById("mycanvas");
var points = {};
var counter = 0;

function f(x) {
    return 10 * Math.sin(0.07 * x) + 50;
}

if (canvas.getContext) {
    var ctx = canvas.getContext("2d");
    ctx.lineWidth = 3;
    ctx.strokeStyle = '#ffffff';

    var x = 0,
        y = f(0);
    var timeout = setInterval(function() {
        if(counter < 100) {
            ctx.beginPath();
            ctx.moveTo(x, y);
            x += 1;
            y = f(x);
            points[x] = y;
            ctx.lineTo(x, y);
            ctx.stroke();
            if (x > 1000) {
                clearInterval(timeout);
            }
        }
        if (counter == 100) {
          clearInterval(timeout);
        }
        counter++;
    }, 40);
}</script>

