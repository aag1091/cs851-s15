    <% _(messages).each(function(message_array, key) { %>
      <% map = {success: "success", error: "danger", status: "info", warning: "warning"}; %>
      <% type = map[key]? map[key]: "info" %>
      <% if (!_.isEmpty(message_array)) { %>
        <div class="alert alert-<%= type %>" style="margin-bottom: 5px">
          <a class="close" data-dismiss="alert">×</a>
