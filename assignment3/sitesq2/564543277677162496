        <script type="text/javascript">
    jQuery(function () {
        var twitterSignIn = jQuery('#twitter-sign-in');
        twitterSignIn.click(function() {
            UserAction.gaEvent(GaEventsCategory.HOMEPAGE, 'SignInWithTwitter_click','SignInWithTwitter_click')
            return signInWithTwitter(twitterSignIn);
        });
        var stickyHeaderBtn = jQuery('#sticky-header-btn');
        stickyHeaderBtn.click(function() {
            UserAction.gaEvent(GaEventsCategory.HOMEPAGE, 'SignUp-FloatingHeader','SignUp-FloatingHeader')
            return signInWithTwitter(stickyHeaderBtn);
        });
    });

    function signInWithTwitter(that) {
        var hosted_app = window.self != window.top;
        var communit_mbsy_short_code = ((typeof(mbsy_short_code) !== 'undefined') ? mbsy_short_code : '');
        href = that.attr('href');
        href += '?hosted_app_registration=' + hosted_app;
        href += '&communit_mbsy_short_code=' + communit_mbsy_short_code;
        href += '&sign_in_origin=' + document.location.href;
        if (hosted_app) {
            return popupWindow(href, 'twitter_authentication');
        } else {
            window.location = href;
            return false;
        }


    };

    function redirectToDashboard() {
        window.location = '/quick_actions'
    }
</script>
