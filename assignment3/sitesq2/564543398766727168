				var contents=new Array()
				contents[0]='<a href="http://www.directive21.com" target="_blank"><img alt="Directive 21" src="http://blacklistednews.com/images/lpc.jpg" border="0"></a><br />'
				contents[1]='<a href="http://whatliesinyourdebt.com?pmc=BLN-1" target="_blank"><img src="http://www.blacklistednews.com/images/WLIYD_banner1100.jpg" alt="" border="0"/></a><br />'
