jQuery(document).ready(function() {
$('#baslaform').submit(function() {
    //$('#baslaform').fadeOut("normal");
    $('#hatalar').hide();
    $('#ajax-load').fadeIn("normal");
    //$("#followusInput").attr('disabled','disabled');
    //$("#tweetitInput").attr('disabled','disabled');
	var sValues = $("#baslaform").serialize();
    $.post('/unfollower-upd.php', sValues,function(data) {
      if(data == "1") { //olumlu
        window.location.replace("/unfollowers.php");
        }
      else {
        $('#ajax-load').fadeOut("normal");
        $('#hatalar').html(data);
        $('#hatalar').show();
        //$('#baslaform').fadeIn("normal");
        //$("#followusInput").removeAttr('disabled');
        //$("#tweetitInput").removeAttr('disabled');
        
     
      }
    });
	return false;
});
    
$('.remove').click(function() {
	unfollowid = this.id;
    var kapat=unfollowid.replace("unf","prt");
    var getit = "/unfollow.php?id=" + unfollowid;
    $.get(getit, function(data) {
        $("#" + kapat).fadeOut("normal");
    });
	return false;
});
});
</script>
