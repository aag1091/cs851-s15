	function equalHeight() {
			if (document.getElementById) {

				var divs = new Array(document.getElementById('rightContent'),document.getElementById('leftContent'));

				var maxHeight = 0;
				for (var i = 0; i < divs.length; i++) {
					if (divs[i].offsetHeight > maxHeight) maxHeight = divs[i].offsetHeight;
				}

				for (var i = 0; i < divs.length; i++) {
					divs[i].style.height = maxHeight + 'px';

					if (divs[i].offsetHeight > maxHeight) {
						divs[i].style.height = (maxHeight - (divs[i].offsetHeight - maxHeight)) + 'px';
					}
				}
			}
		}

		function fixColumnHeigth(){
			var total=0;
			list=document.getElementById('rightContent');
			nodes = list.childNodes;
			for(var i=0; i< nodes.length; i++){
				if(nodes[i].nodeName=="DIV"){
					total+=nodes[i].offsetHeight;
				}
			}
			list.style.height = total+"px";

			total=0;
			list=document.getElementById('leftContent');
			nodes = list.childNodes;
			for(var i=0; i< nodes.length; i++){
				if(nodes[i].nodeName=="DIV"){
					total+=nodes[i].offsetHeight;
				}
			}
			list.style.height = total+"px";

			equalHeight();
		}

		window.onload = function() {
			fixLowResolution();
			equalHeight();
			setPostNavHandlers();
			setPaginationNavHandlers();
			loadCurrentPageFromHash();
			handleBackButton();
			setValidation();
		}

		window.onresize = function() {
			equalHeight();
		}

		function fixLowResolution(){
			if(window.screen.availWidth < 1000){
				document.getElementById("container").style.margin = 0;
				
				document.getElementById("container").style.marginTop = "177px";
				
				document.getElementById("container").style.marginLeft = "5px";
				document.getElementById("container").style.left = 0;
			}
		}

		

		function navigateToRegister(){
			
				location.href = "#";
			
		}

		

