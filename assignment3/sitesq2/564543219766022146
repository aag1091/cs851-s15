				<script>
				
					/**
					* replaced logic from redirect /ProcessDonation
					*/
					function submitDonationForm(){
						var checkedRadio = selectCheckedRadio()
						if (checkedRadio==null ||  checkedRadio.value=="0"){
							//no radio or "Other" is selected
							var amount = getFirstElementValueByName("enteredAmount");
						} else {
							//amount in input
							var amount = checkedRadio.value;
						}
						
						document.forms['proccessDonationForm'].elements['amount'].value=amount;
						//document.getElementsByName("amount")[0].value = amount;
					}
					
					/**
					* return checked radio; name=donateAmount by default
					*/
					function selectCheckedRadio(name){
					
						var elements = document.getElementsByName(name || "donateAmount");
						for(var element=0;element<elements.length;element++){
							var radio=elements[element];
							if (radio.checked){
																
								var isSocksPromotionFlag = document.getElementsByName("isSocksPromotionFlag")[0].value;
								if(isSocksPromotionFlag){													
										 document.forms['proccessDonationForm'].elements['promotion'].value="socksoption150";	   								   			  	
								}								
								return radio;
							}
						}
						return null;
					}	
					
					/**
					* return first input with existing value
					*/
					function getFirstElementValueByName(name){
						var elements = document.getElementsByName(name);
						for (var element in elements) {
							var htmlElement=elements[element];
							if (typeof(htmlElement.value)!="undefined"){
								return htmlElement.value;
							}
						}
						return null;
					}
				</script>
				
