<script>
  $(function() {
    $('#vyrtMenu .authModal').on('click', function(e) {
      e.preventDefault();
      $('.sb-toggle-left').click();

      setTimeout(function() {
        $("#authModal").appendTo($("body"));
        $('#authModal #return_to').val(window.location.href);
        $('#authModal').modal('show');
      }, 400);
    });

    $('.chat-toggle-control').on('click', function() {
      $chatToggleControl = $(this),
      $chat = $('#chat'),
      $chatToggleItems = $('.pt-page, .vyrt-sections.vyrt-sections-nav');
      if ($chatToggleControl.hasClass('active')) {
        $chatToggleControl.removeClass('active').text('Turn Chat Off');
        $chat.fadeIn();
        $chatToggleItems.not('#chat').animate({'width' : '70%'});
      } else {
        $chatToggleControl.addClass('active').text('Turn Chat On');
        $chat.fadeOut();
        $chatToggleItems.not('#chat').animate({'width' : '100%'});
      }
    });
  });
</script>

