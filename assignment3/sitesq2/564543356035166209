<script>
    var campaignDate = 1428260400000,
        start = new Date(campaignDate),
        countdownContainer = document.getElementById('tzcd');

    var vyrtCountdown = countdown(
      start,
      function(ts) {
        if (ts.value > 0) {
          $('.product .additional-content').fadeOut("slow", function() {
            $('.product .additional-content').remove();
          });
          $('#tzcd').fadeOut("slow");
          window.clearInterval(vyrtCountdown);
          return;
        } else {
          countdownContainer.innerHTML =
          '<p><span id="daysLeft"> ' + ts.days + '</span><span class="label">day' + (ts.days == 1 ? '' : 's') + '</span></p>' +
          '<p><span id="hours"> ' + ts.hours + '</span><span class="label">hour' + (ts.hours == 1 ? '' : 's') + '</span></p>' +
          '<p><span id="minutes"> ' + ts.minutes + '</span><span class="label">minute' + (ts.minutes == 1 ? '' : 's') + '</span></p>' +
          '<p><span id="seconds"> ' + ts.seconds + '</span><span class="label">second' + (ts.seconds == 1 ? '' : 's') + '</span></p>'
          ;
        }
      },
      countdown.DAYS|countdown.HOURS|countdown.MINUTES|countdown.SECONDS
    );
</script>

          <ticket-countdown campaign-id="144" slug="into-the-wild"></ticket-countdown>
