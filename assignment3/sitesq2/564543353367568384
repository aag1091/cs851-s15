<script type='text/javascript'>
//	jQuery('input#author').val('');	
	var emailAddress = '';
	jQuery('input#email').val('');
	if(checkPolusaCookie('polusa_cmnt_email')){
		var emailAddress = getPolusaCookie('polusa_cmnt_email');
		jQuery('p.comment-form-email').hide();		
		jQuery('input#email').val(emailAddress);
	}
	
	var cmntAuthor = '';
	jQuery('input#author').val('');
	if(checkPolusaCookie('polusa_cmnt_author')){
		var cmntAuthor = getPolusaCookie('polusa_cmnt_author');
		jQuery('input#author').val(cmntAuthor);
	}
	jQuery('input#submit').click(function(){
		jQuery(setPolusaCookie('polusa_cmnt_email', jQuery('input#email').val(), (24 * 30)));
		jQuery(setPolusaCookie('polusa_cmnt_author', jQuery('input#author').val(), (24 * 30)));
	});
</script>

<script type="text/javascript" >
jQuery(window).load(function(){	
	jQuery(function() {	
		jQuery('.c img').unwrap();
		jQuery('.c a').addClass('ahplnk scalable');
	});
	jQuery('.ahplnk').each(function(i, value) {
		var c_link = jQuery(value);
		c_link.attr('title', 'Link to: ' + c_link.text());
		text = c_link.text().replace("http://www.","").replace("https://www.","").replace("http://","").replace("https://","");
		if(text.length > 40) {
			c_link.text(text.substring(0, 37) + "...");
		}
		if(text.length <= 40) {
			c_link.text(text);
		}
	});
	jQuery('.c img').not('.cmt-wink').each(function(){
		var image = jQuery(this);
		image.wrap('<a class="comment-img" href="' + image.attr('src') + '" target="_blank"></a>');
	});
	jQuery('#commentform p.form-allowed-tags').after('<p>You may add a "wink" smiley <img style="display: inline; width: 32px; height: 32px; margin-bottom: -8px;" src="http://mx2.politicususa.netdna-cdn.com/wp-content/uploads/2014/10/wink.gif" /> by using the code <b>[WINK]</b></p>');
});
</script>
