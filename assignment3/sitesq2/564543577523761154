<script type="text/javascript"> 
var d = new Date();
var n = d.getMonth();
if ((sportCode === "goldmine")||(sportCode === "comm-service")){
	if (n <= 2){
		sportCode = "m-baskbl"}
	else if ((n > 2)&&(n<6)){
		sportCode = "m-basebl"}
	else {
		sportCode = "m-footbl"}
}
var jsCalendar = new csuiCalendar();
jsCalendar.launch({
timezone: 'CT', //only for general, options: AK, HI, PT, MT, CT, ET  
year: '2013', // only for sport
sport: sportCode, //only for sport      
reload: 300000, //5 minutes
animationSpeed: 400,
calWidth: 960,
calHeight: 232,
eventWidth: 130
});
</script>
