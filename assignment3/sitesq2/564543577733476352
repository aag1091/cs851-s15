<script type="text/javascript">
function review_point(point,review_id,num,review_user_id){

	var SES="";
	
	if(SES==""){
		alert("You Must Be A Registered NRTeam Member....")
	} else {
				if(review_user_id==SES){
		alert("NRTeam Members Can Not Rate Their Own Reviews.")
		}else{

	
		//alert("votes_popup.php?points="+ point + "&user_id=" + SES + "&review_id=" + review_id,"","width=300,height=200");
		 window.open("votes_popup.php?points="+ point + "&user_id=" + SES + "&review_id=" + review_id,"","width=550,height=180,left=200,top=200,toolbar=No,location=No,scrollbars=No,status=No,resizable=No,fullscreen=No");
		}
   }

}
