		function amcn_pixeldrop(o) {
			if (!o.ok) {
				var oImg = document.createElement('img');
				var src = '';
				switch (o.isp) {
					case 'time-warner':
					case 'brighthouse':
						src = "http:\/\/domdex.com\/con?image=true&s=50ed99c359fbb17f7800000a";
						break;
					case 'rogers':
						src = "http:\/\/ads.magnetic.is\/seg?add=587711&t=2";
						break;
					case 'mediacom':
						src = "";
						break;
					case 'cable-one':
						src = "http:\/\/ads.magnetic.is\/seg?add=587718&t=2";
						break;
				}
				if (src) {
					oImg.setAttribute('src', src);
					oImg.setAttribute('alt', 'na');    
					oImg.setAttribute('height', '0px');
					oImg.setAttribute('width', '0px');
					document.body.appendChild(oImg);
				}
			}
		}
