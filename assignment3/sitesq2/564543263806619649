			<a class="prev" href="#"></a>
			<a class="next" href="#"></a>
			<div class="buttonContainer">
                        <div class="pagination-carousel"></div>            </div>
		</div>
	</div>
</div>
<script type="text/javascript">
	jQuery(function($) {
		$('#hero-carousel-featured-cfct-module-556f0fd22431693453e4842f43e223dc .hero-carousel-featured-container ul').cycle({
			fx: 'none',
			speed: 300,
			timeout: 0,
			pager: '#hero-carousel-featured-cfct-module-556f0fd22431693453e4842f43e223dc .hero-carousel-featured-control-panel .pagination-carousel',			next: '#hero-carousel-featured-cfct-module-556f0fd22431693453e4842f43e223dc .next',
			prev: '#hero-carousel-featured-cfct-module-556f0fd22431693453e4842f43e223dc .prev',
			pause: 1,
			pauseOnPagerHover: 1
		});
	});


	(function($){
		$(function(){
			var carousel = $("#hero-carousel-featured-cfct-module-556f0fd22431693453e4842f43e223dc");
			carousel.find('.next, .prev').on('click', function(){
				_ca.trackEvent({
					'action': 'hero_carousel:' + $(this).hasClass('next') ? 'next' : 'prev',
					'label': carousel.find('a.activeSlide').text()
				});
			});
			carousel.find('.pagination-carousel a').on('click', function(){
				_ca.trackEvent({
					'action': 'hero_carousel:pagination:' + $(this).text(),
					'label': 'click'
				});
			});
			var items = carousel.find('ul.amc-hero-carousel-items .amc-hero-carousel-item');
			$.each(items, function(i, v){

				$(this).find('a').on('click', function(){
					var title = $(this).parents('.amc-hero-carousel-item').find('h2.title').text().slugify().replace(/-/g, '_');
					var target = $.trim($(this).attr('href'));
					target = target.length !== 0 ? target : 'unknown';
					var index = carousel.find('a.activeSlide').text();
					_ca.trackEvent({
						'action': 'hero_carousel:clickthrough:' + index,
						'label': title + ":" + target
					});
				});

			});
			_ca.trackEvent({
				'action': 'hero_carousel:init',
				'label': 'load'
			});
		});
	})(jQuery);
</script>

