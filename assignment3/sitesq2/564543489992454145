<script>
(function() {
  var context = null, elems = null;
  try {
    context = document.getElementsByClassName('article-txt')[0];
    elems = context.getElementsByTagName('a');
  } catch (e) {}
  if (context && elems) {
    var href = '';
    for (var i = 0; i < elems.length; i++) {
      if (elems[i].nodeType == 1 && 
          elems[i].hasAttribute('href')) {
        href = new String(elems[i].href);
        href = href.toLowerCase();
        if (/^https?\:\/\//.test(href) &&
          href.indexOf(location.protocol + '//' + location.host + '/') != 0) {
          elems[i].className = ' extlink';
          elems[i].target = '_blank';
        } else if (/_(article|gallery|video)_\d+/.test(href)) {
          elems[i].onclick = function() {
            try {
              _gaq.push(['_trackEvent', 'News Detail', 'Content Link Click', 'Story ' + location.pathname.match(/_article_(\d+)/)[1] + ': ' + document.title.replace(/\s+\|.*$/, ''), 0, true]);
            } catch (e) { ; }
          }
        }
      }
    }
  }
})();
</script>


