      }
  });

  window.openPostOverlay = function openPostOverlay(postId) {
      var historyId = History.getState().id;
      if (jQuery('.post .mobile_post').is(":visible")) {
          document.location = "/actor/updatePostOverlay?postId=" + postId + "&historyId=" + historyId + "&mobile=true" + '';
      } else {
          History.pushState({postId: postId, historyId: historyId}, document.title, '?postId=' + postId);
          location.href = '#activity'
      }
  }

