  jQuery('.has_sub_menu').mouseenter(function (event) {
      jQuery('.has_sub_menu').css('height', '70px');
      var menuContainer = jQuery(this);
      if (menuContainer.find('.has_sub_menu_mobile').length > 0 && menuContainer.find('.has_sub_menu_mobile').is(":visible")) {
          event.stopPropagation();
      } else {
          menuContainer.find('.sub_menu').show();
          menuContainer.find('.sub_menu').find(".actor_page_search #searchActorList").show()
      }
  });

  jQuery('.has_sub_menu').mouseleave(function () {
      jQuery('.has_sub_menu').css('height', '53px');
      jQuery(this).find('.sub_menu').hide();
  });

  jQuery('.has_sub_menu_mobile').click(function() {
      var icon = jQuery(this);
      var menuId = icon.attr('menuId');
      jQuery(".has_sub_menu_mobile").each(function() {
          var subMenuIcon = jQuery(this);
          var subMenuId = subMenuIcon.attr('menuId');
          if (menuId != subMenuId) {
              if (subMenuIcon.is('img')) {
                  subMenuIcon.attr('src', '/assets/mainMenu_black-0941a7f6a57ff8330f494781ed99a338.svg');
                  subMenuIcon.removeClass("selected");
              }
              jQuery("#" + subMenuId).removeClass("selected_menu");
              
                jQuery('.mobile_menu_margin').css('margin-top', '0');
              
          }
      });
      jQuery('#' + menuId).toggleClass("selected_menu");
      if (jQuery('#' + menuId).hasClass('selected_menu')) {
        
      } else {
        
          jQuery('.mobile_menu_margin').css('margin-top', '0');
        
      }
      if (icon.is('img')) {
          icon.toggleClass("selected");
          if (icon.hasClass("selected")) {
              icon.attr('src', '/assets/mainMenu_green-86d4a3b04b798260c03f510bb0c1a480.svg');

          } else {

              icon.attr('src', '/assets/mainMenu_black-0941a7f6a57ff8330f494781ed99a338.svg');
          }
      }
  });

