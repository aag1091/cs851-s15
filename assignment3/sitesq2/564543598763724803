    function requestActorNotLoggedInCallback(data) {
      jQuery('#requestActorFormDialog').jqmHide();
      var actorName = encodeURIComponent(jQuery("#requestActorFormDialog input[name='actorName']").val());
      jQuery("span.not_exists[actorName=" + actorName.replace(/([ #;&,.+*~\':"!^$[\]()=>|\/@])/g,'\\$1') + "]").parents(".actor_footer_container").html(data);
    jQuery.ajax({type:'POST',data:'actorName=' + actorName, url:'/actor/showActorRequestedOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
    }
  


  function showValidationErrors(json) {
      jQuery(".form_field_error").hide();
      var errors = json.errors;
      for (var i=0; i <= errors.length-1; i++) {
          jQuery("#field_error_" + errors[i].field).show();
          jQuery("#field_error_" + errors[i].field).html( errors[i].message);
      }
  }
