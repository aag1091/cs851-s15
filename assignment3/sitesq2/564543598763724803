<script type='text/javascript'>
  window.App = {
      mobile: function () {
        var constants = {
          maxWidth: 750,
          addToHomeConfig:{
            autostart: false,			// Automatically open the balloon
            returningVisitor: true,	// Show the balloon to returning visitors only (setting this to true is highly recommended)
            animationIn: 'drop',		// drop || bubble || fade
            animationOut: 'fade',		// drop || bubble || fade
            startDelay: 2000,			// 2 seconds from page load before the balloon appears
            lifespan: 15000,			// 15 seconds before it is automatically destroyed
            bottomOffset: 31,			// Distance of the balloon from bottom
            expire: 0,					// Minutes to wait before showing the popup again (0 = always displayed)
            message: 'Install this web app on your %device- <strong>Tap</strong> %icon and then<br/><strong class="isAddToHome">Add to Home Screen.</strong>',				// Customize your message or force a language ('' = automatic)
            touchIcon: false,			// Display the touch icon
            arrow: true,				// Display the balloon arrow
            hookOnLoad: true,			// Should we hook to onload event? (really advanced usage)
            closeButton: true,			// Let the user close the balloon
            iterations: 100,				// Internal/debug use
            maxDisplayCount: 1
          }
        };

        return {
          isMobile: function () {
            return ($(document).width() < 750);
          },
          getMaxWidth: function () {
            return constants.maxWidth;
          },
          ifLoggedIn: function () {
            var ifLoggedIn = false;
            
            return ifLoggedIn;
          },
          ifWebBrowser: function () {
            return !this.ifMobile();
          },
          ifMobile:function(){
            var ifMobile = false;
            
            return ifMobile;
          },
          isAddToHome: function () {
            var autostart = false;
            if (window.localStorage.getItem('isAddToHome') !== '1') {
              autostart = true;
            }
            return autostart;
          },
          setLocalStorage: function (e) {
            window.localStorage.setItem('isAddToHome', 1);
          },
          getConfigAddToHome: function () {
            constants.addToHomeConfig.autostart = this.ifLoggedIn() ? this.ifMobile() && this.isAddToHome() : false;
            return constants.addToHomeConfig;
          }
        }
      }()
    };
    //custom settings addToHome.js
  window.addToHomeConfig = App.mobile.getConfigAddToHome();

</script>


