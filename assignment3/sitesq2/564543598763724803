  $(function () {
    //set Events in add2Home
    $(document).on('click', '#addToHomeScreen .isAddToHome', window.App.mobile.setLocalStorage);
    $(document).on('click', '#addToHomeScreen .addToHomeClose', window.App.mobile.setLocalStorage);
    //Resolving problem with double click for iOS and links with images and hover state
    var isScrolling = false;
    $('a').on('touchstart',function () {
      isScrolling = false;
    }).on('touchmove',function (e) {
          isScrolling = true;
        }).on('touchend', function (e) {
          if (!isScrolling) {
            window.location = $(this).attr('href');
          }
        });

    var lazyLoadImg = (function ($, _) {
      var _$images;
      var _effect_time = 500;

      var _fUpdatedImg = function () {
        _.each(_$images, function (img) {
          var $self = $(img).hide();

          $self.removeClass('lazy')
              .addClass('load_lazy_img');

          $("<img />").bind('load', function () {
            $self.attr('src', $self.attr('data-original'))
                .bind('load', function () {
                  $(this).removeClass('load_lazy_img');
                })
                .removeAttr('data-original')
                .fadeIn(_effect_time);
          })
              .bind('error', function () {
              })
              .attr('src', $self.attr('data-original'));
        });
      };

      var LoadImg = function () {
        this.loadImg();
      };

      LoadImg.prototype = {
        constructor: LoadImg,
        bindAjaxElem: function (selectName, ajaxEvent) {
          $(selectName).bind(ajaxEvent, this.loadImg);
        },
        loadImg: function () {
          _$images = $('img.lazy');
          _fUpdatedImg();
        }
      };
        return new LoadImg();
    })(jQuery, _);

    lazyLoadImg.bindAjaxElem('body', 'ajaxSuccess');
  });

  window.onload = function () {
    initialiseStateFromURL();
    setInterval(initialiseStateFromURL, 100);
  };

  var recentHash = '';

  var History = window.History;

  function getVarValueFromURL(url, varName) {
    var query = url.substring(url.indexOf('?') + 1);
    var vars = query.split("&");
    for (var i = 0; i < vars.length; i++) {
      var pair = vars[i].split("=");
      if (pair[0] == varName) {
        return pair[1];
      }
    }
    return null;
  }

  function initialiseStateFromURL() {
    if (location.hash == recentHash) {
      return; // Nothing's changed since last polled.
    }

    var State = History.getState(); // Note: We are using History.getState() instead of event.state
    var url = location.href;
    var hash = location.hash;
    recentHash = hash;
    var historyId = '';
    if (State.data && State.data.historyId) {
      historyId = State.data.historyId
    }

    if (hash.indexOf('#join') > -1) {
      url = url.replace('#join', '');
      var actorId = getVarValueFromURL(url, 'actorId');
      var postId = getVarValueFromURL(url, 'postId');
      var isRedirectTo =  false;
      if (State.data) {
         isRedirectTo =  State.data.isRedirectTo;
         if (!postId) {
           postId = State.data.postId;
          }
      }
    jQuery.ajax({type:'POST',data:'actorId=' + actorId + '&postId=' + postId + '&isRedirectTo=' + isRedirectTo, url:'/actor/showJoinOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
    } else if (hash.indexOf('#login') > -1) {
      var actorId = getVarValueFromURL(url, 'actorId')
      var postId = getVarValueFromURL(url, 'postId')
      jQuery.ajax({type:'POST',data:'actorId=' + 8 + '&userLocationURL='+url, url:'/actor/loginOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
    } else if (hash.indexOf('#recalculatePoints') > -1) {
      var   url = url.replace('#recalculatePoints', ''),
      displayName = getVarValueFromURL(url, 'displayName');
      jQuery.ajax({type:'POST',data:'displayName=' + displayName + '&recalculateHeartbeat=' + true, url:'/actor/myScore',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});

      var $badges_container = $('.badges_container');
      if ($badges_container.length !== 0) {
         $badges_container.find('.recalculate').addClass('make-transparent');
         $badges_container.find('.progressbar-container').addClass('make-visible').removeClass('display-none');
      } else {
         var $score_info = $('.score_info');
         $score_info.find('.main_container').addClass('make-transparent');
         $score_info.find('.progressbar-container').addClass('make-visible').removeClass('display-none');
      }
    } else if (hash.indexOf('#report') > -1) {
      var postId = getVarValueFromURL(url, 'postId');
      if (postId) {
        postId = postId.replace("#report", '');
        var idReturnToPost = (State.data && State.data.idReturnToPost) || false;
        jQuery.ajax({type:'POST',data:'&postId=' + postId, url:'/actor/showReportOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
      }
    } else if (hash.indexOf("#badge") > -1) {
      var fanBadgeId = getVarValueFromURL(url, 'fanBadgeId');
      if (fanBadgeId) {
        fanBadgeId = fanBadgeId.replace("#badge", '')

        jQuery.ajax({type:'POST',data:'fanBadgeId=' + fanBadgeId, url:'/actor/updateBadgeInfo',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
      }
    } else if (hash.indexOf('#activity') > -1) {
      
        var postId = getVarValueFromURL(url, 'postId')
        if (postId) {
          postId = postId.replace("#activity", '');
          jQuery.ajax({type:'POST',data:'&postId=' + postId + '&historyId=' + historyId, url:'/actor/updatePostOverlay',success:function(data,textStatus){jQuery('#postContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
        }
      
    } else if (hash.indexOf('#editStory') > -1) {
        var actorUsername = getVarValueFromURL(url, 'actorUsername');
        var postId = getVarValueFromURL(url, 'postId');
        postId = postId ? postId.replace("#editStory", '') : '';
        actorUsername = actorUsername ? actorUsername.replace("#editStory", '') : '';
        var isActorPage = getVarValueFromURL(url, 'isActorPage');
        
          if (isActorPage) {
            jQuery.ajax({type:'POST',data:'&actorUsername=' + actorUsername + '&isActorPage=' + true + '&postId=' + postId, url:'/user/showStoryOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
          } else {
            jQuery.ajax({type:'POST',data:'&actorUsername=' + actorUsername + '&postId=' + postId, url:'/user/showStoryOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
          }
        
        
    } else if (hash.indexOf('#bounce') > -1) {
      var postId = getVarValueFromURL(url, 'postId');
      if (postId) {
        var idReturnToPost =  false;
        if (State.data) {
          idReturnToPost =  State.data.idReturnToPost;
        }

        postId = postId.replace("#bounce", '');
        jQuery.ajax({type:'POST',data:'&postId=' + postId + '&idReturnToPost=' + idReturnToPost, url:'/actor/showBounceOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
      }
     } else if (hash.indexOf('#confirmEmail') > -1) {
        jQuery.ajax({type:'POST', url:'/user/showConfirmEmailOverlay',success:function(data,textStatus){jQuery('#overlayContainer').html(data);},error:function(XMLHttpRequest,textStatus,errorThrown){}});
    }
  }

