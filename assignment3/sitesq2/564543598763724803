  jQuery('.delete-comment').live('click', function() {
      var comment = jQuery(this).parents('.comment-container');
      var commentId =  comment.attr('commentId');
      var postId = jQuery(this).parents('.post-comments').attr('postId');
      jQuery.ajax({
        url:"/actor/deleteComment",
        type:"POST",
        data:{commentId: commentId},
        success: function (data) {
          var post = jQuery(".post[postId='" + postId + "']");

          if (jQuery('.mobile_post_container').is(':visible')) {
            post = jQuery(".mobile_post_container[postId='" + postId + "']");
          }
          var commentCountHolder = post.find('.comment-action span');
          var commentCount = +data;
          if (jQuery('#postOverlayDialog').is(':visible')) {
            if (commentCount == 0) {
              jQuery('.comment_container ').removeClass('commented');
              jQuery('.commented_icon').removeClass('fan_commented');
              jQuery('.commented_icon img').attr('src', "/assets/actor/ICN_comment-8365e050d718b086f281c8708be6ddff.svg");
            }
            var showMoreContainer = jQuery('#postOverlayDialog').find('.show-more-comment');
            if (showMoreContainer.length > 0) {
              showMoreContainer.find('span').text(commentCount);
            }

            jQuery('.comment_counter span').text(commentCount);
          } else {
            var showMoreContainer = post.find('.show-more-comment');
            if (showMoreContainer.length > 0) {
              showMoreContainer.find('span').text(commentCount);
            }
          }

          if (commentCount == 0) {
              post.find('.comment-action img').removeClass('selected');
              commentCount = "";
          }

          
            if (!commentCount) {
               commentCountHolder.text(commentCount);
            } else {
              commentCountHolder.text(commentCount + " Comment");
            }
          
          
          comment.remove();
        }
      })
  })

