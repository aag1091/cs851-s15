   jQuery(window).scroll(function() {
   if (jQuery('.actor_actions').is(':visible')) {
       if (f_scrollTop() >= jQuery('.main_header').height()*2) {
         jQuery('.join_artist_holder').fadeOut(500);
       } else {
         jQuery('.join_artist_holder').fadeIn(500);
       }
   }

    updateHeader();
