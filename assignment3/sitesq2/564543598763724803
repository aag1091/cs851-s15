    $(function() {
      $('#rewardFansContainer').isotope({
        itemSelector : '.reward-post'
      });

      var page = 15;

      function loadRewardPosts() {

        jQuery.ajax({
          type:'POST',
          url: "/actor/rewardLeaderBoard",
          data: {offset: page, rewardId: '205'},
          success:function(data, textStatus) {
             if (data != '') {
                page += 15;
             }
          }
        })
      }

      jQuery(".share-post").live("click", function() {
        var postId = jQuery(this).attr('postId');
        History.pushState({postId: postId}, document.title, '?postId=' + postId+'&callback='+'updatedBounceSize');
        location.hash = "#bounce";
      });

      jQuery('#rewardFansContainer').scrollExtend({
        'target': '#rewardFansContainer',
        'url': function() {
            return "/actor/rewardLeaderBoard?&offset="+ page + '&rewardId=' + '205';
            },
            'beforeStart': function(scroller, container) {
                if (!$('#loader').hasClass('loaded')) {
                   $('#loader').delay(500).addClass('loaded', 500, 'linear');
                }
                return true;
            },
            'onSuccess': function(scroller, container, new_elem) {
                var elements = $(jQuery(container).find(".reward-post"));
                console.log(new_elem);
                $('#rewardFansContainer').append( elements ).isotope( 'appended', elements )
                $('#rewardFansContainer').isotope('reLayout');
                $('#loader').delay(500).removeClass('loaded', 500,'linear');
                if (container[0].childNodes.length > 0) {
                    page += 15;
                } else {
                    jQuery('#rewardFansContainer').scrollExtend("disable");
                }
            }
      });
    })
