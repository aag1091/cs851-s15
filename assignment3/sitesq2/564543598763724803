  jQuery('.like_grey_icon').live("click", function () {
      var parent = jQuery(this).parents('.post');
      postId = parent.attr('postId');
      if (postId && jQuery(this).hasClass('loggined')) {
        jQuery.ajax({url:"/actor/likePost",
          type:"POST",
          data:{postId:postId},
          success:function (responseJSON) {
              if (!responseJSON.isUserFanOfActor) {
                  if (true) {
                      History.pushState({postId: postId, actorId: '8'}, document.title, '?actorId=' + '8');
                  } else {
                      History.pushState({postId: postId}, document.title, '?postId=' + postId);
                  }
                  location.hash = "#join";
              } else {
                  var postBeat = (parseInt(responseJSON.postBeat) !== 0) ? responseJSON.postBeat : "";
                  if (postBeat > 0) {
                    
                      jQuery(".post[postId=" + postId + "]").find('.like_icon span').text(postBeat + " Like");
                    
                    
                  } else {
                    jQuery(".post[postId=" + postId + "]").find('.like_icon span').text(postBeat);
                  }
                  jQuery(".post[postId=" + postId + "]").find('.like_grey_icon').toggleClass('selected');
              }
          }
        });
      }
  });

  jQuery(document).on("click",".post .report_icon",function() {
      var parent = jQuery(this).parents('.post');
      if (jQuery(this).hasClass('loggined')) {
          postId = parent.attr('postId');
          if (true) {
              History.pushState({postId: postId, actorId: '8'}, document.title, '?actorId=' + '8');
              location.hash = "#join";
          } else {
              History.pushState({postId: postId}, document.title, '?postId=' + postId);
              location.hash = "#report";
          }
      }
  });

  window.updatedBounceSize = function (postId,postBounce){
