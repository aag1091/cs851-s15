  function updateCookie() {
    if(!App.mobile.isMobile()) {
         var emailBanner = 0;
         var cookieDisclaimer = 0;
         if($('.email_verification_banner').is(':visible'))  {
            emailBanner = 30 +  $(".email_verification_banner").height();
         }
         if(!$.cookie("BUDDYBOUNCE_DISCLAIMER_WAS_READ"))  {
            cookieDisclaimer = $("#cookieDisclaimer").height() ;
         }
         $(".main_header").css("top",  cookieDisclaimer + emailBanner);
         if ($('.email_verification_banner').is(':visible')) {
            $('.email_verification_banner').css("top",  cookieDisclaimer);
         }

         if($(".content").hasClass("contactPage") ||  $(".content").hasClass("editProfilePage") || $(".content").hasClass("application_form_page"))  {
               $(".content").css("marginTop", cookieDisclaimer + emailBanner + "px");
         } else if($(".content").hasClass("content_page")) {
             $(".content").css("marginTop", cookieDisclaimer + emailBanner + "px");
         } else if ($(".content").hasClass("actorHomePage")) {
             if (jQuery('.fans_page').is(":visible") || jQuery('.reward_page').is(":visible") || jQuery('.shop_page').is(":visible")) {
                $(".content").css("marginTop",  $(".main_header").height() + 2 + cookieDisclaimer + emailBanner + "px");
             }  else if (jQuery('.my_likes_page').is(":visible")) {
                $(".my_likes_page").css("paddingTop",  $(".main_header").height() + 2 + cookieDisclaimer + emailBanner  + "px");
             }  else {
                $(".content").css("marginTop",  $(".main_header").height() + 2 + cookieDisclaimer + emailBanner + "px");
             }
         } else if($(".content").hasClass("create-actor-page")) {
            $(".content").css("paddingTop", cookieDisclaimer + emailBanner  + "px");
         } else if(!$(".content").hasClass("actorHomePage")) {
            $(".content").css("marginTop", cookieDisclaimer + emailBanner  + "px");
         }
    }
  }

