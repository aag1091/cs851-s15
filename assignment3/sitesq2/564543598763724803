    jQuery(window).resize(function() {
        var cookieHeight = 0;
        var activity = 0;
        if(!App.mobile.isMobile()) {

            if (false) {
                if (false) {
                    activity = jQuery('.actor_fans').height() + parseInt($(".actor_fans").css('padding-top')) + parseInt($(".actor_fans").css('padding-bottom')) +parseInt($(".actor_fans").css('margin-bottom'));
                    if (jQuery(".actor_info_holder_mobile").is(":visible")) {
                        activity = jQuery(".actor_info_holder_mobile").parent().height() + 10;
                    }
                } else {
                    activity += 10;
                }

            }
            if (jQuery('#cookieDisclaimer').is(":visible")) {
                cookieHeight = jQuery('#cookieDisclaimer').height();
                updateHeader();
            } else if(f_scrollTop() < (jQuery('.actor_image.main_background_image').height() - ((jQuery('.main_header').height() +  10)))) {
                 jQuery('.actor_actions').css('top', Math.round(jQuery('.actor_image.main_background_image').height() + jQuery('.main_header').height() + activity - f_scrollTop())+ 'px');
            }
        }
    });

     function updateHeader () {
          if(!App.mobile.isMobile()) {

             var cookieHeight = 0;
             var activity = 0;
             var notActivity = 0;
            if (false) {
                if (false) {
                    activity = jQuery('.actor_fans').height() + parseInt($(".actor_fans").css('padding-top')) + parseInt($(".actor_fans").css('padding-bottom')) +parseInt($(".actor_fans").css('margin-bottom'));
                } else {
                    activity += 10;
                }

            } else {
                notActivity  = jQuery('.main_header').height();
            }

            if (jQuery('#cookieDisclaimer').is(":visible")) {
                cookieHeight = jQuery('#cookieDisclaimer').height() ;
            }

            var emailBanner = 0;
            if($('.email_verification_banner').is(':visible'))  {
               emailBanner =  parseInt($(".email_verification_banner").css('padding-top')) +  parseInt($(".email_verification_banner").css('padding-bottom')) + $(".email_verification_banner").height();
            }

            if(f_scrollTop() >= (jQuery('.actor_image.main_background_image').height() + activity - 10)) {

              var top = jQuery('.main_header').height() + cookieHeight + emailBanner + 2;

              jQuery('.actor_actions').css('top', Math.round(top) + 'px');
              if (false) {
                 jQuery('.actor_actions').css('position', 'fixed');
                 jQuery('.actor_actions').addClass('full_width');
              }
            } else {
              if (false) {
                 activity += jQuery('.main_header').height();
                 jQuery('.actor_actions').removeClass('full_width');
                 jQuery('.actor_actions').css('position', 'static');
              }

              jQuery('.actor_actions').css('top', Math.round(jQuery('.actor_image.main_background_image').height() + emailBanner + cookieHeight +  activity + 2 -  f_scrollTop()) + 'px');
            }
          }
     }

     jQuery(window).on('load', function() {
          if (jQuery('#cookieDisclaimer').is(":visible")) {
            updateHeader();
            jQuery(".join_artist_holder").addClass("cookie_disclaimer");
          }
    });

    jQuery("#closeCookieDisclaimer").click(function() {
        if (jQuery(".join_artist_holder").is(":visible")) {
            jQuery(".join_artist_holder").removeClass("cookie_disclaimer");
        }
    });

