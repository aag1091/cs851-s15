   $('.search_input_container .actor_name').blur(function () {
     $(this).attr('placeholder', placeholderValue);
   })
   $('.search_input_container .actor_name').focus(function () {
      placeholderValue = $(this).attr('placeholder'); // save the value
      $(this).attr('placeholder', ''); // remove the value for that
   })

   jQuery('.search_input_container .actor_name').keyup(function() {
     var username = jQuery(this).val();
     if (username != '' && username.trim() != '') {
       var isActorPage =  jQuery(this).parents('.find_actor_menu_item').hasClass("actor_page_search");
       clearTimeout(searchTimer);
       searchTimer = setTimeout(function() {
           jQuery("#searchActorList").show().html('<img class="header-spinner" src="/assets/spinner_big-0317594cf5315ffa11a7060f12e77638.gif"/>');
