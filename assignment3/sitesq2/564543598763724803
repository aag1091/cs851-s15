History.pushState({fanBadgeId: fanBadgeId}, document.title, '?fanBadgeId=' + fanBadgeId);
location.hash = "#badge";
  });

  //Prevent doubleclick
  $("a, button, input[type=submit]").click(function (e) {
    if (!$(this).data('isClicked')) {
      var link = $(this);

      // Your code on successful click

      // Set the isClicked value and set a timer to reset in 3s
      link.data('isClicked', true);
      setTimeout(function () {
        link.removeData('isClicked')
      }, 3000);
    } else {
      // Anything you want to say 'Bad user!'
      e.preventDefault();
    }
  });
