  function findActor(actorName) {
      if(actorName != ''){
      jQuery.ajax({
          type:'POST',
          url:"/actor/findActor",
            data: {actorName: actorName, sort: '', order: ''},
            success:function(data, textStatus){
                jQuery('#actorsContainer').html(data);
                if(actorName){
                    $('.find_actors').scrollExtend("disable");
                } else {
                    $('.find_actors').scrollExtend("enable");
                }
             },
            error:function(XMLHttpRequest,textStatus,errorThrown){}
        });
        }else{
        jQuery.ajax({
          type:'POST',
          url:"/actor/getAllActors",
