	                 //<![CDATA[
					$(document).ready(function() {
	function hotfixSocialSubscribe() {
		if(ndm.social && ndm.social.sitename && $('.social-activity-centre .btn-subscribe').length) {
			var newUrl = '';
			switch(ndm.social.sitename) {
				case "adelaidenow":
					newUrl = "http://www.adelaidenow.com.au/subscribe/news/1/?sourceCode=AAWEB_ONS539";
					break;
				case "dailytelegraph":
					newUrl = "http://www.dailytelegraph.com.au/subscribe/news/1/?sourceCode=DTWEB_ONS539";
					break;
				case "heraldsun":
					newUrl = "http://www.heraldsun.com.au/subscribe/news/1/?sourceCode=HSWEB_ONS539";
					break;
			}
			if(newUrl !== '') {
				$('.social-activity-centre .btn-subscribe').attr('href', newUrl);
			}
		} else {
			setTimeout(hotfixSocialSubscribe, 1000);
		}
	}
	hotfixSocialSubscribe();
});
	                 //]]>
		</script>
