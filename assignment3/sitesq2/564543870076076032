<style type="text/css">
    #mma-lightbox {
        background-color: rgba(0,0,0,0.8);
        display: none;
        position: fixed;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        z-index: 10000;
    }
    #mma-lightbox .lightbox-container {
        display: inline-block;
        position: absolute;
    }
    #mma-lightbox .lightbox-close {
        display: block;
        position: absolute;
        top: 20px;
        right: 20px;
        height: 38px;
        width: 38px;
        background-image: url('/content/img/presentation/buttons/mma-lightbox-close.png');
        z-index: 11000;
    }
</style>
<script type="text/javascript">
    $(function () {
        var $lightbox = $("#mma-lightbox");
        var id = $lightbox.data("lightbox-id");
        var expire = $lightbox.data("expiration");

        var repositionLightbox = function () {
            var $container = $lightbox.find(".lightbox-container");
            var viewportWidth = $(window).width();
            var viewportHeight = $(window).height();
            var lightboxWidth = $container.width();
            var lightboxHeight = $container.height();
            var top = (viewportHeight - lightboxHeight) / 2;
            top = top < 0 ? 0 : top;
            var left = (viewportWidth - lightboxWidth) / 2;
            left = left < 0 ? 0 : left;
            $container.css('top', top).css('left', left);
        }
        var saveCookie = function (cookieId) {
            var now = new Date();
            now.setDate(now.getDate() + parseInt(expire));
            document.cookie = cookieId + '-lightbox=visited; expires=' + now.toUTCString() + '; path=/';
        }

        if ($lightbox.length > 0) {
            $lightbox.show();
            repositionLightbox();
            $(window).resize(function () {
                repositionLightbox();
            });
            $lightbox.find(".lightbox-close").click(function (e) {
                $lightbox.hide();
                saveCookie(id);
                e.preventDefault();
            });
            $lightbox.click(function (e) {
                $lightbox.hide();
                saveCookie(id);
            });
            $lightbox.find(".lightbox-container").click(function (e) {
                e.stopPropagation();
            });
            $lightbox.find("a, area").click(function (e) {
                saveCookie(id);
            });
            $(document).keydown(function (e) {
                if (e.keyCode == 27) {  // escape
                    $lightbox.hide();
                    saveCookie(id);
                }
            });
        }
    })
</script>


    
    
