<script language="javascript" type="text/javascript">
	function sethbxvars(obj){
	  if(typeof(s_code)!="undefined"){
	    s.linkTrackVars="prop1";
	    s.prop1=document.search.keyword.value;
		s.tl(obj,'o','Keyword Search');
	  }
	}

var keyword_message_text = "Search/Keyword";
function clearText(field){
  if (field.defaultvalue == field.value) field.value = '';
  else if (field.value == '') field.value = field.defaultvalue;
}
</script>


<form name="search" id="search" action="/cc-common/RadioKeywordSystemSearch/search3.php" method="get" class="rks_form" onsubmit="if(this.keyword.value == 'Search/Keyword') { this.keyword.value = '' } sethbxvars(this);" lang="en"><script type="text/javascript">
jQuery(function(){
    jQuery('#search').submit(function(event){
        var searchUrl = '/search/'+encodeURIComponent(jQuery('#rks_keybox input').val());
        window.location = searchUrl;
        event.preventDefault();
    })
})
</script>
