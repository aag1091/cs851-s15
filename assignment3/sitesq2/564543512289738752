            $(document).ready(function(){
                var target = $('#ls_mod_1797368');
                var showID = '20799901';

                var requestURL = '/services/ihr_show.html?showId=' + showID + '&offset=0&maxRows=3&t=1';
                $.ajax({
                    type: 'GET',
                    url: requestURL,
                    dataType: 'json',
                    success: function(data){
                        if(data.showRestValue){

                            var siteID = '1209';
                            var cid = encodeURIComponent( window.location.pathname );
                            var episodes = data.showRestValue.allepisodes;

                            for(var i = 0; i < episodes.length; i++ ){

                                var ep = episodes[i];

                                var item = $('<li></li>');
                                var anchor = $('<a></a>');
                                var image  = $('<img />');
                                var headline = $('<h6></h6>');

                                image.attr('src', 'http://iscale.iheart.com/img/' + ep.image.replace('http://','') + '?default=true&ops=max(240,240)');
                                image.attr('class', 'xSmallImage');

                                var itemUrl = 'http://www.iheart.com/show/'+ ep.showSlug  +'/?episode_id=' + ep.episodeId + '&campid=ls_ihrshow&pname=' + siteID  + '&cid=' + cid;
                                anchor.attr('href', itemUrl);
                                anchor.attr('target', '_blank');

                                var anchor2 = anchor.clone();
                                anchor2.text( ep.title );

                                headline.attr( 'class', 'moduleContentTitle' );
                                headline.append( anchor2 );
                                anchor.append( image );

                                item.append( anchor );
                                item.append( headline );
                                item.append( '<div class="clearfix">&nbsp;</div>');
                                target.find('.moduleContentContainer .moduleSubItems').append( item );

                            }//for

                            var moreUrl = 'http://www.iheart.com/show/'+ data.showRestValue.slug  +'/?campid=ls_ihrshow&pname=' + siteID  + '&cid=' + cid;
                            target.find('.moduleFooter a').attr('href', moreUrl);

                        }else{
                            target.hide();
                        }
                    },
                    error: function(){
                        //there was an error
                        target.hide();
                        if(console){
                            console.log('ERROR: Unable to load episodes for show: %s', showID);
                        }
                    }
                });
            });
        </script>
    </div>
</div>
