<script>
$(document).ready(function() {
    if (window.streamId && window.streamId != "") {

        var timeout,
            qio2 = QuickIo('ws://quickio.iheart.com'),
            $ihrnp = $('#ihrnp'),
            $ihrnp_info = $('#ihrnp_info'),
            $ihrnp_track = $('#ihrnp_track'),
            $ihrnp_artist = $('#ihrnp_artist'),
            $ihrnp_image = $('#ihrnp_image'),
            $ihrnp_link = $('#ihrnp_link');

        qio2.on('/ihr/now-playing/live/' + window.streamId, function(data) {

            if( data && data.length > 0 ){

                var item = data[0];

                $ihrnp_info.show();
                $ihrnp.css('height','110px');
                $ihrnp_track.text(item.track);
                $ihrnp_artist.text(item.artist);
                //due to how the new iheart scaler works we need to preload the image and only display it if a valid image has been returned
                var imagePath = "http://iscale.iheart.com/catalog/track/" + item.trackId + "?ops=fit%2890%2C90%29";
                var oImg = new Image;
                oImg.src = imagePath;
                oImg.onload=function(){
                    $ihrnp_image.find("img").attr("src", imagePath).attr('alt', item.track);
                }

                $ihrnp_link.attr('href','http://www.iheart.com/live/'+window.streamId+'/?autoplay=true&pname='+window.siteId+'&campid=desktop_now_playing_right&cid=%2Fonair%2Flance-mcalister-7818%2Fweekend-hoops-uc-xu-uk-13232477%2F');

                clearTimeout(timeout);
                timeout = setTimeout(function() {
                    $ihrnp_info.hide();
                    $ihrnp.css('height','0px');
                }, item.endTime - $.now());
            }
        });
    }

});
</script>
