<script>
	(function(win){
		// This code disables HP personalization
		nfl.Y.use('event-custom', function(Y){
			var pathName = win.location.pathname,
				isDesktop = nfl.constants.DEVICE_CONTEXT === 'DESKTOP';
			if(isDesktop && (pathName === '/' || pathName.substr(0, 5) === '/home')){
				nfl.Y.Global.once('home:nflNowVideoLoaded', function(e){
					nfl.Y.Global.detachAll('home:nflNowVideoLoaded');
				});
			}
		});
	})(window);
</script>

