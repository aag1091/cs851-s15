    article.themeValue = 
    article.partnerId = around-the-league
	AD FILTER VALUES
	ad_site 		= .news
	ad_zone 		= story
	ad_zone_url 	= url=story
	ad_zone_keys 	= s1=story
	pageKeyValues 	= prtnr=around-the-league;team=car;conf=nfc;dvsn=ncs
