		<script type="text/javascript">
			 $(document).ready(function() {
			    $(".note-counter").each(function() {
			        var number = parseInt($(this).text());
			        if (number > 999) {
			            number = number / 1000;
			            $(this).text(number.toFixed(0) + 'K');
			        }
			    });
			}); 
		</script>
		 
		
		<script type="text/javascript">
				$(document).ready(
				    function colorVimeo() {
				    
				    /* Better Vimeo Embeds 2.1 by Matthew Buchanan
				    Modelled on the Vimeo Embedinator Script
				    http://mattbu.ch/tumblr/vimeo-embeds/
				
				    Modified by Matt Schwarz 1.10.12
				
				    Released under a Creative Commons attribution license:
				    http://creativecommons.org/licenses/by/3.0/nz/ */
				
				    var color = "ffffff";
				    var opts = "title=1&byline=1&portrait=1";
				        $("iframe[src^='http://player.vimeo.com']").each(function() {
				            var src = $(this).attr("src");
				            var w = $(this).attr("width");
				            var h = $(this).attr("height");
				        if (src.indexOf("?") == -1) {
				            $(this).replaceWith("<iframe src='"+src+"?"+opts+"&color="+color+"' width='"+w+"' height='"+h+"'webkitAllowFullScreen mozallowfullscreen allowFullScreen frameborder='0'></iframe>");
				        }
				        });
				    }
				);
	        </script>
        
