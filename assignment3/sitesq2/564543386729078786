	<script type="text/javascript">

	
		
	liveTvClickEvents();
	//Function also called when browser back button pressed
	function liveTvClickEvents(){
	$('div.header_links div').on('click', function(){
		
		if($(this).attr('id') === 'liveA'){ //audio
			
			document.location.href='/player/';  	
			
			// will need something to build the show content in the player area here
			//addJplayer( "http://pri.gocaster.net/98", "640", "30", "true", "mp3", "live", false, true );
			
			
		}
		else if($(this).attr('id') === 'bulletin'){ //audio	
			
			addJplayer( "", "640", "30", "true", "mp3", "live", false, true );
			//Log with analytics
						_gaq.push(["_trackPageview", "/player/HourlyNews"]);
			
			
		}
		else { // video			
			
			   document.location.href='/player/tv';			   
			   //addJplayer( "http://nodeb.gocaster.net:1935/CGL/mp4:CGA_98FM_LOW/playlist.m3u8", "640", "360", "true", "", "live", false, true );
			
		}
    });
	}



	// update show data in header strip + on audio player overlay + TV live feed info every 5 mins
	setInterval("csData()",300000);
	//setInterval("csData()",10000);
    function csData(){

            // call ajax
            $.ajax({
                url: '/player/assets/includes/ajax/current_show_data.php',
    			dataType: 'json'
            }).then(function( data ) {

                //playlistMode
                $('body #cs_times').html(data.time);
                $('body #cs_title').html(data.title.replace(/\\/g,''));

                if ($('body #cs_num').length) {// has phone num in header
                	$('body #cs_num').html('Text: ' + data.sms);
				}


				if ($('body #audio_player_content').length) {// audio
                	$('body #streamCaptionFlash').html(data.titlecc.replace(/\\/g,'') + ' ' + data.timeonly);
				}
                
                
				if ($('body #player_title h1').length) {// tv

					var contactStr = 'Phone: ' + data.phone + ' Text: ' + data.sms + '<div class="fs15 bold"><a class="station_color" href="mailto:'+ data.email +'">'+ data.email +'</a></div>';
					
                	$('body #player_title h1').html(data.titlecc.replace(/\\/g,''));
                	$('body #player_title p').html(data.timeonly);
                	$('body #player_title div.bold').html(contactStr);
				}
            });

    }
	
	
	var listener;
	
	// player builder
	// mediaType is used to get the info for the player title
	function addJplayer( file, w, h, as, type, mediaType, itemID, noAnalytics, logID, titleUrl, lbPart, norealod, updateCopyOnly){

		
		
		//Clear video description
		$('#videoDesc').empty();

		if(file.indexOf("m3u8") != -1){
			noAnalytics = true;
		}
        
		//reload the page if not live stream
		if (mediaType != 'live' && !noAnalytics && !norealod) {
			window.location.href = titleUrl;
		}
		else{
		
	    
        runwjplayer = false;

        
		if (mediaType === 'listenback') {

			if (!lbPart) {
				lbPart = document.URL.charAt(document.URL.length - 1);
			}

	        
            // Array of month names since JS doesn't have these built in

            var month_names = new Array ( );

            month_names[month_names.length] = "January";
            month_names[month_names.length] = "February";
            month_names[month_names.length] = "March";
            month_names[month_names.length] = "April";
            month_names[month_names.length] = "May";
            month_names[month_names.length] = "June";
            month_names[month_names.length] = "July";
            month_names[month_names.length] = "August";
            month_names[month_names.length] = "September";
            month_names[month_names.length] = "October";
            month_names[month_names.length] = "November";
            month_names[month_names.length] = "December";

            // get date from url (url is different for lb on show page and lb page)

            var a1 = document.URL.split('/')

                a2_start = a1.length - 1,

                a2 = a1[a2_start].split('_'),

                date_day=a2[0].substr(0,2),

                date_ord=a2[0].substr(2,2);
                
                date_day=date_day.replace(/[^0-9]/g,'');

                lbDate_ms = Date.parse(date_day + ' ' + a2[1] + ' ' + a2[2]);

			if(!isNaN(lbDate_ms)){
	                
               	var lbDate_date= new Date(lbDate_ms);

                lbDate=lbDate_date.getDate() + date_ord + ' ' + month_names[lbDate_date.getMonth()] + ' ' + lbDate_date.getFullYear();
			} 
			else {
				lbDate = '';
			}
		}


		var isBulletin 	= 0,
			isLive 		= 0,
			isTV 		= 0;
	   
	   
		// player height animation
		var pHeight = $('.hero .wrap').css('height');
		if (type === 'mp3') {
			
			// under mpu
			$('div#player_title').empty();

			
			
			$('.hero .wrap').animate({
				height: '275px'
			});		
			
			
			$('#social_sub_player').fadeOut();
			
		}
		else if(mediaType === 'live'){

			if(type==='vadio'){

				$('.hero .wrap').animate({
					height: '550px'
				});

			}else{

				$('.hero .wrap').animate({
					height: '381px'
				});

			}
			

		}
		else {
			$('.hero .wrap').animate({
				height: '449px'
			});
		}

        var skinDate = new Date().getTime();
		
	
		var skin = '/player/jwplayer/skins/98fm_firefox/six.xml?'+skinDate;
		
		// Listen Live / Live tv active state handler
		$('.header_bt').removeClass('station_color_bg');
		
		if (mediaType === 'live') {
			
            if(file != ''){
				skin = '/player/jwplayer/skins/98fm_firefox/sixlive.xml?v=1?'+skinDate;
            }
            
			if (type === 'mp3') {
				
				if (file == '') {
					$('#bulletin').addClass('station_color_bg');
					isBulletin = 1;
					document.title = 'Hourly News - 98FM Radio Player';
					window.clearInterval(listener);
				}
				else {
					$('#liveA').addClass('station_color_bg');
					isLive = 1;
				}
			}
			else {
				$('#liveV').addClass('station_color_bg');
				//isLive = 1;
				document.title = 'TV - 98FM Radio Player';
				window.clearInterval(listener);
			}
		}


		
			
		if (updateCopyOnly !== 1){

			var primaryType = 'flash';		
			
			if(typeof jwplayer("player").getRenderingMode() != 'undefined'){
				jwplayer("player").remove();
		    }
			
			jwplayer("player").stop();
			// update jplayer
			if(file.indexOf("mirror-image") == -1 || file.indexOf("youtube") == -1 ){
				$("#player").empty();
			}

			
			if(type==='vadio'){
				
				var streamCode = '<div id="dc-vadio-wrapper" class="dc-video-on"></div> <scr' + 'ipt src="http://www.vadio.com/embed.php?id=dc-vadio-wrapper&stream=&remove_explicit_content=1&" type="text/javascript" charset="utf-8"></scr' + 'ipt>';
				
				$('.player_left').html(streamCode);
			}		
			else if((isAndroid || iOS) && (file.indexOf("m3u8") == -1) && (file.indexOf("mirror-image") == -1) && (file.indexOf("youtube") == -1) && (file.indexOf("mp4") == -1)){
				$("#player").html('<audio id="mobilePlayer" autoplay controls><source src="'+file+'"></audio>');
			}
			else if ((!isAndroid && !iOS) || (file.indexOf("m3u8") != -1) || (file.indexOf("youtube") != -1) || (file.indexOf("mirror-image") != -1) || (file.indexOf("mp4") != -1)){
				
				if (file.indexOf("m3u8") != -1) {// video
					
					jwplayer("player").setup({
					    file: file,
					    width: w,
					    height: h,
					    autostart: as,
					    type: type,
					    skin: skin,
			    		"analytics": {
					    	enabled: false
					    },
					    primary: primaryType,
					   	//advertising: {
					    	//"client": "vast",
					   		//"tag": "http://ad.dc2.adtech.de/?adrawdata/3.0/1090.1/4757827/0/16/noperf=1;cc=2;header=yes;alias=myalias;cookie=yes;adct=204;key=key1+key2;grp=[group];misc=[TIMESTAMP]",
					   		//"admessage": 'Ad plays for XX seconds.'
					    //},
					    events: {
					    	onReady: function() {
					    		
					    		if (mediaType === 'live' && file != ''){
					    			
					    			var showName = $('#audio_player_content_text h2').text().replace(/\\/g,'');
					                var ShowTime = $('#audio_player_content_text p').text();
	
						    		if(jwplayer("player").getRenderingMode()=='html5'){
	
						    			$('.jwcenter').html('<span id="streamCaption">'+showName+' '+ShowTime+'</span>');
						    		}
						    		else{
	
						    			$('#player_wrapper').prepend('<span id="streamCaptionFlash">'+showName+' '+ShowTime+'</span>');
							        }
						        }
				        	 }
					    }
				    });	
				}
				else {// audio
					
					jwplayer("player").setup({
					    file: file,
					    					    width: w,
					    height: h,
					    					    
					    autostart: as,
					    type: type,
					    skin: skin,					    
			    		"analytics": {
					    	enabled: false
					    },
					    primary: primaryType,
					    events: {
					    	onReady: function() {

					    		if(!as){
					    			 $('#playIcon').show();
						    	}
					    		
					    		if (mediaType === 'live' && file != ''){
		
					    			
					    			var showName = $('#audio_player_content_text h2').text().replace("\\", "");
					                var ShowTime = $('#audio_player_content_text p').text();
		
						    		if(jwplayer("player").getRenderingMode()=='html5'){
		
						    			$('.jwcenter').html('<span id="streamCaption">'+showName+' '+ShowTime+'</span>');
						    		}
						    		else{
						    									    			$('#player_wrapper').prepend('<span id="streamCaptionFlash">'+showName+' '+ShowTime+'</span><div class="plm_audio_controls"><span data-volume="0" class="plm_audio_icom plm_volume_control plm_volume_0"></span><span data-volume="20" class="plm_volume_control plm_volume_20"></span><span data-volume="40" class="plm_volume_control plm_volume_40"></span><span data-volume="60" class="plm_volume_control plm_volume_60"></span><span data-volume="80" class="plm_volume_control plm_volume_80"></span><span data-volume="100" class="plm_volume_control plm_volume_100"></span></div>');
						    											    }
						        }
				        	 },onPause: function() {
				        		$('#pauseIcon').hide();
							    $('#playIcon').show();  	
							    							    		
						     },
						     onPlay: function() {

							    	$('#playIcon').hide();
								    $('#pauseIcon').show();	

								    
								    $('.plm_audio_controls').show();

								    jwplayer("player").setVolume('60');						            
						            setVolumeGraphics('60', 'click'); 
								    plm_volume_control("player");
						     }
					    }
				    });	
				}	
			}

			if (isLive === 1) {


								listener = window.setInterval(function(){ 
				    //code goes here that will be run every 5 mins
					addJplayer( file, w, h, as, type, mediaType, itemID, noAnalytics, logID, titleUrl, lbPart, true, 1 );
				}, 300000);
							}
		}


		
		// update player info
		
		$.ajax({
			url: '/player/assets/includes/ajax/player_builder.php',
			data: { type: type, mediaType: mediaType, itemID: itemID, logID: logID, isBulletin: isBulletin },
			dataType: 'json'
		}).then(function( results ) {

			
			
			$('#audio_player_content').remove();
			
			//Update the embed iframe
			var stationUrl = 'http://www.98fm.com/';

			//Spin sw exception
			if(stationUrl=='http://www.spinsouthwest.com/'){
				stationUrl='http://player.spinsouthwest.com/';
			}

			//98fm exception
			if(stationUrl=='http://www.98fm.com/'){
				stationUrl='http://player.98fm.com/';
			}			
			
			if(mediaType=='podcast'){
				var ecLarge = '<iframe frameborder="0" height="60" width="630" src="'+stationUrl+'player/embed.php?mediaType='+ mediaType +'&id='+itemID+'&w=630"></iframe>',
					ecSmall = '<iframe frameborder="0" height="60" width="560" src="'+stationUrl+'player/embed.php?mediaType='+ mediaType +'&id='+itemID+'&w=560"></iframe>';
				$('#embedCode').val(ecSmall);
			}
			else if(mediaType=='video'){
				var vidID = '',
					ecLarge = '<iframe frameborder="0" height="60" width="630" src="'+stationUrl+'player/embed.php?mediaType='+ mediaType +'&id='+vidID+'&w=630"></iframe>',
					ecSmall = '<iframe frameborder="0" height="60" width="560" src="'+stationUrl+'player/embed.php?mediaType='+ mediaType +'&id='+vidID+'&w=560"></iframe>';
				$('#embedCode').val(ecSmall);
			}
			else if(mediaType=='listenback'){	
				var ecLarge = '<iframe frameborder="0" height="60" width="630" src="'+stationUrl+'player/embed.php?mediaType='+ mediaType +'&id='+logID+'&showID='+itemID+'&w=630"></iframe>',
					ecSmall = '<iframe frameborder="0" height="60" width="560" src="'+stationUrl+'player/embed.php?mediaType='+ mediaType +'&id='+logID+'&showID='+itemID+'&w=560"></iframe>';			
				$('#embedCode').val(ecSmall);
			}


			var myURL = $(location).attr('href');

			$('#email').attr('href', 'mailto:?subject=' + slugify(results.title) + ' | from the 98fm player&body=' + myURL);

			$('.embed_size').on('click', function(){
				
				var $this = $(this);
				
				if ($this.val() == 2) {
					$('#embedCode').val(ecLarge);
				}
				else {
					$('#embedCode').val(ecSmall);
				}
			});
			
			
			var timeString 		= '',
				my_sub 			= '',
				my_show_details = '',
				saveID 			= '',
				showID 			= '',
				shareButtons 	= '';
			
			if (mediaType === 'listenback'){
				saveID = logID;
				showID = itemID;
			}
			else {
				saveID = itemID;
			}
			
			
			if (results.show_title) {
				
				if (mediaType === 'podcast') {
					my_title 	= results.title.replace(/\\/g,'');
					my_sub 		= '<div class="bold">Show: ' + results.show_title + '</div>';

					//document.title = 'Podcasts - 98FM Radio Player';
				}
				else if (mediaType === 'listenback') {
					my_title 	= results.title.replace(/\\/g,'');
					my_sub 		= '<div class="bold">Listen Back: ' + lbDate + ' Part ' + lbPart + '</div>';

					//document.title = 'Listen Back - Archive - 98FM Radio Player';
				}
				else {
					my_title 	= results.show_title.replace(/\\/g,'');
				}
			}
			else {
				my_title = results.title.replace(/\\/g,'');
			}
			
			if (results.display_time) {
				timeString = '<div class="fs15">' + results.display_time + '</div>';
			}
			
			
			
			if (results.start_time && results.end_time) {
				if (mediaType=='live' && type === 'mp3') {
                     var hideText = " style='display:none;' "
				}
				else{
					var hideText = "";
				}
				timeString = '<p'+hideText+'>' + results.start_time + ' - ' + results.end_time + '</p>';
			}


			if (results.phone || results.sms) {
				my_show_details = '<div class="fs15 bold">';
			}
			
			if (results.phone) {
				my_show_details += 'Phone: ' + results.phone + ' ';
			}
			
			if (results.sms) {
				my_show_details += 'Text: ' + results.sms;
			}
			
			if (results.email) {
				my_show_details += '<div class="fs15 bold"><a class="station_color" href="mailto:' + results.email + '">' + results.email + '</a></div>';
			}
			
			
			if (type === 'mp3') {
				var subBlock = '';
				var imgString = '';
				
				
				if (results.image) {

											imgString = '<img src="' + results.image + '" alt="Player Image" title="' + my_title + '" class="imgStd left playerImage" width="263" height="200" data-remitemid="' +saveID+ '" data-remmediatype="' +mediaType+ '" data-remshowid="' +showID+ '" data-remlbpart="' +lbPart+ '"><div id="share_plus" style="right: 0; top: 168px;">';
																	
						if (mediaType !== 'live') {
							imgString += '<span class="share_bt black_bt shadowBlack pointer right"><span></span>share</span>';
						}
						
						imgString += '</div>';
					
				}
				
				
				if (my_sub || results.subtitle) {
					subBlock = '<div>';
				
					if (my_sub) {
						subBlock += my_sub;
					}
					if (results.subtitle) {
						subBlock += results.subtitle.replace(/\\/g,'');
					}
					
					subBlock += '</div>';
				}

									$('div.player_left').prepend( '<div id="audio_player_content"><img id="playIcon" class="player_button" style="display: none;" src="/player/assets/graphics/playIcon.png"/><img id="pauseIcon" class="player_button" style="display: none;" src="/player/assets/graphics/pauseIcon.png"/>' + imgString + '<div id="audio_player_content_text"><h2>' + my_title + '</h2>' + timeString + subBlock + my_show_details + '</div></div>');
								// area below mpu
				// $('h1#player_title').html( my_title + ' ' + my_show_details );
			}
			else {// video
				
				// area below mpu
				
				
								
				if (mediaType !== 'live') {
					shareButtons += '<span class="share_bt black_bt shadowBlack pointer" style="margin: 0 13px 0 0;"><span></span>share</span>';
					//document.title = 'Video - 98FM Radio Player';
					
				}
				

				if (shareButtons) {
					shareButtons = '<div id="share_plus">' + shareButtons + '</div>';
				}

				if (mediaType !== 'live') {
					$('<div id="videoDesc"><h1>' + my_title  + '</h1><h2>'+results.subtitle+'</h2></div>').insertAfter('#clearBar');
					$('#player_title_holder').html( '<div id="player_title">'+ shareButtons + "</div>");
				}
				else{
					$('#player_title_holder').html( '<div id="player_title"><h1>' + my_title  + '</h1>'+ ' ' + timeString + ' ' + my_show_details + shareButtons + "</div>");
				}

				
			}

			//Update twitter and facebook links
			var twitterAccount 	= '@98FM';
			var twitterAccount 	= twitterAccount.replace("@","");
			var twitterURl 		= 'https://twitter.com/share?url=' + encodeURIComponent(document.URL) + '&via='+twitterAccount+'&text=' + encodeURIComponent(slugify(my_title));
			$('#twitterShare').attr('href', twitterURl);
			
            var facebookUrl 	= 'http://www.facebook.com/sharer.php?u=' + encodeURIComponent(document.URL);
            $('#faceBookShare').attr('href', facebookUrl);


            //Show play icon on mobile browsers
            
            if((isAndroid || iOS) && (file.indexOf("m3u8") == -1) && (file.indexOf("mirror-image") == -1) && (file.indexOf("youtube") == -1)){
            	$('#playIcon').show(); 
            }

			
			
			//Track pageview with google analytics
			if(!noAnalytics){
				
			    if(titleUrl !== false){
				
				  var analyticsTitle = titleUrl.substring(titleUrl.indexOf("/player/"));
					
				    					_gaq.push(["_trackPageview", analyticsTitle]);
					
			    }	

			
			if(html5Support){
				if(titleUrl !== false){
					history.pushState({bodyContent:$('#portfolioWrapper').html(), nav:$('.nav').html()}, document.title, titleUrl);
				}			
			}

			// load ads

			$('body #mpu1').empty();
			$('#pre').empty();
			writeCapture(document.getElementById('mpu1')).write(results.mpu1).close();
			//writeCapture(document.getElementById('pre')).write(results.pre).close(); -removed for launch
			 
			}			
			
		});
		 
		}
	}


	
		(function() {
			
			// ad overlay
			var adCheck = $.cookie('ad_played');
			//var adCheck;
			
							
				// no video tag, build player normally
				addJplayer( "http://pri.gocaster.net/98", "640", "30", true, "mp3", "live", "false", true, "");
						
			
	    	// content area builder
	    	contentBuilder( 'home', false, 'false', 'false', false, '', '', false, '', '', '' );
			
	    	
	    	
	    	//---------------------------------------------------------
	    	// player live stream (audio / video) loaders
	    		    	   	
			
	    	// tweet box (this doesn't like being in global.js)

	    	
	    	
	    	
	    		    	
		})();
	    
	</script>
</body>
