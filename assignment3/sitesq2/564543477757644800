<script type="text/javascript">
    function utilityNavLoggedOut(){
        $('#loggedIn').hide();
        $('#notLoggedIn').show();

        $('#rewards-loggedIn-redirect').hide();

        completeUtilNav();
    }

    function utilityNavLoggedIn(event, profile){
        $('#loggedIn').show();
        $('#notLoggedIn').hide();
        $('#utility-nav-name').text(
            profile.first_name ? profile.first_name : profile.name
        );
        profile.points && $('#utility-nav-points').text(profile.points + ' points');

        $('#rewards-loggedIn-redirect').hide();

        completeUtilNav();
    }

    function utilityNavRegister(event, name){
        $('#loggedIn').hide();
        $('#notLoggedIn').hide();
        
        var group = $('#rewards-loggedIn-redirect');

        group.find('[data-source="profile.name"]').text(name);
        group.show();

        completeUtilNav()
    }

    function completeUtilNav(){
        $("#utility-nav-rewards").addClass("complete");
    }

    $(function(){
        function setLoginLink(event, settings){
            $(".logInOut.rewardsActionLink").attr('href', settings.get('loginLink'));
            settings.on('missingLoyalty.ufc.rewardsChecker', utilityNavRegister);
            settings.on('loggedIn.ufc.rewardsChecker', utilityNavLoggedIn);
            settings.on('loggedOut.ufc.rewardsChecker', utilityNavLoggedOut);
            $('#utility-nav-rewards .logInOut a').on('click', function(){
                settings.trigger('doLogout.ufc.rewardsChecker');
            })

        }

        var context = $('#ufc-rewards-settings'),
            settings = context.data('settings');
        
        if(settings)
            setLoginLink({}, settings);
        else
            context.on('settings.ufc.rewardsChecker', setLoginLink);
    });
</script>

