<script type="text/javascript">
	$(function(){
		$('#improvEmailAddress').focus(function(){
			if($(this).val()=='Enter Your Email Address'){
				$(this).val('');
			}
		});
		$('#improvEmailAddress').blur(function(){
			if($(this).val().length==0){
				$(this).val('Enter Your Email Address');
			}
		});
	});
</script>








