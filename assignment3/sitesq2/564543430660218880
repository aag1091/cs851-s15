<script type="text/javascript" src="js/jquery.easing.1.3.js"></script>
<script type="text/javascript">

	var toggleClubs = function() {
		if($('#clubsMenu').is(':visible')) {
			closeClubMenu();
		} else {
			$('ul','#clubsMenu').fadeOut(1,function(){
				$('#clubsMenu').slideDown(350,'easeInCirc',function(){
					$('ul','#clubsMenu').fadeIn(250);
				});
			});
		}
	}


	var closeClubMenu = function() {
		$('ul','#clubsMenu').fadeOut(250,function(){
			$('#clubsMenu').slideUp(350,'easeInCirc');
		});
	}


	$(function(){

		

			$('body').click(function(){
				if($('#clubsMenu').is(':visible')) {
					closeClubMenu();
				}
			});

			$('#clubsMenu').click(function(e){
			  e.stopPropagation();
			});

			$('#clubsMenuLink').click(function(){
				toggleClubs();
				return false;
			});

		

	});
</script>

