	                <script language="javascript" type="text/javascript">
	                    var fbShareLink = '';
	                    var fbSummary = '&p[summary]=' + $('meta[property="og:description"]').attr("content");
	                    $(function(){
	                        var fbImage1 = '';
	                        var fbImage2 = '';
	                        var fbImage3 = '';
	                        var fbImage4 = '';
	                        try{
	                            fbImage1 = fbImage1.replace("&", "%26");
	                        } catch(err){ }
	                        var fbImages = '&p[images][0]='+fbImage1;
	                        var inImage = 1;
	                        if (fbImage2.length) {
	                            fbImages += '&p[images]['+inImage+']='+ fbImage2;
	                            inImage++;
	                        }
	                        if (fbImage3.length) {
	                            fbImages += '&p[images]['+inImage+']='+ fbImage3;
	                            inImage++;
	                        }
	                        if (fbImage4.length) {
	                            fbImages += '&p[images]['+inImage+']='+ fbImage4;
	                            inImage++;
	                        }
	                        fbShareLink = 'http://www.facebook.com/sharer.php?s=100&p[url]=http://chicago.improv.com/event.cfm?id=352966&cart&ref=C9FF253994D345BBDE5289BD4B3AF3AE46899F41FE3B700D47271552147CF280' + fbImages + fbSummary;
	                        //alert(fbShareLink);
	                      });
	                      function linkToFB() {
	                          window.open(fbShareLink);
	                      }
	                </script>
	                                
