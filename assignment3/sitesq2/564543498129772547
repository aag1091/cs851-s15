                                <div id="ctl00_divSocialMediaShareBar" class="share-bar vertical left">
<script type="text/javascript">
    j$(document).ready(function () {
        var ua = j$.browser;
        var sharebarobj = j$('div.share-bar');
        var windowWidth = j$(window).width();
        var middleWidth = parseInt(j$('div.content').css('width'));
        var shareWidth = parseInt(j$('div.share-bar ul li').css('width'));
        var totalWidth = middleWidth + shareWidth + 120;
        var classes = j$('div.share-bar').attr('class');

        if (ua.msie) {
            j$('#share-bar li.facebook .sub').css({
                'left': '-99999px',
                'display': 'block'
            });
        }

        j$('#share-bar li').hover(function () {
            j$(this).find('div.sub').removeAttr('style').stop(true, true).animate({ 'height': 'show' }, 250).css({ 'display': 'block', 'z-index': 500 });
            if (ua.mozilla) {
                j$('#share-bar li.facebook .sub .fb-like span, #share-bar li.facebook .sub .fb-like span iframe').css({ 'width': '100px', 'height': '100px' });
            }  
        }, function () {
            if (!j$('table.pls-container').length) {
                j$(this).find('div.sub').stop(true, true).animate({ 'height': 'hide' }, 50).css({ 'z-index': 450 }).hide();
            }
        });

        if (ua.msie && j$.browser.version == 7.0) {
            if (j$('div.share-bar').hasClass('vertical')) {
                if (j$('div.share-bar').hasClass('left')) {
                    j$('div.share-bar').css({
                        'position': 'absolute',
                        'left': '-45px',
                        'top': 0
                    });
                }
                else if (j$('div.share-bar').hasClass('right')) {
                    j$('div.share-bar').css({
                        'position': 'absolute',
                        'right': '-45px',
                        'top': 0
                    });
                }
                if (totalWidth > windowWidth) {
                    j$('div.share-bar').insertAfter('div.breadcrumb').removeClass('vertical left right').removeAttr('style');
                }
            }
            j$(window).resize(function () {
                windowWidth = j$(window).width();
                if (totalWidth > windowWidth) {
                    j$('div.share-bar').insertAfter('div.breadcrumb').removeClass('vertical left right').removeAttr('style');
                } else if ((totalWidth < windowWidth) && j$('div.share-bar').hasClass('vertical')) {
                    j$('div.share-bar').appendTo('div.middle');
                    if (j$('div.share-bar').hasClass('left')) {
                        j$('div.share-bar').css({
                            'position': 'absolute',
                            'left': '-45px',
                            'top': 0
                        })
                    }
                    else {
                        j$('div.share-bar').css({
                            'position': 'absolute',
                            'right': '-45px',
                            'top': 0
                        })
                    }
                } else {
                    j$('div.share-bar').attr('class', classes);
                }
            });
        }
        else {
            //move sharebar on pageload if window is smaller than content
            if (totalWidth > windowWidth) {
                j$('div.share-bar').removeClass('vertical').removeClass('left').removeClass('right');
            }

            //on window resize, test to see if window is smaller than content, if so, move sharebar
            j$(window).resize(function () {
                windowWidth = j$(window).width();
                if (totalWidth > windowWidth) {
                    j$('div.share-bar').removeClass('vertical').removeClass('left').removeClass('right');
                } else if ((totalWidth < windowWidth) && j$('div.share-bar').hasClass('vertical')) {
                    //do nothing
                } else {
                    j$('div.share-bar').attr('class', classes);
                }
            });
        }

        j$('div:not(#share-bar)').click(function () {
            j$('#share-bar li').each(function () { j$(this).find('div.sub').stop(true, true).animate({ 'height': 'hide' }, 50).css({ 'z-index': 450 }).hide(); });
        });

        j$('#share-bar li > a[href="#"]').click(function () { return false; })
    });

</script>
