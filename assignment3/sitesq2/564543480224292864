<script type="text/spacebars" name="formErrors">
  <div class="alert alert-warning animated fadeIn" role="alert">
    <ul class="list tl pln fn center mbl">
      {{#each errorMessages}}
      <li>{{.}}</li>
      {{/each}}
    </ul>
  </div>
</script>

<script type="text/spacebars" name="inlineErrorMessage">
{{#if errorMessage attribute}}<div class="tl orange mts" role="alert">{{errorMessage attribute}}</div>{{/if}}
</script>

<script type="text/spacebars" name="inputFormGroup">
<div class="pos-rel form-group has-feedback {{hasFeedback attribute}}" aria-live="assertive">
  <div class="{{#if hasError attribute}}animated shake-short{{/if}}">
    <input type="{{type}}" name="{{model}}[{{attribute}}]" class="form-control input-lg {{attribute}}" placeholder="{{placeholder}}" value="{{value}}" 
    aria-label="{{placeholder}}" aria-required="{{required}}" aria-describedby="{{attribute}}Status" aria-invalid="{{hasError attribute}}" />
    <i class="fa fa-{{icon}} form-control-feedback" aria-hidden="true"></i>
  </div>
  <span id="{{attribute}}Status">{{> inlineErrorMessage attribute=attribute}}</span>
</div>
</script>

<script type="text/spacebars" name="submitButton">
<button type="submit" class="btn btn-lg btn-block bg-orange-gd white clearfix tl f1">
  {{text}}
  <i class="fa fa-chevron-right fr mtxs opa-40"></i>
</button>
</script>

