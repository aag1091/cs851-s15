/* CITY TWEETS */
function $(src){
    if(typeof(src) == 'string'){
        src = document.getElementById(src);
	}
    return src;
}
function addScript(url,callback){
	var s = document.createElement("script");
    s.type = "text/javascript";
    s.language = "javascript";
    s.src = url;
	document.body.appendChild(s);
	//if(callback){callback(s);}
	return s;
}
// handle broken twitter image links
// has to be put at the top. Called before the js file is loaded.
var brokenImages = function() { // object that stores all broken images on the page
	var images = new Array();
	function add(img, name){
		if (!$('SchmapKit.Maps__GStatic.Map.0.container')) {
			var tmp = function tmp () {brokenImages.add(img, name);};
			setTimeout(tmp, 200);
			return;
		}
		else {
			img.src = "/images/spacer.png";
			img.className = "defaultTwitterPic_noBG ";
			images.push([name, img]);
			var url = "http://twitter.com/users/show/"+name+".json?callback=setTwitterImg";
			addScript(url);
			setTimeout(brokenImages.fail.bind(null, name, img), 5000);
		}
	}
	function resolve(name, url){
		//log('resolve: '+name+" "+url);
		for (i=0; i<images.length; i++) {
			if (images[i][0] && images[i][0].toLowerCase()==name.toLowerCase()) {
				images[i][1].src = url;
				images[i][1].className = '';
				images[i][1].onerror = function(){setTwitterImgError(this);};
				images[i][1].onload = function(){checkImage(this, name, true)};
				//images[i][0] = null;
	}}}
	function fail(name, img) {
		//log('fail : '+name);
		for (i=0; i<images.length; i++) {
			if (images[i][0] && images[i][0].toLowerCase()==name.toLowerCase()) {
				if (images[i][1].src.match("/images/spacer.png")) {
					images[i][1].className = "defaultTwitterPic";
					images[i][0] = 'setToDefault';
					return;
		}}}}
	return {add:add, resolve:resolve, fail:fail};
}();
function setTwitterImg(r){
	brokenImages.resolve(r.screen_name, replaceMini(r.profile_image_url));
}
function replaceMini(url){
	url1 = url.split('_normal');
	if (url1[1])
		return url1[0]+"_mini"+url1[1];
	url2 = url.split('_bigger');
	if (url2[1])
		return url2[0]+"_mini"+url2[1];
	else
		return url;
}
function setTwitterImgError(img) {
	img.src = "/images/spacer.png";
	img.className = "defaultTwitterPic";
	log('broken image not handled');
	return;
}
function checkImage(img, name, secondTry) {
	image = new Image(); image.src = img.src;
	image.onload = function(){
		if (!(this.width > 1 || this.height > 1)) {
			if (secondTry) {
				log('broken image not handled');
				img.src = "/images/spacer.png";
				img.className = "defaultTwitterPic";
			}
			else
				brokenImages.add(img, name);
		}
	};
	img.onload = "";
	img.onerror = "";
}
// This function is used to put entries into the blue menu
function populateBlueMenu() {
	li = document.createElement('li'); li.id = "tNparent";
	if (!document.location.href.match('/search')) {
		a = document.createElement('a'); a.id = "toggleNeigh_temp";
		a.innerHTML = "Select neighborhood <span class='last-child'><img src='/images/x.gif' /></span>"; li.appendChild(a);
	}
	$('filter_preload').innerHTML = '';
	$('filter_preload').appendChild(li);

	if (menuMODefaut == 'whatOn') {count = categories.whatOn.menuCount; categories_ = categories.whatOn.categories_;
		rows = categories.whatOn.numberRows;}
	else if (menuMODefaut == 'restaurants') {count = categories.restaurants.menuCount; categories_ = categories.restaurants.categories_;
		rows = categories.restaurants.numberRows;}
	else if (menuMODefaut == 'barsClubs') {count = categories.barsClubs.menuCount; categories_ = categories.barsClubs.categories_;
		rows = categories.barsClubs.numberRows;}
	else if (menuMODefaut == 'shopServices') {count = categories.shopServices.menuCount; categories_ = categories.shopServices.categories_;
		rows = categories.shopServices.numberRows;}
	else if (menuMODefaut == 'artEnt') {count = categories.artEnt.menuCount; categories_ = categories.artEnt.categories_;
		rows = categories.artEnt.numberRows;}
	else if (menuMODefaut == 'healthBeauty') {count = categories.healthBeauty.menuCount; categories_ = categories.healthBeauty.categories_;
		rows = categories.healthBeauty.numberRows;}
	else if (menuMODefaut == 'toDoSee') {count = categories.toDoSee.menuCount; categories_ = categories.toDoSee.categories_;
		rows = categories.toDoSee.numberRows;}
	else if (menuMODefaut == 'more') {count = categories.more.menuCount; categories_ = categories.more.categories_;
		rows = categories.more.numberRows;}
	else /*if (menuMODefaut == 'nodefault')*/ {count = 0; return;}

	buf = [];
	for (i=0; i<count; i++) {buf[i] = "";}
	for (i=0; i<categories_.length; i++) {
		if (categories_[i] && categories_[i][2]) {
			li = document.createElement('li');
			if (categories_[i][2]==1 || (menuMODefaut=="more" && categories_[i][2]==2)) li.className="first-li";
			a = document.createElement('a'); a.innerHTML = categories_[i][0];
			a.href = 'http://'+document.location.host+'/'+categories_[i][1]+document.location.search;
			span = document.createElement('span'); span.innerHTML = "&nbsp;|&nbsp;"; span.className = "spacer";
			li.appendChild(a);
			// only put the spacer after the last element if there is a 'show all' link
			if (categories_[i][2] != count || rows > 0)
				li.appendChild(span);
			buf[categories_[i][2]-1] = li;
		}
	}
	for (i=0; i<buf.length; i++) {
		if(buf[i])
			$('filter_preload').appendChild(buf[i]);
	}

	//if ($('filter_preload')) $('filter_preload').parentNode.removeChild($('filter_preload'));
	li = document.createElement('li');
	if (rows > 0) {
		a = document.createElement('a');  a.innerHTML = 'Show all';
		span = document.createElement('span'); span.innerHTML = '<img src="/images/x.gif" />'; span.className = "last-child";
		a.appendChild(span); li.appendChild(a); $('filter_preload').appendChild(li);
	}
	if (menuMODefaut == 'nodefault') {li.innerHTML = '&nbsp;'; $('filter_preload').appendChild(li);}
}

	





