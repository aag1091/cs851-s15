<script src="http://s2.reutersmedia.net/resources_v2/js/libraries/jquery.qrcode.min.js"  type="text/javascript"></script>
<script src="http://s2.reutersmedia.net/resources_v2/js/libraries/jquery.mCustomScrollbar.concat.min.js"  type="text/javascript"></script>
