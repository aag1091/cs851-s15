  function lettersOnly(evt) {
    evt = (evt) ? evt : event;
    var charCode = (evt.charCode) ? evt.charCode : ((evt.keyCode) ? evt.keyCode : 
        ((evt.which) ? evt.which : 0));
    if (charCode > 31 && (charCode < 65 || charCode > 90) && 
        (charCode < 97 || charCode > 122)) {
        alert("Please enter letters only.");
        return false;
    }
    return true;
  }

  function numbersOnly(field, e) {
  	var key;
	var keychar;

	if (window.event) {
		key = window.event.keyCode;
		if(window.event.ctrlKey) {
			return true;
		}
	}
	else if (e) {
		key = e.which;
		if(e.ctrlKey) {
			return true;
		}
	}
	else
		return true;
	keychar = String.fromCharCode(key);

	// control keys
	if ((key==null) || (key==0) || (key==8) || (key==9) || (key==13) || (key==27) || 
	(key==17) || (key==35) || (key==36) || (key==37) || (key==39))
		return true;

	// numbers
	else if ((("0123456789").indexOf(keychar) > -1))
		return true;

	else
		return false;
  }
  
  function initAJAXRequest(url) {
    if (window.XMLHttpRequest) {
        return new XMLHttpRequest();
    } else if (window.ActiveXObject) {
        isIE = true;
        return new ActiveXObject("Microsoft.XMLHTTP");
    }
  }

function onChangeColor(branch, frontImagePath, persImagePath) {
	var form = $('step01_form');
	// change the hidden field's value
	var field = $('cardBranchName');
	field.value=branch;
	// change the card front image
	var frontImage = $('cardFrontImage');
	frontImage.setAttribute("src",frontImagePath);
	var perspectiveImage = $('perspective_image');
	if(perspectiveImage != null) {
		perspectiveImage.setAttribute("src",persImagePath);
	}

	// update card thumb images to either selected or unselected
	// first unselected thumbs
            if ("ACE_RF_MV" != branch) {
            var btnImage = $('cardBtnImage_ACE_RF_MV');
            if (btnImage != null) {
                var unselectedSrc = btnImage.getAttribute("unselectedSrc");
                if (unselectedSrc != null) {
                    btnImage.setAttribute("src",unselectedSrc);
                }
            }
        }
        // then the selected thumb
    var btnImage = $('cardBtnImage_' + branch);
    if (btnImage != null) {
        var selectedSrc = btnImage.getAttribute("selectedSrc");
        if (selectedSrc != null) {
            btnImage.setAttribute("src",selectedSrc);
        }
    }

}

function verifyLoginName(name) {
	// check for invalid characters 
	var regExp = /^(?!\d{16})^(?!\d{4}-\d{4}-\d{4}-\d{4})[a-zA-Z0-9]{1,1}[a-zA-Z0-9\-\.]{1,30}$/;
	if(regExp.test(name) == false) {
		alert("Username should start with a letter or number and can contain only letters, numbers, dashes and periods. It should have a minimum of 2 characters and a maximum of 30 characters. Your username cannot contain a 16 digit card number.");
        return false;
	}

    return true;
}

function isValidPassword(pwd) {
    // check for invalid characters
    var regExp = new RegExp("[<>()%&]");
    if(pwd.search(regExp) != -1) {
        alert("Password cannot contain '<>()%&'.");
        return false;
    }
}

function processSubModalClose(returnVal) {
	eval(returnVal);
}

function toggleAccordion(headerId, contentId) {
	var temp = "$('" + headerId + "')";
	var headerElem = eval(temp);
	temp = "$('" + contentId + "')";
	var contentElem = eval(temp);
	headerElem.toggleClassName('fold');
	headerElem.toggleClassName('unfold');
	contentElem.toggle();
}

function disableFormCheckbox(enabledId, disabledId, disabledWrapperId) {
	var temp = "$('" + enabledId + "')";
	var enabledElem = eval(temp);
	var temp = "$('" + disabledId + "')";
	var disabledElem = eval(temp);
	var temp = "$('" + disabledWrapperId + "')";
	var wrapperElem = eval(temp);
	if (enabledElem.checked) {disabledElem.writeAttribute('disabled','disabled'); wrapperElem.writeAttribute('class','disabled');}
	else {disabledElem.removeAttribute('disabled'); wrapperElem.writeAttribute('class','default');}
}

function editAlertAddress(wrapperID, entryID, addressID) {
	var temp = "$('" + wrapperID + "')";
	var wrapperElem = eval(temp);
	temp = "$('" + entryID + "')";
	var entryElem = eval(temp);
	temp = (addressID);
	var addressElem = eval(temp);

	fieldContent = wrapperElem.innerHTML;
	wrapperElem.replace('<input type=\"text\" value=\"' + fieldContent + '\" name=\"entry.' + addressElem + '.address\" onkeyup=\"evalElement(this);\" />');

	entryElem.toggleClassName('enabled');
	entryElem.toggleClassName('disabled');

}

var currentTipID = null;
var currentCueID = null;

function showTooltip(tipCueID,tipID,tipType) {
    if (currentTipID != null) {
        if (currentTipID != tipID) {
            hideTooltip(currentCueID, currentTipID);
        }
    }
    currentTipID = tipID;
    currentCueID = tipCueID;
	var temp = "$('" + tipCueID + "')";
	var tipTriggerElem = eval(temp);
	var temp = "$('" + tipID + "')";
	var tipBalloonElem = eval(temp);
	var tipTypeElem = tipType;
	var tipBalloonCreateElem = tipID + "_balloon";
	temp = "$('" + tipBalloonCreateElem + "')";
	var ele = eval(temp);
	if(ele.visible()) {
		return ;
	}

	var tooltipTriggerDimensions = tipTriggerElem.getDimensions();
	var tooltipTriggerPosition = tipTriggerElem.positionedOffset();
	var tooltipBalloonDimensions = ele.getDimensions();

	var triggerLeft = tooltipTriggerPosition[0];
	var toolTipPositionLeft = '' + triggerLeft + 'px';

	var brsVersion = parseInt(window.navigator.appVersion.charAt(0), 10);
	if(brsVersion <= 6 && window.navigator.userAgent.indexOf("MSIE") > -1) {
		var triggerBottom =  tooltipTriggerDimensions.height + tooltipBalloonDimensions.height - 18;
		} else {
		var triggerBottom =  tooltipTriggerDimensions.height + tooltipBalloonDimensions.height;
		}

	if(ele != null && ele != undefined) {
    	ele.setStyle("left:" + toolTipPositionLeft);
    	ele.setStyle("margin-top:-" + triggerBottom + "px");
        document.getElementById(tipBalloonCreateElem).style.display = "block";
		//ele.show();
    	ele.setOpacity(0.95);
	}
}

function showHTMLTooltip(tipCueID,tipID,tipType) {
    if (currentTipID != null) {
        if (currentTipID != tipID) {
            hideTooltip(currentCueID, currentTipID);
        }
    }
    currentTipID = tipID;
    currentCueID = tipCueID;
	var temp = "$('" + tipCueID + "')";
	var tipTriggerElem = eval(temp);
	var temp = "$('" + tipID + "')";
	var tipBalloonElem = eval(temp);
	var tipTypeElem = tipType;
	var tipBalloonCreateElem = tipID + "_balloon";
	temp = "$('" + tipBalloonCreateElem + "')";
	var ele = eval(temp);
	if(ele.visible()) {
		return ;
	}

	var tooltipTriggerDimensions = tipTriggerElem.getDimensions();
	var tooltipTriggerPosition = tipTriggerElem.positionedOffset();
	var tooltipBalloonDimensions = ele.getDimensions();

	var triggerLeft = tooltipTriggerPosition[0];
	var toolTipPositionLeft = '' + triggerLeft + 'px';

	var brsVersion = parseInt(window.navigator.appVersion.charAt(0), 10);
	if(brsVersion <= 6 && window.navigator.userAgent.indexOf("MSIE") > -1) {
		//var triggerBottom =  tooltipTriggerDimensions.height + tooltipBalloonDimensions.height - 18;
		var triggerBottom =  tooltipTriggerDimensions.height + tooltipBalloonDimensions.height - 1;
		} else {
		var triggerBottom =  tooltipTriggerDimensions.height + tooltipBalloonDimensions.height;
		}

	if(ele != null && ele != undefined) {
    	ele.setStyle("left:" + toolTipPositionLeft);
    	ele.setStyle("margin-top:-" + triggerBottom + "px");
        document.getElementById(tipBalloonCreateElem).style.display = "block";
		//ele.show();
    	//ele.setOpacity(0.95);
	}
}

function hideTooltip(tipCueID,tipID) {
	var tipBalloonCreateElem = tipID + "_balloon";
	var temp = "$('" + tipBalloonCreateElem + "')";
	var ele = eval(temp);
	if(ele != null && ele != undefined) {
		ele.hide();
	}
    currentTipID = null;
    currentCueID = null;
}

/////// Toggle Tabs

function toggleTabs(selectedTab, tabs) {
	var sTab = "$('tab_" + selectedTab + "')";
	var tab = eval(sTab);
	var sPanel = "$('panel_" + selectedTab + "')";
	var panel = eval(sPanel);

	// if the tab is already selected, do nothing
	if(tab.hasClassName('active')) {
		return;
	}

	// hide the other tab that is active at this time
	if(tabs.length > 0) {
		var name;
		for(i=0; i < tabs.length; i++) {
			var sPrevTab = "$('tab_" + tabs[i] + "')";
			var prevTab = eval(sPrevTab);
			if(prevTab.hasClassName('active')) {
				var sPrevPanel = "$('panel_" + tabs[i] + "')";
				var prevPanel = eval(sPrevPanel);
				prevPanel.hide();
				prevTab.removeClassName('active');
				prevTab.addClassName('inactive');
			}
		}
	}

	// show the selected tab
	tab.addClassName('active');
	tab.removeClassName('inactive');
	panel.show();
}

/////// End Toggle Tabs

