<script type="text/javascript">

function makeRequest() {
	var selector = $('userNameList');
//	selector.disable();
	selector.options.length = 1;
	new Ajax.Request('ajaxOps.m', {
	    method:'post',
    	parameters: {method: 'usernames', 
        	         firstName: $F('firstNameElem'), 
            	     lastName: $F('lastNameElem'), 
                	 middleInitial: $F('middleInitialElem')},
		onSuccess: function(transport) {
      		var response = transport.responseText || "no response text";
	  		var obj = response.evalJSON(true);
	  		for(i = 0; i < obj.names.length; i++) {
	     		selector.options[i+1] = new Option(obj.names[i], obj.names[i]);
	  		}
//	  		selector.enable();
    	}
  	});
}

function handleNameFinish() {
	if($F('firstNameElem').length != 0 && $F('lastNameElem').length != 0) {
		makeRequest();
	}
}

function bindEvents() {
	Event.observe('firstNameElem', 'change', handleNameFinish);
	Event.observe('lastNameElem', 'change', handleNameFinish);
	Event.observe('middleInitialElem', 'change', handleNameFinish);
	Event.observe(window, 'load', handleNameFinish);
}

</script>

			
