function textFlashPayId(form) {
    var re = /[\D]*/g;
    var phone = form.phone.value;
    phone = phone.replace(re,"");
    if (phone.length != 10) {
        alert("Please enter a valid 10 digit phone number.");
        pageTracker('$trackFlowErrorNumber');
        return false;
    }
    new Ajax.Request('ajaxOps.m', {
        method:'post',
        parameters: {method: 'textFlashPayId',
            phone: phone},
        onSuccess: function(transport) {
            // do nothing
        }
    });
}


/**
 * speedbump
 *
 * Confirms with the user that they are leaving the OAC
 *
 * examples:
 *  <a href="www.foo.com" onclick="speedbump();">Leave the site!</a>
 *  <button onclick="speedbump('www.foo.com');">Leave the site!</button>
 *
 * @param destinationUrl optional url, not needed if used on an anchor tag's onclick event
 * @returns {boolean} value used by some events to determine if they can continue processing, <a> for example
 */
function speedbump(destinationUrl) {
    // Anchor Tag?
    var aTag = this.tagName? this.tagName.toLowerCase() === 'a' : false;

    if ((aTag || destinationUrl) && confirm('Clicking on this link will take you to an external website not managed or maintained by NetSpend Corporation ("NetSpend"). , the issuing bank for which NetSpend is an authorized Independent Sales Organization, neither endorses or guarantees any of the information, recommendations, optional programs, products, or services advertised, offered by, or made available through NetSpend or on this website ("Products and Services") and disclaims any liability for any failure of the Products and Services.')) {
        // Navigate
        if (!aTag) {
            window.open(destinationUrl);
        }

        // Allow the event to bubble back to the browser to assign the HREF (anchor tag only)
        return true;
    }

    return false;
}

