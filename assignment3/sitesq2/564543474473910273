		function updatePromoLogResponse(logId, event) {
		  if(logId == null || logId == "null" || logId == "undefined")
		  {
		  	return;
		  }
        	new Ajax.Request('ajaxOps.m', {
        	    method:'post',
            	parameters: {method: 'updatePromoLog', 
                	         id: logId, 
                    	     action: event},
        		onSuccess: function(transport) {
    				// do nothing
            	}
          	});
		}

/////// End Marketing logs

/////// Terms and conditions logs
function updateTermsLogs(value) {
  if(value == null || value == "null" || value == "undefined")
  {
      return;
  }
    new Ajax.Request('terms.m', {
        method:'post',
        parameters: {log: 'log',
                     value: value},
        onSuccess: function(transport) {
            // do nothing
        }
      });
}

