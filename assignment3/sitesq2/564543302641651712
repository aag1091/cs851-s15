<script>
	// create the masonry-style layout
	// use JQuery code within (function($) { ... }(jQuery));
	(function($) { 
		var entries_grid = $('#entries_grid');
		entries_grid.imagesLoaded( function(){
			entries_grid.masonry({
				itemSelector : '.entry_brick',
				columnWidth : 280,
				gutterWidth: 20
			});
		});
	}(jQuery));
</script>

<!-- scroll the pages instead of using page links -->
<script src="/javascripts/jquery.infinitescroll.min.js"></script>
<script>
	(function($) {
		var entries_grid = $('#entries_grid');
		entries_grid.infinitescroll({
			navSelector  : '#page-nav',    // selector for the paged navigation
			nextSelector : '#page-nav a',  // selector for the NEXT link (to page 2)
			itemSelector : '.entry_brick',     // selector for all items you'll retrieve
			loading: {
				selector: '#loading-prompts',
				finishedMsg: "<em>There are no more condolences to load.</em>",
			    img: "http://www.infinite-scroll.com/loading.gif",
			    msgText: "<em>Loading the next set of condolences...</em>"
			}
		},
		// trigger Masonry as a callback
		function( newElements ) {
			// hide new items while they are loading
			var $newElems = $( newElements ).css({ opacity: 0 });
			// ensure that images load before adding to masonry layout
			$newElems.imagesLoaded(function(){
				// show elems now they're ready
				$newElems.animate({ opacity: 1 });
				entries_grid.masonry( 'appended', $newElems, true, function() {
					if (parent.adjustIFrameSize) {
						parent.adjustIFrameSize(window, undefined, 'none');
					}
				});
			});
			$(".edit_guestbook_entry").click( function(event) {
				event.preventDefault();
				$.post( $(this).attr("href"));
			});
			$(".edit_guestbook_entry").removeClass('edit_guestbook_entry');
		}
		);

	}(jQuery));

	function reload_masonry() {
		(function($) {
			var entries_grid = $('#entries_grid');
			entries_grid.masonry( 'reload');
		}(jQuery));
	}
</script>


                            

