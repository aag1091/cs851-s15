        } 
    });
</script><span style="display:none" itemprop="printEdition">Mon, Feb 09 2015 00:00:00 E</span><span style="display:none" itemprop="printPage">A16_ISSUES</span><p class="byline"><br/><div style="font-size:12px;padding-left:5px;"><span style="display:inline;" itemprop="datePublished">02/06/2015 06:51 PM ET</span></div></p><div class="mod1 pagination" id="pagination"><div class="paginframe_left"><div class="paginframe_mid" style="display:none;"><ul class="share-menu share-top"><li id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mLiEmail" class="share-email"><a id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mlnkEmail" title="Email" class="share-email" onclick="popup=window.open(this.href, 'contentservices', 'width=508,height=550,scrollbars=yes,resizable=yes'); popup.focus();return false;" href="http://license.icopyright.net/g1/3.8218?icx_id=738419" target="_blank">Email</a></li><li id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mLiPrint" class="share-print"><a id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mlnkPrint" title="Print" class="share-print" onclick="popup=window.open('http://license.icopyright.net/rights/printServiceGroup.act?inprocess=t&amp;tag=3.8218%3Ficx_id%3D738419', 'contentservices', 'width=508,height=550,scrollbars=yes,resizable=yes'); popup.focus();return false;" href="http://license.icopyright.net/rights/printServiceGroup.act?inprocess=t&amp;tag=3.8218%3Ficx_id%3D738419" style="cursor: pointer;">Print</a></li><li id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mLiReprints" class="share-reprints"><a id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mlnkReprints" title="Reprints" class="share-license" href="http://www.ibdreprints.com/" target="_blank">Reprints</a></li><li id="ctl00_ctl00_secondaryContent_leftContent_artBody_mctlUtilities_mLiComment" class="share-comment"><a class="share-comment" href="#comments" title="Comment">Comment</a>