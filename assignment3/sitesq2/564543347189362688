$(document).ready(function(){
extendedForecastURL = 'http://www.thejournal.co.uk/weather/';
iconURL = 'http:\/\/s.thejournal.co.uk\/skins\/shared\/gfx/widgets/weather/';
ajaxURL = 'http:\/\/www.thejournal.co.uk\/weather\/?service=ajax&item=weather';
$.ajax({
url: ajaxURL,
dataType: 'json',
success: function(forecast) {
var linkURL = 'http:\/\/www.thejournal.co.uk\/weather\/';
$("#weather-hdr-icon-link").attr("href", linkURL);
$("#weather-hdr-icon-image").attr("src", iconURL + forecast.conditions.weatherIcon + ".png");
