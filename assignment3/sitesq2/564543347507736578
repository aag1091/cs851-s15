        ibsys.oonPlaceholders(function() {
            ibsys.ad.gpt.init({
                positionKeyword : 'kw'
            });
        });
    } else {
        ibsys.ad.gpt.init({
            positionKeyword : 'kw'
        });
    }

</script>
