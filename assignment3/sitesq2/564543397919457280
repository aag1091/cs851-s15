	</script><script>//Track .NET errors in GTM
	function trackErrorsOnThisPage(pagename) {
		function listen(evnt, elem, func) {
			if (elem.addEventListener) // W3C DOM
				elem.addEventListener(evnt, func, false);
			else if (elem.attachEvent) { // IE DOM
				var r = elem.attachEvent("on" + evnt, func);
				return r;
			}
		}
		function track(o) {
			//do not log on live site
			if(location.href.indexOf('dev.youcaring.com')>=0
				|| location.href.indexOf('preview.352inc.com')>=0
				|| location.href.indexOf('beta.youcaring.com')>=0)
				console.log(o);
			dataLayer.push(o);
		}
		function trackErrors(func){
			var isValid = func? func.apply(this, [].slice.call(arguments).slice(1)) : true; //pass through original arguments
			if(!isValid) {
				for (var i=0; i<Page_Validators.length; i++) {
					if (!Page_Validators[i].isvalid && typeof(Page_Validators[i].errormessage) == "string") {
						track({
							event: 'validation-error',
							page: pagename || '',
							message: Page_Validators[i].errormessage
						});
					}
				}
			}
			return isValid;
		}
		listen('load', window, function () {
			window.Page_ClientValidate = _.wrap(window.Page_ClientValidate, trackErrors);
		});
	}
	</script><script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js" type="text/javascript"></script><script src="http://www.youcaring.com/tft-js/core/moment.min.js"></script><script src="http://www.youcaring.com/tft-js/vendor/numeral.min.js" type="text/javascript"></script><script>!function (d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (!d.getElementById(id)) { js = d.createElement(s); js.id = id; js.src = "//platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs); } }(document, "script", "twitter-wjs");</script><script type="text/javascript">var doLazyLoadingOnUpdates = false;

		//Lazy loading of Updates
		function scrollToUpdate(desiredId) {
			//scroll to the specific update
			var $updateElement = $('#' + desiredId);
			if ($updateElement.length > 0) {
				$('html, body').animate({
					scrollTop: $updateElement.offset().top
				}, 2000);
			}
		}
		
		var updateModule = (function(){
			var desiredId;
			var vm = {
				updateCount: 1,
				updates: ko.observableArray([]),
				afterAdd: function(element, index, context) {
					var toolbox = $('.addthis_toolbox', element).get(0);
					addthis.toolbox(toolbox);

					if(desiredId) {
						if(context.id == desiredId) { //if this is the specific update we want

							scrollToUpdate(desiredId);

							desiredId = null;
						}
					}
				},
				getMore: function() {
					getNextPage();
				},
				isBusy: ko.observable(false)
			};
			function getNextPage(id) {
				if(vm.isBusy()) return; //too busy, come back later
				if(vm.updates().length >= vm.updateCount) return; //maxed out!

				desiredId = id;

				vm.isBusy(true);
				var pageSize = 1;
				var pageNumber = (vm.updates().length/pageSize) + 1;
				var method = desiredId? 'getUpdate' : 'getUpdates';
				var url = tft.settings.rootUrl + 'tft-services/YouCaring/YouCaringProfileWebMethods.asmx/'+method+'?pageNumber='+pageNumber+'&pageSize='+pageSize+'&fundraiserId=293282';
				if(desiredId) url += ('&fundraiserUpdateID=' + desiredId);

				$.ajax({
					url: url,
					type: "GET",
					contentType: "application/json; charset=utf-8"
				}).done(function (data, textStatus, jqXHR) {
					for(var i=0;i<data.d.length;i++){
						var d = data.d[i];
						vm.updates.push({
							id: d.UpdateID,
							dateCreated: d.DateCreatedDisplay,
							organizerName: tft.settings.fundraiserOrganizerName,
							message: d.Message,
							urlToShare: tft.settings.fundraiserShareUrl+'/update/'+d.UpdateID,
							title: 'New Update posted to ' + tft.settings.fundraiserTitle
						});
					}
				}).fail(function (data, textStatus, jqXHR) {
					// TODO: Handle Failure
				}).always(function(){
					vm.isBusy(false);
				});
			}
			function getUntil(id) {
				getNextPage(id);
			}
			ko.applyBindings(vm, $('.js-tabPanel--updates')[0]);
			
			return {
				vm:vm,
				getNextPage:getNextPage,
				getUntil:getUntil
			};
		})();
		
		//Lazy loading of Gallery
		var galleryModule = (function(){
			var fancyboxConfigs = {
				image: {
					'transitionIn': 'elastic',
					'transitionOut': 'elastic',
					'speedIn': 600,
					'speedOut': 200,
					'minWidth':0,
					'minHeight':0
				},
				video: {
					'transitionIn': 'elastic',
					'transitionOut': 'elastic',
					'speedIn': 600,
					'speedOut': 200,
					'padding': 0,
					'width': 1120,
					'height': 630,
					'aspectRatio': true,
					'type': 'iframe',
					'minWidth':0,
					'minHeight':0
				}
			}
			var vm = {
				galleryItemCount: 2,
				galleryItems: ko.observableArray([]),
				afterAdd: function(element, index, context) {
					if($(element).is('.gallery-item--fancyBox')) //filter not working
						$(element).fancybox(context.isVideo? fancyboxConfigs.video : fancyboxConfigs.image);
				},
				getMore: function() {
					getNextPage();
				},
				isBusy: ko.observable(false)
			};
			function getNextPage() {
				if(vm.isBusy()) return; //too busy, come back later
				if(vm.galleryItems().length >= vm.galleryItemCount) return; //maxed out!

				vm.isBusy(true);
				var pageSize = 10;
				var pageNumber = (vm.galleryItems().length/pageSize) + 1;
				$.ajax({
					url: tft.settings.rootUrl + 'tft-services/YouCaring/YouCaringProfileWebMethods.asmx/getGallery?pageNumber='+pageNumber+'&pageSize='+pageSize+'&fundraiserId=293282',
					type: "GET",
					contentType: "application/json; charset=utf-8"
				}).done(function (data, textStatus, jqXHR) {
					for(var i=0;i<data.d.length;i++){
						var d = data.d[i];
						vm.galleryItems.push({
							url: d.FullImageURL,
							thumbnail: d.ThumbnailProfile,
							isVideo: d.IsVideo
						});
					}
				}).fail(function (data, textStatus, jqXHR) {
					// TODO: Handle Failure
				}).always(function(){
					vm.isBusy(false);
				});
			}
			ko.applyBindings(vm, $('.js-tabPanel--gallery')[0]);

			return {
				vm:vm,
				getNextPage:getNextPage
			};
		})();
		galleryModule.getNextPage();

