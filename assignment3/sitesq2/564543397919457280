			var router = crossroads.create();
			router.addRoute('{frType}/{frName}/{frId}/update/{updateId}', function (frType, frName, frId, updateId) {

				//select the updates tab when looking at a specific update
				activateTab('updates');

				if (doLazyLoadingOnUpdates) {
