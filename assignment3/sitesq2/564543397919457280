			hasher.init(); //initialize hasher (start listening for history changes)

			//route handling function
			function parseHash(newHash, oldHash) {
				// Case-insensitive string replacement
				var needle = new RegExp('(' + tft.redteam.preg_quote(appPath) + ')', 'i');
				router.parse(window.location.pathname.replace(needle, ''));
			}
		});

		function changeWidget(w, h) {
			$('#widgetTextarea').val(function (index, text) {
				return text.replace(/width="\d+/, 'width="' + w).replace(/height="\d+/, 'height="' + h);
			});
			$('#widgetIframe').attr({ 'width': w, 'height': h });
			$('#widgetIframe').css({ 'width': w, 'height': h });
		}

		var customWidth = 190,
			customHeight = 326;
		$('input.customWidgetWidth').on('change', function () {
			customWidgetSize('width');
		});
		$('input.customWidgetHeight').on('change', function () {
			customWidgetSize('height');
		});

		function customWidgetSize(dimension) {
			if (dimension === 'width') {
				customWidth = $('input.customWidgetWidth').val();
				customHeight = Math.floor(customWidth * 1.716);
			} else {
				customHeight = $('input.customWidgetHeight').val();;
				customWidth = Math.floor(customHeight * .424);
			}
			//customWidth = $('input.customWidgetWidth').val();
			//customHeight = $('input.customWidgetHeight').val();
			changeWidget(customWidth, customHeight);
		}

		//Expandable sections
		$('.js-sectionToggle').click(function (e) {
			var sectionSelector = $(this).data('section');
			$(sectionSelector).toggleClass('current active');
			e.preventDefault();
		});

		// Tabs
		function activateTab(tab) {
			$('.js-tabPanel').addClass('hidden'); //Hide all tab content
			$('.js-tab').removeClass('current active'); //Deactivate all tabs

			$('.js-tab--' + tab).addClass('current active'); //Activate current tab
			$('.js-tabPanel--' + tab).removeClass('hidden'); //Show current tab content
		}
		$('.js-tab a').click(function (e) {
			activateTab($(this).attr("href").substr(1));

			//scroll to tab area only if it's above the window view
			var tabTop = $('.js-tabsList').offset().top;
			var windowTop = $(window).scrollTop();
			if(windowTop > tabTop) window.scrollTo(0, tabTop);

			e.preventDefault();
		});

		//If you're on mobile with the Description tab selected, select "Updates"
		//The designs are different, which is the reason for this.
		function activateDefaultTab() {
			var isMobile = $('.js-sectionToggle--details').is(':visible');
			var isDescTabCurrent = $('.js-tab--details').is('.current, .active');
			if (isMobile && isDescTabCurrent) activateTab('updates');
		}
		$(window).on('resize', function () {
			activateDefaultTab();
		});
		activateDefaultTab();


		//Load on a particular tab
		$(function () {
			var hash = location.hash;
			// Replacing the hash on the variable not on the location so that it doesn't cause a location change
			hash = hash.replace("#", "");
			//filter for actual tab aliases (ignore otherwise)
			if (hash && 'details|updates|supporters|comments'.indexOf(hash) >= 0) {
				activateTab(hash);
				//scroll down to tabbed area
				window.scrollTo(0, $('.js-tabsList').offset().top);
			}
		});

		function toggleSubscriberDiv() {
			if (document.getElementById("uxSubscribe").style.display == "none") {
				document.getElementById("uxSubscribe").style.display = "block";
			}
			else {
				document.getElementById("uxSubscribe").style.display = "none";
			}
		}

		function toggleMessages() {
			if (document.getElementById("uxSubscribe").style.display == "none") {
				document.getElementById("uxSubscribe").style.display = "block";
			}
			else {
				document.getElementById("uxSubscribe").style.display = "none";
			}
		}
		
		// Subscribe an email address to the mailing list for this fundraiser.
		function SubscribeToUpdates() {
			var email = $.trim($('#uxSubscriberEmail').val()),
				receiveSiteUpdates = $('#receiveSiteUpdates').prop('checked'),
				$responseMessage = $('#js-subscribeUpdatesResponse');

			if (email === '' || email === 'youremail@example.com') {
				$responseMessage.html('Please input your email.').removeClass().addClass('error');
			} else if (!tft.redteam.isValidEmail(email)) {
				$responseMessage.html('The email address entered is invalid.').removeClass().addClass('error');
			} else {
				$responseMessage.html('');
				$.ajax({
					type: "POST",
					contentType: "application/json; charset=utf-8",
					url: "http://www.youcaring.com/fundraiser-profile.aspx/subscribetoupdates",
					data: '{FundraiserID:293282, SubscriberEmail:"' + email + '", ReceiveSiteUpdates:' + receiveSiteUpdates + '}',
					dataType: "json",
					success: function(data) {
						$responseMessage.html(data.d.Response).removeClass().addClass(data.d.Success);
					},
					error: function() {
						$responseMessage.html('An error occurred.  Please try again later.').removeClass().addClass('error');
					}
				});
			}
		}

		function SendEmail() {
			var $contactName = $('.js-contact-name').first(),
				$email = $('.js-contact-email').first(),
				$msg = $('.js-contact-message').first(),
				$comment = $('.js-contact-required-comment').first(),
				$responseMessage = $('#js-contact-response-message');

			if ($email.val() == '' || $contactName.val() == '' || $msg.val() == '') {
				$responseMessage.text('Please fill out Name, Email, and Message.').removeClass().addClass('error').show();
			} else if (!tft.redteam.isValidEmail($email.val())) {
				$responseMessage.text('You must enter a valid Email address.').removeClass().addClass('error').show();
			} else {
				$.ajax({
					type: "POST",
					contentType: "application/json; charset=utf-8",
					url: "http://www.youcaring.com/fundraiser-profile.aspx/sendcontactemail",
					data: '{ FundraiserID: 293282, Name: "' + $contactName.val().replace(/"/g, '&quot;') + '", Email: "' + $email.val() + '", Msg: "' + $msg.val().replace(/"/g, '&quot;') + '", Comment: "' + $comment.val() + '" }',
					dataType: "json",
					success: function(data) {
						if (data.d.Error) {
							$responseMessage.text(data.d.Response).removeClass().addClass("alerts alerts-warning").show();
						} else {
							$responseMessage.text(data.d.Response).removeClass().addClass("alerts alerts-success").show();
							$contactName.val('');
							$email.val('');
							$msg.val('');
							$comment.val('');
						}
					}
				});
            }
	    }

