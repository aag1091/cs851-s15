<script type="text/javascript">

$('#comments').mouseover(function(){
    if(!$(this).find('#captcha_source').length)
        $(this).find('.captcha_container').append($("#captcha_source"));
});


$('form.submit-comment').validate({
    errorElement: 'span',
});

$('a.reply-comment').bind('click', function(){
    
    $("#hidden_captcha").append($("#captcha_source"));
    
    $("form.reply-comment").parent().find("h2").remove();
    $('form.reply-comment').remove();

    var id = $(this).attr('href').substring(1);
    var form = $('form.submit-comment').html();
    var elem = document.createElement('form');
    $(elem).attr('class', 'submit-comment reply-comment');
    $(elem).attr('method', 'post');
    $(elem).html(form);
     
    $(elem).mouseover(function(){
        if(!$(this).find('#captcha_source').length)
            $(this).find('.captcha_container').append($("#captcha_source"));
    });
     
    $(elem).find('input[name="parentId"]').val(id);

    var h2 = document.createElement('h2');
    $(h2).attr('class', 'form-title');
    $(h2).html($('div.user-comments h2.form-title').html());

    var created = false;
    $(this).parent().parent().find('ul').each(function(i){
        created = true;
        $(this).before(h2);
        $(this).before(elem);
    });

    if (!created) {
        $(this).parent().parent().append(h2);
        $(this).parent().parent().append(elem);
    }
    
    
    return false;
});
</script>



