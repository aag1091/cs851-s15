<script type="text/javascript">
	window.OpenGraphTagManager = function() {
		var ogurl = "http://www.cbc.ca/marketplace/blog/survey-ambulance-fees";
		var debug = "false";

		var inject = function ()
		{
			if (!YUI || !YUI(_CBC_LOADER))
			{
				if (debug) console.log("social_metatags.html: No YUI!");
				return;
			}

            YUI(_CBC_LOADER).use("event", "selector-css3", function(Y) {
                Y.on("load", function(e) {

					var OGURLTag = Y.one("meta[property='og:url']");
					if (Y.Lang.isNull(OGURLTag))
                    {
                        var newOGURLTag = Y.Node.create('<meta property="og:url" content="' + ogurl + '"/>');
                        var head = Y.one ('head');
                        head.append (newOGURLTag);
					}
				});
			});
		}

	    var oPublic = function() {
			inject: inject;
		}

        inject();

		return oPublic;
	}();
</script>


