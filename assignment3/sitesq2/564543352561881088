    function toggleMenu() {
      var e = $telerik.$(".custom_main_menu > .modBodyMenu");
      var a = $telerik.$(".custom_main_menu > .modTitle > a");
      if (!_menu_visible) { e.fadeIn(); a.html("Hide"); } else { e.fadeOut(); a.html("Show"); }
      _menu_visible = !_menu_visible;
    }
    function hideMenu() {
      var e = $telerik.$(".custom_main_menu > .modBodyMenu");
      var a = $telerik.$(".custom_main_menu > .modTitle a");
      e.hide(); a.html("Show");
      _menu_visible = false;
    }
  </script>
