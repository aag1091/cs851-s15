function trackEvent(eventtype,instance)
{
    $.ajax({
       type: "POST",
       url: "/rest/track.php",
       data: { incid: 69893, usrid: usrid, tempid: getCookie('incrnd'), abgrp: getCookie('incab'), eventtype: eventtype, instance: instance }
     });
}
</script>



