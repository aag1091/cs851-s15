  (function selectCurrentMenu() {
    var pageObject = { "/soccer":8162,"/tournaments":8162,"/champions-league":8162,"/championsleague/home":8162,"/championsleague/scores":8162,"/championsleague/fixtures":8162,"/championsleague/standings":8162,"/championsleague/stats":8162,"/championsleague/match-report":8162,"/europa-league":8162,"/europaleague/home":8162,"/europaleague/scores":8162,"/europaleague/fixtures":8162,"/europaleague/standings":8162,"/europaleague/stats":8162,"/europaleague/match-report":8162,"/libertadores":8162,"/world-cup":8162,"/conmebol":8162,"/concacaf":8162,"/uefa":8162,"/leagues":8162,"/la-liga":8162,"/serie-a":8162,"/ligue-1":8162,"/premier-league":8162,"/premierleague/home":8162,"/premierleague/scores":8162,"/premierleague/fixtures":8162,"/premierleague/stats":8162,"/premierleague/standings":8162,"/premierleague/match-report":8162,"/bundesliga":8162,"/eredivisie":8162,"/mexico":8162,"/mls":8162,"/football-championship":8162,"/russian-premier-league":8162,"/cups":8162,"/capital-one-cup":8162,"/coppa-italia":8162,"/copa-del-rey":8162,"/copadelrey/home":8162,"/copadelrey/scores":8162,"/copadelrey/fixtures":8162,"/copadelrey/standings":8162,"/copadelrey/stats":8162,"/copadelrey/match-report":8162,"/copa-do-brasil":8162,"/motor-sports":15008,"/rugby":8260,"/cycling":15014,"/more-sports":8166,"/sochi-2014":8166,"/handball":8166,"/tennis":8166,"/boxing":8166,"/video":8052,"/en-espanol":15025 },
    href = window.location.pathname,
    pageId = "";

	if(href === "/" || href === "/home"){
      pageId = "0000";
    }else{    
      for (var key in pageObject) {
        if (href.indexOf(key) === 0){
          pageId = pageObject[key];
          if (key !== '/news') break;
        }
      }
    }
    if (pageId !== "") {
    	var menuItem = document.getElementById('page-id-' + pageId);
    	menuItem.className += ' current selected';
    }

  })();
  
  /* MEGANAV FOR TOUCH DEVICES */
  
(function meganavTouch() {     
  var meganavItems = document.getElementsByName('inmeganav');
  var lastTouched = ''; //some mobile browser doing hover before touchstart.. need to spend this variable
  
  function addEvent(target,event,callback){
    if(typeof callback === "function"){
      if(!!target.addEventListener){
        target.addEventListener(event, callback, false);
      }else if(!!target.attachEvent){
        target.attachEvent('on'+event, callback);
      }
    }
    return target;
  };
  
  // attaching events to meganav lists
  for (var i = 0; i < meganavItems.length; i++) {
  
    //attach touch event only to those who have meganav
    if (meganavItems[i].parentNode.getElementsByTagName('div')) {
      if (meganavItems[i].addEventListener) {
        meganavItems[i].addEventListener('touchstart', doTouchEvent, false);
      }
    }
    
    addEvent(meganavItems[i],'mouseover', doMouseoverEvent);
    addEvent(meganavItems[i].parentNode, 'mouseleave', doMouseleaveEvent);   
  }
    
  /* Touch event function
  	 If there is no last touched menu item then the next touch would just do hover action.
     if it is twice the sime item clicked then press the link */
  function doTouchEvent(event) {
    event.stopPropagation();
    
    var target = (!!event.currentTarget) ? event.currentTarget : event.srcElement;

    if(target != lastTouched) {
      event.preventDefault();

      // remove current selected item
      var selectedLists = document.querySelectorAll('nav li.selected');
      removeClassFrom(selectedLists, 'selected');
      
      var hoveredLists = document.querySelectorAll('nav li.hovered');
      removeClassFrom(hoveredLists, 'hovered');

      addClass(this.parentNode, 'hovered');
      lastTouched = target;
    }
    else {
      this.removeEventListener('mouseover', doMouseoverEvent, false);
    }
  }
  
  function doMouseoverEvent(event) {
    var target = (!!event.currentTarget) ? event.currentTarget : event.srcElement;
    addClass(target.parentNode, 'hovered');
  }
  
  function doMouseleaveEvent(event) {
    var target = (!!event.currentTarget) ? event.currentTarget : event.srcElement;

    removeClass(target, 'hovered');
    addEvent(target.children[0], 'mouseover', doMouseoverEvent);
    lastTouched = '';
  }
  
  function removeClassFrom(itemList, classToRemove) {
    for (var i = 0 ; i < itemList.length; i++) {
        removeClass(itemList[i], classToRemove);
    }
    return itemList;
  }
  
  function removeClass(element, Class) {  
    element.className = element.className.replace(Class, '');
  }
  function addClass(element, Class) {
    if(element.className.indexOf(Class) < 0) {
      element.className = element.className + " " + Class;
    }
  }
  
  /* if user clicks away from meganav do the reset
     and if user clicks in meganav do not consider it as a click away */
  if(document.body.addEventListener) {
    var meganavMenus = document.querySelectorAll('nav .meganav');
    for (var i = 0; i < meganavMenus.length; i++) {
      meganavMenus[i].addEventListener('touchstart', function(event) {
          event.stopPropagation();
      });
    }
    
    document.body.addEventListener('touchstart', function(){
      removeClassFrom(this.querySelectorAll('nav li.hovered'), 'hovered');
      document.querySelector('nav li.current').classList.add('selected');
      lastTouched = '';
    });
  }
  
})();
</script>    
