	<script type="text/javascript" src="/JS/AlbumPlayer.js"></script>
    <script type="text/javascript">
    	jQuery.support.cors = true;
    	var addthis_config = {
    		username: "cdbaby",
    		data_track_clickback: true
        }

        var addthis_share = {
        	email_template: 'ShareAlbum',
        	email_vars: {ArtistName: 'Benjamin Bubb', AlbumName: 'Night of the Full Moon'},
        	templates: {
        		twitter: '♫ ; Benjamin Bubb. Listen @cdbaby {{url}}'
            },
            url_transforms: {
                shorten: {
                    twitter: 'bitly'
                }
            },
            shorteners: {
                bitly: {
                    username: 'cdbabyweb',
                    apiKey: 'R_c2a4ea11b87569a52b3d6822640ee33c'
                }
            }
        }


        //***************Read-more/Read-less           

            var $link = $("#albumNote").parent().find(".read-more-about-album");

            if ($("#albumNote").height() > 90) {
                $("#albumNote").css("height", "90");
                $link.show();
            }
            else {
                $link.hide();
            }
            
            $("#albumNote").css("overflow", "hidden");                

            $("#readMoreAboutAlbum").click(function () {               
                var $link = $("#readMoreAboutAlbum");
                var $reviewText = $("#readMoreAboutAlbum").parent().find(".note");

                if ($("#readMoreAboutAlbum").text() == "Read more...") {                    
                    $reviewText.css("height", "100%");
                  
                    $link.text("Read less....");
                    $link.removeClass("read-more-about-album");
                    $link.addClass("read-less-about-album");
                }
                else {                  
                    $reviewText.animate({ height: '90' }, 500, function () { });
                    $link.text("Read more...");
                    $link.removeClass("read-less-about-album");
                    $link.addClass("read-more-about-album");
                }
            });
        //********End-of "Read more/Read less"
         
	</script>

