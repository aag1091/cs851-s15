<script type="text/javascript">
	$('.Toggle-Container').click(function() {
	    $(this).children('.Toggle-Content').toggle();
	    $(this).find('.Arrow').toggleClass('active');
	});

</script><script>
$(".home-Twitter .tabsContainer .bishopTab").click(function(){
	$(this).addClass("active");
	$(".home-Twitter .tabsContainer .portTab").removeClass("active");
	$(".home-Twitter .Twitter-Container .portContent").fadeOut();
	$(".home-Twitter .Twitter-Container .bishopContent").fadeIn();
});

$(".home-Twitter .tabsContainer .portTab").click(function(){
	$(this).addClass("active");
	$(".home-Twitter .tabsContainer .bishopTab").removeClass("active");
	$(".home-Twitter .Twitter-Container .bishopContent").fadeOut();
	$(".home-Twitter .Twitter-Container .portContent").fadeIn();
});
</script><script>
$("#mapImageLightbox").click(function(){
	$("#lightbox-map, #lightbox-bg").fadeIn();
})

$("#lightbox-map, #lightbox-bg").click(function(){
	$("#lightbox-map, #lightbox-bg").fadeOut();
})
$("#lightbox-map, #lightbox-bg").appendTo("#header");
</script>
    
