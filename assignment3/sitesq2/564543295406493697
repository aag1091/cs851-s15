function voteFor(dialogName) 
{
    var banner = document.getElementById("youVotedFor");
    var hiddenInput = document.getElementById("votedFor");
    banner.innerHTML = dialogName;
    hiddenInput.value = dialogName;
    $('#cboxOverlay').hide();
    $('#colorbox').hide();
}

