		<script type="text/javascript">
            var count = 2;
            jQuery(window).scroll(function(){
                    if(jQuery(window).scrollTop() + jQuery(window).height() > jQuery(document).height() - 180){
                       loadArticle(count);
                       count++;
                    }
            }); 
 
            function loadArticle(pageNumber){    
					jQuery('.loading-anim').show();
                    jQuery.ajax({
                        url: "http://www.sickchirpse.com/wp-admin/admin-ajax.php",
                        type:'POST',
                        data: "action=infinite_scroll&page_no="+ pageNumber + '&loop_file=loop', 
                        success: function(html){
							jQuery('.loading-anim').hide('1000');
                            jQuery(".scroll ul.newsfeed").append(html);   // This will be the div where our content will be loaded
                        }
                    });
                return false;
            }
			
