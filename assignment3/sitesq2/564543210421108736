    <script type="text/javascript">
      amznJQ.onReady('dmusic_desktop', function() {
        amznJQ.onReady('dmusic_webstore_util', function() {
          amznJQ.onReady('dmusic_webstore_modal_purchasing', function() {
            var inputParams = {"csrfToken":"","htmlId":"dmusic_digital_buy_button_tracklistPlayer_B004679HGG","ASIN":"B004679HGG"};
            setupModalPurchaseOn(inputParams);
          });
        });
      });
      amznJQ.available('jQuery', function() {
        var button = jQuery("#dmusic_digital_buy_button_tracklistPlayer_B004679HGG a");
        if (typeof(button) == 'object' && button.length > 0) {
          var url = button.attr('href');
          url += '&js=1';
          button.attr('href',url);
        }
