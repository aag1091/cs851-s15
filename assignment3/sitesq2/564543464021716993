<script type="text/javascript">
// <![CDATA[

	$j("#footerEmail").blur(function () {
    	if(this.value == ''){ 
        	this.value = 'Your Email Address';
    	}
	}); 

 	$j("#footerEmail").focus(function () {
    	if (this.value == 'Your Email Address') {
        this.value = ''; 
