    		<script type="text/javascript">
    		// Wait until the DOM has loaded before querying the document
    		jQuery(document).ready(function () {
        		jQuery("ul.tabs").each(function () {
					// For each set of tabs, we want to keep track of which tab is active and it's associated content
            		var $active, $content, $links = jQuery(this).find("a");
					var $liActive = jQuery(this).find("li");
			
            		$active = $links.first().addClass("active"); // Use the first link as the initial active tab
					$liActive = $liActive.first().addClass("active");
            		$content = jQuery($active.attr("href"));
            
					// Hide the remaining content
            		$links.not(":first").each(function () {
                		jQuery(jQuery(this).attr("href")).hide();

            		});
            		// Bind the click event handler
           		 	jQuery(this).on("click", "a", function (e) {

                		// Make the old tab inactive.
                		$active.removeClass("active");
						$liActive.removeClass("active");
                		$content.hide();

                		// Update the variables with the new link and content
                		$active = jQuery(this);
						$liActive = jQuery(this).parent('li');

                		$content = jQuery(jQuery(this).attr("href"));

                		// Make the tab active.
                		$active.addClass("active");
						$liActive.addClass("active");
                		$content.show();

                		// Prevent the anchor's default click action
                		e.preventDefault();
            		});
        		});
    		});
		</script>
