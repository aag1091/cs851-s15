  //<![CDATA[
    (function() {
    
      window.SOI_SITE     = 'twitch';
      window.SOI_SUBSITE  = 'home'; // first level (home for home)
      window.SOI_SUB2SITE = ''; // second level
      window.SOI_SUB3SITE = ''; // third level
      window.SOI_CONTENT  = 'content'; // content|gallery|game|...
      window.SOI_TLD      = String(PP.geo).toLowerCase();
      // WERBUNG  = Advertisment in German. Boolean that indicates whether sevenone ads are enabled.
    
      // When viewing a backbone page, the page refreshes and adds a location hash if the browser doesn't have push state enabled. This variable is true when using a good browser or on the second load. 
      var validDirectory = (history.pushState !== undefined || window.location.hash.length);
      // This variable is true when we are on a page we want to display ads on.
      var validPage = (SOI_SUBSITE !== 'video' && (SOI_SUBSITE !== 'games' || validDirectory));
    
      // Available tags
      window.SOI_PU1 = true; // popup1
      window.SOI_FB2 = true; // fullbanner2
      window.SOI_SC1 = false; // skyscraper1
      window.SOI_RT1 = false; // rectangle1
    
      // Suitability for special ads
      // - from popup1
      window.SOI_PU = false; // popup/popunder
      window.SOI_PL = false; // powerlayer
      window.SOI_FA = true; // baseboard
    
      // - from fullbanner2
      window.SOI_PB = false; // powerbanner (728x180)
      window.SOI_PD = false; // pushdown
      window.SOI_BB = true; // billboard
      window.SOI_WP = true; // wallpaper
      window.SOI_FP = true; // fireplace
    
      // - from rectangle1
      window.SOI_HP = false; // halfpage (300x600)
    
      // - from skyscraper1
      window.SOI_SB = false; // sidebar (300|400x600, auto-scaling, fixed position)
    
    })();
  //]]>
</script>
