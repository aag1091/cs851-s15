<script type="text/javascript">
var _gaq = _gaq || [];
_gaq.push(['_setAccount', 'UA-3863475-12']);
_gaq.push(['_setCampSourceKey', 'c1']);
_gaq.push(['_setCampTermKey', 'l']);
_gaq.push(['_setCampContentKey', 'c5']);
_gaq.push(['_setDomainName', 'jobs2careers.com']);
_gaq.push(['_setAllowLinker', true]);
_gaq.push(['_trackPageview']);

(function() {
  var ga=document.createElement('script'); ga.type='text/javascript'; ga.async=true;
  ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
  var s=document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();
 
function loadURI(uri) {
  var xhr=createXMLHttpRequest();
  xhr.open("GET", uri, false);
  xhr.send(null);
  return xhr.responseText;
}

function setCookie(c_name,value,expiredays,domain){
        var exdate=new Date();
        if (undefined == domain)
            domain = "."+window.location.host;
        exdate.setDate(exdate.getDate()+expiredays);
        document.cookie=c_name+ "=" +escape(value)+";path=/"+
          ((expiredays==null) ? "" : ";expires="+exdate.toUTCString())
          +";domain="+domain;
};

function delCookie(name,domain) {
        if (undefined == domain)
            domain = "."+window.location.host;
        document.cookie = name + "=;path=/;expires=Thu, 01-Jan-1970 00:00:01 GMT;domain="+domain;
};

function getCookie(c_name){
  if (document.cookie.length>0) {
    c_start=document.cookie.indexOf(c_name + "=");
    if (c_start!=-1) {
      c_start += c_name.length+1;
      c_end=document.cookie.indexOf(";",c_start);
      if (c_end==-1) c_end=document.cookie.length;
      return unescape(document.cookie.substring(c_start,c_end));
    }
  }
  return "";
}

function decodeJ2C(s) {
  var r = "", ch, code, i;
  for (i=s.length; i--;) {
    ch = s.charAt(i),
    code = s.charCodeAt(i);
    if ('a' == ch) r += 'z';
    else if ('A' == ch) r +='Z';
    else if ('/'==ch) r +='^'; 
    else if ('^'==ch) r +='/'; 
    else if (':'==ch) r +='~'; 
    else if ('~'==ch) r +=':';
    else if (ch<='z' && ch > 'a') r += String.fromCharCode(code-1);
    else if (ch<='Z' && ch > 'A') r += String.fromCharCode(code-1);
    else r += ch;
  }
  return r;
}

var initj2c = function() {

  // add gaq tracking 
  on(document.getElementById('find_jobs'), 'click', function() {
    _gaq.push(['_trackEvent', 'IndexClick', 'Click', 'FindJobBtn']);
  });
  var links = document.getElementsByTagName('a');
  for (var i = 0; i < links.length; i ++)
  {
    if (links[i].getAttribute('href') != 'https://www2.jobs2careers.com/retail_login.php')
      on(links[i], 'click', function() {
        _gaq.push(['_trackEvent', 'IndexClick', 'Click', 'NavLink']);
      });
  }
  on(document.getElementById('q'), 'focus', function() {
    _gaq.push(['_trackEvent', 'IndexClick', 'Focus', 'what']);
  });
  on(document.getElementById('l'), 'focus', function() {
    _gaq.push(['_trackEvent', 'IndexClick', 'Focus', 'where']);
  });

  function CreateHotJobSearch()
  {
    if (document.getElementById("directory")){
      var ph = document.getElementById("directory");
      var url = "/explore.php?&l=Norfolk%2C+VA&s=%2Fresults3.php&c9=278";
      loadAsync(url, function(data){
	ph.innerHTML = data;
	if (document.getElementById('directory').children.length > 0)
	{
	  var trs = document.getElementById('directory').children[2].children[0].children[0].children, // div#directory > div.keywords > table > tbody > tr
	    i, j, k, tds, divs;
	  for (i = 0; i < trs.length; i ++)
	  {
	    tds = trs[i].children; // tr > td
	    for (j = 0; j < tds.length; j ++)
	    {
	      divs = tds[j].children; // td > div
	      for (k = 0; k < divs.length; k ++)
		on(divs[k].children[0], 'click', function() {
		    _gaq.push(['_trackEvent', 'IndexClick', 'Click', 'NavLink']);
		});
	    }
	  }
	}
	// "show all cities" link
	if (document.getElementsByClassName('show-all-cities').length > 0)
	  on(document.getElementsByClassName('show-all-cities')[0].children[0], 'click', function() {
	    _gaq.push(['_trackEvent', 'IndexClick', 'Click', 'NavLink']);
	  });
      });
    }
  }
    if (document.getElementById('pf'))
  {
    var pf = document.getElementById('pf'), url = '/professional_field.php?&l=Norfolk%2C+VA&city_id=9586&url=%2Findex3.php';
    loadAsync(url, function(data){
      pf.innerHTML = data;
      if (pf.children.length > 0)
      {
        var trs = pf.children[2].children[0].children[0].children,// div#directory > div.keywords > table > tbody > tr
          i, j, k, tds, divs;
        for (i = 0; i < trs.length; i ++)
    	{
    	    tds = trs[i].children; // tr > td
    	    for (j = 0; j < tds.length; j ++)
    	    {
    	      divs = tds[j].children; // td > div
    	      for (k = 0; k < divs.length; k ++)
    	        on(divs[k].children[0], 'click', function() {
    	          _gaq.push(['_trackEvent', 'IndexClick', 'Click', 'NavLink']);
    	        });
    	    }
    	}
      }
      else // no professional field, then show hot job search section
      {
	CreateHotJobSearch();
      }
    });
  }
    CreateHotJobSearch();  if (getCookie("in_reg")) {
    delCookie("in_reg");
    setCookie("over", 1, 7);
    var url = getCookie("result");
    if (url) {
      document.getElementById("redirect_notice").style.display="";
      _gaq.push(['_trackEvent', 'Index', 'SkipReg']);
      window.location = url;
    }
  }

  if (document.getElementById('q'))
  {
    j2c_autocomplete('q', {
      url: '/q_auto.php',
      minLength: 3
    });
  }

  if (document.getElementById('l'))
  {
    j2c_autocomplete('l', {
      url: '/l_auto.php',
      minLength: 3
    });
  }
  
    document.getElementById('q').focus();
}

if (window.onpageshow || window.onpageshow === null)
  window.onpageshow = initj2c;
else
  window.onload = initj2c;
</script>

