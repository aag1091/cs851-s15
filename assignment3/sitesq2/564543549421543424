        var _ApplyCfg = {}; /* use this to store values */

        /*
        id: job id
        campaign_id
        url: dest url
        loc: job location
        */
        function jc(id, campaign_id, url, loc)
        {
            // preview test
            if ($.inArray(campaign_id, pv_campaigns) >= 0)
            {
                $.ajax({
                    url:'/job_info.php?id='+id,
                    success:function(data){
                        data = $.trim(data);
                        if (!data.length)openpage(url);
                        else{
                                                        
                            var job_info=$.parseJSON(data);
                            $('#preview').modal('show');
                            $('#preview .modal-title').html(job_info.title);
                            $('#preview .company').html(job_info.company);
                            $('#preview .city').html(loc);
                            $('#preview .jobdescription').html(job_info.desc);
                            _ApplyCfg = {URL: url};
			    $('#preview button.orange_btn').unbind();
                            $('#preview button.orange_btn').click(function() {
                                PreviewApply();
                            });
                        }
                    },
                    error: function(){
                        openpage(url);
                    }
                });
            }
            else
            {
                dataLayer.push({'event': 'click'});
                openpage(url);
            }
        }

        function openpage(url) {
            $('#preview').modal('hide');
            window.open(decodeJ2C(url));
        }

        function PreviewApply(){
            $('#preview').modal('hide');
            window.open(decodeJ2C(_ApplyCfg.URL)+"&pv=1");
        }
