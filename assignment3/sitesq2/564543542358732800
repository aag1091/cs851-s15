    function getFullId() {
      var pth = window.location.pathname;
      var exp = new RegExp("(newsid|gallery)/([0-9]{1,3}/){1,}index.html");
      if (exp.test(pth)) {
        var start = pth.indexOf("newsid/");
        if (start > -1) {
          start = start + 7;
        }
        else {
          start = pth.indexOf("gallery/") + 8;
        }
        var end = pth.indexOf("index.html");
        var newsid = pth.substring(start, end);
        newsid = newsid.replace(/\//g, "");
