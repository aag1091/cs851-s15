<script type="text/javascript">// <![CDATA[
$(document).ready(function () {


 var actionID = '1971';
//action alert count


   $.ajax({
        type: "POST",
        url: "https://secure3.convio.net/hrc/site/CRAdvocacyAPI?method=getAdvocacyAlert&api_key=T4u5tM3&v=1.0&response_format=xml&alert_id="+actionID+"&alert_type=action",
                dataType: "xml",
        success: printCount
    });



});




function printCount(xml) {
    var count = parseInt($(xml).find("alert").children("interactionCount").text());

   $("#actionCount").html(addCommas(count*2));
    };

 function addCommas(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}
// ]]></script>
