		var sesid;
		
		function submitOnEnter(evt){
			try	{
				var mlfrm=document.getElementById('login_form');
				var charCode = (evt.which) ? evt.which : event.keyCode;
				if(charCode == "13"){
					mlfrm.submit();
				}
			}
			catch(err) {
				return false;
			}
			return true;
		}
		
		function YYYYMM() {
	    var ymnow = new Date;
	    var rv = ymnow.getFullYear() * 100 + ymnow.getMonth() + 1;
	    return rv;
		}
		
		YAHOO.util.Event.onDOMReady(function() { 
		  var ctr = 0, 
		      storageEngine; 
		 
		  YAHOO.util.StorageEngineSWF.SWFURL = '/_flash/swfstore/swfstore.swf'; 		
			
			try	{
				var storageEngine = YAHOO.util.StorageManager.get(
				  YAHOO.util.StorageEngineHTML5.ENGINE_NAME,
				  YAHOO.util.StorageManager.LOCATION_LOCAL,
				  {force: false,
				   order: [
										YAHOO.util.StorageEngineHTML5,
										YAHOO.util.StorageEngineSWF
				          ]
				  }
				);
			}
			catch(err) {
				storageEngine = false;
			}
		
			if(storageEngine) { 
				var storageEngine = YAHOO.util.StorageManager.get();
				sesid = YAHOO.util.Cookie.get("PBCSSESSIONID");
			
			
				storageEngine.subscribe(storageEngine.CE_READY, function() {
					//storageEngine.removeItem('sid');
					//storageEngine.removeItem('mnth');
					var csid = YAHOO.util.Cookie.get("PBCSSESSIONID");
					var ssid = storageEngine.getItem('sid');
					var cmnth = YYYYMM();
					var smnth = storageEngine.getItem('mnth');
					
					if (!ssid) {
						storageEngine.setItem('sid', csid);
						storageEngine.setItem('mnth', cmnth);
					}	else if (ssid != csid) {
						if (!smnth) {
							storageEngine.setItem('mnth', cmnth);
						}	else if (smnth == cmnth) {
							window.location="/apps/pbcs.dll/misc?url=" + "/misc/assignSession.pbs&returnUrl=%2Fapps%2Fpbcs.dll%2Farticle%3FAID%3D%2F20150207%2FOPINION%2F150209530&sessionId=" + ssid;
						}	else {
							storageEngine.setItem('sid', csid);
							storageEngine.setItem('mnth', cmnth);				
						}
					}
				});
			}
		});
		function toggleMiniLogin() {
			var mobileFull = YAHOO.util.Cookie.get("sitePref");

			if (mobileFull == "full")
			{
				window.location.href = "/apps/pbcs.dll/misc?url=/misc/mobile/login.pbs&returnurl=%2Fapps%2Fpbcs.dll%2Farticle%3FAID%3D%2F20150207%2FOPINION%2F150209530"
			}
			else
			{
				//not a mobile user viewing full site
				var mlfrm=document.getElementById('miniLogin');
				mlfrm.style.display=(mlfrm.style.display=='block')?'none':'block';
				if (mlfrm.style.display=='block') {
					document.getElementById('user').focus();
				}
			}
		}				
