<script type="text/javascript">
	var paracount = document.getElementById('paracount');
	var paralength=paracount.getElementsByTagName('p').length;
	if (paralength>1)  
	{
		var factplace= Math.floor(paralength/2);
		var factplace=factplace-1;
		var $x=$('#paracount .article-text:eq('+factplace+')');
		$('.article-fact-box').insertAfter($x).show();
	}
	else if (paralength==1)  
	{
		var para=$('#paracount .article-text').text();
		var charcount=$('#paracount .article-text').text().length;
		var $charcountEnd=Math.floor(charcount/2);
		var strtemp = para.substring(0,$charcountEnd);
		var strtemp1 = para.substring($charcountEnd);
		var str1 = "<p class='article-text'>"+strtemp +"</p>"+"<p class='article-text'>"+ strtemp1+"</p>";
		$('#paracount').html(str1);
		var $x=$('#paracount .article-text:eq(0)');
		$('.article-fact-box').insertAfter($x).show();
	}
	else 
	{
		$('.article-fact-box').show();
	}
</script>
