		<script type="text/javascript">
			$(document).ready(function(){
				$('img').bind('contextmenu', function(e){
					return false;
				}); 
				setTheTime();
				document.getElementById("missedadaylink").href = "/section/missedaday?eventdate=" + getMissedADayDate()  + "&CalDate=" + getMissedADayDate();
				$("#fade").click(function() {
					document.getElementById('light').style.display='none';
					document.getElementById('fade').style.display='none';
				});
			});
			function getMissedADayDate(){
				var returnDate;
				var yesterday = new Date().add(-1).day();
				returnDate = yesterday.toString("yyyyMMdd");
				return returnDate;
			}
			function setTheTime(){
				var now = new Date();
			var hours = now.getHours();
			if(hours == 0){
				hours = 12;
			}
			if(hours > 12){
				hours = hours - 12;
			}
			var dateTimeString = now.toString("dddd, MMMM d, yyyy - ") + hours + ":" + now.toString("mm tt");
			$("#current_time").html(dateTimeString);
			}
		</script>
