                                </div>
                            </form>
                        </div>
                    </div>
                    <div id="overlay_site"></div>
                                                                        </div>
            <script type="text/javascript">
        //<![CDATA[
        $(document).ready(function() {

            var resizeTimer;
            var http_img = "http://statics.lfp.es/img/fondo_estadio_01";

            resize();

            $(window).bind('resize', function() {
                clearTimeout(resizeTimer);
                resizeTimer = setTimeout(resize, 100);
            });

            function resize() {
                var ancho = $("#fondo").width();
                if (ancho >= 1400) {
                    $("#fondo").html("<img src='" + http_img + "_1920.jpg' alt='' />");
                } else {
                    if (ancho >= 1100) {
                        $("#fondo").html("<img src='" + http_img + "_1366.jpg' alt='' />");
                    } else {
                        if (ancho >= 500) {
                            $("#fondo").html("<img src='" + http_img + "_1024.jpg' alt='' />");
                        } else {
                            $("#fondo").html("<img src='" + http_img + "_480.jpg' alt='' />");
                        }
                    }
                }
            }

        });
        //]]>
    </script>
