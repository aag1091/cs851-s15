    <input type="hidden" name="temporada" value="temporada-2014-2015" />
    <input type="hidden" name="competicion" value="liga-bbva" />
    <input type="hidden" name="jornada" value="22" />
    <input type="hidden" name="partido" value="athletic_barcelona" />
</form>
<script type="text/javascript" language="javascript">
    var intervaloDeRecarga = parseInt('60');
    function lpad(s, l) {
        return(l.substr(0, (l.length - s.length)) + s);
    }
    function cuentaAtras() {
        if (intervaloDeRecarga >= 0) {
            document.getElementById('tiempo').firstChild.nodeValue = lpad(intervaloDeRecarga.toString(), '00');
            intervaloDeRecarga--;
            setTimeout('cuentaAtras()', 1000);
        } else {
            timerEvent();
        }
    }
    function timerEvent() {
        try {
            jQuery("#loading_home").show();
            var get_comp = 'liga-bbva';
            jQuery.ajax({
                type: 'GET',
                url: "/includes/ajax.php?action=reload_minuto_minuto",
                data: {id_partido_directo: '1515', fecha: '2015-02-08', temporada: jQuery("input[name=temporada]").val(), competicion: jQuery("input[name=competicion]").val(), jornada: jQuery("input[name=jornada]").val(), partido: jQuery("input[name=partido]").val(), get_competicion: get_comp},
                success: function (data) {

                    obj = JSON.parse(data);

                    jQuery("#contenedor_partidos_disputados").empty().html(obj.slider);
                    jQuery('#mycarousel').jcarousel({
                        scroll: 7,
                        start: parseInt(jQuery("#mycarousel .resultado_partidos_disputados.current").parent().index() - 2)
                    });
                    jQuery("#top_izquierda").empty().html(obj.partido);
                    jQuery("#clasificacion_directo").find("a").removeClass("pestanya_inactiva");
                    jQuery("#clasificacion_directo #box_iconos_liga_bbva_adelante a").not("#icono_" + get_comp.replace("-", "_")).addClass("pestanya_inactiva");

                    jQuery("#box_titulo_contenedor_clasificacion").empty().html(obj.clasif);
                    jQuery("#loading_home").hide();
                    jQuery('#incidencias_directo_partido').jScrollPane({animateScroll: true});
                }
            })

        } catch (error0) {
        }
        intervaloDeRecarga = parseInt('60');
        cuentaAtras();
    }
    cuentaAtras();

    $(".actualizar").click(function (e) {
        e.preventDefault();
        intervaloDeRecarga = 0;
        jQuery("#loading_home").show();

    })

</script>
<script type="text/javascript" src="http://statics.lfp.es/js/scroll.js?2"></script>
<script type="text/javascript">
    jQuery(document).ready(function ($) {
        jQuery('#incidencias_directo_partido').jScrollPane({animateScroll: true});
    });
</script>
