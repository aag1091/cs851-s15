	<script type="text/javascript">
(function() {
    new as.RandomizedContentDisplay('#watch-trio-lockup', {
        contentSelector: '.watch-trio-random-content'
    });

    var $ = jQuery;
    $(function() {
        // -------------------- CONFIGURATION --------------------
        var id = "watch-trio-lockup";
        var videoWait  = [0, 2666, 5666];
        var animators  = '.hero,.hero-header,.info';

        // -------------------- CODE --------------------
        var animationEnd = 'animationend webkitAnimationEnd oanimationend ' +
                           'MSAnimationEnd';
        var videoFn = function(vid, nm) {
            if (vid[nm] && typeof vid[nm] === 'function') {vid[nm]();}
        };
        var $b    = $('#' + id);
        var $imgs = $b.find('img');
        var $vids = $b.find('video');
        var $anims= $b.find(animators);

        //show images instead of videos for ie 8 and iOS
        if (($.AsSupport.isIe && $.AsSupport.ieVersion < 9) ||
                $.AsSupport.isIpad || $.AsSupport.isMobileIos) {
            $vids.each(function(i, el) {$(el).remove();});
            return;
        }

        //when all images load => load each video
        $.when.apply($, $imgs.map(function(i, el) {
            if (el.src) {
                var def = $.Deferred();
                var img = new Image();
                img.onload = function () {def.resolve();}
                img.src = el.src;
                return def.promise();
            }
            return true;

        //load videos in series, waiting for metadata to load
        })).then(function() {
            var waitTime = 200;
            var vidsLoad = 0;

            $vids.each(function(i, el) {
                $(el).on('loadedmetadata stalled', function(e) {
                    if (e.type === 'loadedmetadata') {
                        if (el) {el.autoplay = false;}
                        videoFn(el, 'pause');
                        if (el) {el.currentTime = 0;}
                    }
                    if (e.type === 'loadedmetadata' && i < vidsLoad &&
                            i < $vids.length-1) {
                        vidsLoad += 1;
                        waitTime = 200;
                        videoFn($vids[i+1], 'load');
                        $vids[i+1].autoplay = true;

                    } else if (e.type === 'stalled' && i < vidsLoad) {
                        waitTime = Math.min(waitTime * 2, 10000);
                        setTimeout($.proxy(videoFn, 0, el, 'load'), waitTime);
                    }
                });
                
            });
            
            vidsLoad += 1;
            videoFn($vids[0], 'load');
            if ($vids[0]) {$vids[0].autoplay = true;}
        });

        //when all videos are finished playing, restart
        var playAll = function playAll() {
            $vids.each(function(i, el) {
                window.setTimeout($.proxy(videoFn,0,el,'play'),
                                  videoWait[i % videoWait.length]);
            });
        };

        //when all video load and all animations finish => play the first video
        $.when.apply($, $vids.map(function(i, el) {
            var def = $.Deferred();
            $(el).on('canplaythrough', $.proxy(def.resolve, def));
            return def.promise();

        }).add($anims.map(function(i, el) {
            var def = $.Deferred();
            $(el).one(animationEnd, $.proxy(def.resolve, def));
            if ($.AsSupport.isIe && $.AsSupport.ieVersion <= 9){def.resolve();}
            return def.promise();

        }))).then(function() {
            $b.addClass('billboard-of-watches-ready');
            playAll();
        });
    });
})();
</script>
</div>

        
        
        
        
        
        
        
        
        
    </div>

