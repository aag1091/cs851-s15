  loadScriptAsync("//platform.twitter.com/oct.js", "twitteroct")
  var timer;
  timer = every(500, function() {
    if ((typeof twttr !== "undefined" && twttr !== null) && (twttr.conversion != null)) {
      clearInterval(timer);
      return twttr.conversion.trackPid('l4o6d');
    }
  });
</script>
