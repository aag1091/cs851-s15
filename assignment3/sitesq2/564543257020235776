<script>
CN.dartCalls = function(){
    if(CN.isMobile){
        if(document.getElementById("header320x50_frame"))CN.dart.call('header', {sz:'320x50', kws:["top"]});
                if(document.getElementById("m_polar_article_b150x150_frame"))CN.dart.call('m_polar_article_b', {sz:'150x150', kws:[]});
                if(document.getElementById("polar_article150x150_frame"))CN.dart.call('polar_article', {sz:'150x150', kws:[]});
                if(document.getElementById("bottomBanner320x51_frame"))CN.dart.call('bottomBanner', {sz:'320x51', kws:["bottom"]});
    }else{
        if(document.getElementById("header728x90_frame"))CN.dart.call('header', {sz:'728x90', kws:["top"]});
                if(document.getElementById("polar_article_b650x200_frame"))CN.dart.call('polar_article_b', {sz:'650x200', kws:[]});
                if(document.getElementById("polar_article150x150_frame"))CN.dart.call('polar_article', {sz:'150x150', kws:[]});
                if(document.getElementById("defaultAd300x250_frame"))CN.dart.call('defaultAd', {sz:'300x250', kws:["top"]});
                if(document.getElementById("articleBottom300x250_frame"))CN.dart.call('articleBottom', {sz:'300x250', kws:["bottom"]});
                if(document.getElementById("monoTop200x45_frame"))CN.dart.call('monoTop', {sz:'200x45', kws:[], collapse: true});
    }
    
};

if((typeof CN.dart !== "undefined")&&(typeof CN.dartConfig !== "undefined")) CN.dartCalls();
</script>



































    











    













