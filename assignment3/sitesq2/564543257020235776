    if ((typeof pageAds !== 'undefined') && (typeof pageAds[placementName] !== 'undefined') && pageAds[placementName].indexOf('No creatives found') === -1 && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById(placementName);

        

        jQuery("#AMS_TNV_GLOBAL_NAVBAR_ROLLOVER_COLLAPSE").html(pageAds[placementName].replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
    }

    
        var callback = { func: CN.ecom.collapseAd.init,params: ['collapseAd'] };
        if(typeof(callback.func) === "function"){
            callback.func.call(CNP.ecom, callback.params);
        }else{
            setTimeout(function(){
                callback.func.call(CNP.ecom, callback.params);
            }, 500);
        }
    
})(window.pageAds);

