     <script>

         /*
          *  CN Object
          *  As JavaScripts begin to load, their promises will be publicly accessible
          *  so that callbacks may be defined.
          */
        CN.promises = {

                scripts: {},
                render: {}
             },
        CN.site.name = 'teenvogue',
        CN.page.type = 'ArticleV2',
        CN.masonry = {
                 refresh : function() {}
             };


        /*
         *  The DOM is loading so fast that ad iframes are setting document.domain before
         *  CN.site.init, causing security errors in IE.  This is why we set it here and now.
         */
        try {
            document.domain = location.hostname.split('.').slice(-2).join('.');
        }
        catch(e) {
            CN.exceptions = [e];
        }
         /*
          *  Promotheus init
          *  Sets up the promotheus url to be fetched.
          */
        window._ghearst_vars = (function() {
        var
            metaKeyWords = (function(){
                var
                    i                  = 0,
                    metaKeywordContent ='',
                    nameAttribute      = null,
                    metaCollection     = document.getElementsByTagName('meta');

                for (i = 0; i < metaCollection.length; i++) {
                    nameAttribute = metaCollection[i].name.search(/keywords/);
                    if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/,/g,'%2C'); // to replace ,
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                    }
                }
                return metaKeywordContent;
            })();
        return {
            'ams_ads_script_src': 'http://subscribe.teenvogue.com/ams/page-ads.js?ad_category_prefix=beauty&browser_path=%2Fbeauty%2Fcelebrity-beauty%2F2015-01%2Fselena-gomez-short-hairstyle%2F&cat_prefixes=%2Cbeauty%2Ccelebrity-beauty%2C2015-01%2Cselena-gomez-short-hairstyle%2C&keywords='+metaKeyWords+'&position_list=AMS_TNV_GLOBAL_PROMOTOP%2CAMS_TNV_MOBILE_PROMOTOP%2CAMS_TNV_CONTENTPAGE_ZRAIL%2CAMS_TNV_GLOBAL_FOOTER%2CAMS_TNV_MOBILE_GLOBAL_FOOTER%2CAMS_TNV_FOOTER_SUBSCRIBE%2CAMS_TNV_GLOBAL_INSIDER_HEADER_FULL%2CAMS_TNV_GLOBAL_NAVBAR_ROLLOVER%2CAMS_TNV_GLOBAL_NAVBAR%2CAMS_TNV_MOBILE_GLOBAL_NAV%2CAMS_TNV_GLOBAL_HEADER_FULL%2CAMS_TNV_GLOBAL_HEADER_COLLAPSE%2CAMS_TNV_GLOBAL_HEADER%2CAMS_TNV_LITEREG_SUBSCRIBE%2CAMS_TNV_MOBILE_LITEREG_SUBSCRIBE%2CAMS_TNV_POPUP_EXIT%2CAMS_TNV_POPUP_FLOATINGAD%2CAMS_TNV_POPUP_ENTRY%2CAMS_TNV_CONTENTPAGE_ZRAIL%2CAMS_TNV_CONTENTPAGE_TEXTOFFER%2CAMS_TNV_GLOBAL_NAVBAR_ROLLOVER_COLLAPSE&site_prefix=teenvogue&subdomain=www&url_name='
        };
    })();

        /*
         *  Asynchronous script loading
         *  Use LAB.js to load resources in parallel, utilizing wait() to pause
         *  for dependencies and configuration.
         */
        
        


















    
        
    
    



    $LAB.setOptions({AlwaysPreserveOrder : true})


    .script(function() {
    if(CN.isMobile) {
        return ['/js/teenvogue_ads_core_mobile.js'];
    }else {
        return ['/js/teenvogue_ads_core.js'];
    }
})

    .wait(function () {
                    setTimeout(function () {
                        
        var adSite = (CN.isMobile) ? 'teenvogue.mobile.dart' : 'teenvogue.dart';

        CN.dartConfig = {
            site: adSite,
            zone: 'beauty;',
            kws:[ "selena-gomez-short-hairstyle","2015-01","beauty","articlev2","celebrity-beauty"],
            charmap : {' ' : '+', '-' : '_'},
            gptCallback: function(){ jQuery(document).trigger('adsRendered')}
        };

        if(CN.site && CN.site.currentSection) CN.dartConfig.contentType = CN.site.currentSection;
        if(CN.isMobile) CN.dartConfig.device = "mobile";
        CN.dart.init(CN.dartConfig);
        if(typeof CN.dartCalls === "function") CN.dartCalls();
    
                    }, 500);
                })


        

        


















    
        
    
    



    $LAB.setOptions({AlwaysPreserveOrder : true})


    .script(function() {
    if(CN.isMobile) {
        return ['/js/teenvogue_core_mobile.js'];
    }else {
        return ['/js/teenvogue_core.js'];
    }
})

    .wait(function() {
                    CN.promises.scripts = {
                        base       : new jQuery.Deferred(),
                        ads        : new jQuery.Deferred(),
                        ecom       : new jQuery.Deferred(),
                        stats      : new jQuery.Deferred(),
                        site       : new jQuery.Deferred(),
                        brightcove : new jQuery.Deferred(),
                        typekit    : new jQuery.Deferred(),
                        livefyre   : new jQuery.Deferred(),
                        last       : new jQuery.Deferred(),
                        thirdParty : new jQuery.Deferred()
                    };

                    CN.promises.render = {
                        ecom       : new jQuery.Deferred()
                    };
                    CN.promises.scripts.base.resolve();
                    CN.response.lazyLoadImg();
                    CN.promises.scripts.site.resolve();

                })



       


















    
    




    .script(['//fonts.condenast.com/dge5ldm.js'])

    .wait(function() {
                    CN.promises.scripts.typekit.resolve();
                })



        



.wait()
.script([

