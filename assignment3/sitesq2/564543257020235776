                        // logic to fire device based targetters
                        var i, targets,
                            acceptable = CN.isMobile,
                            url = window._ghearst_vars.ams_ads_script_src;
                            targets = CN.url.params('position_list', url).split(/%2c/i);
                            for(i = targets.length-1; i >= 0; i--) {
                                if(/^AMS_TNV_MOBILE/.test(targets[i]) !== acceptable) {
                                    targets.splice(i, 1);
                                }
                            }
                            // reconstruct the url
                            collectible = window._ghearst_vars.ams_ads_script_src.split(/&/);
                            for(i=0; i<collectible.length; i++) {
                                if(/^position_list/i.test(collectible[i])) {
                                    collectible[i] = 'position_list='+targets.join('%2C');
                                    break;
                                }
                            }
                            CN.debug.info('ECOM TARGETTERS', targets);
                            return [collectible.join('&'), '/js/teenvogue_ecom.js'];
                    }())).wait(function() {
                            CN.promises.scripts.ecom.resolve();
                    });
                
            });

