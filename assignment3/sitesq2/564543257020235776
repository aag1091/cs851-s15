             <header class="story-hed">

                
                
                  
                
               
                
                    
                    








        
            <h1 itemprop="headline" class="content-headline">Selena Gomez Debuts a Waaaay Shorter 'Do</h1>
        
    


                    
                
           </header>

            
            <span class="response-zone" data-display-mode="full" data-display-zone="zone2"></span>

        <div class="main-column">
                
                
                    
                        
                        





 
    
         
            
                <h2 class="sub-header"><span itemprop="description">It's the year of the lob, we're telling you.</span></h2>
            
        
  



                        
                    
                    
                         
                         














    
    

    

    

    
        
            
        
        
            
            
            
        
    
    

    
    
    

    


    

    

    









    

    
        <div class="byline">
            <div class="contributors"><div class="contributor-type first last"><span class="contributor"><strong class="label">by </strong><span itemprop="author" class="name"><a href="/contributors/kathryn-amey">Kathryn Amey</a></span><span class="contributor-divider"></span><span class="contributor-type-divider"></span></span></div></div>
        </div>
    



    










                        
                    
                



            
            

                
                

                
                <div class="social-bar-v2 mobile-bar">
                





























































    <ul class="utilities utilities-narrow">

        
            
                   
                   <li class="utility-livefyre-comment"><span class="livefyre-commentcount" button-action="javascript:;" data-lf-site-id="" data-lf-article-id=''></span></li>
                   
                
            
            
                






            
        
            
