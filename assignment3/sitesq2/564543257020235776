    <script>
    /*
     * INITIALIZATION
     * Consolidate all invocations, instantiations, and initializations here
     *
    */
    (function initialize(window, CN) {
        var timer;

        function init() {


            if (!CN.promises.scripts.base) {
                return false;
            }

            clearInterval(timer);

            CN.promises.scripts.base.done(function() {
                
    CN.config.set({
        title        : document.title,
        url          : 'http://www.teenvogue.com/beauty/celebrity-beauty/2015-01/selena-gomez-short-hairstyle',
        description  : "Posing in mirrored sunnies and a stylish pink coat, Selena showed off her brand spanking new long, layered bob.",
        keywords     : "",
        facebook     : {pageid: "6636341311"}
    });


            window.platformEnvironment = "production";
            CN.site.init({
                env: 'PROD',
                code: 'TNV',
                title: 'Teen Vogue',
                name: 'teenvogue',
                alias: 'Teenvogue.com',
                tld: 'teenvogue.com',
                eventTrackingUrl: 'http://result.teenvogue.com',
                eventTrackingEnv: 'PROD',
                userServiceUrl: 'http://user-service.condenast.com',
                userServiceKey: 'ncuN8VodX2PiRLdTL2fXv8xIWFM=',
                ecomUrl: 'https://subscribe.teenvogue.com/api/circ/xml/Transaction',
                mobilecompatible: true,
                httpStatusCode: 200
            });

            CN.config.set({
                facebook : {},
                reg: {
                    urls: {
                        login: '/user/login',
                        reg:   '/user/registration'
                    }
                },

                tweet : {
                    source : "TeenVogue"
                }
            });

            CN.page.type = 'ArticleV2';
            

            
    
    CN.config.set({
        defaultHost : "www.teenvogue.com",
        secureHost : "secure.teenvogue.com",
        isSecure : location.protocol == "https:" ? true : false
        });

            });
            CN.promises.scripts.stats.done(function() {

                


                CN.stats.omniture.setStatus(200);
        if (CN.isMobile) {
            CN.stats.omniture.setMobileView();
        }else {
            CN.stats.omniture.setMobileView('Non-Mobile');
        }
        
               CN.stats.omniture.doPageTracking(); // does init and also tracks
            });
            CN.promises.scripts.ads.done(function() {

                if (CN.isMobile) {

                } else {

                    jQuery('.displayAd','.racetrack-holder').each(function() {
                        jQuery(window).on('CN.customEvents.dartResize.' + jQuery(this).attr('id').replace(/_frame/gi,''), function(e, name, selector, ad) {
                            setTimeout('CN.masonry.refresh()', 3000);
                        });
                    });

                }
                


            });
            CN.promises.scripts.ecom.done(function() {
                


                try {
                    (function(pageAds){
    var
        placementName = 'AMS_TNV_GLOBAL_PROMOTOP',
        div = null;

    
        if (CN.isMobile) {
        
                // No mobile targetters for this placement
            
        }
    

    if ((typeof pageAds !== 'undefined') && (typeof pageAds[placementName] !== 'undefined') && pageAds[placementName].indexOf('No creatives found') === -1 && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById(placementName);

        

        jQuery("#AMS_TNV_GLOBAL_PROMOTOP").html(pageAds[placementName].replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
    }

    
})(window.pageAds);

(function(pageAds){
    var
        placementName = 'AMS_TNV_MOBILE_PROMOTOP',
        div = null;

    
        if (CN.isMobile) {
        
                // No mobile targetters for this placement
            
        }
    

    if ((typeof pageAds !== 'undefined') && (typeof pageAds[placementName] !== 'undefined') && pageAds[placementName].indexOf('No creatives found') === -1 && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById(placementName);

        

        jQuery("#AMS_TNV_MOBILE_PROMOTOP").html(pageAds[placementName].replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
    }

    
})(window.pageAds);

