            <script type='text/javascript'>
            function readCookie(e) {
                var t = e + "=";
                var n = document.cookie.split(";");
                for (var r = 0; r < n.length; r++) {
                    var i = n[r];
                    while (i.charAt(0) == " ")i = i.substring(1, i.length);
                    if (i.indexOf(t) == 0)return i.substring(t.length, i.length)
                }
                return null
            }

            function createMeta(name, content) {
                var meta_user_type = document.createElement('meta');
                meta_user_type.name = name;
                meta_user_type.content = content;

                return meta_user_type;
            }

            function isNotEmptyObject(objt) {
                for (var prop in objt) {
                    return true;
                }
                return false;
            }

            var gwa_cookie = readCookie("fwi-auth");

            if (gwa_cookie && isNotEmptyObject(gwa_cookie)) {
                document.getElementsByTagName('head')[0].appendChild(createMeta('gwa_authStatus', 'logged in'));

                var gwa_data = decodeURIComponent(gwa_cookie).split('|')[0];
                var gwa_data_object = JSON.parse(gwa_data);

                var gwa_user_type = gwa_data_object.subscription_status ? gwa_data_object.subscription_status : '';
                var gwa_user_guid = gwa_data_object.Uid ? gwa_data_object.Uid : '';
                var gwa_user_entitlements = gwa_data_object.entitlements ? gwa_data_object.entitlements : '';
                if (gwa_user_type) {
                    document.getElementsByTagName('head')[0].appendChild(createMeta('gwa_userType', gwa_user_type));
                }
                if (gwa_user_guid) {
                    document.getElementsByTagName('head')[0].appendChild(createMeta('gwa_userID', gwa_user_guid));
                }
                if (gwa_user_entitlements) {
                    document.getElementsByTagName('head')[0].appendChild(createMeta('gwa_userEntitlement', gwa_user_entitlements));
                }
            } else {
                document.getElementsByTagName('head')[0].appendChild(createMeta('gwa_authStatus', 'logged out'));
            }
        </script>
    
