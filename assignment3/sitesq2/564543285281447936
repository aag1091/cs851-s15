		//menu code
		function pmc_show_dropdown_menu() {
			var submenu = jQuery(this).find(".drop");

			if ( submenu.length > 0 ) {
				submenu.show();

				//track event when any mega menu is shown
				_gaq.push([ '_trackEvent', 'mega-menu-performance', 'view', jQuery(this).find("strong a").text(), 0, false ]);
			}
		}

		function pmc_hide_dropdown_menu() {
			var submenu = jQuery(this).find(".drop");

			if ( submenu.length > 0 ) {
				submenu.hide();
			}
		}

		jQuery(document).ready(function() {
			//track availability of mega menu
			_gaq.push([ '_trackEvent', 'mega-menu-performance', 'available', '', 0, true ]);

			jQuery(".menu-item").hoverIntent({
				sensitivity: 7, // number = sensitivity threshold (must be 1 or higher)
				interval: 300,	 // number = milliseconds of polling interval
				over: pmc_show_dropdown_menu,
				out: pmc_hide_dropdown_menu
			});

			//track clicks on mega menu
			jQuery(".menu-item .drop .drop-holder .container section a").on( "click", function() {
				var menu_item = jQuery(this).parentsUntil(".menu-item").parent();
				_gaq.push([ '_trackEvent', 'mega-menu-performance', 'click', menu_item.find("strong a").text(), 0, false ]);
			} );
		});
	</script>
