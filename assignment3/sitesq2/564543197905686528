    <script type="text/javascript">
    DM.later('bundle', function (){
        DM.SHOUT = {};
        DM.SHOUT.shoutChannels = '{ "disabledMolShout": false, "disabledChannels": [] }';
    	DM.SHOUT.isOldArticle = true;
        DM.has("reader-comments", "ReaderComments", {
            gplusClientId: '746589970956-e10ciaf67a1id9ggu75ph3ds0sthn7j6.apps.googleusercontent.com',
