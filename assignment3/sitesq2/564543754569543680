							jQuery(function($){
								var shownCount = 1, slideSize = 0, viewportSize = 0, left = 0;
								
								$.getJSON("/sports/ajax/editors-picks", function(response){
									if( response.status == "success" ){
										$("#editorspicks ul").html(response.html);
										
										$("#editorspicks ul li").each(function(i){
											var thumbSize = parseInt($(this).css("width")) + parseInt($(this).css("margin-right"));
											
											if( i < shownCount ){ viewportSize += thumbSize; }
											
											slideSize += thumbSize;
										});
									}
								});
								
								$("#ep-slide-nav-prev, #ep-slide-nav-next").click(function(e){
									e.preventDefault();
									
									if( $(this).attr("id") == "ep-slide-nav-prev" ){
										if( left < 0 ){
											left = left + viewportSize;
										} else {
											left = 0 - slideSize + viewportSize;
										}
									} else if( $(this).attr("id") == "ep-slide-nav-next" ){
										if( left - viewportSize > 0 - slideSize ){
											left = left - viewportSize;
										} else {
											left = 0;
										}
									}
									
									$("#editorspicks ul").animate({"left" : left + "px"}, 100);
								});
							});
						</script>
