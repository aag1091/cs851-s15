<script type="text/javascript">
    var cX = cX || {}; cX.callQueue = cX.callQueue || [];

    function no_ad( a )
    {
        if( a.matchedAdCount == 0 ) {
            console.log("# No defined advertisements");
        }
    }

    function backfillAdvertisement( a )
    {
        if( a.matchedAdCount == 0 ) {
            cX.callQueue.push(['insertAdSpace', {
                adSpaceId:'000000015fc4b29e',
                insertBeforeElementId: 'expandingAdTarget',
                resizeToContentSize: true,
                width: 300, height: 250,
                clickTargetFrame:'_blank',
                ps: 1, // One ad
                renderTemplateUrl: 'http://cdn.cxpublic.com/BannerAd_300x250.html',
                onImpressionResult: no_ad
            } ]);
            console.log("# All section backfills");
        } else {
            window.document.getElementById("impression_tracker").src = "http://ads.allaccess.com.ph/deliveredir.nmi?sid=0003&zoneid=99990";
            console.log("AdRoit Tracker");
        }        
    }

    cX.callQueue.push(['insertAdSpace', {
        adSpaceId: '00000001604d3797',
        insertBeforeElementId: 'expandingAdTarget',
        resizeToContentSize: true,
        width: 300, height: 250,
        clickTargetFrame:'_blank',
        onImpressionResult: backfillAdvertisement,
        renderTemplateUrl: 'http://cdn.cxpublic.com/BannerAd_300x250.html'
    }]);

</script>


















