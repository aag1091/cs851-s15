	function is_touch_device() {  
	  try {  
		document.createEvent("TouchEvent");  
		return true;  
	  } catch (e) {  
		return false;  
	  }  
	}
	
	var touchy=false;
	
	if(is_touch_device()){
		document.write('<link rel="stylesheet" type="text/css" href="http://www.friendshipcircle.org/blog/wp-content/themes/fc-blog/css/touch.css" />');
		touchy=true;
	}
</script>

