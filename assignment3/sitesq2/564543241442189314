        <script>
		var info_display = document.getElementById("display_infographics");			
		var info_mask = document.getElementById("info_mask");
		var info_graph = document.getElementById("info_graph");	
		
		info_graph.onclick=function()
		{
			document.cookie = "mwc15stats = 1;expires = Fri, 31 Dec 9999 23:59:59 GMT;domain=.mobileworldcongress.com;path = /;";					
			info_mask.className = info_mask.className + " faded_info";
			info_graph.className = info_graph.className + " faded_info";
			setTimeout(function(){
				info_mask.className = info_mask.className.replace("faded_info","info_none");
				info_graph.className = info_graph.className.replace("faded_info","info_none");
			},300);	
			return false;			
		}
				
		info_display.onclick=function()
		{	 
			info_mask.className = info_mask.className.replace("info_none","");
			info_graph.className = info_graph.className.replace("info_none","");
			return false;				 
		}	
				
		</script>
		
