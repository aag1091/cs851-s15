            <div class="container"> 
                 
        		<div class="featured_holder">                	    
        			<div class="featured_image">                    	
						<img width="800" height="400" src="http://cdn.mobileworldcongress.com/wp-content/uploads/header_twitter.jpg" class="attachment-full wp-post-image" alt="header_twitter" />						
                        <div class="honeycomb"></div>
