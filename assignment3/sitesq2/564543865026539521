<script>

$(window).bind("load scroll resize", function() {

	if( ($("body").width() < 884 ) ){

		$('#responsive_bg').removeClass('app_menu');

		$('#responsive_bg').addClass('app_menu');

		

		$('#red_bg').removeClass('desk_menu');

		$('#red_bg').addClass('desk_menu');

		responsivemenuf();

		

	}else {

		$('#red_bg').removeClass('desk_menu');

		$('#responsive_bg').removeClass('app_menu');

	}

});

function responsivemenuf(){

	$('.menu-link').click(function() {

		$(".desk_menu").slideDown(700);   

	  return false;

	}), $("html").click(function() {

	 //do nothing if hovered over

	}, function(){

		$(".desk_menu").slideUp(500);   

	});

	

	/*$(".menu-link").click(function(){

    	$(".desk_menu").toggle('slow');

  	});*/

}

$(document).ready(function(){

  $("#searchdiv").click(function(){

    $("#the_options").toggle('slow');

  });

 

