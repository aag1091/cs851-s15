     var n           = players_arr.length;
    
     
     jQuery(document).ready( function() { 
    jQuery('a.playbiogshowpop-uplink').hover(function() {
		
		var pid = parseInt(this.getAttribute('data-playerid'));


		jQuery("#dialog").dialog({ width:350, height:250, zIndex: 3999, closeText: 'hide' }).parent().find('.ui-dialog-titlebar-close').hide();
	    //jQuery("#dialog").dialog('open');
	    
		jQuery("#dialog").dialog('close');
		
		jQuery("#dialog").dialog({autoOpen: false});
    	
    	var plinfo = '<div class="playcontainer">';
    	for(k = 0; k < n; k++){
        	
        	if(players_arr[k]['plid'] == pid){
            	
            	//var playimg = jQuery('div#playerlist_' + pid + ' div.playerphoto img').attr('src');
            	
            	var plfullname = players_arr[k]['knownas'] + ' ' + players_arr[k]['surname'];
            	
            	plinfo += '<div class="playimg" style="float:left;display:inline;margin-top:2px;margin-left:2px;margin-right:6px;"><img src="' + players_arr[k]['plimg'] + '" alt="' + plfullname + '"></div>';
            	plinfo += '<div class="playdetails" style="display:inline;">';
            	plinfo += '<div class="name">' + plfullname + '</div>';
            	plinfo += '<table><tbody>'
            	plinfo += '<tr class="row"><td class="heading">DOB</td><td>' + players_arr[k]['dob'] + '<td></tr>';
            	plinfo += '<tr class="row odd"><td class="heading">Birthplace</td><td>' + players_arr[k]['birthplace'] + '</td></tr>';
            	plinfo += '<tr class="row"><td class="heading">Height</td><td>' + players_arr[k]['height'] + '</td></tr>';
            	plinfo += '<tr class="row odd"><td class="heading">Weight</td><td>' + players_arr[k]['width'] + '</td></tr>';
            	plinfo += '<tr class="row"><td class="heading">Position</td><td>' + players_arr[k]['position'] + '</td></tr>';
            	
            	
            	if(players_arr[k]['club'] != ""){
            		plinfo += '<tr class="row odd"><td class="heading">Club</td><td>' + players_arr[k]['club'] + '</td></tr>';
            	}

            	plinfo += '<tr class="row"><td class="heading">Played</td><td>' + players_arr[k]['caps'] + '</td></tr>';
            	plinfo += '<tr class="row"><td class="heading">Points</td><td>' + players_arr[k]['points'] + '</td></tr>';
            	
            	plinfo += '</tbody></table></div>'; 
            }
    	}	
       plinfo = plinfo + "</div>";
       jQuery('div#dialog #popup-in').html(plinfo);
       
       
       return false;
        
    }, function(){
    	jQuery("#dialog").dialog('close');
    	
    }); 

     });

</script></div>
<!-- end of include --></div>
<!-- end of stats_playpage rhinos_dynamic_dropdown na--></div>
