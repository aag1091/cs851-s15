//<![CDATA[
jQuery(document).ready( function() {
    fademenu = '';
    //HOVER CONTROL
            jQuery("div[id*='slidecontrol-']").hover(function(){
            var overlay = jQuery(this).find('.horizontal-menu-overlay');
            if(overlay[0]){
                if(fademenu === '1'){
                    overlay.fadeIn();
                } else {
                    overlay.show();
                }
            }
        },function(){
            jQuery(this).find('.horizontal-menu-overlay').hide();
        });
        //CLICK CONTROL
              jQuery("div[id*='clickcontrol-']").bind("click", function(){
             var overlay = jQuery(this).find('.horizontal-menu-overlay');
             if(overlay[0]){
                 overlay.on("click", function(e){ e.stopPropagation(); });
                 if(fademenu === '1'){
                    overlay.fadeToggle();
                 } else {
                     overlay.toggle();
                 }
                 return false;
             }
         });
      }); // end document.ready
//]]>
</script>
