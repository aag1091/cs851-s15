<script type="text/javascript">
//<![CDATA[
jQuery.noConflict();
jQuery(document).ready( function() {
    
    var fadeMenu = '0';
    var slideMenu = '0';

    if (fadeMenu === '1' || slideMenu === '1') {
        var cont = jQuery('.nav_multilevel17873 ul.level1');
        var contOrigTop = parseInt(cont.css("top"), 10);
        var contStartPos = contOrigTop + 10;
        
        jQuery(".nav_multilevel17873 .level0 li").hover(function () {
            var overlay = jQuery(this).find('ul.level1');
            overlay.hide();
            if (overlay[0] && fadeMenu === '1') {
                overlay.fadeIn();
            } else if (overlay[0] && slideMenu === '1') {
                overlay.css("top", contStartPos);
                overlay.css("opacity", "0");
                overlay.show();
                overlay.animate({top: contOrigTop, opacity: 1}, 500);
            }
        }, function () {
            jQuery(this).find('ul.level1').hide();
            if (slideMenu === '1') {
                cont.stop(true, true);
            }
        });
    }
}); // end document.ready
//]]>
</script>
