	            var qStr = "commitmentTerm="+leaseTerm+"&showMoreListSize=12"; 
                if (typeof (Storage) !== "undefined"  && leaseTerm !="") { // check if web storage is browser supported 
                 window.localStorage.setItem(ATT.listPage.getCookieName(),ATT.util.getCookie("SHOPSESSIONID") + ";"+qStr); 
                 } 
 						ATT.listPage.generateFilterTokens(); 
                        ATTDF.load(); 
            } 
		if ((/(UP)/).test(buyflow)) { 
			if(selectedFilters == 0) 
			{ 
