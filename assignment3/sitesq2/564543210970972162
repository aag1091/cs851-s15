			   var $r = jQuery("div[data-teaserid]"), teaser =[], i = $r.length, href = location.href,temp, 
			   prodid, urlpart1, urlpart2, fullurl, fullpath; 
			   while(i--){ 
				   teaser[i] = $($r[i]).data('teaserid');// jQuery($r[i]).data('cqpath'); 
				   if($('head > meta[name="DCSext.wtCQDynId"]').attr('content')===teaser[i]){ 
								 $('head > meta[name="DCSext.wtCQDynId"]').remove(); 
				   } 
			   } 
			   ATT.log("teaser length:" + teaser.length); 
				 teaser = teaser.join('|'); 
			ATT.log(teaser); 
