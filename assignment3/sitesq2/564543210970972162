        if(authCustRedn) { 
            $.colorbox({html:'<div class="modalHeader"><h1>Important Information</h1><a href="#Close" class="closeModal">Close</a>'+ 
                             '</div><div style="padding:25px;"><div><p>Please add a device to your cart to continue.</p></div>', width:520, onComplete:function(){modalUtils.init();jQuery.colorbox.resize();}}); 
        } 
        if(getURLParam("commitmentTerm")) 
        	jQuery("input[type=radio][class=listFilterGroup]:checked").click(); 
    }); 
    ATT.namespace('DeviceList'); 
    ATT.DeviceList = (function ($) { 
    	var getParameterByName; 
    	getParameterByName = function (name) { 
    		return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null; 
        }; 
        return { 
        	getParameterByName: getParameterByName 
        }; 
    }(jQuery)); 
    /* Modal to open on Click of See offer or See price Details */ 
    jQuery(".devicelegal").live("click", ATT.listPage.showseeofferLegal); 
</script> 
