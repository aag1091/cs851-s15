                        jQuery('<meta/>', {name: 'DCSext.wtSuccessFlag', content: '0'}).appendTo('head'); 
                	} 
                	else { 
                		// not back/cancel, and we already know no errors, so indicate success 
                		jQuery('<meta/>', {name: 'DCSext.wtStatusCode', content: '0'}).appendTo('head'); 
                        jQuery('<meta/>', {name: 'DCSext.wtSuccessFlag', content: '1'}).appendTo('head'); 
