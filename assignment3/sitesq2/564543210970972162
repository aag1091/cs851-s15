						jQuery('#commitmentTerm1').val('lease20-install'); 
						jQuery('#commitmentTerm1').attr('checked', 'checked'); 
						jQuery('#commitmentTerm1').trigger('click'); 
                        } 
                    else if((deviceType == 'Cellphone') && (false)){ 
                        if(leaseTerm == v_lease20 || leaseTerm == v_lease26) 
                        { 
						jQuery('#commitmentTerm1').val(leaseTerm); 
						jQuery('#commitmentTerm1').attr('checked', 'checked'); 
						jQuery('#commitmentTerm1').trigger('click'); 
                        } 
                        else {  
