					} 
				} else if (sessionStorage['desktop-wtEvent'] === 'HRock_Check_NotifyMe_Submit') { 
					if (typeof sessionStorage['desktop-wtNotifyMeOpt'] != 'undefined') { 
						jQuery('<meta/>', {name: 'DCSext.wtNotifyMeOpt', content: sessionStorage['desktop-wtNotifyMeOpt']}).appendTo('head'); 
