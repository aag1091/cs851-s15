    <script>
        $(document).ready(function () {
            $('.FooterHeader').click(function () {
                if ($(this).children('span').is(':visible')) {
                    $(this).children('span').toggleClass('glyphicon-chevron-down');
                    $(this).children('span').toggleClass('glyphicon-chevron-up');
                    $(this).siblings('.FooterMenuWrap').slideToggle(325);
                }
            });
        });
    </script>

