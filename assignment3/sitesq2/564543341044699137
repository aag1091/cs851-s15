#newsdigest-widget input[type='text']:hover,#newsdigest-widget input[type='email']:hover {
  border:solid 1px #999;
  -moz-box-shadow:inset 0 0 10px #ccc;
  -webkit-box-shadow: inset 0 0 10px #ccc;
  box-shadow:inset 0 0 5px #ddd;
}

#newsdigest-widget input[type='text']:focus,#newsdigest-widget input[type='email']:focus {
  border:solid 1px #80cbff;
  -moz-box-shadow:inset 0 0 10px #000;
  -webkit-box-shadow: inset 0 0 10px #000;
  box-shadow:inset 0 0 5px #ddd;
}
.hideInfo {display:none;}
#newsdigest-widget #subscribe {margin:20px 0 0 0;width:140px; padding:5px 0;}
#newsdigest-widget label {color:#000; margin:0 0 3px 0;}

#newsdigest-widget .digestSelection {margin:0 0 10px 0;padding:0 0 10px 0;text-align:left;list-style:none; border-bottom:solid 1px #e5e5e5;float:left;}
#newsdigest-widget .digestSelection li {float:left; width:100px;}
#newsdigest-widget .personalInfo {margin:0;padding:0 0 10px 0;text-align:left;list-style:none; float:left;}
#newsdigest-widget .inputWrapper { width: 150px;float:left; }
.alert {border:none;background:none;padding:0; font-family:Arial,Helvetica, sans-serif; color:#000;font-size:14px;}
.inputContainer .error {display:inline; color:#cc0000 !important; font-weight:bold; font-size:12px; text-transform:uppercase; padding:0 0 0 5px;}
.successOverlay {height:200px; }
#newsdigest-widget { border:solid 1px #ccc; padding:0 4px; width:290px !Important;}
#newsdigest-widget .digestSelection li { width:95px !Important;}
#newsdigest-widget .inputWrapper { width:145px !Important;}
