<script type="text/javascript">
  $j('#fan_handler_753882').bind('fan:became_fan',
      function (e, fan) {
    if ('profile_become_a_fan_button_' + fan.page_object_id + '_small' !== 'profile_become_a_fan_button_artist_4532400_small') {
      return;
    }

    var $button = $j('#profile_become_a_fan_button_artist_4532400_small');
    if (fan.is_fan) {
      $button.html('Fan Settings').attr('title', 'Update your Fan settings for this Artist');
