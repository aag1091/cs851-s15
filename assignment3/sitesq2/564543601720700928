<script>
function prepareMenu()
{
        $('ul.secondary-menu').each(function() {
            //$(this).width($(this).parent().width());
        });

        $('.community-menu').find('li').mouseenter(function() {
            $(this).children('ul').show();
            $(this).children('a').addClass('selected');
            $(this).mouseleave(function() {
                $(this).children('ul').hide();
                $(this).children('a').removeClass('selected');
            });
        });
}

$(document).ready(function() {
	prepareMenu();
	$('.lia-component-common-widget-user-navigation').append('<li><span class="separator lia-component-common-widget-user-nav-separator-character">·</span></li>');
	$('.PrivateNotes').appendTo('.lia-component-common-widget-user-navigation');
	//$('.lia-component-common-widget-user-navigation').append('<li><span class="separator lia-component-common-widget-user-nav-separator-character">·</span></li><li><a href="/t5/notes/privatenotespage">Private Messages</a></li>');
});
</script>

