                function setTranslationState(state){ // one of "loading", "done", "initial"
                    jQuery('#translate-rollback').css('display', state == 'done'?'block': 'none');
                    jQuery('#translate-loading').css('display', state == 'loading'?'block': 'none');
                    jQuery('#translate-action').css('display', state == 'initial'?'block': 'none');
                }

                jQuery('#translate-rollback').click(function(e) {
                    jQuery('#page-title h1').text(window.original_title);
                    jQuery('p.art-text').html(original_text);
                    setTranslationState("initial");
                });

                // handle translate operation in 2 steps: obtain a valid access token and make the actual request
                jQuery('.btn-translate,#translate-action').live('click', function(e) {
                    setTranslationState("loading");
                    if(!window.original_text){
                        window.original_text = $('p.art-text').text();
                    }
                    if(!window.original_title){
                        window.original_title = $('#page-title h1').text();
                    }
                    var text = original_text;
                    var headline = original_title;
                    var tolang = jQuery(this)[0].id.replace('translate-', '');
                    doTranslateLocal(220585917, tolang, headline, text, function(o) { //success
                        jQuery('#page-title h1').html(o.h);
                        jQuery('p.art-text').html(o.c);
                        setTranslationState("done");
                        jQuery('i#trstatus').html('');
                    }, function(){ //failure
                        //jQuery('#page-title h1').html(original_title);
                        //jQuery('p.art-text').html(original_text);
                        setTranslationState("initial");
                        jQuery('i#trstatus').html('Translation failed. Something went wrong. Please try again later.');
                });
                    });

                function doTranslateLocal(articleId, toLang, headline, content, success, failure) {
                    var d = {
                        articleid: articleId,
                        to: toLang,
                        h: $.trim(headline),
                        c: $.trim(content)
                    };
                    jQuery.ajax({
                        url: 'http://upge.wn.com/api/upge/cheetah-article/bingtranslate',
                        data: d,
                        dataType: 'jsonp',
                        cache: true,
                        timeout: 5000,
                        type: 'GET'
                    })
                    .done(function (jqXHR, textStatus, errorThrown) {
                        if (success != undefined) {
                            success(jqXHR);
                        }else{
                            failure();
                        }
                    })
                    .fail(function (jqXHR, textStatus, errorThrown) {
                        failure();
                    });
                }
		});
        // -->
        </script>


