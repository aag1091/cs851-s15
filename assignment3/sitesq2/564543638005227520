        function detectTranslateLanguage(country) {
            jQuery.ajax({
                url: 'http://upge.wn.com/api/upge/cheetah-photo-search/geoname',
                data: {country: country, username: 'portsdotcom', style: 'full'},
                dataType: 'jsonp',
                type: 'GET',
                success: function(data, status) {
                    if (data.geonames) {
                        var geonamelangs = data.geonames[0].languages
                        langs = geonamelangs.split(',')
                        if (langs.length == 0) {
                            jQuery('.btn-translate').slideIn();
                        }
                        jQuery.each(langs, function(i, v) {
                            if (v.split('-').length > 1) {
                                v = v.split('-')[0]
                            }
                            if (v == 'en') {
                                return false;
                            }
                            if (available_langs[v]) {
                                jQuery('#translate-group').append('<button id="translate-'+v+'" class="btn-translate btn btn-default" style="">'+available_langs[v].originlang+'</button>')
                            }
                        });
                        if (jQuery('.btn-translate:visible').length == 0) {
                            jQuery('.btn-translate').show();
                        }
                    }
                }
            });
        }

