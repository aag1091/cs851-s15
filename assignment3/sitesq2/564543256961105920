<script language="javascript" type="text/javascript">
    function listentowin() {
        var intervalId = setInterval(function () {
            if (win && win.closed) {
                clearInterval(intervalId);
                window.location = "http://www.careerbuilder.com/jobseeker/help/";
            } 
        }, 1000);
    }
</script>

