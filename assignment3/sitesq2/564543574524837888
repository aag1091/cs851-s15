                 <div id="magic" style=" display: none; background-color: white;  width: 300px; height: 117px; bottom: 0px; right:50px; border-color:#eeece7; position: fixed; z-index: 1000000; box-shadow:0px -4px 3px rgba(50, 50, 50, 0.75);border-top: 3px solid black;">
                 <div style="width:97.5%; text-align: left; float: left; margin-left:10px; color: #333; margin-top:5px; display:block;font-size: 1.4em;">Recommended post <a class="myButton" style ="float:right; margin-right: 10px; color: black; " href="javascript:close();"><p>x</p></a></div>
                     
                 <div style="width:50%; color: #115b8f; line-height: 1.2em; float:left;margin-left:15px; margin-top: 10px;font-size: 1.5em;
                 text-align : left ;font-weight: normal;"><a href="http://blogs.wsj.com/economics/2015/04/02/where-are-global-interests-rates-headed/?mod=blog_flyover">Where Are Global Interest Rates Headed?</a></div>

                 <div style="font-size: 1.2em;float:left; padding-left:11px; margin-top:0px;">
                 <img src="http://si.wsj.net/public/resources/images/OG-AE083_GlobeB_A_20150401180055.jpg">
                 </div>
                 
                  </div>
          </div>      <script>
       
       var div_style = document.getElementById('magic').style; 
     
       
       function fly_in(pixel) {

           div_style.display = 'inline-block';  
           div_style.marginBottom = '-102px' ;
            $("#magic").animate({ marginBottom: pixel }); 
      }

        function fly_away(pixel) {

           div_style.right = '';   
           div_style.marginLeft = '580px' ;
           $("#magic").animate({ marginLeft: pixel });
          
          }

        function flyover() {
           
           if(document.body.scrollHeight - window.pageYOffset < 2040){
                fly_in(0);
                removeListeners(flyover); 
           }else{ 
                div_style .display = 'none';
                }
          }
        
        function close(){
            fly_away(1000);
            removeListeners(flyover);
         }
       
    
     function attachListeners(fn) {
        
        if (window.addEventListener) {
        
        //window.addEventListener('DOMContentLoaded', fn, false);
        //window.addEventListener('load', fn, false);
        window.addEventListener('scroll', fn, false);
        window.addEventListener('resize', fn, false);
      }  else if (window.attachEvent) { // IE fallback
        window.attachEvent('DOMContentLoaded', fn);
        window.attachEvent('load', fn);
        window.attachEvent('onscroll', fn);
        window.attachEvent('onresize', fn);
      }
 }

     function removeListeners(fn) {
      if (window.removeEventListener) {
       // window.removeEventListener('DOMContentLoaded', fn, false);
        window.removeEventListener('load', fn, false);
        window.removeEventListener('scroll', fn, false);
        window.removeEventListener('resize', fn, false);
      } else if (window.attachEvent) { // IE fallback
        window.detachEvent('DOMContentLoaded', fn);
        window.detachEvent('load', fn);
        window.detachEvent('onscroll', fn);
        window.detachEvent('onresize', fn);
      }
    }

    attachListeners(flyover); //settimeout

  </script>

