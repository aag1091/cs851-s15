          if (loggedIn) {
           dj.module.sitewidget.popoutWidgetize.init();
           dojo.setObject("dj.context.sitewidgets.popupurl", "http://online.wsj.com/page/0_0_WGT_INVESTORCENTER_MST.html");
          }
         });
       }
     });
  }
