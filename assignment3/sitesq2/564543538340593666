	<script type="text/javascript">
		function getWidth() {
			xWidth = null;
			if(window.screen !== null) xWidth = window.screen.availWidth;
			if(window.innerWidth !== null) xWidth = window.innerWidth;
			if(document.body !== null) xWidth = document.body.clientWidth;
			return xWidth;
		}

		function getHeight() {
			xHeight = null;
			if(window.screen !== null) xHeight = window.screen.availHeight;
			if(window.innerHeight !== null) xHeight = window.innerHeight;
			if(document.body !== null) xHeight = document.body.clientHeight;
			return xHeight;
		}
		
		function setCookie(cname,cvalue,exdays) {
			var d = new Date();
			d.setTime(d.getTime()+(exdays*24*60*60*1000));
			var expires = "expires="+d.toGMTString();
			document.cookie = cname + "=" + cvalue + "; " + expires;
		}
		
		function getCookie(cname) {
			var name = cname + "=";
			var ca = document.cookie.split(';');
			for(var i=0; i<ca.length; i++) {
				var c = ca[i].trim();
				if (c.indexOf(name)==0) return c.substring(name.length,c.length);
			}
			return "";
		} 

		function isMobileOff() {
			var params = window.location.search.substring(1).split("&");
			for (var i=0; i<params.length; i++) {
				var param = params[i].split("=");
				if (param[0] == "mobile" && param[1] == "off") return true;
			}
			return false;
		}
		
		function isMobile() {
			// Check for mobile disable cookie, return false if present
			if (getCookie("italiaMobile") == "off") return false;
			
			// Check for mobile disable param, set cookie and return false if present.
			if (isMobileOff()) {
				setCookie("italiaMobile","off",1);
				return false;
			}
			
			// Blackberry
			if (/Blackberry/i.test(navigator.userAgent) && (/Version\/6/i.test(navigator.userAgent) || /Version\/7/i.test(navigator.userAgent))) return true;
			// Opera
			if (navigator.userAgent.indexOf('Opera Mobi') > -1 || navigator.userAgent.indexOf('Opera Mini') > -1) return true;
			// Apple & Android
			if (/iPhone|iPod/i.test(navigator.userAgent) || (navigator.userAgent.indexOf("Android") > -1)) return true;
			var android_stock = /applewebkit\/([0-9]+)/gi.exec(navigator.userAgent);
            if (navigator.userAgent.indexOf('Macintosh')==-1 && navigator.userAgent.indexOf('Windows')==-1 && navigator.userAgent.indexOf('iPad')==-1 && android_stock && parseInt(android_stock[1])<=534) return true;
			var windows_mobile = /IEMobile\/([0-9]+)/gi.exec(navigator.userAgent);
			if (windows_mobile && parseInt(windows_mobile[1])==9) return true;
			// Small screen devices
			if (screenWidth < 240 && screenHeight < 240) return true;
			// Everything else is not classed as mobile
			return false;
		}
		var screenWidth = parseInt(getWidth()*window.devicePixelRatio),
			screenHeight = parseInt(getHeight()*window.devicePixelRatio);
	</script>
