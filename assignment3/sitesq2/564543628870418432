        <script>
            jQuery(document).ready(function() {
                if(window.location.hash=='#openchat'){
                    window.scrollBy(0,500);
                    jQuery("#livelychatsupport-chatbox").toggleClass("open");
                    var href = jQuery(this).data("href");
                    var open = !(!jQuery("#livelychatsupport-chatbox.open").length);
                    jQuery(this).attr("href", href + open);
                    jQuery("#livelychatsupport-chatbox-new-message").focus();
                }
            });
        </script>
