    <script type="text/javascript">
        jQuery("document").ready(function()
        {
            jQuery( "#add_job_to_dashboard" ).click(function() {
                var jobId = jQuery(this).data('job');
                var userId = jQuery(this).data('user');

                jQuery.ajax({
                    type: 'POST',
                    url: '/wp-admin/admin-ajax.php',
                    data: {
                        action:'add_job_to_dashboard',
                        jobId: jobId,
                        userId: userId
                    },
                    dataType: 'json',
                    success: function(data)
                    {
                        if(data.success)
                        {
                            jQuery('#add_job_to_dashboard').hide();
                            jQuery('#del_job_from_dashboard').show();
                        }
                    }
                });
                return false;
            });
        });
    </script>
        <script type="text/javascript">
        jQuery("document").ready(function()
        {
            jQuery( "#del_job_from_dashboard" ).click(function() {
                var jobId = jQuery(this).data('job');
                var userId = jQuery(this).data('user');

                jQuery.ajax({
                    type: 'POST',
                    url: '/wp-admin/admin-ajax.php',
                    data: {
                        action:'delete_job_from_dashboard',
                        jobId: jobId,
                        userId: userId
                    },
                    dataType: 'json',
                    success: function(data)
                    {
                        if(data.success)
                        {
                            jQuery('#del_job_from_dashboard').hide();
                            jQuery('#add_job_to_dashboard').show();
                        }
                    }
                });
                return false;
            });

            jQuery( ".del_job_from_dashboard_link" ).click(function() {

                var thisLi = jQuery(this).parent().parent();
                var jobId = jQuery(this).data('job');
                var userId = jQuery(this).data('user');

                thisLi.css("background-color","rgba(255,0,0,0.3)");

                jQuery.ajax({
                    type: 'POST',
                    url: '/wp-admin/admin-ajax.php',
                    data: {
                        action:'delete_job_from_dashboard',
                        jobId: jobId,
                        userId: userId
                    },
                    dataType: 'json',
                    success: function(data)
                    {
                        if(data.success)
                        {
                            thisLi.remove();
                        }
                    }
                });
                return false;
            });
        });
    </script>
