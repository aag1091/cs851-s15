	initialize: function () {
		// iOS detection
		if (navigator.userAgent.match(/(iPad|iPhone);.*CPU.*OS 4_\d/i)) {
			$('body').addClass('iOS4');
		}
		WidgetUI.selectDropdown = $('.widget-dropdown');
		WidgetUI.initSelectDropdown();
	},
	addListeners:function(){},
	initSelectDropdown: function() {
		WidgetUI.selectDropdown.select2({
			minimumResultsForSearch: 1000,
			placeholder: "Select a State"
		});
		WidgetUI.selectDropdown.change(function(){
			var theVal = $(this).select2('val');
			window.open(theVal);
		})
	},
	addListeners:function(){},
}
