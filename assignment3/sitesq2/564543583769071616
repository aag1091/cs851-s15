                            function bp_thumbnail_resize(e,d){var c=300;var a=true;var b="http://2.bp.blogspot.com/-erTXCq61ULM/TmHYAQBZ0GI/AAAAAAAACCs/6cBX54Dn6Gs/s72-c/default.png";if(a==true&&e==""){e=b}image_tag='<img src="'+e.replace("/s72-c/","/s"+c+"-c/")+'" class="postthumb" alt="'+d+'"/>';if(e!=""){return image_tag}else{return""}};
                            //]]>
</script>
<a href='http://www.amplifiercircuit.info/2015/02/100w-guitar-amplifier.html'>
<script type='text/javascript'>
                              document.write(bp_thumbnail_resize("http://2.bp.blogspot.com/-NyCKPcGI6L8/VOCAiSrUdCI/AAAAAAAALHU/1evmg4En09U/s72-c/100W%2BGuitar%2BAmplifier.gif","100W Guitar Amplifier"));
