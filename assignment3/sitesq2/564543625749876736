		function clickLink(which_store, linkName) {
			var url = '';
			if(which_store=='app') {
				url = 'http://hastrk2.com/serve?action=click&publisher_id=22564&site_id=46928&offer_id=274464';
				label = 'iOS - Home Page';
			} else if(which_store=='play') {
				url = 'http://hastrk2.com/serve?action=click&publisher_id=22564&site_id=46926&offer_id=274462';
				label = 'Android - Home Page';
            } else if(which_store=='amazon') {
                url = 'http://www.amazon.co.uk/Free-Prints-Photos-Delivered/dp/B00K8H0MDU';
                label = 'Amazon - Home Page';
            } else if(which_store=='winphone') {
				url = 'http://www.windowsphone.com/en-us/store/app/freeprints/7c34e3a2-8122-4dd5-b85c-80d869b5e799';
				label = 'Windows Phone - Home Page';
			} else if(which_store=='win8') {
				url = 'http://apps.microsoft.com/windows/app/free-prints/45d6824c-8d3d-4a45-8405-41a52619465c';
				label = 'Windows Store - Home Page';
            } else if(which_store=='use_online') {
				url = 'http://www.photoaffections.com/freeprints';
				label = 'Use it online - FreePrints by PhotoAffections';
			} else if(which_store=='fp_on_pa') {
				url = 'http://www.photoaffections.com/freeprints?utm_source=FreePrintsApp.com&utm_medium=badge&utm_campaign=FPonPA_badge';
				label = 'PhotoAffections - FreePrints';
			}
			if(url!='') {
				ga('send', label, 'Click', linkName);
				
				window.location.href = url;
			}
		}
	</script>
	
