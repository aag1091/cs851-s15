				<!-- mylocal_module.inc -->
<script type="text/javascript">
  /* CONFIGURATION VARIABLES */
  // directory_domain     - String: the domain of the directory website for this publisher
  // business_category    - String: what category of business should be displayed (set to null for all categories).
  //                        Should be all lower-case replacing spaces with underscores (e.g. "beauty_and_wellness")
  // business_list_length - Integer: how many businesses to load in the list
  // minimum_directory_level - 0,1,2 or 3: only display businesses upgraded to at least this level in the directory
  var ownlocal_widget_config = {
    directory_domain: 'springfield.montgomerynews.com',
    business_category: '',
    business_list_length: 5,
    minimum_directory_level: 0
  };
</script>

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
 var jq_1_8_3 = jQuery.noConflict(true);
    var jQueryOriginal = jQuery || jq_1_8_3;
    window.jQuery = window.$ = jq_1_8_3;
  

function ownlocal_insert_business_list (returned_businesses) {
  jq_1_8_3(function(){
    list = $("#ownlocal-business-list ul");
	list.empty();
    jq_1_8_3(returned_businesses).each(function(index, biz) {
	  var new_item = jq_1_8_3("#ownlocal-business-list-template li").clone();
	  new_item.find('.ownlocal-business-link').attr('href', biz.link).text(biz.name);
	  list.append(new_item);
	  var info = new_item.find('.ownlocal-business-popup-info');
	  info.find('h4 a').attr('href', biz.link).text(biz.name);
	  var info_html = biz.city + ", " + biz.state;
	  if (biz.address) info_html = biz.address + "<br/>" + info_html;
	  if (biz.website) {
	     
	    var website = biz.website.replace('http://', '');
	    info_html += " | <a href='http://" + website +"'>"+ website +"</a>"
	  }
	  info.find('p').html(info_html);
	  if (biz.ad_image) {
	    info.append('<a target="_blank" href="'+ biz.ad_url +'"><img src="'+biz.ad_full_image+'"/></a>');
	  }  else {
	    info.append('<a target="_blank" href="'+ biz.link +'"><img src="'+biz.image+'"/></a>');
	  }
    });
  });
}
  
//On document load...
jq_1_8_3(function() {
  // set the url for the add business link
  var link_url = "http://" + ownlocal_widget_config['directory_domain']  ;
  jq_1_8_3('#ownlocal-add-business-link a').attr('href', link_url + "/#add_business");
  jq_1_8_3('form.ownlocal-search-form').attr('action', link_url + "/search");

  // calculate the URL for the widget, based on configuration variables
  var widget_url = "http://" + ownlocal_widget_config['directory_domain'];
  widget_url += "/widget/json/featured_business_list?callback=ownlocal_insert_business_list&include_ads=1&image_size=small";
  if (ownlocal_widget_config['business_category'])
    widget_url += "&category=" + ownlocal_widget_config['business_category'];
  widget_url += "&limit=" + ownlocal_widget_config['business_list_length'];
  widget_url += "&level=" + ownlocal_widget_config['minimum_directory_level'];
  
  // Make the search field text disappear when clicked:
  var search_field = jq_1_8_3('#ownlocal-custom-search-field');
  search_field.data('default_text', search_field.val());
  search_field.on('focus', function() {
    if (jq_1_8_3(this).data('default_text') == jq_1_8_3(this).val())
      jq_1_8_3(this).val('');
  })
  search_field.on('blur', function() {
    if ('' == jq_1_8_3(this).val())
      jq_1_8_3(this).val($(this).data('default_text'));
  })
  
 // Activate the Dropdown
  jq_1_8_3('#ownlocal-dropdown').on('click', '.arrow', function() {
    jq_1_8_3('#ownlocal-dropdown .dropdown-container').toggleClass('active');
  });
  jq_1_8_3('#ownlocal-dropdown').on('mouseleave', '.dropdown-container', function() {
    jq_1_8_3(this).removeClass('active');
  });
  jq_1_8_3('#ownlocal-dropdown').on('click', 'li', function() {
    jq_1_8_3('#ownlocal-dropdown .dropdown-container').addClass('active');
  });
  
  // load the widget script
  var s = document.createElement('script');
  s.setAttribute('src', widget_url);
  document.body.appendChild(s);
});


window.jQuery = window.$ = jQueryOriginal;
</script>
