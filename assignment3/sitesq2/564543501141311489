    function calculateDetails3Discount() {
        var selEl;
        selEl = document.getElementById("selList1");
        if (selEl != null) {
            //alert(details3DiscountArray.length);
            if (details3DiscountArray.length > selEl.selectedIndex) {
                for (li=0;li<details3DiscountArray[selEl.selectedIndex].length;li=li+1)
                {
                    spanEl = document.getElementById("discountLevel_" + (li + 1));
                    if (spanEl != null) {
                        spanEl.innerHTML = details3DiscountArray[selEl.selectedIndex][li];
                    }
                }
            }
        }
    }
