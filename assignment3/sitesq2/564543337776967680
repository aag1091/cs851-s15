<script type="text/javascript">
				// Place to stash data (currently to only resize iframes when actually needed)
				mtmads.iFrameData = mtmads.iFrameData || {};

				function iFrameAutoResize(id){
					var newheight;

					if( document.getElementById ){
						newheight = document.getElementById( id ).contentWindow.document .body.scrollHeight;
					}

					if ( "undefined" === typeof mtmads.iFrameData[ id ] || mtmads.iFrameData[ id ] !== newheight ) {
						document.getElementById( id ).height= ( newheight ) + "px";
						mtmads.iFrameData[ id ] = newheight;
					}
				};

				googletag.cmd.push(function() {
					var pos1homemapping = googletag.sizeMapping().
						addSize([1000, 200], [[970, 90],[970,30],[728, 90],[1,1]]). // SLB only for desktop and tablet wide
						addSize([750, 200], [728, 90]). // 728 only for Tablet Portrait and above
						addSize([0, 0], [[320, 50], [300, 50]]). //all sizes
						build();

					var pos1mapping = googletag.sizeMapping().
						addSize([1000, 200], [[970, 90],[728, 90]]). // SLB only for desktop and tablet wide
						addSize([750, 200], [728, 90]). // 728 only for Tablet Portrait and above
						addSize([0, 0], [[320, 50], [300, 50]]). //all sizes
						build();

					var pos2mapping = googletag.sizeMapping().
						addSize([785, 200], [[300,600],[300,250]]). // Half Page in desktop and tablet wide
						addSize([0, 0], [300, 250]). // MR all sizes
						build();

					var pencilmapping = googletag.sizeMapping().
						addSize([1000, 200], [970,30]). // pencil only in desktop and tablet wide
						addSize([0, 0], []). // supress below desktop
						build();

					var pos3mapping = googletag.sizeMapping().
						addSize([785, 200], [[300,600],[300,250]]). // add half page in tablet wide and desktop
						addSize([400, 200], [300, 250]). // medium rectangle only in tablet portrait
						addSize([0, 0], []). // supress mobile -- additonal comment
						build();

					var pos4mapping = googletag.sizeMapping().
						addSize([750, 200], [728, 90]). // Portait tablet
						addSize([0, 0], []). //supress Position 4 below Tablet Portrait
						build();

					var btfmrmapping = googletag.sizeMapping().
						addSize([785, 200], [300,250]). // Desktop
						addSize([0, 0], []). //supress positions 8 and 9 below desktop.
						build();

				
						mtmads.pos_1_atf_728x90 = googletag.defineSlot('/6119/pressherald.com/news/opinion/article', [728, 90], "pos_1_atf_728x90").setTargeting("pos", ["1","atf"]).defineSizeMapping(pos1mapping).addService(googletag.pubads());

						
						mtmads.pos_2_atf_300x250 = googletag.defineSlot('/6119/pressherald.com/news/opinion/article', [300, 250], "pos_2_atf_300x250").setTargeting("pos", ["2","atf"]).defineSizeMapping(pos2mapping).addService(googletag.pubads());

						
						mtmads.pos_3_btf_300x250_300x600 = googletag.defineSlot('/6119/pressherald.com/news/opinion/article', [300, 250], "pos_3_btf_300x250_300x600").setTargeting("pos", ["3","btf"]).defineSizeMapping(pos3mapping).addService(googletag.pubads());

						if ( viewportwidth > 730 ) {
						mtmads.pos_btf_728x90 = googletag.defineSlot('/6119/pressherald.com/news/opinion/article', [728, 90], "pos_btf_728x90").setTargeting("pos", ["4","btf"]).defineSizeMapping(pos4mapping).addService(googletag.pubads());

						}				    //tracking section via ad code manager
				    AdSection = "pressherald.com/news/opinion";

					// Manually Controlled Ad Slots
					// Interstitial - Originally googletag.defineOutOfPageSlot('/6119/mnt.pressherald.home', 'div-gpt-ad-1401897265346-0-oop').addService(googletag.pubads());
					mtmads.interstitial = googletag.defineOutOfPageSlot('/6119/pressherald.com/news/opinion/article', 'div-gpt-ad-1401897265346-0-oop').addService(googletag.pubads());

					mtmads.native_ad = googletag.defineSlot( '/6119/pressherald.com/news/opinion/article', [1,1], 'native_ad').addService(googletag.pubads());

					mtmads.native_ad.oldRenderEnded = mtmads.native_ad.renderEnded;
					mtmads.native_ad.renderEnded = function() {
						// Remove height/width since the actual content of this ad will be larger than 1x1
						var $iframe =  jQuery('#native_ad iframe');
						$iframe.removeAttr('height').removeAttr('width');
						iFrameAutoResize( $iframe.attr('id') );

						mtmads.nativeAd.oldRenderEnded();
					};
					var is_logged_in = !!document.cookie.match(/MSPAN_Paywall_user/);
					
						if(is_logged_in) {
							googletag.pubads().setTargeting("sub","true");
						} else {
							googletag.pubads().setTargeting("sub","false");
						}
						googletag.pubads().collapseEmptyDivs();
						googletag.enableServices();
										});
			</script>
</head>
