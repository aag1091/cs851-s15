            jQuery.ajax({
                type: 'get',
                url: reqUrl,
                dataType: 'json',
                success: function (r) {
                    cok_c_splash_page = psp_getCookie( 'c_splash_page' );
                    if (r.status == 'S' && ( r.psp_device == device || r.psp_device == 'all' ) && cok_c_splash_page !=r.splash_id   ) {
                        jQuery('#pspModal').addClass(r.splash_size);
                        jQuery('#pspModal').addClass(r.splash_color_scheme);
