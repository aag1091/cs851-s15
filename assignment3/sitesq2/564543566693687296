        jQuery(document).ready(function () {
           device = '';
            var tt=new Date().getTime();
           
            if( psp_is_mobile( navigator.userAgent||navigator.vendor||window.opera  ) ){
                device='mobile';
            } else{
                device = 'desktop';
            }
            var reqUrl = 'http://www.peta.org/wp-admin/admin-ajax.php?post_id=125320&action=psp_display&time_094649=150403094649&device='+device+'&rand_'+tt+'='+tt;
            
