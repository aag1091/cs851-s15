                        $(document).ready(function()
                        {
                            var intLimitWidth = $('#footer table').width()+30;

                            if(GetClientWidth() <= intLimitWidth)
                            {
                                $('#footer a').css('font-size', 9);
                                $('#footer').css('font-size', 9);
                            }

                            $(window).resize(function()
                            {
                                $('#footer').css('font-size', 11);
                                $('#footer a').css('font-size', 11);

                                intLimitWidth = ($('#footer table').width()+30 < intLimitWidth)? $('#footer table').width()+30 : intLimitWidth;

                                if(GetClientWidth() <= intLimitWidth)
                                {
                                    $('#footer a').css('font-size', 9);
                                    $('#footer').css('font-size', 9);
                                }
                            });
                        })
