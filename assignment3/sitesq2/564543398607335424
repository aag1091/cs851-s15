                                <script type="text/javascript">
                                    var three_links_pic= new Image(263,85);
                                    three_links_pic.src="/i/img/three_arrow.png";

                                    $(document).ready(function()
                                    {
                                        var intLimitWidth = 1380;

                                        if(getClientWidth() > intLimitWidth)
                                        {
                                            $('#header_map_l').show();
                                        }
                                        else
                                        {
                                            $('#header_map_s').show();
                                        }

                                        $(window).resize(function()
                                        {
                                            var intClientWidth = getClientWidth();
                                            if(intClientWidth > intLimitWidth)
                                            {
                                                $('#header_map_l').show();
                                                $('#header_map_s').hide();
                                            }
                                            else
                                            {
                                                $('#header_map_s').show();
                                                $('#header_map_l').hide();
                                            }
                                        });
                                    })
                                </script>

