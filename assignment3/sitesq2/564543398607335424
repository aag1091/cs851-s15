		                        <script>
                            var sponsors = $('.sponsors_item');
                            var currentSp = 0;
                            var maxIndex = sponsors.length - 1;
                            setInterval(function(){
                                currentSp++;
                                if (currentSp > maxIndex)
                                    currentSp = 0;
                                sponsors.fadeOut(1500);
                                sponsors.parent().find('.sp_item_'+currentSp).fadeIn(1500);
                            }, 7000);
                        </script>
                
