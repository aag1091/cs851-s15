				<script type='text/javascript'>
					function CheckLogIn()
						{
							var idLogin		= (typeof(arguments[0]) == 'undefined') ? false : gE(arguments[0]).value;
							var idPassword	= (typeof(arguments[1]) == 'undefined') ? false : gE(arguments[1]).value;
							var idAction    = (typeof(arguments[2]) == 'undefined') ? false : gE(arguments[2]).value;

							if (!idLogin.length || !idPassword.length)
							{
								alert('You should fill out both Login and Password');
								return false;
							}

							$(document).ready(function()
							{
								//$(".cab-auth :submit").attr("disabled", "disabled");

								$.post("/checkipcabinet.php",
										{
											login: idLogin,
											lang: "en",
											act: idAction
										},
										function(data)
										{
											if(typeof(data.formId) != 'undefined' && data.url.length > 0)
											{
												$(data.formId).attr("action", data.url).submit();
											}
										},
										"json"
								);
							});

							return false;
						}

						function PreLoadImages()
						{
							var picName = '';
							var arrImages = new Array('bt-cab-auth-hover.png');
							for (i in arrImages)
							{
								picName = arrImages[i];
								pic = new Image();
								pic.src = "https://fx.instaforex.com/i/img/"+picName;
							}
						}

						PreLoadImages();

						$(document).ready(function() {
							// Toggle Client Cabinet Login form
							$(".cab-current").click(function() {
								//$(this).next().slideToggle('lng-list');
								$(".cab-list").slideToggle();
                                if ($(".cab_par-list").css('display') == 'block')
                                {
                                    $(".cab_par-list").slideToggle();
                                }
							});

							$(".hide-cab").click(function() {
								//$(this).next().slideToggle('lng-list');
								$(".cab-list").slideToggle();
							});

							// Make a defaultText for inputs
							 $(".defText").focus(function(srcc)
							{
								if ($(this).val() == $(this)[0].title)
								{
									$(this).removeClass("defTextActive");
									$(this).val("");
								}
							});

							$(".defText").blur(function()
							{
								if ($(this).val() == "")
								{
									$(this).addClass("defTextActive");
									$(this).val($(this)[0].title);
								}
							});

							$(".defText").blur();
						});

						// Toggle Partner Cabinet Login form
						$(document).ready(function() {
							$(".cab_par-current").click(function() {
								//$(this).next().slideToggle('lng-list');
								$(".cab_par-list").slideToggle();
                                if ($(".cab-list").css('display') == 'block')
                                {
                                    $(".cab-list").slideToggle();
                                }
							});

							$(".hide_par-cab").click(function() {
								//$(this).next().slideToggle('lng-list');
								$(".cab_par-list").slideToggle();
							});

							// Make a defaultText for inputs
							 $(".defText").focus(function(srcc)
							{
								if ($(this).val() == $(this)[0].title)
								{
									$(this).removeClass("defTextActive");
									$(this).val("");
								}
							});

							$(".defText").blur(function()
							{
								if ($(this).val() == "")
								{
									$(this).addClass("defTextActive");
									$(this).val($(this)[0].title);
								}
							});

							$(".defText").blur();

