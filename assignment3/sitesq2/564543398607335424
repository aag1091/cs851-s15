<script type="text/javascript">
    (function ($) {
        jQuery.fn.instaWidgets = function (options) {
            options = $.extend({}, options);

            var parentElem = $(this);

            $('.widgets-tabs li', parentElem).bind('click', function(){
                var tab = $(this),
                    index = $(this).prevAll().length; //$(this).index() for new query

                tab.addClass('active').siblings().removeClass('active');
                tab.parent().next().find('div.tab-content').eq(index).addClass('active')
                    .siblings('div.tab-content').removeClass('active');
            });


            return this;
        };
    })(jQuery);
    $(document).ready(function () {
        $('.wrapper_sb2').hover(function () {
                $(this).find('.text_sb2').css('color', '#cf1300');
            },
            function () {
                $(this).find('.text_sb2').css('color', '#515151');
            });

        var intClientWidth = getClientWidth();
        var intLimitWidth = 1660;

        if (intClientWidth > intLimitWidth) {
            $('#supertable2_large').show();
        }
        else {
            $('#supertable2_small').show();
        }

        $(window).resize(function () {
            var intClientWidth = getClientWidth();
            var intLimitWidth = 1660;

            if (intClientWidth > intLimitWidth) {
                $('#supertable2_large').show();
                $('#supertable2_small').hide();
            }
            else {
                $('#supertable2_large').hide();
                $('#supertable2_small').show();
            }
        });

        $(window).resize(function () {
            if (GetSwfVer() != -1) {
                var intClientWidth = getClientWidth();
                var intLimitWidth = 1400;

                if (intClientWidth > intLimitWidth) {
                    $('.supertable object').each(function () {
                        $(this).attr('width', '200');
                        $(this).attr('height', '110');
                    });
                }
                else {
                    $('.supertable object').each(function () {
                        $(this).attr('width', '140');
                        $(this).attr('height', '75');
                    });
                }
            }
        });

        $('#insta-widgets').instaWidgets();
    });


</script>

