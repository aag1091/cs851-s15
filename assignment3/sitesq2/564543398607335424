                                                    NY_hidden();
                                                    function NY_hidden()
                                                    {
                                                        if ($(window).width() < 1400){
                                                            if ($(window).width() > 1380){
                                                                var opacity = 1 - (1400 - $(window).width())/20;
                                                                $(".ny-logo").css('opacity', opacity);
                                                            }
                                                        }
                                                        if ($(window).width() < 1380){
                                                            $(".ny-logo").css('opacity', "0");
                                                        }
                                                        if ($(window).width() > 1400){
                                                            $(".ny-logo").css('opacity', "1");
                                                        }
                                                    }
						});

                    $(function () {
                        ScrollButton().attr('title', "Up");
                    })
