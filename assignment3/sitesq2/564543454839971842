<section id="ja-mainbody" class="ja-mainbody wrap">
  <div class="container">
    <div class="row">
      
      <!-- MAIN CONTENT -->
      <div id="ja-content" class="ja-content span8"  data-default="span8" data-xtablet="span12" data-tablet="span12">
        <div class="main-content">
		  
<div id="system-message-container">
</div>
		  		  ﻿



<!-- Start K2 Item Layout -->
<span id="startOfPageId1123"></span>

<div id="k2Container" class="itemView">

	<!-- Plugins: BeforeDisplay -->
	
	<!-- K2 Plugins: K2BeforeDisplay -->
	
	<div class="itemHeader">

	  	  <!-- Item title -->
	  <h2 class="itemTitle clearfix">
			

	  	Henry Cavill at the BAFTA Awards - 2015
	  			
	  </h2>
	  
  </div>

  <div class="item-meta">
    
    
        <!-- Item category -->
    <span class="itemCategory">
      <strong>Category:</strong>
      <a href="/en/media-gallery/images/events">Events & Public Appearances</a>
    </span>
    
            
    <!-- Item source -->
    <span class="itemCategory">
	
      <strong>Discussion:</strong>

	  <!-- Plugins: BeforeDisplayContent -->
	  <div class="jacomment-count">
    <a rel="nofollow" class="jacomment-counter" data-disqus-identifier="com_k2:item:1123" href="http://henrycavill.org/media-gallery/images/events/item/1123#disqus_thread" onclick="location.href='http://henrycavill.org/index.php?option=com_k2&view=item&id=1123&Itemid=577#disqus_thread'; return false;" title=""></a>
    </div>
    </span>
    

    
    
  </div>

  <!-- Plugins: AfterDisplayTitle -->
  
  <!-- K2 Plugins: K2AfterDisplayTitle -->
  
	
  <div class="itemBody">

	  <!-- K2 Plugins: K2BeforeDisplayContent -->
	  
	  
	  	  	  <!-- Item introtext -->
	  <div class="itemIntroText">
	  	<p>Henry Cavill attended at the BAFTA Awards Ceremony held at Royal Opera House in London, on February 8, 2015. He presented the award in one of the nominations.</p>
	  </div>
	  	  	  <!-- Item fulltext -->
	  <div class="itemFullText">
	  	
<p>

<!-- JoomlaWorks "AllVideos" Plugin (v4.4) starts here -->

<div class="avPlayerWrapper avVideo">
	<div style="width:600px;" class="avPlayerContainer">
		<div id="AVPlayerID_f89aaaf1_505169519" class="avPlayerBlock">
			<iframe src="http://www.youtube.com/embed/HqulKDKwyto?rel=0&amp;fs=1&amp;wmode=transparent" width="600" height="400" frameborder="0" allowfullscreen title="JoomlaWorks AllVideos Player"></iframe>					</div>
	</div>
</div>

<!-- JoomlaWorks "AllVideos" Plugin (v4.4) ends here -->

</p>
<p>&nbsp;</p>
<p><strong>Henry Cavill poses for pics on the red carpet and presenting the Best Leading Actress award (to Julianne Moore) together with Chris Evans, and poses for photos in the winners room</strong></p>
<p>

