		<script>
		jQuery(document).ready(function(e) {
			jQuery(function() {
				jQuery( "#volume" ).slider({
					min: 0,
					max: 100,
					value: 80
				});
			});
			jQuery( "#volume" ).on( "slide", function( event, ui ) {
				jQuery("#jquery_jplayer_1").jPlayer("volume", ui.value/100);
			});
			setTimeout('jQuery("#jquery_jplayer_1").jPlayer("play")',500);
			
			curr_show_info = function() {
				data={action: 'curr_show_info'};
				jQuery.post(ajax_url, data, function(e){
					show = jQuery.parseJSON(e);
					if (show.next == 1) {
						if (show.time==0) {
							show.time = '12:00am';
						} else if (show.time<12) {
							show.time = show.time+':00am';
						} else if (show.time==12) {
							show.time = '12:00pm';
						} else if (show.time>12) {
							show.time = (show.time - 12) + ':00pm';
						}
						jQuery('#up_next').html('Up Next @ '+show.time).show();
					} else {
						jQuery('#up_next').hide();
					}
					jQuery('.dj_name').html(show.dj_name);
					jQuery('.show_name').html(show.show_name);
					if (show.show_image) {
						jQuery('.show_image').attr('src',show.show_image).show();
					} else {
						jQuery('.show_image').hide();
					}
				});
			}
			curr_show_info();
			setInterval(curr_show_info,60000);
				

		});
		
		function PanimateTrackLeft(){
			if (jQuery('#popupplayer #track').width()>172) {
				jQuery('#popupplayer #track').css('left',0).animate({
						left:	- (jQuery('#popupplayer #track').width())/2,
					}, jQuery('#popupplayer #track').width()*30, 'linear', PanimateTrackLeft);
		}
}
function PanimateArtistLeft(){
	if (jQuery('#popupplayer #artist').width()>172) {
		jQuery('#popupplayer #artist').css('left',0).animate({
				left:	- (jQuery('#popupplayer #artist').width())/2,
			}, jQuery('#popupplayer #artist').width()*30, 'linear', PanimateArtistLeft);
	}
}
</script>
