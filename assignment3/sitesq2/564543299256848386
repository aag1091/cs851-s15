<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_3987374").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/3987374?view=json", function(json){
				jQuery("#upv_3987374").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_3987374").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_3987374").append("Not available");
				}
			});
		}
	}
</script>
