<script type="text/javascript">
// Don't run the sharebox on section fronts
//if ( typeof(article_id) == "undefined" )
if (articleId == "")
{
    //var share_url = 'http://www.denverpost.com' + document.location.pathname;
    jQuery('#sharebox').css('display', 'none');
}
else
{
    var share_url = 'http://www.denverpost.com/ci_' + article_id;

var fb_like_url = 'http://www.facebook.com/widgets/like.php?href=' + share_url + '?source=share_fb&amp;action=like&amp;layout=box_count';
var su_url = "<iframe src=\"http:\/\/www.stumbleupon.com\/badge\/embed\/5\/?url=" + share_url + "?source=share_su\" scrolling=\"no\" frameborder=\"0\" style=\"border:none; overflow:hidden; width:50px; height: 60px;\" allowTransparency=\"true\"><\/iframe>";



// Place this render call where appropriate
(function() {
var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
po.src = 'https://apis.google.com/js/plusone.js';
var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
})();

// Add the facebook like button in the sharebox div
//var sharebox_fb = '<iframe src="' + fb_like_url + '" scrolling="no" frameborder="0" id="facebook-like-top"      style="border:none; width:120px; padding-right:0px; height:auto; margin-top:20px;"></iframe>';
setTimeout ('jQuery(\'#sharebox_fb\').append(\'<iframe src="\' + fb_like_url + \'"        scrolling="no" frameborder="0" id="facebook-like-top"      style="border:none; width:100px; padding-right:0px; float:right; height:75px;"></iframe>\');', 500);
setTimeout ('jQuery(\'#sharebox_su\').append(su_url);', 600);

jQuery(window).load(function() 
{
    $window = jQuery(window);
    $sharebox = jQuery('#sharebox');
    var window_top = jQuery('#sharebox').offset().top - parseFloat(jQuery('#sharebox').css('marginTop').replace(/auto/, 0));
    
    // This if statement applies to skinny widows.
    if ( $window.width() < 1200 )
    {
        jQuery('.articleFooterLinks > div.articleOptions').append('<hr />').append($sharebox.html());
        jQuery('.articleFooterLinks > div.articleOptions').css('text-align', 'left');
        jQuery('.sharebox_item').css('float', 'left');
        jQuery('.sharebox_item').css('margin-top', '3px');
        
        jQuery('#sharebox').remove();    
    }
    else
    {
        $window.scroll( function(event) 
        { 
            var y = jQuery(this).scrollTop();
            if ( y <= window_top )
            {
                $sharebox.removeClass('floating');
            }
            else
            {
                $sharebox.addClass('floating');
            }
        });
        
        if ( jQuery.browser.msie == true )
        {
            $sharebox.fixedPosition({
                debug: false,
                fixedTo: "top",
                effect: "slideDown",
                effectSpeed: 200
            });
            
        }

    }
    
});

}
