<!-- END FREEFORM RENDER, ID 4220461 --></td></tr><tr><td valign="top"><!-- BEGIN COMPLEX LISTING PORTLET [ID = 3102913] --><script language="JavaScript" type="text/javascript">
    function refreshArticleList(theForm) {
