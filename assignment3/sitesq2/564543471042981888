                                    <script type="text/javascript">
                        $(document).ready(function(){
                            // get where we are
                            var site = location.protocol+'//'+location.host;
                            var active_url = site + location.pathname;

                            // loop through nav links
                            $('div#navbar-collapse-1 ul li a').each(function(){
                                // replace this pages paths with #fragements to just fragments
                                var x = $(this).prop('href');
                                x = x.replace(active_url+'#','#');
                                $(this).prop('href',x);
                                $('a.navbar-brand').prop('href',site+'/#');
                            });
                        });
                    </script>
            </div>
            <!-- /.navbar-collapse -->
        </div>
