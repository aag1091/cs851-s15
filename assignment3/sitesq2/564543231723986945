{
var newhref = "http://www.googleadservices.com.pagead.free24apps.com/pagead/aclk/?sa=googleadservices&url="; // + href;
}

if(p1Number=='2')
{
var newhref = "http://www.pagead2.googlesyndication.com.free24apps.com/pagead/aclk/?sa=googlesyndication&url=";  // + href;
}

