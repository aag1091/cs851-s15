    <h1 class="cN-headingPage" itemprop="name">Google paying a fraction of the tax in Australia it should</h1>
    <!-- google_ad_section_end -->
    <div class="meta hnews">
        <dl>
            <dt class="hiddenVisually">Date</dt>
            <dd class="updated dtstamp">
                <time itemprop="datePublished" datetime="February 9, 2015">
                February 9, 2015</time>
            </dd>
        </dl>
        <!--todo: on UGC-->
        <ul class="tools">
            <li class="reading collectorHidden" style="display:none">(29)</li>
            <li class="clipping" data-assetid="d-139176" data-assettype="ARTICLE" data-asseturl="http://www.smh.com.au/business/google-paying-a-fraction-of-the-tax-in-australia-it-should-20150208-139176.html">
                    <button class="btnClipping btnFauxLink" type="button">Read later</button>
