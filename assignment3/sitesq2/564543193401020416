//<![CDATA[
jQuery(window).load(function() {
	var $ = jQuery;
	jQuery("#main-slider").flexslider({
		animation: "slide",
		easing: "easeInCubic",
		slideshowSpeed: 4000,
		pauseOnHover: true,
		start: function() {
			$(".slider-preloader").css({
				display: "none"
			});
			$("#main-slider").css({visibility:"visible"}).find("img").animate({opacity:"1"},800);
			$(".flex-caption").css({
				display: "block"
			}).stop().animate({
				bottom: "30px"
			});
		},
		before: function() {
			$(".flex-caption").stop().css({
				display: "none",
				bottom: "0",
				opacity: "0"
			});
		},
		after: function() {
			$(".flex-caption").css({
				display: "block"
			}).stop().animate({
				bottom: "30px",
				opacity: "0.9"
			});
		}
	});
	jQuery("#main-slider").hover(function() {
		$(this).find(".cap-title").addClass("cap-hov");
		$(this).find(".flex-direction-nav a").css({
			opacity: "1"
		});
		}, function() {
			$(this).find(".cap-title").removeClass("cap-hov");
			$(this).find(".flex-direction-nav a").css({
				opacity: "0"
		});
	});
});
//]]>
