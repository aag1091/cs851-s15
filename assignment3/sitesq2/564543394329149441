	<script>
		$("#lrr").click(function()
		{
				var a=$("#cdell li").css("width");
				a=a.replace(/px/,"");
				a=parseInt(a)+10;
				var len=$("#cdell li").size();
				
				zong=len*a;
				
				len2=len%3*a;
				if(len2==0)
				{
					zong=zong-300;
				}
				else
				{
					zong=zong-len2;
				}
				len=3*a;
				var left=$("#cdell").css("left");
						left=left.replace(/px/,"");
						
				if(left>-zong)
				{
						$("#ltt").show();
						var left=$("#cdell").css("left");
						left=left.replace(/px/,"");
						len=left-len;
						$("#cdell:not(:animated)").animate({left:len+"px"},function(){
						var left=$("#cdell").css("left");
						left=left.replace(/px/,"");
						len2=len%3*a;
						zong=zong+len2;
						if(left==-zong+len2)
						{
							$("#lrr").hide();
						}
										});
			}
		});

		$("#ltt").click(function()
		{
				var a=$("#cdell li").css("width");
				a=a.replace(/px/,"");
				a=parseInt(a)+10;
				var len=$("#cdell li").size();
				
				zong=len*a;
				
				len2=len%3*a;
				zong=zong-len2;
				
				len=3*a;
				var left=$("#cdell").css("left");
						left=left.replace(/px/,"");
				if(left<0)
				{
						$("#lrr").show();
						var left=$("#cdell").css("left");
						left=left.replace(/px/,"");
						len=parseInt(left)+parseInt(len);
						$("#cdell:not(:animated)").animate({left:len+"px"},function(){
						var left=$("#cdell").css("left");
						left=left.replace(/px/,"");
						if(left==0)
						{
							$("#ltt").hide();
						}
						});
				}
		});

		$(document).ready(function(){
		$("#ltt").hide();
		$("#cdell li").each(function(i)
			{
				$(this).click(function()
				{
					$(this).siblings("li").removeClass("cur").end().addClass("cur");
					var a=$(this).find(".image a").attr("class");
					var b=$(this).find(".image a").attr("id");
					var c=$(this).find(".zi").html();
					$("#dtp a").attr("href",a);
					$("#dtp .tp1 a").attr("href",a);
					$("#dtpzi a").attr("href",a);
					$("#dtp").find("img").attr("src",b);
					$("#dtpzi a").text(c);
				});
			});
		});
		</script>
