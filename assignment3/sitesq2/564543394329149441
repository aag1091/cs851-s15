					document.onkeydown = function(e){
					if(!e) e = window.event;//火狐中是 window.event
					if((e.keyCode || e.which) == 13){
					searchcon();
					return false;
					}
					}
					function searchcon(){
					var king = document.getElementById("king").value;
					if(bb()=="3")
					{
					var url ="http://so.cntv.cn/language/english?qtext="+king;
							
					window.open(url);
					}else if(bb()=="2")
					{
					var url ="http://so.cntv.cn/language/english?qtext="+king+"&type=0";
