<!-- Perfect Market Ad Initialization -->
<script type="text/javascript">
var pm_pgtp = "";
if (mps.pagevars.type == "franchise" || mps.pagevars.type == "wildcard" )
{
   if (mps.pagevars.cat == "US Homepage" || mps.pagevars.cat == "International Homepage")
     {
      pm_pgtp = "home";
     } 
   else if (mps.pagevars.slug == "Pre-Markets")
     {
      pm_pgtp = "chartssf";
     }
   else if (mps.pagevars.hierarchy.indexOf("Markets")!= -1 && mps.pagevars.cat != "Investing" && mps.pagevars.cat != "Stock Blog" && mps.pagevars.cat != "Financial Advisors")
     {
      pm_pgtp = "marketssf";
     }
   else 
    {
      pm_pgtp = "newssf";
    }
} 
else if (mps.pagevars.type == "blogpost" || mps.pagevars.type == "cnbcnewsstory" || mps.pagevars.type == "partnerstory" || mps.pagevars.type == "pressrelease" || mps.pagevars.type == "wirestory") 
{
 pm_pgtp = "article";
} 
else if (mps.pagevars.type == "slideshow") 
{
 pm_pgtp = "gallery";
}
else 
{
  pm_pgtp = "newssf";
}
</script>
