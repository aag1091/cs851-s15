			CNBC_Utils.execute('Remove URL Params', function() {
				$(function () {
					var intervalVar, url, maxTries, tries, sleepMiliseconds;
					sleepMiliseconds = 2000;
					maxTries = 10;
					tries = 0;
					intervalVar = setInterval(function () {
						tries++;
						if (tries > maxTries) {
							clearInterval(intervalVar);
							return;
						}
						if (window.s && window.s.prop1) {
							url = CNBC_Utils.removeUrlParams(window.location.href, ["trknav"]);
							CNBC_Utils.replaceBrowserUrl(url);
							clearInterval(intervalVar);
						}
					}, sleepMiliseconds);
				});
			});
	
