<script>
  //<![CDATA[
    (function() {
      var notLoggedIn = ($.cookie('user_credentials') === undefined && $.cookie('_current_user_id') === undefined);
      var dfpId = '10846520';
      var isDevMode = false;
      var targetingKeyValues = gon.targetingKeyValues || {};
    
      if(notLoggedIn){
        root.ChallongeAds.show(dfpId, isDevMode, targetingKeyValues);
      } else {
        $.ajax({
          url: "/users/show_ad",
          method: "GET",
          dataType: 'JSON',
          accepts: 'application/json'
        }).done(function(showAd, textStatus, jqXHR){
          if(showAd){
            root.ChallongeAds.show(dfpId, isDevMode, targetingKeyValues);
          }
        }).fail(function(jqXHR, textStatus, errorThrown){
          root.ChallongeAds.show(dfpId, isDevMode, targetingKeyValues);
        });
      }
    }).call(this);
  //]]>
</script>

<script>
  //<![CDATA[
    $(document).ready(function(){
      $('.premier-ax').click(function(){
        window.location = "http://challonge.com/premier";
        return false;
      });
    
      $('#premier-corner-ax').popover({
        html      : true,
        trigger   : 'hover',
        title     : '<div class="premier-badge-container"><img alt="Premier badge" src="//challonge.com/assets/premier-badge-a07c4b0176ee1a91b32c9ab441e4aacc.png" title="Challonge Premier Subscriber" /></div> Challonge Premier',
        content   : $('#challonge-premier-content').html(),
        placement : 'left',
        placement: function (tooltip, trigger) {
          _.defer(function(){
            $(tooltip).css({
              top  : $(trigger).offset().top,
              left : ($(trigger).offset().left - $(tooltip).width() - 15)
            });
          });
        },
        container : 'body',
        delay     : { show: 100, hide: 250 }
      });
    
      $('#premier-footer-ax').popover({
        html      : true,
        trigger   : 'hover',
        title     : '<div class="premier-badge-container"><img alt="Premier badge" src="//challonge.com/assets/premier-badge-a07c4b0176ee1a91b32c9ab441e4aacc.png" title="Challonge Premier Subscriber" /></div> Challonge Premier',
        content   : $('#challonge-premier-content').html(),
        placement : 'top',
        placement: function (tooltip, trigger) {
          _.defer(function(){
            $(tooltip).css({
              top  : ($(trigger).offset().top - $(tooltip).height() - 5),
              left : ($(trigger).offset().left)
            });
          });
        },
        container : 'body',
        delay     : { show: 100, hide: 250 }
      });
    
    
    });
  //]]>
</script>


