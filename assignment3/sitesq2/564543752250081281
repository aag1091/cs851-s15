  //<![CDATA[
    $('.embed-code-toggle').click(function() {
      var $modal = $('.tournament-bracket-modal');
      $('div', $modal).html($('.embed_code').html());
      $modal.modal();
      $('textarea:first', $modal).select();
      return false;
    });
  //]]>
</script>
<script>
  //<![CDATA[
    $('.overthrow_bracket').dragscrollable({dragSelector:'div:first'});
    $('.full-screen-toggle').click(function() {
      if(BigScreen.enabled) {
        BigScreen.request($('.full-screen-target')[0]);
        $('.overthrow_bracket').append($('.tournament-bracket-modal'));
        return false;
      }
      else {
        return true;
      }
    });
    $('table.match-tipsy td.core').tipsy({
      live: true, html: true, gravity: 'w', offset: -5, opacity: 1, hideTip: true, allowTipHover: true, delayOut: 20,
      title: function() {
        var content = $('#' + this.getAttribute('id') + 'content');
        if(content.hasClass('enabled')) {
          return content.html();
        }
        else {
          return '';
        }
      }
    });
    
    $('.tournament-bracket').on('ajax:success', '.mark-underway a', function(xhr, data) {
      $(this).closest('.match_table').removeClass('open-not-underway');
      $(this).closest('.match_table').addClass('open-underway');
    });
    
    $('.tournament-bracket').on('ajax:success', '.unmark-underway a', function(xhr, data) {
      $(this).closest('.match_table').removeClass('open-underway');
      $(this).closest('.match_table').addClass('open-not-underway');
    });
  //]]>
</script>
<script>
  //<![CDATA[
    root.ScoreReportingPermissions.getInstance().definePermissions(gon.participantUserIdMap, gon.adminIds);
    
    var _cookie_u = $.cookie('_current_user_id');
    if (_cookie_u === undefined) {
      var credentialsVal = $.cookie('user_credentials'); // for caching, not permissions
      _cookie_u = (credentialsVal === undefined ? '' : credentialsVal.split('::')[1]);
    }
    
    function applyBracketPermissions() {
      root.ScoreReportingPermissions.getInstance().showElementsForUserId(_cookie_u);
    }
    
    function refreshBracket() {
      $.ajax({
        url: '/tournaments/1452742/matches/refresh?num_rounds=0&last_update=' + $('#last_update').val() + '&u=',
        beforeSend: function() { $('#bracket_refresh_loader').show(); },
        complete: function() {
          _.defer(function() {
            $('#bracket_refresh_loader').hide();
            applyBracketPermissions();
          });
        }
      });
    }
  //]]>
</script>

