(function(Etsy){
  Etsy.Monitoring = {page_group:'Akamai',BW:{enabled:false}};
  Etsy.RequestUUID = 'mrCPJY4chYGQP9ltUjjtVOFjhpUj';

  BOOMR = window.BOOMR || {};
