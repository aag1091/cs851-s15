	<script>
		function sendtafform(){
			if($.trim($('#TAF_myname').val())=="") {alert("Please fill this in!");$('#TAF_myname').focus();return;}
			if($.trim($('#TAF_name').val())=="") {alert("Please fill this in!");$('#TAF_name').focus();return;}
			if($.trim($('#TAF_email').val())=="") {alert("Please fill this in!");$('#TAF_email').focus();return;}
			if($('#TAF_email').val().indexOf('@') == -1 || $('#TAF_email').val().indexOf('.') == -1) {alert("Email is incorrect!");$('#TAF_email').focus();return;}
			$('#prodlink').val(window.location.href);
			//alert($('#tafform').serialize());
			$.post("http://www.chichiclothing.com/etail/emailafriend.php",$('#tafform').serialize(),function(data){$('#myModal').trigger('reveal:close');});
		}	

	</script>
	
