	$(document).ready(function () {
		

		
		$("a.refinea").on('click',function() {
			if($(window).width()<=700) {
				$("div#LayoutColumn1.Left").fadeOut('slow');
			}
		});
		
		$(window).resize(function() {
		  //resize just happened, pixels changed
		  if($(window).width()>=700) {
				$("div#LayoutColumn1.Left").fadeIn('slow');
			}
		});
	});
	
