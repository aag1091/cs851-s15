		function reDoBag() {
			$.ajax({
				type: 'GET',
				url: 'http://www.chichiclothing.com/ajax-get-mini-cart.php',
				data: {bagtype:4},
				dataType: 'text',
				success: function (data) {
	
					//$("#bagcontent").html(data);
					$("#Bag").html(data);
		
				}
			});

		}

		reDoBag();
		</script>
					
