				<script>
				function showAccord(whichhdr,whichaccord){
					if($('.'+whichhdr).is('.inactive-header')){
						$('.active-header').removeClass('inactive-header').removeClass('active-header').addClass('inactive-header');
						//$('.open-content').slideUp().removeClass('open-content');
						$('.open-content').hide().removeClass('open-content');
						$('.'+whichhdr).removeClass('inactive-header').addClass('active-header');
						//$('.'+whichaccord).slideDown().addClass('open-content');
						$('.'+whichaccord).show().addClass('open-content');
					} else {
						$('.active-header').removeClass('inactive-header').removeClass('active-header').addClass('inactive-header');
						$('.open-content').hide().removeClass('open-content');
					}
				}
				$(document).ready(function()
				{
					showAccord('acpd','acpdcontent');

				});
				</script>
				
