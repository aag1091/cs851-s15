<script>

function removeZoom(){
	try{
		var element = document.getElementById("piczoom");
		element.parentNode.removeChild(element);	
		element = document.getElementById("pzbg");
		element.parentNode.removeChild(element);	
	}catch(err){}

}

function showZoom(){
	removeZoom();
	
	var dd=document.createElement("div");
	dd.id='pzbg';
	document.body.appendChild(dd);
	$('#pzbg').css('height',$(document).height()+'px').show();
	$('#pzbg').click(function(){removeZoom();});

	var dd=document.createElement("iframe");
	dd.id='piczoom';
	document.body.appendChild(dd);


	
	var divwidth=($(window).height()-100)*0.75;
	var divheight=$(window).height()-100;
	var divleft=($(window).width()/2)-(divwidth/2);
	var divtop=$('body').scrollTop()+25;
	$('#piczoom').css('width',divwidth).css('height',divheight).css('top',divtop+'px').css('left',divleft+'px').fadeIn('slow');
	$('#piczoom').attr('src','/fashionnexus/prodzoom.php?prodid='+productId);


}
</script>
<style>
#piczoom{
	background:white;
	z-index:9999;
	position:absolute;
	box-shadow:10px 10px 5px #888888;
}
#pzbg{
	z-index:9998;
	width:100%;
	background:#333;
	opacity:0.5;
	position:absolute;
	top:0px;
}
</style>

			
		
