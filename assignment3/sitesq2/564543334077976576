<script>
$(document).ready(function(){
	$('.sizeli').click(
		function(){
			if(!$(this).hasClass('sizeli-selected')){
				$('.sizeli').removeClass('sizeli-selected').removeClass('sizeli-unselected').addClass('sizeli-unselected');
				$(this).removeClass('sizeli-unselected').addClass('sizeli-selected');
				var opt=$(this).find('input[type=radio]');
				opt.attr('checked','checked');
				opt.trigger('click');
			}
		}
	);
	
	/*$('.sizeli').hover(
		function(){
			$('#stocktooltip>div').html($(this).attr('instock')=="1"?"In Stock":"Out of Stock");
			//$('#stocktooltip').css('left',(55+($(this).attr('optcount')*36))+'px');
			$('#stocktooltip').css('left',$(this).position().left+'px');
			$('#stocktooltip').show();
		},
		function(){$('#stocktooltip').hide();}
	);*/
});
</script>

