<script>
function check_small_search_form2() {
	var search_query = g("search_query2");

	if((search_query.value == "")||(search_query.value =='Type Keyword')||(search_query.value =='SEARCH ITEM...')) {
		alert(lang.EmptySmallSearch);
		search_query.focus();
		return false;
	}
	return true;
}
</script>					
			
