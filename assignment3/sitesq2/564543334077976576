<script>
$(document).ready(
	
	function(){
		urltext =  $('.thumb0').attr('zoom');
		$('.prodpicsidethumb img').mouseover(
			function(){
				$('.ProductThumbImage a').attr('href',$(this).attr('zoom'));
				$('.ProductThumbImage img').attr('src',$(this).attr('src'));
				if($(window).width()>=1024){
					//forace image to be zoom sized when big screens to stop interpolation
					$('.ProductThumbImage img').attr('src',$(this).attr('zoom'));
				}	
				
				
				var urltext =  $(this).attr('zoom');
				CurrentProdThumbImage=$(this).attr('thumbnumber');	
				$('.ProductThumbImage a').zoom({url: urltext, icon: false});
			}
		);
		//setTimeout("initiateImageZoomer()",500);
		$('.ProductThumbImage a').attr('href',$('.thumb0').attr('zoom'));
		$('.ProductThumbImage img').attr('src',$('.thumb0').attr('src'));
		$('.ProductThumbImage a').zoom({url: urltext, icon: false});
	}
	
);
</script>

