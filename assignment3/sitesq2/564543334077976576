				function doTranslate() {
				document.getElementById("google_translate_element").innerHTML = "<ul class='loader'><li></li></ul>";
				var element = document.createElement("script");
				element.type = "text/javascript";
				element.src = "http://translate.google.com/translate_a/element.js?cb=googleTranslateElementInit";
				document.body.appendChild(element);
				}
				function googleTranslateElementInit() {
				document.getElementById("google_translate_element").innerHTML = "";
				new google.translate.TranslateElement({pageLanguage: 'en',  gaId: 'UA-31300768-1'}, 'google_translate_element');
				}
				</script>
				</form>		
