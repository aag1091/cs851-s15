  function doLogin()
  {
    var objHttp;
//    var login = document.getElementById("LOGIN").value;
    var login = document.HeaderFormular.LOGIN.value;
    var pwd = document.HeaderFormular.PWD.value;
    var url = "/cgi-app/login?LOGIN=" + login + "&PWD=" + pwd + "&AJAX=1";
    objHttp = getHttpObjectMember();

    objHttp.onreadystatechange = function()
    {
      var szResponse;

      switch( objHttp.readyState )
      {
        case 4:
          var szResponse = objHttp.responseText.split("\n");
          if( szResponse[0] == "ok" ){
            location.reload();
            break;
          }else{
            var MemberWindowFailed = window.open("/cgi-app/login?LOGIN="+login+"&PWD="+pwd, "MemberPopup", "menubar=no,toolbar=no,width=520,height=500,left=0,top=150,resizable=auto")
          }
          break;
        default:
	    break;
      }
    }
    objHttp.open("GET", url, true);
    objHttp.send("");
  }
