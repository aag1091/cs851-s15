<script>
$(document).ready(function(){
// Show Episode scripts

  $("img.lazy").lazyload();
  
$("#btnnotasamigos").click(function(){

$("#notasamigos").load("ajaxrequest.php?context=showFriendsWatched&serieid=4810&epnumber=109&type=");
})
  
$(".spoiller").click (function (event) {
  event.preventDefault();
  id=this.id;
  str=id.split("_"); 
  msgid=str[1];
  var count;
  frase=$(this).text();
  str=frase.split("(");
  if (str[1] == undefined )
  {
    count=0;
  } 
  else
  {
    str=str[1].split(")");
    count=str[0];
  }  
  rege=/Denunciar/
  if (rege.test(frase) ) 
  {
    count++; 
    $(this).text("Remover Denúncia (" + count + ")");
    $.ajax({
      url: "ajaxrequest.php?context=incSpoiller&msgid=" + msgid,
    });
  }
  else
  {
    count--;
    $(this).text("Denunciar Spoiller ("+count+")");
    $.ajax({
      url: "ajaxrequest.php?context=delSpoiller&msgid=" + msgid,
    });
  }  

}) 
  
});


</script>
