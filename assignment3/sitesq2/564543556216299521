<script>
function PopupCenter(url, title, w, h) {
    // Fixes dual-screen position                       Most browsers      Firefox
    var dualScreenLeft = window.screenLeft != undefined ? window.screenLeft : screen.left;
    var dualScreenTop = window.screenTop != undefined ? window.screenTop : screen.top;

    var left = ((screen.width / 2) - (w / 2)) + dualScreenLeft;
    var top = ((screen.height / 2) - (h / 2)) + dualScreenTop;
    var newWindow = window.open(url, title, 'scrollbars=yes, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);

    // Puts focus on the newWindow
    if (window.focus) {
        newWindow.focus();
    }
}
</script>


	

<script>
function UpdateNextPage(frm,nextpagevar,id)
{
	frm.NextPage.value=nextpagevar;
	
	if (id == 'Checkout')
		$.colorbox({html:'<p id="addcartprocessing" style="display: inline;" class="subhead1">Processing...<br></p>'});

}

function isNumber(n) {
  return !isNaN(parseFloat(n)) && isFinite(n);
}
</script>





		<form name="ProductList" action="Shop.cfm?pid=18978690497993872" method="Post">
		
		
		
		
		<script>
$(document).ready(function() {
     
    //if add button is clicked
    $('#AddProduct732').click(function () {  
	 var poststr = "";     
        obj=document.forms.ProductList;
		qtyadded=0;
	  //loop through all form fields creating the serialized data for the ajax hit. Data stored in variable poststr
      for (i=0; i<obj.elements.length; i++) 
      {
	  
	  //alert(obj.elements[i].type);
        if (obj.elements[i].type == "checkbox") {
           if (obj.elements[i].checked) {
              poststr += obj.elements[i].name + "=" + 
                   obj.elements[i].value + "&";
           } else {
              poststr += obj.elements[i].name + "=&";
           }
        } 
	else if (obj.elements[i].type == "textarea") 
	{ 
			temp_var = obj.elements[i].value.replace(/\n/g,'<br>')
			poststr += obj.elements[i].name + "=" + encodeURIComponent(temp_var) + "&";
	} 
	else if (obj.elements[i].type == "radio") 
	{
           if (obj.elements[i].checked) {
              poststr += obj.elements[i].name + "=" + 
                   obj.elements[i].value + "&";
           }
        }  
	else if (obj.elements[i].type == "select") {
  	    for (j=0; j<obj.elements[i].length; j++) {
            	var sel = obj.elements[i][j];
						if (sel.name.search(/ITEM_/i) != -1 && isNumber(sel.options[sel.selectedIndex].value))
						{
							qtyadded=qtyadded+Number(sel.options[sel.selectedIndex].value);
							poststr += sel.name + "=" + sel.options[sel.selectedIndex].value + "&";
						}
            
  	    }
    }  
	else if (obj.elements[i].type == "select-multiple" ) {
	    var vals = '';
  	    for (j=0; j<obj.elements[i].length; j++) {
            	var sel = obj.elements[i][j];
		if (sel.selected) {
			if (vals != '') {
				vals = vals + "," + sel.value;
			} 
			else 
			{
				vals = vals + sel.value;
			}
		}
  	    }
	    		if (vals != '') 
				{
						if (obj.elements[i].name.search(/ITEM_/i) != -1 && isNumber(vals))
						{
							qtyadded=qtyadded+Number(vals);
						}
            			poststr += obj.elements[i].name + "=" + vals + "&";
	    		}
	} 
   else if (obj.elements[i].type == "select-one")
   {
      			var sel = document.getElementById(obj.elements[i].name);
 				if (obj.elements[i].name.search(/ITEM_/i) != -1 && isNumber(sel.options[sel.selectedIndex].value))
				{
						qtyadded=qtyadded+Number(sel.options[sel.selectedIndex].value);
				}
				poststr += obj.elements[i].name + "=" + encodeURIComponent(sel.options[sel.selectedIndex].value) + "&";
   }
	else 
	{

				if (obj.elements[i].name.search(/ITEM_/i) != -1 && isNumber(obj.elements[i].value))
				{
						qtyadded=qtyadded+Number(obj.elements[i].value);
				}
           		poststr += obj.elements[i].name + "=" + 
                   	encodeURIComponent(obj.elements[i].value) + "&";
        }
      }

	 if (qtyadded > 0 && obj.NextPage.value != 'MixMatchKit') //they've added a quantity so do the ajax to update the cart and update the cart summary
	 {
		//bring up the color box popup
		//jQuery.fn.colorbox({href:'AddToCart_colorbox.cfm?pid=18978690497993872&CategoryID=732'});
		jQuery.fn.colorbox({href:'AddToCart_colorbox.cfm?pid=18978690497993872'});

        //start the ajax
        $.ajax({

            //this is the what we are hitting 
            url: "Shop.cfm?AjaxAdd=1&template=mynikken_cartsummary&pid=18978690497993872",
             
            //POST method is used
            type: "POST",
 
            //pass the form data
            data: poststr,    
             
            //Do not cache the page
            cache: false,
             
            //success
            success: function (html) {           

				//remove the word processing and show the text that it has been added
				document.getElementById('addcartprocessing').style.display='none';
				document.getElementById('addcartcompleted').style.display='inline';
				//show the checkout and continue buttons
				document.getElementById('addcartcompletedbtns').style.display='inline';
				//update the cart summary div layer with data returned
				document.getElementById('cartsummarydiv').innerHTML=html;
				//$.colorbox.resize();     
				
				//reset all the quantity boxes to empty
				
				if (document.getElementById('ITEM_1525__C732').type.search(/select_/i)) {$("#ITEM_1525__C732").val('');}
				
            }      
        });
	   }
	   else //they didnt add any quantity so show error
	   {
	   		if (obj.NextPage.value != 'MixMatchKit')
	   		{
	   		//bring up the color box popup
			//jQuery.fn.colorbox({href:'AddToCart_colorbox.cfm?NoItems=1&CategoryID=732&pid=18978690497993872'});
			jQuery.fn.colorbox({href:'AddToCart_colorbox.cfm?NoItems=1&pid=18978690497993872'});
	   		}  
	   }
		
        //cancel the submit button default behaviours
        if (obj.NextPage.value != 'MixMatchKit')
        	return false;
    });
}); 
		</script>
		
		
