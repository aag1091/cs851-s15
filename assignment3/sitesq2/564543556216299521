	    		if ( $(this).hasClass("minus") ){
					$(this).removeClass("minus");
					$(this).attr("src","https://nettrax.myvoffice.com/nikkenusa-iact-test/custom/nikken/images/plusicon.png");
	    		}else{
	    			$(this).addClass("minus");
				$(this).attr("src","https://nettrax.myvoffice.com/nikkenusa-iact-test/custom/nikken/images/minusicon.png");
	    		}
                        //$(this).siblings("ul").toggle();
	    		$(this).parent().siblings("td").children("table").toggle();
	    	});
			var CartCountry = $(".skuTable:contains('C$'),.skuTable2:contains('C$')");
            if (CartCountry.length > 0){$('#selectCanada').prop('checked', true);
            }else{$('#selectUSA').prop('checked', true);}
			
			//Add pwp={name} to all links to keep the user on the pwp. 
			//Otherwise, users will go to corp site (if not logged in.)
			var FindConsult = $("#pwpBoxMain a[href*='find']");
			if (FindConsult.length == 0){
				$("a").attr('href', function(i, h) {
				   return h + (h.indexOf('?') != -1 ? "&pwp=eyecu" : "?pwp=eyecu");
				});
			}

            // 11-14
            <!-- Black Friday - Shipping Promo - 11/14 -->
            var pwpTst = 'eyecu';
            console.log(pwpTst);
            if( pwpTst != ""){
                var pwpName = 'http://nikken.com/eyecu/esuite/corp/dPage?pageName=blackFriday';
                //$("#myLink a").attr('href', pwpName);http://nikken.com/esuite/corp/dPage?pageName=blackFriday
            }else{
                var pwpName = 'http://nikken.com/esuite/corp/dPage?pageName=blackFriday';
            }
            $("#myLink a").attr('href', pwpName);
            <!-- end Black Friday - Shipping Promo - 11/14 -->
		});
