var defaultStr = 'Search...';
var currentVal = $('#header_search_box .search_term').val();

if(currentVal == '' || currentVal == defaultStr) {
  $('#header_search_box .search_term').val(defaultStr).addClass('unclicked');
}

$('#header_search_box .search_term.unclicked').focusin(function() {
  if($(this).val() == '' || $(this).val() == defaultStr) {
    $(this).val('').removeClass('unclicked');
  }
});

$('#header_search_box .search_term').focusout(function() {
  if($(this).val() == '') {
    $(this).val(defaultStr).addClass('unclicked');
  }
});

