<script type="text/javascript">
function get_thd_thumb (el, videoid) {
	el.onerror = null;
	$.ajax({url: "http://www.trillhd.com/api/video/"+videoid, dataType: 'jsonp',
		success: function(data){
			if (typeof data.video.thumb_small !== 'undefined') {
				$(el).attr('src', data.video.thumb_small);
			}
		}
	});
}
</script>
