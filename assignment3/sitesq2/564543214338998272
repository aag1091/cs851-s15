<script type="text/javascript">

P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "There%20are%20lots%20of%20introductory%20C%20books%2C%20but%20this%20is%20the%20first%20one%20that%20has%20the%20no-nonsense%2C%20practical%20approach%20that%20has%20made%20Nutshell%20Handbooks%28r%29%20famous.%20C%20programming%20is%20more%20than%20just%20getting%20the%20syntax%20right.%20Style%20and%20debugging%20also%20play%20a%20tremendous%20part%20in%20creating%20programs%20that%20run%20well%20and%20are%20easy%20to%20maintain.%20This%20book%20teaches%20you%20not%20only%20the%20mechanics%20of%20programming%2C%20but%20also%20describes%20how%20to%20create%20programs%20that%20are%20easy%20to%20read%2C%20debug%2C%20and%20update.%20Practical%20rules%20are%20stressed.%20For%20example%2C%20there%20are%20fifteen%20precedence%20rules%20in%20C%20%28%26%26%20comes%20before%20%7C%7C%20comes%20before%20%3F%3A%29.%20The%20practical%20programmer%20reduces%20these%20to%20two%3A%20%20*%20Multiplication%20and%20division%20come%20before%20addition%20and%20subtraction.%20%20Contrary%20to%20popular%20belief%2C%20most%20programmers%20do%20not%20spend%20most%20of%20their%20time%20creating%20code.%20Most%20of%20their%20time%20is%20spent%20modifying%20someone%20else%27s%20%20code.%20%20This%20books%20shows%20you%20how%20to%20avoid%20the%20all-too-common%20obfuscated%20uses%20%20of%20C%20%28and%20also%20to%20recognize%20these%20uses%20when%20you%20encounter%20them%20in%20existing%20programs%29%20and%20thereby%20to%20leave%20code%20that%20the%20programmer%20responsible%20for%20maintenance%20does%20not%20have%20to%20struggle%20with.%20Electronic%20Archaeology%2C%20the%20art%20%20of%20going%20through%20someone%20else%27s%20code%2C%20is%20described.%20This%20third%20edition%20introduces%20popular%20Integrated%20Development%20Environments%20%20on%20Windows%20systems%2C%20as%20well%20as%20UNIX%20programming%20utilities%2C%20and%20features%20a%20large%20statistics-generating%20program%20to%20pull%20together%20the%20concepts%20and%20%20features%20in%20the%20language.",
	    bookDescriptionAvailableHeight,
	    minBookDescriptionInitialHeight = 112,
	    options = {};
	    
    function resizeCallback() {
       	var bdOffsetTop = document.getElementById("bookDescription_feature_div").offsetTop;
   		var imageBlockOffsetTop = document.getElementById("booksImageBlock_feature_div").offsetTop;
   		var imageBlockHeight = document.getElementById("booksImageBlock_feature_div").offsetHeight;
   		bookDescriptionAvailableHeight = imageBlockOffsetTop + imageBlockHeight -bdOffsetTop - 30;
   		if(bookDescriptionAvailableHeight < minBookDescriptionInitialHeight) {
   	        bookDescriptionAvailableHeight = minBookDescriptionInitialHeight;
   		}
    		
    	var psTotalHeight = document.getElementById("postBodyPS").offsetHeight;

    	if(psTotalHeight > bookDescriptionAvailableHeight + 30){
    		if(document.getElementById("bdSeeLessPrompt").style.display == "none"){
    	    	document.getElementById("outer_postBodyPS").style.height = bookDescriptionAvailableHeight + 'px';
    	    	document.getElementById("psPlaceHolder").style.display ="block";
    	    	document.getElementById("bdSeeAllPrompt").style.display ="block";
    		}
    		else{
    			document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
    		}
        }
        else{
        	document.getElementById("outer_postBodyPS").style.height = psTotalHeight + 'px';
        	document.getElementById("psPlaceHolder").style.display ="none";
        	document.getElementById("bdSeeAllPrompt").style.display ="block";
	    	document.getElementById("bdSeeLessPrompt").style.display ="none";
	    	document.getElementById("bdExpanderIcon").className = document.getElementById("bdExpanderIcon").className.replace("rotate","");
        }
    }
	    
	options.iframeId = "bookDesc_iframe";
	options.iframeWrapperId = "bookDesc_iframe_wrapper";
	options.overriddenCSSId = "bookDesc_override_CSS";
	options.encodedIframeContent = bookDescEncodedData;
	options.initialResizeCallback = resizeCallback;
	
	BookDescriptionIframe = new DynamicIframe(options);
	BookDescriptionIframe.createIframe();
	    
	if ((typeof BookDescriptionIframe != 'undefined') && (BookDescriptionIframe instanceof DynamicIframe)) {
	  P.when('jQuery').execute(function($) {
	    $(window).resize(function() {
	        BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	    $(window).bind('imageResize', function() {
	    	BookDescriptionIframe.resizeIframe(resizeCallback);
	    });
	  });
	}
});
</script>

        
        
        
        
        
        
    
    
    







