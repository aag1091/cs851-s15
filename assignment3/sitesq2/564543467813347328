document.domain = 'northjersey.com';
    var openCustTownWindow = function() { 
      var myWindow = window.open( 'http://ipost.northjersey.com/mynorthjersey/form.html', 'customizeNews', 'scrollbars=1,width=700,height=1000' );
    }
function readCCookie(name) {
	var cookiename = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++)
	{
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(cookiename) == 0) return c.substring(cookiename.length,c.length);
	}
	return null;
}

</script>




