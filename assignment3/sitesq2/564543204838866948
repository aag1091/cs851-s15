	require(["jquery"], function() {
		jQuery('div.trackable a').click(function(e) {
			var trackingTarget = e.currentTarget;
			//console.log("Path: " + trackingTarget.pathname);
			
			var parent = $(e.target).closest("div.trackable");
			//console.log("Tracking name: " + parent.attr("data-trackingname"));

			if(parent.length > 0){
				trackModuleClick(parent.attr("data-trackingname"), trackingTarget.pathname);
			}
		});
	});
</script>
