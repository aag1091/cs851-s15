        <!-- Hover More -->
      <script type="text/javascript" language="javascript">
       var backcolor_over = $("#more-item-backcolor").val(); // select item backcolor from hidden field
       var backcolor_out = $("#more-item-backcolor").val(); // select item backcolor from hidden field
       if (backcolor_over == "") {
         backcolor_over = "#662C91";
         backcolor_out = "transparent";
       }
       $("#sub_more").mouseover(function(){
          $("#sub_more .more").css("background-color", backcolor_over);
          // $(".more").css("background-color", "#662C91");          
          $("#sub_more .more").css("box-shadow","1px 1px 6px 1px #333 inset");
       }).mouseout(function(){
          $("#sub_more .more").css("background-color", backcolor_out);
          $("#sub_more .more").css("box-shadow","none");
       });

       $("#menu .more").mouseover(function(){
          $("#menu .more").css("background-color", backcolor_over);
          // $(".more").css("background-color","#662C91");
       }).mouseout(function(){
          $("#menu .more").css("background-color", backcolor_out);
       });
      </script>
        <!-- end Hover More -->

