    $(function(){ $("label").inFieldLabels(); });
  </script>

  <!-- Windows most read/comments -->
  <script type="text/javascript">
  function showlayerwin(layer){
    var myLayer=document.getElementById(layer);
    if(myLayer.style.display=="none" || myLayer.style.display==""){
      document.getElementById('win_most_read').style.display = 'none';
      document.getElementById('win_comments').style.display = 'none';
      myLayer.style.display="block";
    } else { 
      myLayer.style.display="none";
    }
  }

  function showlayerwin_1(layer){
    var myLayer1=document.getElementById(layer);
    if(myLayer1.style.display=="none" || myLayer1.style.display==""){
      document.getElementById('win_reactions').style.display = 'none';
      document.getElementById('win_trending').style.display = 'none';
      myLayer1.style.display="block";
    } else { 
      myLayer1.style.display="none";
    }
  }
  </script>

