<script type="text/javascript">
// jquery used to paginate the published articles
$(document).ready(function() {
    $("a.bt_picks_prev").click(function() {
        var offset = $("#prev-offset").val();
        var limit = $("#limit").val();
        var total = $("#total").val();
        var dataString = 'offset='+ offset + '&limit=' + limit + '&total=' + total;
        $.ajax
        ({
            type: "POST",
            url: "get_published_articles.php",
            data: dataString,
            cache: false,
            beforeSend: function() { $('#published_content').html("<img src='images/loading1.gif' />"); },
            success: function(html) { 
              $("#published_content").html(html); 
              $(document).scrollTop( $("#published_content").offset().top ); // scroll up to the articles beginning
            }
        });
    });
    $("a.bt_picks_next").click(function() {
        // alert("clicked en next");
        var offset = $("#next-offset").val();
        var limit = $("#limit").val();
        var total = $("#total").val();
        var dataString = 'offset='+ offset + '&limit=' + limit + '&total=' + total;
        $.ajax
        ({
            type: "POST",
            url: "get_published_articles.php",
            data: dataString,
            cache: false,
            beforeSend: function() { $('#published_content').html("<img src='images/loading1.gif' />"); },
            success: function(html) { 
              // alert("success");
              $("#published_content").html(html); 
              // $(document).scrollTo("#published_content"); // Scroll Screen to Target Element
              $(document).scrollTop( $("#published_content").offset().top ); // scroll up to the articles beginning

            }
        });
    });
});
</script>

