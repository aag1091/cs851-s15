  <!-- Popup login/signup/tip -->
  <script type="text/javascript">
  function showlayer(layer){
    var myLayer=document.getElementById(layer);
    if(myLayer.style.display=="none" || myLayer.style.display==""){
      document.getElementById('popup_login').style.display = 'none';
      document.getElementById('popup_signup').style.display = 'none';
      document.getElementById('popup_sendtip').style.display = 'none';
      // document.getElementById('popup_signup_reactions').style.display = 'none'; // for article reactions
      myLayer.style.display="block";
    } else { 
      myLayer.style.display="none";
    }
  }
  </script>

  <!-- Form labels -->
  <script type="text/javascript" charset="utf-8" src="js/jquery.infieldlabel.min.js"></script>
