<!-- recaptcha code -->
<script type="text/javascript">
function validateCaptcha()
{
    challengeField = $("input#recaptcha_challenge_field").val();
    responseField = $("input#recaptcha_response_field").val();
    // alert(challengeField);
    // alert(responseField);
    //return false;
    var html = $.ajax({
    type: "POST",
    url: "_ajax.recaptcha.php",
    data: "recaptcha_challenge_field=" + challengeField + "&recaptcha_response_field=" + responseField,
    async: false
    }).responseText;
    // alert(html);
    if (html == "success") {
      $("#captchaStatus").html(" ");
      return true;
    } else {
      $("#captchaStatus").html("Your captcha is incorrect. Please try again");
      Recaptcha.reload();
      return false;
    }
}
</script>

<script type="text/javascript">  
  $(document).ready(function(){
     
      $('.popup_signin_form').submit(function(e){
          $.ajax({
            type:'POST',
            url: 'find_email_users.php', 
            data: $('.popup_signin_form').serialize(), 
            success: function(salida) {
              var result = $('#user_password').html(salida);              
              // alert(result);
            }
          })
        });

      // this jquery function changes dynamically the form's action attibute value.
      $("#top_search_form").submit(function(e) {
          var initial_action_setting = $("#top_search_form").attr("action"); //will retrieve action="search/"
          var search_string = $("#search_field").val(); // search field value after "search/" string
          search_string = search_string.replace(/\s/g, '-').toLowerCase(); // replace spaces with dashes
          var action_path = initial_action_setting + search_string;
          if (action_path != "") {
            $("#top_search_form").attr("action", action_path); //will set it                      
          } else { // no value search value
            e.preventDefault();
          }
      });
    
     $(".popup_signup_form").submit(function() {
        var error_validation = true;  
          // email duplication validation 
        if ($("#error_email").text() == "1") {
          $("#error_ema").text("User already exists in our database.").show().fadeOut(5000);
          error_validation = false;
        }         
        return error_validation;
     });     
  }); // end Jquery  
</script>  

