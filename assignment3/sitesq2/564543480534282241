}
	
	
function iphoneURL()
{
	document.location="http://wiki.alumni.net/wiki/Wikimap";
}
 
function loadNewPoint()
{
	var mapLat=theMap.getCenter().lat();
	var mapLng=theMap.getCenter().lng();
	
	var mapZoom=theMap.getZoom();
	
				if(document.URL.match("wikitest"))
				{
					document.location="http://wikitest.alumni.net/AddNewWikiPoint.html#" + mapLat + "&" + mapLng + "&" + mapZoom;
				}
				else
				{
					document.location="http://wiki.alumni.net/AddNewWikiPoint.html#" + mapLat + "&" + mapLng + "&" + mapZoom;
				}
	
}



