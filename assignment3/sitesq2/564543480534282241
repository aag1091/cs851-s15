	<script type="text/javascript">
		$(document).ready(function() {
			$("#search_q").watermark('search...', "#999999");
		
			$("#search_q").blur(function(data) {
				$(this).css("color", "#8C8C8C");
			});
		});
		
		function changeField(form) {
			if(form.value == 'Member') {
				$("#search_q").watermark('e.g. Doe, John; user@email.com', "#999999");
		
			} else if(form.value == 'Affiliation') {
				$("#search_q").watermark('friend\'s or groupmate\'s company...', "#999999");
			}else {
				$("#search_q").watermark('search...', "#999999");
			}
		}
	</script>
