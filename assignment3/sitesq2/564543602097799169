			<script>
			  var storylinkloadcomments = storylink.replace(" ","") + '?template=loadcomments';	
			  var storylinkpostcomments = storylink.replace(" ","") + '?template=postcomments';	
			  var commentsURL = storylinkloadcomments;
			  var commentsURL2 = storylinkpostcomments;
			  $td(document).ready(function(){
				if (enableForum == 1 || enableForum == 3) {
						$td("#ajaxloadedcomment").load(commentsURL, function() {
							$td(".comment-time").each(function() {
								var timeTemp = moment($td(this).text()).tz("America/New_York").fromNow();
								$td(this).text(timeTemp);
							});
							if(Modernizr.mq('(min-width: 769px)')) {
								$td(".report-abuse").hide().mouseenter(function() {
									$td(this).find(".report-text").stop().fadeIn();
									}).mouseleave(function(){
									$td(this).find(".report-text").stop().fadeOut();
								});
								$td(".comment-panel").mouseenter(function() {
									$td(this).find(".report-abuse").stop().fadeIn();
									}).mouseleave(function(){
									$td(this).find(".report-abuse").stop().fadeOut();
								});
							}
						    //show comments if enabled.
							if($td.cookie('SHOW_COMMENTS')=="yes") {
								$td("#commentsViewAll").fadeIn();
								$td("#readComments").hide();
							}
						});       
						$td("#postComments").load(commentsURL2);
				}
			    else {
					$td("#postComments").html('Comments have been disabled');
					$td("#commentsViewAll").hide();
				}
				$td(".viewcomments").click(function(z) {
					z.preventDefault();
					z.stopPropagation();
					$td("#commentsViewAll").fadeIn();
					$td("#readComments").hide();
				});
				$td(".hide-comments").click(function(z){
					z.preventDefault();
					z.stopPropagation();
					$td("#readComments").fadeIn();
					$td("#commentsViewAll").fadeOut();
					$td(".comments-select button.active").removeClass("active");
					$td("#hide-comments").addClass("active");
					 $td.cookie('SHOW_COMMENTS', 'no', {expires:10000, path:'/'});
				});
			  }); 
			  </script>
