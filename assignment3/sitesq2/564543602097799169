	<script>
	$td(document).ready(function() {
		if($td(".fullStory p").length) {
			var pCount = $td(".fullStory p");
			if (pCount.length<3) {
				var pLength;
				var bigP;
				pCount.each(function(n) {
					if(n) {
						if (pLength<$td(this).text().length) {
							pLength = $td(this).text().length;
							bigP = n;
						}
					}
					else {
						pLength = $td(this).text().length;
						bigP = 0;
					}
				});
				var splitBigP = $td(".fullStory p:eq("+bigP+")").html().split(/<br><br>/);
				if(splitBigP.length>2) {
					$td(".fullStory").html("");
					for (para in splitBigP) {
						$td(".fullStory").append("<p>"+splitBigP[para]+"</p>");
					}
				}
			}
			var twitterEncodedURL = encodeURI("http://www.theday.com/article/20150208/"+catId+"/302089972");
			var surl1 = "http://urls.api.twitter.com/1/urls/count.json?url="+twitterEncodedURL;
			var surl2 = "http://graph.facebook.com/?id="+twitterEncodedURL;
				$td.ajax({
					type: 'GET',
					url: surl1,
					crossDomain: true,
					contentType: "application/json; charset=utf-8",
					dataType: "jsonp",
					success: function(data) {
						if(data.count) {
							$td("#twshares").html(data.count).addClass("badge");
						}
					},
					error: function(xhr, status, error) { 
						console.log(xhr+','+status+','+error); 
					},
					async: true,
					cache: false
				});
				$td.ajax({
					type: 'GET',
					url: surl2,
					crossDomain: true,
					contentType: "application/json; charset=utf-8",
					dataType: "jsonp",
					success: function(data) {
							if(data.shares) {
								$td("#fbshares").html(data.shares).addClass("badge");
							}
							if (data.shares>1000) {
								$td("#facebookShare").css("paddingRight","7px");
							}
							else if(data.shares>100) {
								$td("#facebookShare").css("paddingRight","2px");
							}
							else {
								$td("#facebookShare").css("paddingRight","0px");
							}
					},
					error: function(xhr, status, error) { 
						console.log(xhr+','+status+','+error); 
					},
					async: true,
					cache: false
				});
			$td("#facebookShare a").attr("href","https://www.facebook.com/sharer/sharer.php?u="+twitterEncodedURL);
			var encodedHead = encodeURI("10 ways to give your credit score 100-point boost");
			$td("#twitterShare a").attr("href","http://twitter.com/share?text="+encodedHead+"&url="+twitterEncodedURL+"&via=thedayct");
			$td("#twitterShare a").mouseover(function() {
				if($td(this).attr("href").indexOf("bit.ly")==-1) {
					$td.getJSON('http://api.bitly.com/v3/shorten?callback=?',
						{
							format: "json",
							apiKey: "R_d4a7d3145af84183a35e3901a50150da",
							login: "sfcath",
							longUrl: "http://www.theday.com/article/20150208/"+catId+"/302089972"
						},
						function(response) {
							if(response.data.url) {
								$td("#twitterShare a").attr("href","http://twitter.com/share?text="+encodedHead+"&url="+response.data.url+"&via=thedayct");
							}
						}
					);
				}
			});
			$td("#emailShare a").attr("href","mailto:?subject=I%20wanted%20you%20to%20see%20this%20article%20on%20theday.com&body=Here%20is%20the%20link:%20"+twitterEncodedURL);
			$td(".sharebox-left").prependTo(".fullStory");
			pCount = $td(".fullStory p");
			var pInjectAd = (pCount.length/2).toFixed(0);
			if(pInjectAd>=5) {
				$td("#box-STF").css({"float": "right", "clear": "right"}).appendTo(".fullStory p:nth-child("+pInjectAd+")");
			}
			if($td(".extra-img-cont").length > 1) {
				if(pCount.length>14) {
					var insertMult = .23;
					$td(".extra-img-cont").each(function() {
						$td(this).insertAfter(".fullStory p:nth-child("+(pCount.length*insertMult).toFixed(0)+")");
						insertMult = insertMult+.21;
					});
				}
				else {
					$td(".extra-img-cont").insertAfter(".fullStory");
				}
			}
			else {
				if(pCount.length>10) {
					$td(".extra-img-cont").insertAfter(".fullStory p:nth-child("+(pCount.length*0.75).toFixed(0)+")");
				}
				else {
					$td(".extra-img-cont").insertAfter(".fullStory");
				}
			}
			if(relatedFound) {
				var pInjectMedia = (pCount.length*.60).toFixed(0);
				var pInjectGallery = (pCount.length*.60).toFixed(0);
				if(pCount.length>7) {
					if(relatedMedia) {
						$td(".related-media").css({"float": "right", "clear": "right"}).appendTo(".fullStory p:nth-child("+pInjectMedia+")").show();
						$td(".related-media").find(".section-header").addClass("inline-head");
					}
					if(relatedGallery) {
						$td(".related-galleries").css({"float": "right", "clear": "right"}).appendTo(".fullStory p:nth-child("+pInjectGallery+")").show();
						$td(".related-galleries").find(".section-header").addClass("inline-head");
					}
				}
				else {
					if(relatedMedia) {
						$td(".related-media").appendTo(".related-media-small").show();
						$td(".related-media").find(".section-header").addClass("inline-head");
						$td(".related-content-cont").addClass("col-md-5 pull-left-md").show();
						$td(".article-assets-cont .ad-cont").addClass("col-md-6 pull-right-md");
					}
					if(relatedGallery) {
						$td(".related-galleries").appendTo(".related-gallery-small").show();
						$td(".related-galleries").find(".section-header").addClass("inline-head");
						$td(".related-content-cont").addClass("col-md-5 pull-left-md").show();
						$td(".article-assets-cont .ad-cont").addClass("col-md-6 pull-right-md");
					}
				}
			}
		}
		if(Modernizr.mq('(max-width: 769px)')) {
			$td(".prev-next-cont, .fullStory, .article-head").swipe({
			  threshold:125,
			  allowPageScroll:"vertical",
			  swipe:function(event, direction, distance, duration, fingerCount)
				{
					if (direction == "right") {
						window.location.href = ($td("#prev-article a").attr("href"));
					}
					else if (direction == "left") {
						window.location.href = ($td("#next-article a").attr("href"))
					}
				}
			});
		}
		//Overrides 
		// -- make sure that every table has class "table" and "table-striped"
		$td("table").each(function() {
			$td(this).addClass("table table-striped");
		});	
		//Print it
		$td("#printit").click(function(n) {
			n.preventDefault();
			n.stopPropagation();
			window.print();
		});
	});
	
		var enableForum = 1;
	
	var storylink = '/business/20150208/10-ways-to-give-your-credit-score-100-point-boost';
	</script>
	
