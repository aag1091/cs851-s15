            $(window).scroll(function() {
                var scrollSpot =  200;
                checked = true;

                //display nav if the distance between the current height of the viewport is higher than the "scrollSpot" variable
                if ($('body').scrollTop() >= scrollSpot && $('.navbar-fixed-top').hasClass('nav-hidden')) {
                    $('.navbar-fixed-top').removeClass('nav-hidden');
                } else if ($('body').scrollTop() < scrollSpot && !$('.navbar-fixed-top').hasClass('nav-hidden')) {
                    $('.navbar-fixed-top').addClass('nav-hidden');
                }
                if ($('body').scrollTop() >= scrollSpot && $('.social-sticky').hasClass('sticky-hidden')) {
                    $('.social-sticky').removeClass('sticky-hidden');
                } else if ($('body').scrollTop() < scrollSpot && !$('.social-sticky').hasClass('sticky-hidden')) {
                    $('.social-sticky').addClass('sticky-hidden');
                }
            });
        </script>

