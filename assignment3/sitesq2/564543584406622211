    $(document).ready(function () {
        // run only if mobile width
        if ($(window).width() < '661') {
            // read cookie value
            var cookieValue = $.cookie('whosay-mobile-signup');

            // if cookie is not set to 1, then slide up
            if (cookieValue != 1) {
                setTimeout(function() {
                    //slide up
                    $('.mobile-signin').addClass('slideUp');
                // wait 6 seconds
                }, 6000);
            }

            // close button, slide down
            $('.mobile-signin .close-btn').on('click', function () {
                $('.mobile-signin').removeClass('slideUp');

                //set cookie to expire in 1 day
                $.cookie('whosay-mobile-signup', 1, { expires : 1 });
            });
