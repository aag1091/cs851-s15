<script>
  var auto = true;
  var img_length = "3";
  (img_length > 1) ? a = 4 : a = 0;
  $(document).ready(function(){
    $('.jcarousel-skin-tango').jcarousel({
        scroll: 1, 
        auto: a,
        wrap: "circular",
        buttonNextHTML: null,
        buttonPrevHTML: null,
        initCallback: init_carousel
      });
      
      if(auto){
        $('#carousel-pause').show();
        $('#carousel-play').hide();
      }
      else{
        $('#carousel-pause').hide();
        $('#carousel-play').show();
      }
        
  });
  
  function init_carousel(carousel){
  
    carousel.startAutoOrig = carousel.startAuto; 
    
    carousel.startAuto = function() { 
      if (!carousel.paused) { 
        carousel.startAutoOrig(); 
      } 
    } 
    carousel.pause = function() { 
      carousel.paused = true; 
      carousel.stopAuto(); 
    }; 
    carousel.play = function() { 
      carousel.paused = false; 
      carousel.startAuto(); 
    };
  
    $('#carousel-prev').bind('click', function(){
      carousel.prev();
    });
  
    $('#carousel-next').bind('click', function(){
      carousel.next();
    });
  
    $('#carousel-play').bind('click', function(){
      carousel.play();
      $(this).hide();
      $('#carousel-pause').show();
    });
    $('#carousel-pause').bind('click', function(){
      carousel.pause();
      $(this).hide();
      $('#carousel-play').show();
    });
    
    var auto = true;
    if(auto){
      carousel.play();
      $('#carousel-play').hide();
    }
    else{
      carousel.pause();
      $('#carousel-pause').hide();
    }
  }
</script>

