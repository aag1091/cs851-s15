<script>
    $(document).ready(function(){
      $('.more-genres a').click(function(){
        $('.genres').toggle('slow');
        $(this).text($(this).text() === "More..." ? "Hide" : "More..." );
      });
    
      if ($("#change-profile-picture").length == 0) {
          $("#verified").hover(function(){
            $("#badged_message").show();
          }, function(){
            $("#badged_message").hide();
          })
      }
  
      $('#contact_tabs div').hide();
      $('#contact_tabs div:first').show();
      $('#contact_tabs ul li:first').addClass('active');
      $('#contact_tabs ul li a').click(function(){
        $('#contact_tabs ul li').removeClass('active');
        $(this).parent().addClass('active');
        var currentTab = $(this).attr('href');
        $('#contact_tabs div').hide();
        $(currentTab).show();
        return false;
      });
  
      $('#bios div').hide();
      $('#bios div:first').show();
      $('#bios ul li:first').addClass('active');
      $('#bios ul li a').click(function(){
        $('#bios ul li').removeClass('active');
        $(this).parent().addClass('active');
        var currentTab = $(this).attr('href');
        $('#bios div').hide();
        $(currentTab).show();
        if ($(this).text() == 'About') {
          $('.bio-btn').text('Edit bio');
          $('.bio-btn').attr('href','/pages/groups/inspace/profile/biography');
        }else{
          $('.bio-btn').text('Edit press');
          $('.bio-btn').attr('href',"/pages/groups/inspace/quotes");
        }
  
        return false;
      })
  
  
  
  
      //JPLAYER STUFF
  
      var Playlist = function(instance, playlist, options) {
          var self = this;
  
          this.instance = instance; // String: To associate specific HTML with this playlist
          this.playlist = playlist; // Array of Objects: The playlist
          this.options = options; // Object: The jPlayer constructor options for this playlist
  
          this.current = 0;
  
          this.cssId = {
            jPlayer: "jquery_jplayer_",
            interface: "jp_interface_",
            playlist: "jp_playlist_"
          };
          this.cssSelector = {};
  
          $.each(this.cssId, function(entity, id) {
            self.cssSelector[entity] = "#" + id + self.instance;
          });
  
          if(!this.options.cssSelectorAncestor) {
            this.options.cssSelectorAncestor = this.cssSelector.interface;
          }
  
          $(this.cssSelector.jPlayer).jPlayer(this.options);
  
          $(this.cssSelector.interface + " .jp-previous").click(function() {
            self.playlistPrev();
            $(this).blur();
            return false;
          });
  
          $(this.cssSelector.interface + " .jp-next").click(function() {
            self.playlistNext();
            $(this).blur();
            return false;
          });
        };
  
        Playlist.prototype = {
          displayPlaylist: function() {
            var self = this;
            $(this.cssSelector.playlist + " ul").empty();
            for (i=0; i < this.playlist.length; i++) {
              var listItem = (i === this.playlist.length-1) ? "<li class='jp-playlist-last'>" : "<li>";
              listItem += "<a href='#' id='" + this.cssId.playlist + this.instance + "_item_" + i +"' tabindex='1'>"+ this.playlist[i].name +"</a>";
              
              if(this.playlist[i].curated){
                listItem += "<i class='curation_label'>Added to Musicpage Curation Library</i>";
                listItem = listItem.replace('>', ' style="background-color: #91E1FD;">');
              }
  
              // Create links to free media
              if(this.playlist[i].free) {
                var first = true;
                listItem += "<div class='jp-free-media'>(";
                $.each(this.playlist[i], function(property,value) {
                  if($.jPlayer.prototype.format[property]) { // Check property is a media format.
                    if(first) {
                      first = false;
                    } else {
                      listItem += " | ";
                    }
                    listItem += "<a id='" + self.cssId.playlist + self.instance + "_item_" + i + "_" + property + "' href='" + value + "' tabindex='1'>" + property + "</a>";
                  }
                });
                listItem += ")</span>";
              }
  
              listItem += "</li>";
  
              // Associate playlist items with their media
              $(this.cssSelector.playlist + " ul").append(listItem);
              $(this.cssSelector.playlist + "_item_" + i).data("index", i).click(function() {
                var index = $(this).data("index");
                if(self.current !== index) {
                  self.playlistChange(index);
                } else {
                  $(self.cssSelector.jPlayer).jPlayer("play");
                }
                $(this).blur();
                return false;
              });
  
              // Disable free media links to force access via right click
              if(this.playlist[i].free) {
                $.each(this.playlist[i], function(property,value) {
                  if($.jPlayer.prototype.format[property]) { // Check property is a media format.
                    $(self.cssSelector.playlist + "_item_" + i + "_" + property).data("index", i).click(function() {
                      var index = $(this).data("index");
                      $(self.cssSelector.playlist + "_item_" + index).click();
                      $(this).blur();
                      return false;
                    });
                  }
                });
              }
            }
          },
          playlistInit: function(autoplay) {
            if(autoplay) {
              this.playlistChange(this.current);
            } else {
              this.playlistConfig(this.current);
            }
          },
          playlistConfig: function(index) {
            $(this.cssSelector.playlist + "_item_" + this.current).removeClass("jp-playlist-current").parent().removeClass("jp-playlist-current");
            $(this.cssSelector.playlist + "_item_" + index).addClass("jp-playlist-current").parent().addClass("jp-playlist-current");
            this.current = index;
            $(this.cssSelector.jPlayer).jPlayer("setMedia", this.playlist[this.current]);
          },
          playlistChange: function(index) {
            this.playlistConfig(index);
            $(this.cssSelector.jPlayer).jPlayer("play");
          },
          playlistNext: function() {
            var index = (this.current + 1 < this.playlist.length) ? this.current + 1 : 0;
            this.playlistChange(index);
          },
          playlistPrev: function() {
            var index = (this.current - 1 >= 0) ? this.current - 1 : this.playlist.length - 1;
            this.playlistChange(index);
          }
        };
  
        var audioPlaylist = new Playlist("2", [{"name":"Dump - This Town - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff75e922bf8b50001000047/02_dump_-_this_town_in_space_-_andrew_roth.mp3","curated":false},{"name":"Springtime - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff75f75058a3b000100003a/01_springtime__in_space.mp3","curated":false},{"name":"Wasted - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff75fd453069d000100002d/in_space-wasted.mp3","curated":false},{"name":"Greed - The Greed EP","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff762d553069d0001000036/02_greed_inspace.mp3","curated":false},{"name":"Summer Sorrow - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff762f5058a3b000100003e/01_summersorrow_inspace.mp3","curated":false},{"name":"Down Below - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff7630e53069d0001000037/downbelow.mp3","curated":false},{"name":"Torment In You - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff763422bf8b5000100004a/in_space-torment_in_you.mp3","curated":false},{"name":"So Glad - Single","mp3":"https://assets.musicpage.com/uploads/groups/song/file/4ff765342bf8b5000100004e/04_so_glad_andrew_scott_roth.mp3","curated":false}], {
          ready: function() {
            audioPlaylist.displayPlaylist();
            audioPlaylist.playlistInit(false); // Parameter is a boolean for autoplay.
          },
          ended: function() {
            audioPlaylist.playlistNext();
          },
          play: function() {
            $(this).jPlayer("pauseOthers");
          },
          swfPath: "/javascripts",
          solution: "flash, html",
          supplied: "oga, ogv, m4a,mp3"
        });
  
  
    });
</script>


</section>
