        <script LANGUAGE="JavaScript">
        	function ValidarHeader(){
			var RegEmail = /[\w-\.]{3,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;
			validar = true;
			if(document.Principal.text285090983.value.length == 0){
				document.getElementById('diverrorHeader').style.display = "block";
				document.Principal.text285090983.focus();
				return false;
			}
			if(document.Principal.text285090983.value.match(RegEmail)){
				$('.OKheader').fadeIn('fast');
				$('.ocultarBulletinHeader').fadeOut('fast');
				document.getElementById('diverrorHeader').style.display = "none";
			    	return validar;	
			} else {
				document.getElementById('diverrorHeader').style.display = "block";
				document.Principal.text285090983.focus();
				return false;
			}				   	
		}
		function ValidarFooter(){
			var RegEmail = /[\w-\.]{3,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;
			validar = true;
			if(document.Footer.correo.value.length == 0){
				document.getElementById('diverrorFooter').style.display = "block";
				document.Footer.correo.focus();
				return false;
			}
			if(document.Footer.correo.value.match(RegEmail)){
				$('.OKfooter').fadeIn('fast');
				$('.ocultarBulletinFooter').fadeOut('fast');
				document.getElementById('diverrorFooter').style.display = "none";
			    	return validar;	
			} else {
				document.getElementById('diverrorFooter').style.display = "block";
				document.Principal.correo.focus();
				return false;
			}				   	
		}
		function ValidarBloque(){
			var RegEmail = /[\w-\.]{3,}@([\w-]{2,}\.)*([\w-]{2,}\.)[\w-]{2,4}/;
			validar = true;
			if(document.Bloque.text285090983.value.length == 0){
				document.getElementById('diverror').style.display = "block";
				document.Bloque.text285090983.focus();
				return false;
			}
			if(document.Bloque.text285090983.value.match(RegEmail)){
				$('.OK').fadeIn('fast');
				$('.ocultarBulletin').fadeOut('fast');
				document.getElementById('diverror').style.display = "none";
			    	return validar;	
			} else {
				document.getElementById('diverror').style.display = "block";
				document.Bloque.text285090983.focus();
				return false;
			}				   	
		}
	</script>
		
