<script>
var showSignal = false;
  function mostrar_ocultar() {
	  // cambia de true a false o de false a true
	  showSignal = !showSignal; 
	
	  if (showSignal){
	    document.getElementById("cajaPlayerUltra").style.display="block";
	    document.getElementById("cajaPlayerUltra").innerHTML='<!--CODIGO IFRAME DE ULTRABASE (STREAMING ALTERNO 2) --><iframe src="http://mbliveocasional.telesur.ultrabase.net" width="520" height="340" frameborder="0" name="alterna2" id="alterna2id" allowfullscreen="" webkitallowfullscreen="" mozallowfullscreen=""></iframe>';
	  }
	  else {
	    document.getElementById("cajaPlayerUltra").style.display="none";
	    document.getElementById("cajaPlayerUltra").innerHTML=" ";
	  }
  }
</script>














 

        		 









