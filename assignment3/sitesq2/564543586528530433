				<script>
			$(document).ready(function()
			{
				bannerStart = new Date().getTime();
			
				$('#real-banner').waitForImages({
				    finished: function() {
				    	var bannerEnd = new Date().getTime();
				        if ((bannerEnd - bannerStart) < 75)
				        {
				        	slideSwitch('#slider', 'div', 0);
				        }
				        else
				        {
				        	slideSwitch('#slider', 'div', 500);
				        }
				        $('.profile-bar .bg').css('background-image', "url('/lib/img/games/banner.jpg')");
				    },
				    each: function() {},
				    waitForAll: true
				});
			});
		</script>
