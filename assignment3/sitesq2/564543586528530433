		  <script type="text/javascript" charset="utf-8">

      page = 1;

		$(window).scroll(function()
		{
			if (page)
			{
				if ($(window).scrollTop() <= $(document).height() - $(window).height() && $(window).scrollTop() >= $(document).height() - $(window).height() - 350)
				{
					$('#load-more').remove();
					gamesTable.append('<tr id="table-loading"><td colspan="6"><div style="text-align: center; padding: 5px 10px 5px 5px;"><img id="" src="/lib/img/layout/loader_dark_large.gif" style=""><br /><small>Please Wait</small></div></td></tr>');

					$.ajax({
					   type: "GET",
					   dataType: "html",
					   url: "/lahmeyerus&completion=all&order=played&pf=all&p="+(page+1),
					   success: function(data)
					   {
					   		$('#table-loading').remove();
						 	gamesTable.append(data);
					   }
					 });

					page = 0;

				}
			}
		});


	  $(document).ready(function()
	  {
	  		gamesTable = $("#gamesTable > tbody");

	  		
		  $('.recent-trophies #slider').bxSlider({
		    maxSlides: 2,
		    moveSlides: 1,
			speed: 300,
			autoStart: true,
			auto: true,
			pause: 5000,
			autoHover: true,
			slideWidth: 605,
			pager: false
		  });
	  });
	</script>

