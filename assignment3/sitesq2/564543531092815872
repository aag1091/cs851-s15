            var form = $(document).find(".editForm");
            console.log(form);

            var source_field = form.find("#id_source");
            source_field.attr("value", "linkedin");

            var name_field = form.find("#id_name");
            name_field.attr("value", data["person"]["firstName"] + " " + data["person"]["lastName"]);
            name_field.attr("readonly", true);

            var email_field = form.find("#id_email");
            email_field.attr("value", data["person"]["emailAddress"]);
            email_field.attr("readonly", true);

            var message_field = form.find("#id_message");
            if (data["coverLetter"] == null || data["coverLetter"] == undefined || data["coverLetter"] == "") {
                message_field.attr("value", "Applied via LinkedIn");
            } else {
                message_field.attr("value", data["coverLetter"]);
            }
            message_field.attr("readonly", true);

            try {
                var phone_field = form.find("#id_phone");
                phone_field.attr("value", data["person"]["phoneNumbers"]["values"][0]["phoneNumber"]);
                phone_field.attr("readonly", true);
            } catch (ignore) {
                // No phone number present.
            }

            form.find("#id_appli").attr("value", JSON.stringify(data));

            form.submit();
            console.log("submitted form");
        }

        $(document).ready(function() {
            var form = $('#inline-job-apply');
            var nameInput = $('#id_name');
            var emailInput = $('#id_email');
            var phoneInput = $('#id_phone');
            var resumeInput = $('#id_resume');
            var nameErr, emailErr, phoneErr, resumeErr;
            var errorBubbles = $('.error-bubble');

            $(form).submit(function(e) {
                if (applyingViaLinkedIn ){
                    return true;
                }
                $(errorBubbles).hide(); // reset the error bubbles

                // name
                if ( nullValidate(nameInput) ) {
                    nameErr = true; // name input is null
                    showErr(nameInput, "Please enter your name.");
                } else {
                    nameErr = false; // name input is good
                };

                // email
                if ( nullValidate(emailInput) ) {
                    emailErr = true; // email input is null
                    showErr(emailInput, "Please enter your email address.")
                } else if ( emailValidate(emailInput) ) {
                    emailErr = true; // email input is not valid
                    showErr(emailInput, "Please enter a valid email address.")
                } else {
                    emailErr = false; // email is good
                };

                // phone
                if ( nullValidate(phoneInput) ) {
                    phoneErr = true; // phone input is null
                    showErr(phoneInput, "Please enter your phone number.")
                } else {
                    phoneErr = false; // phone input is good
                };

                // resume
                if ( nullValidate(resumeInput) ) {
                    resumeErr = true; // resume attachment is null
                    showErr(resumeInput, "Please attach your resume.")
                } else {
                    resumeErr = false; // resume attachment is good
                }; 

                if (!nameErr && !emailErr && !phoneErr  && !resumeErr) {
                    return true;
                    // console.log("form works");
                    // var url = $(form).attr('action');
                    // $.ajax({
                    //     type: 'POST',
                    //     url: url,
                    //     success: function(data) {

                    //     },
                    //     error: function(err) {
                    //         console.log(err);
                    //     }
                    // });
                } else {
                    e.preventDefault();
                    _gaq.push(['_trackEvent', 'job-ab-test', 'js-validation-error-9', 'ab-test-variation', 9]);
                }
            });
        });

        function nullValidate(input) {
            var val = $(input).val();

            if ( val === "" || val.length < 1 || val ==null ) {
                return true;
            } else {
                return false;
            }
        }

        function emailValidate(input) {
            var val = $(input).val();
            console.log(val);
            var atPos = val.indexOf("@");
            console.log(atPos);
            var dotPos = val.indexOf(".");
            console.log(dotPos);
            if (atPos > 0 && dotPos > 0 ) {
                console.log("true");
                return false;
            } else {
                return true;
            }
        }

        function showErr(item, errorMsg) {
            var bubble = $(item).parent().find('.error-bubble');
            if (bubble.length == 0) bubble = $(item).parent().siblings('.error-bubble');
            var span = $(bubble).find('.error-message');

            $(span).html(errorMsg);
            $(bubble).show();
        }

