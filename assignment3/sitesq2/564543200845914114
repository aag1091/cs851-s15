			<script type="text/javascript">
			jSot(window).load(function() {
				setitemheight_121();
				setEffectHover_121();
				jSot('#SotMegaSlider_121').MegajCarouselLite(
				{
					vertical: false,
					btnNext: "#SotMegaSlider_button_121 .next",
					btnPrev: "#SotMegaSlider_button_121 .prev",
					speed: parseInt(1000),
					auto: parseInt(0),
					timer: parseInt(2000),
					mouseWheel:	false,
					stopHover: 1,
					visible: 5,
					scroll: 4,
					beforeStart: function(a){setEffectHover_121();}
				});				 			
				
			});
			function setitemheight_121(){
					var $list_li = jSot('#SotMegaSlider_121').find('.SotMegaSlider_item');
					var $max_h = 0;
					jSot.each($list_li, function(key, val){
						
						if(parseInt(jSot(val).attr('clientHeight'))>$max_h)
						{
							$max_h = parseInt(jSot(val).attr('clientHeight'));
						}					
					})
					
					jSot.each($list_li, function(key, val){
						jSot(val).height($max_h);
					})							
				}
			function setEffectHover_121(){
				var $list_li = jSot('#SotMegaSlider_121').find('.SotMegaSlider_item');
				jSot.each($list_li, function(key,val){		
					var desc = jSot(val).find('.contentSlider').html();		
					jSot(val).tooltip (desc, { hook: true, width: 300, style: 'alert'});
				})
			}
		</script>
	
