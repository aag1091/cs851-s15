    <div class="nowPlayingWrapper full">
        <script type="text/javascript">
var jsLocalizationNowPlaying = "Now Playing on&nbsp;News Radio 1200 WOAI";
var jsLocalizationBy = "by";
var jsLocalizationUpNext = "Up Next:";
var stationPositionerText = "San Antonio's Breaking News, Weather, Traffic";
var stationNameText = "News Radio 1200 WOAI";
window.trackPageName = "%2Farticles%2Fwoai-local-news-sponsored-by-five-star-cleaners-119078%2Fproposal-would-give-ekgs-to-student-13231649";

jQuery(document).ready(function($){

    var switchNowPlaying = function(){
        if ($(window).scrollTop() <= 90){
            $('.pageWrapper .nowPlayingWrapper').addClass('full');
        }
    }

    $(window).scroll(function(){
        $('.pageWrapper .nowPlayingWrapper').removeClass('full');
        switchNowPlaying();
    });

    var resetMenus = function(){
        $('.headerWrapper, .headerWrapper *, .contentWrapper, .contentWrapper *').unbind('click.touch-menu');
        $('.nowPlaying .onair, .nowPlaying .playlist').children('.dropdown').hide();
        $('.nowPlaying .onair, .nowPlaying .playlist').removeClass('active');
    };

    //touch events
    $('.nowPlaying .onair .jock, .nowPlaying .playlist .current')
    .on('touchstart', function(_evt){
        _evt.preventDefault();
    })
    .on('touchmove', function(_evt){
        _evt.preventDefault();
    })
    .on('touchend', function(_evt){
        _evt.preventDefault();

        if( !$(this).parent().hasClass('active') ){
            resetMenus();
            //add active class and show dropdown
            $(this).parent().addClass('active');
            $(this).siblings('.dropdown').stop(true,true).slideDown('fast');

            //bind event to close menu if clicking outside now playing area
            $('.headerWrapper, .headerWrapper *, .contentWrapper, .contentWrapper *').bind( 'click.touch-menu', function(){
                resetMenus();
            });
        }else{
            //remove active class and hide dropdown
            $(this).siblings('.dropdown').hide();
            $(this).parent().removeClass('active');

            //open child link
            var links  = $(this).children().find('a');

            if( $(this).hasClass('jock') ){
                window.open( $(links[0]).attr('href'), '_self');
            }else{
                window.open( $(links[0]).attr('href'), '_blank');
            }
        }

    });

    //click events
    $('.nowPlaying .header').bind('click', function(_evt){
        _evt.preventDefault();
        //_evt.stopPropagation();

        if( $(this).parent().hasClass('active') ){
            //remove active class and hide dropdown
            $(this).siblings('.dropdown').hide();
            $(this).parent().removeClass('active');
            //open child link
            var links  = $(this).children().find('a');

            if( $(this).hasClass('jock') ){
                window.open( $(links[0]).attr('href'), '_self');
            }else{
                window.open( $(links[0]).attr('href'), '_blank');
            }

        }

    });

    //artist name links open artist url
    $('.nowPlaying .playlist .trackInfo a.artistName').bind('click', function(_evt){
        _evt.preventDefault();
        _evt.stopPropagation();
        window.open( $(this).attr('href'), '_blank');
        resetMenus();
    });

    //hover events
    $('.nowPlaying .onair, .nowPlaying .playlist').hover(function(_evt){
        _evt.preventDefault();
        $(this).addClass('active');
        $(this).children('.dropdown').stop(true,true).slideDown('fast');
    },
    function(_evt){
        _evt.preventDefault();
        $(this).removeClass('active');
        $(this).children('.dropdown').hide();
    });

    //implement quick io for most recent now playing item
    if(window.showPlaylist == '1'){
        try {
            var currentItem = $('.playlist .current');

            $.get('/cc-common/local-poc/html/now_playing_tpl_qio.html?t=2', function(data){
                currentItem.html( data );
                currentItem.find('.playingTitle').html(stationNameText);
                currentItem.find('.trackTitle').html( stationPositionerText );
                currentItem.find('.byLabel').html(jsLocalizationBy);
                currentItem.find('.artistImage').hide();
                currentItem.find('.byLabel').hide();
                currentItem.find('.artistName').hide();
                currentItem.find('a').attr('href', 'http://www.iheart.com/live/2361/?autoplay=true&pname=1229&amp;campid=play_bar&amp;cid=%2Farticles%2Fwoai-local-news-sponsored-by-five-star-cleaners-119078%2Fproposal-would-give-ekgs-to-student-13231649');
            });

            var qio = QuickIo('ws://quickio.iheart.com');
            var imagePath = "http://content.clearchannel.com/cc-common/local-poc/images/no_image.png";
            var timeout;
            var date = new Date();

            qio.on('/ihr/now-playing/live/' + streamId, function(data) {

                if( data && data.length > 0 ){

                    var qioItem = data[0];

                    currentItem.find('.playingTitle').html(jsLocalizationNowPlaying)
                    currentItem.find('.trackTitle').html( qioItem.track );
                    currentItem.find('.artistName').html( qioItem.artist );
                    currentItem.find('.byLabel').show();
                    currentItem.find('.artistName').show();

                    if( qioItem.trackId != "" && qioItem.trackId != "0" ){
                        imagePath = "http://iscale.iheart.com/catalog/track/" +  qioItem.trackId + "?ops=fit(45%2C45)";
                    }else if( qioItem.artistId != "" && qioItem.artistId != "0"){
                        imagePath = "http://iscale.iheart.com/catalog/artist/" +  qioItem.artistId + "?ops=fit(45%2C45)";
                    }

                    var oImg = new Image;
                    oImg.src = imagePath;
                    oImg.onload=function(){
                        currentItem.find(".artistImage img").attr("src", imagePath);
                        currentItem.find('.artistImage').show();
                    }

                    var cleanTitle = qioItem.track.replace(/[^a-z0-9 ]*/gi,'').replace(/\s+/g,'-');
                    var cleanArtist = qioItem.artist.replace(/[^a-z0-9 ]*/gi,'').replace(/\s+/g,'-');

                    var songURL = 'http://www.iheart.com/artist/' + cleanArtist + '-' + qioItem.artistId +'/songs/' + cleanTitle + '-' + qioItem.trackId  + '/';
                    var artistURL = 'http://www.iheart.com/artist/' + cleanArtist  + '-' + qioItem.artistId + '/';
                    var urlTrack =  '?pname=' + window.siteId + '&campid=desktop_now_playing&cid=' + window.trackPageName ;


                    //set artist/track link
                    var artistAnchor = currentItem.find('a');
                    var songAnchor = currentItem.find('.trackInfo a.trackTitle');
                    var imageAnchor = currentItem.find('.artistImage a');

                    var clickString = "";
                    if( qioItem.artistId != "" && qioItem.artistId != "0"){
                        clickString = "s.tl(this,'o','playbar:nowplaying:artist:1');s_objectID='playbar:nowplaying:artist:1';";
                        //artistAnchor.attr("href", artistURL + urlTrack);
                        artistAnchor.attr('href', 'http://www.iheart.com/live/2361/?autoplay=true&pname=1229&campid=desktop_now_playing&cid=%2Farticles%2Fwoai-local-news-sponsored-by-five-star-cleaners-119078%2Fproposal-would-give-ekgs-to-student-13231649');
                        artistAnchor.attr("target","_blank");
                        artistAnchor.attr('onclick', clickString);
                    }

                    if(  qioItem.trackId != "" && qioItem.trackId != "0" ){

                        clickString = "s.tl(this,'o','playbar:nowplaying:song:1');s_objectID='playbar:nowplaying:song:1';";
                        //songAnchor.attr("href", songURL + urlTrack);
                        songAnchor.attr('href', 'http://www.iheart.com/live/2361/?autoplay=true&pname=1229&campid=desktop_now_playing&cid=%2Farticles%2Fwoai-local-news-sponsored-by-five-star-cleaners-119078%2Fproposal-would-give-ekgs-to-student-13231649');
                        songAnchor.attr("target","_blank");
                        songAnchor.attr('onclick', clickString);

                        clickString = "s.tl(this,'o','playbar:nowplaying:coverart:1');s_objectID='playbar:nowplaying:coverart:1';";
                        //imageAnchor.attr("href", songURL + urlTrack);
                        imageAnchor.attr('href', 'http://www.iheart.com/live/2361/?autoplay=true&pname=1229&campid=desktop_now_playing&cid=%2Farticles%2Fwoai-local-news-sponsored-by-five-star-cleaners-119078%2Fproposal-would-give-ekgs-to-student-13231649');
                        imageAnchor.attr("target","_blank");
                        imageAnchor.attr('onclick', clickString);
                    }

                    //make sure the now playing section is shown
                    $('.nowPlaying .stationPositioner').hide();
                    $('.playlist').show();

                    clearTimeout(timeout);
                    timeout = setTimeout(function() {

                        currentItem.find('.playingTitle').html(stationNameText);
                        currentItem.find('.trackTitle').html( stationPositionerText );
                        currentItem.find('.byLabel').html(jsLocalizationBy);
                        currentItem.find('.artistImage').hide();
                        currentItem.find('.byLabel').hide();
                        currentItem.find('.artistName').hide();
                        currentItem.find('a').attr('href', 'http://www.iheart.com/live/2361/?autoplay=true&pname=1229&amp;campid=play_bar&amp;cid=%2Farticles%2Fwoai-local-news-sponsored-by-five-star-cleaners-119078%2Fproposal-would-give-ekgs-to-student-13231649');


                    }, qioItem.endTime - date.getTime());

                }
            });
        }
        catch( _err ){
            if( console ){
                console.log( 'ERROR: %s', _err.message );
            }
        }
    }//if

});
</script>
