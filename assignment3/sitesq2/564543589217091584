function b2c_round_shares( num ) {
	if ( num < 10000 ) {
		return b2c_number_format( num );
	} else if ( num < 1000000 ) {
		return Math.round( num / Math.pow(10,3) ).toString() + "k+";
	} else {
		return Math.round( num / Math.pow(10,6) ).toString() + "M+";
	}
}
function b2c_number_format( num ) {
	return num.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
}
jQuery.ajax({
	url: 'http://www.business2community.com/wp-admin/admin-ajax.php',
	data: {
		'action':'get_shares_for_post',
		'post_id':'1144024'
	},
	success: function(response) {
		//console.log(response);
		var response_count = JSON.parse( response );
		if ( parseInt( response_count ) > parseInt( jQuery('#post-shared-count-digits').data('share-count') ) ) {
			jQuery('#post-shared-count-digits').text( b2c_round_shares( parseInt( response_count ) ) );
		}
	},
	error: function(errorThrown){
		//console.log(errorThrown);
	}
});
