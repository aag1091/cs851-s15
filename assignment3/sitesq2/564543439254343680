<script>
// Facebook like tracking for Google analytics 
var go = function( ) {
    if (window.FB !== undefined && typeof(ipc) == 'object' && typeof(ipc.analytics) == 'object' && typeof(ipc.analytics.trackSocial) == 'function') {
        FB.Event.subscribe('edge.create', function(response, widget) {
              if (typeof(callback_FBedgeCreate) != "undefined") { 
                    callback_FBedgeCreate (response, widget);
              } else {
                     ipc.analytics.trackSocial({
                           'action' : 'Like',
                           'network' : 'Facebook',
                           'url' : response
                     });
              }
        });

        FB.Event.subscribe('edge.remove', function(response) {
           ipc.analytics.trackSocial({
              'action' : 'Unlike',
              'network' : 'Facebook',
              'url' : response
           });
       });

       FB.Event.subscribe('message.send', function(response) {
           ipc.analytics.trackSocial({
              'action' : 'Send',
              'network' : 'Facebook',
              'url' : response
           });
       });

	FB.Event.subscribe('comment.create', function(response) {
           ipc.analytics.trackSocial({
              'action' : 'Comment',
              'network' : 'Facebook',
              'url' : response
           });
	});
    } else {
        setTimeout(go, 100);
    }
};

go();
</script><script> 
//Social Tracking for re-tweets 

//  Ensure global _gaq Google Analytics queue has been initialized.
var _ga = _ga || {};

function trackTwitter(intent_event) {
    if (intent_event) {
      var opt_pagePath;
      if (intent_event.target && intent_event.target.nodeName == 'IFRAME') {
            opt_pagePath = _ga.extractParamFromUri_(intent_event.target.src, 'url');
      }

       if (typeof(ipc) == 'object' && typeof(ipc.utils) == 'object' && typeof(ipc.utils.trackSocial) == 'function') {
          ipc.utils.trackSocial('Twitter', 'tweet', opt_pagePath);
       } else {
          _gaq.push(['_trackSocial', 'twitter', 'tweet', opt_pagePath]);
       }
    }
  }

  //Wrap event bindings - Wait for async js to load
  if(window.twttr !== undefined) {  
    twttr.ready(function (twttr) {
      //event bindings
      twttr.events.bind('tweet', trackTwitter);
    });
  }
//Extracts a query parameter value from a URI
_ga.extractParamFromUri_ = function(uri, paramName) {
  if (!uri) {
    return;
  }
  var regex = new RegExp('[\\?&#]' + paramName + '=([^&#]*)');
  var params = regex.exec(uri);
  if (params != null) {
    return unescape(params[1]);
  }
  return;
};
</script>
