    var urls = [
      "http://www.fcbarcelona.cat/futbol/primer-equip/arxiu-partits/2012-2013/copa-catalunya/final/espanyol_fc-barcelona",
      "http://www.fcbarcelona.es/futbol/primer-equipo/archivo-partidos/2012-2013/copa-cataluna/final/espanyol_fc-barcelona",
