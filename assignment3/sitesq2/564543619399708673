    <div class="main-content" role="main" id="main-content">

      <!-- START - Página amb lateral -->
      <div class="container">
        
          
            <div class="row">
              
                
                  <div class="bloc bloc1 col-12">
                    <div class=" widget widget-marcador-de-partits widget-full-width" id="widget_scoreboard_11777">

  
    <section class="box box-shadow box-dark">

      <header class="widget-marcador-header">

        <div class="widget-marcador-competition">
          <img alt="" height="60" src="http://media2.fcbarcelona.com/media/asset_publics/resources/000/004/195/original_rgb/futbol-lliga.v1318956084.png" title="League" width="60" />
        </div>

        <h1 class="visuallyhidden">Athletic Club - FC Barcelona</h1>

        <a href="http://www.fcbarcelona.com/football/first-team/match-archive/2014-2015/league/round-22/athletic-club_fc-barcelona" class="match-link">
          <span class="sport">
            <span class="visuallyhidden">
              Section: </span>
            Football
          </span>
          <span class="competition-name">
            <span class="visuallyhidden">
              Competition: </span>
            League
          </span>

          
            <span class="competition-stage">Round 22</span>
          

          <span class="venue">
            <span class="visuallyhidden">
              Estadium: </span>
            San Mamés
          </span>

          <span class="date-time">
            <span class="date">
              <span class="visuallyhidden">
              Date: </span>
              02/08/2015
            </span>
            <span class="time local">21:00</span>

            
          </span>
        </a>

      </header>

      <div class="widget-marcador-stats">

  <div class="team team-home">

