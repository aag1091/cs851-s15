    var sizes = googletag.sizeMapping().
    addSize([767,600],[300,250]). //Is the screen wider than 767px and higher than 600px? Show a 300x250 ad.
    addSize([0,0],[300,100]). //For any other size (that is: wider than 0px and higher than 0px), show a 300x100 ad.
    build();
        
    googletag.defineSlot('/53955289/03_EN-01_GENERIC-300X250-03', [[300,100], [300,250]], 'div-gpt-ad-1405428878217-106').defineSizeMapping(sizes).addService(googletag.pubads());
    googletag.pubads().enableSyncRendering();
    googletag.enableServices();
    googletag.display('div-gpt-ad-1405428878217-106');
  </script>
</div>
        </div>

      </section>
    </div>
  




        
  

