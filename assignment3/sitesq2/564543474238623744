        <script type="text/javascript">HDN.onLoginSuccess = print_sfgate_un;</script>
        <script src="/js/hdn/utils/jquery.placeholder.js"></script>
        <script type="text/javascript">
            jQuery(function(){
                $("#modal_signin_close").on("click",function(){
                    $('#modal_signin_div').hide();
                    return false;
                });
                $('#modal_signin_div .checkbox-div').on("click",function(){
                    if ($(this).text() == "X") {
                        $(this).text("");
                        $('#modal_signin_rememberme').removeAttr('checked');
                    } else {
                        $(this).text("X");
                        $('#modal_signin_rememberme').attr('checked', 'checked');
                    }
                    return false;
                });
                try {$('input, textarea').placeholder();} catch (e) {}
            });
        </script>
        <div id="modal_signin_div" class="signin">
            <div id="modal_signin_error_div" style="display:none;"></div>
            <form data-ajax="false" id="modal_signin_form" action="http://www.sfgate.com/cgi-bin/sso/action/mobile_login" method="post">
                <a href="http://www.sfgate.com/cgi-bin/webreg/user/reg_cnt" class="form-link">Need to Register?</a>
                <button id="modal_signin_close">X</button> 
                <div class="line_separator"></div>
                <input type="text" id="modal_signin_user" name="user" maxlength="15" selected autocapitalize="off" value="" data-mini="true" placeholder="USERNAME" />
                <input type="password" id="modal_signin_password" name="password" maxlength="15" selected autocapitalize="off" value="" data-mini="true" placeholder="PASSWORD" />
                <div class="checkbox-container">
                    <div class="checkbox-div"></div>
                    <input type="checkbox" id="modal_signin_rememberme" name="rememberme" value="1" class="custom-check" />
                    <label for="modal_signin_rememberme" data-corners="false">Remember Me</label>
                </div>
                <input type="hidden" name="Submit" value="Update">
