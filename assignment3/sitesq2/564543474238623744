<!-- e design/gallery/main_redesign.tpl --></div><!-- /gallery float --><div class="fixed-asset perfectPixel"><!-- hearst/item/standalone.tpl -->
<!-- mid:freeform.33813 -->
<script type="text/javascript" src="http://launch.newsinc.com/js/embed.js" id="_nw2e-js"></script>
<div class="ndn_embed" id="ndn-video-player-1" data-config-distributor-id="45981"></div>

