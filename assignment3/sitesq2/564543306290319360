                    if (filters.length >= 1) {
                        s.prop9 = "";
                        s.prop10 = "";
                        for (var i = 0; i < filters.length; i++) {
                            s.prop9 += filters[i].filterName + ",";
                            s.prop10 += filters[i].filterName + ":" + filters[i].filterValue + ",";
                        }
                        s.list1 = "D=c9";
                        s.list2 = "D=c10";
                    }
                }

                // categories
                if (page.regCategoryId) {
                    s.prop28 = page.regCategoryId;
                }

                // product page - product info
                if (page.product) {
                    var prod = page.product;
                    s.products = ";" + prod.sku + ";;;prodView|event42=" + prod.price + "; eVar31=" + prod.brand + "|eVar33=" + prod.type + "|eVar34=" + prod.reviewCount + "|eVar35=" + prod.avgRating + "|eVar36=" + prod.inStock + "|eVar37=" + prod.discountPercentage + "|eVar38=" + prod.deliveryTime + "|eVar39=" + prod.id + "|eVar40=" + prod.fulfillmentSource;
                }

                // ecommerce variables
                var step = ecommerce ? ecommerce.checkoutStep : "";
                if (step) {
                    switch (step) {
                        default:
                            s.products = prodLoop(ecommerce.cart);
                        case "1":
                            s.events = s.apl(s.events, 'scCheckout', ",", 1);
                            break;
                        case "2":
                            s.events = s.apl(s.events, 'event1', ",", 1);
                            break;
                        case "2a":
                            s.events = s.apl(s.events, 'event2', ",", 1);
                            break;
                        case "3":
                            s.events = s.apl(s.events, 'event3', ",", 1);
                            break;
                        case "3a":
                            s.events = s.apl(s.events, 'event4', ",", 1);
                            break;
                        case "4":
                            s.events = s.apl(s.events, 'purchase,event21=' + ecommerce.transaction.shippingTotal + ',event22=' + ecommerce.transaction.taxTotal + ',event23=' + ecommerce.transaction.voucherDiscountTotal, ",", 1);
                            s.purchaseID = ecommerce.transaction.id;
