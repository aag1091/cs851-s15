                    s.pageType = 'errorPage';
                s.channel = page.type;
                s.pageName = 'D=ch+":' + page.name + '"';
                s.pageNameFull = s.channel + ':' + page.name;

                s.prop1 = page.id;
                s.prop5 = 'D=g';


