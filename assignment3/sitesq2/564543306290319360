                    var pg = page.name;
                    var st = pg == "OrderTracking" ? "event50" : pg == "OrderTrackingSuccess" ? "event51" : ""
                    if (st) s.events = s.apl(s.events, st, ",", 1);
                }
                else if (page.name == "createAccountSuccess") {
                    s.events = s.apl(s.events, 'event20', ",", 1);
                }

                // catalog pages
                s.prop2 = page.pageNumber;
                s.prop39 = page.productsPerPage;


                if (page.filters) {
