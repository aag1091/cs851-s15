                s.prop12 = 'D=v29';
                s.prop13 = 'D=v30';
                s.prop14 = s.getPreviousValue(s.pageNameFull, 'gpv_pn', '');
                s.prop15 = s.getPercentPageViewed();
                s.eVar24 = s.getVisitNum(365);
                s.eVar25 = s.getDaysSinceLastVisit();

                //internal campaigns
                if (s.Util.getQueryParam('icid')) {
                    s.eVar27 = s.Util.getQueryParam('icid');
                    s.events = s.apl(s.events, 'event54', ",", 1);
                }
                if (s.Util.getQueryParam('icpl'))
                    s.eVar26 = s.Util.getQueryParam('icpl');

                //Responsys
                if (s.Util.getQueryParam('rmid'))
                    s.eVar72 = s.Util.getQueryParam('rmid');
                if (s.Util.getQueryParam('rrid'))
                    s.eVar73 = s.Util.getQueryParam('rrid');


                //tsm config
                var _tsmCalledOnce = false;
                if (!_tsmCalledOnce) {
                    var _tsm = new TrafficOrganizer(s);
                    var tsmIgnoredRef = new RegExp(s.hostsToIgnore);
                    _tsm.addIgnoredReferrer(tsmIgnoredRef);
                    _tsm.addPaidSearchParameter('gclid');

                    //a_* params if they exist
                    if (s.Util.getQueryParam('a_medium') && s.Util.getQueryParam('a_source')) {
                        _tsm.setMediumKeyName('a_medium');
                        _tsm.setSourceKey('a_source');
                        _tsm.setKeywordKey('a_term');
                        _tsm.setContentKey('a_content');
                        _tsm.setCampaignKey('a_campaign');
                    }

                    _tsm.track();

                    s.eVar1 = _tsm.getSource();
                    s.eVar2 = _tsm.getMedium();
                    s.eVar3 = _tsm.getCampaign();
                    s.eVar4 = _tsm.getKeyword();
                    s.eVar5 = _tsm.getContent();

                    _tsmCalledOnce = true;
                }

                //HasOffer UTM override
                if (s.Util.getQueryParam('affiliate_id') && s.Util.getQueryParam('offer_name') && s.Util.getQueryParam('offer_id')) {
                    s.eVar1 = s.Util.getQueryParam('affiliate_id')
                    s.eVar2 = 'lazada_affiliate_program';
                    s.eVar3 = s.Util.getQueryParam('offer_name');
                    s.eVar5 = s.Util.getQueryParam('offer_id');
                }

                //sem classification
                if (s.Util.getQueryParam('wt_sb'))
                    s.eVar2 = 'sem_brand';
                else if (s.Util.getQueryParam('wt_sn'))
                    s.eVar2 = 'sem_non_brand';
            }

            function hasParam(par, url, c) { // parameter to find, url to look in, use "" for current url,  c case sensitivity
                if (!url || url == "") url = document.location.href;
                var reg = new RegExp(par, (c ? "" : "i"));
                return (reg).test(url);
            }

            function prodLoop(items) {
                if (!items) return;
                var ret = "";
                for (var i = 0; i < items.length; i++) {
                    ret += (( i > 0 ) ? "," : "" ) + ";" + items[i].sku + ";" + items[i].quantity + ";" + (items[i].price * items[i].quantity) + ";;eVar39=" + items[i].id + "|eVar46=" + items[i].taxRate;
                }
                return ret;
            }

            function getData() {
                for(var i = 0; i < dataLayer.length; i++) {
                    if (dataLayer[i].core !== undefined) {
                        return dataLayer[i];
                    }
                }
            }

            var url = document.location.href;

            window['_apm'] = s;

            if (url.indexOf('/success/external') === -1 && url.indexOf('/cart/tracking/') === -1) {
                var parser = document.createElement('a');
                parser.href = url;
                var hostnameWWW = parser.hostname;
                var hostname = hostnameWWW.indexOf('www.') === 0 ? hostnameWWW.substr(4) : hostnameWWW;

                var data = getData();
                var core = data.core,
                        visitor = data.visitor,
                        page = data.page,
                        ecommerce = data.ecommerce,
                        pagetype = data.pagetype;

                //dynamic account selection
                s.getAccount = function () {
                    if (core.isPro) {
                        //prod selections
                        switch (core.country) {
                            case 'ID':
                                return 'lazwebid';
                            case 'PH':
                                return 'lazwebph';
                            case 'VN':
                                return 'lazwebvn';
                            case 'MY':
                                return 'lazwebmy';
                            case 'TH':
                                return 'lazwebth';
                            case 'SG':
                                return 'lazwebsg';
                            default:
                                return 'lazwebportal';
                        }
                    } else {
                        return 'lazwebstage';
                    }
                }
                visitor.cookieDomain = hostname;
                s.account = s.getAccount();
                s.visitorNamespace = "lazada"
                s.trackingServer = "lazada.d3.sc.omtrdc.net"; // same as s.trackingServer
                s.trackingServerSecure = "lazada.d3.sc.omtrdc.net"; //same as s.trackingServerSecure
                s.visitor = Visitor.getInstance("126E248D54200F960A4C98C6");

