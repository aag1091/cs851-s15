                            s.eVar46 = ecommerce.transaction.taxRate;
                            s.products = prodLoop(ecommerce.transaction.items);
                            s.state = ecommerce.transaction.state;
                            s.zip = ecommerce.transaction.zip;
                            break;
                    }
                }

                //plugins
                s.eVar28 = s.getTimeParting('h', '-5');
                s.eVar29 = s.getTimeParting('d', '-5');
                s.eVar30 = s.getTimeParting('w', '-5');
