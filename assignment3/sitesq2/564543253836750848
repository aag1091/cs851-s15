		jQuery(function(){
		  jQuery(document.body)
			  .on('click touchend','#swipebox-slider .current img', function(e){
				  jQuery('#swipebox-next').click();
				  return false;
			  })
			  .on('click touchend','#swipebox-slider .current', function(e){
				  jQuery('#swipebox-close').trigger('click');
