                s.linkTrackVars = eVarName;
            else
                s.linkTrackVars = eVarName + "," + s.linkTrackVars;
            var myEval;
            if (usePageNameForEvarValue)
                myEval = "s." + eVarName + " = '" + "" + "';";
            else
                myEval = "s." + eVarName + " = '" + eVarValue + "';";
            eval(myEval);
