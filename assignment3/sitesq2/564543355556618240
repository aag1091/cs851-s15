comparequeue('get','','')
function comparequeue(querystring,cid,cname)
{  if(querystring=="del"){querystring=querystring + "&cid=" + cid + "&cname=" + cname ;}
   var purl=encodeURI(document.URL);
   var clsAjax = new CreateRequest()
    clsAjax.ajaxObject.onreadystatechange=function (){
    if (clsAjax.ajaxObject.readyState == 4)
        {
            $("#dvQueue").html(clsAjax.ajaxObject.responseText)
            
              if($("#dvList").html()=="")
               {
                $('#ulQueue').hide();
                }
              else{
                 $('#ulQueue').show();
                }
        }
    }
    clsAjax.ajaxObject.open("GET", "/Comparison/comparequeue.aspx?type=" + querystring + '&URL=' + purl + '&ran=' + Math.random(), true)
    clsAjax.ajaxObject.send(null)
}

  function AjaxProdcut(funcname,querystring){ 
    var clsAjax = new CreateRequest()
    clsAjax.ajaxObject.onreadystatechange=function (){ 
        if (clsAjax.ajaxObject.readyState == 4){ 
            funcname(clsAjax.ajaxObject.responseText)
        }
    }
    clsAjax.ajaxObject.open("GET", "/review/reviewajax.php?" + querystring, true)
    clsAjax.ajaxObject.send(null)
}

// Add to user Bookmarks
function BookmarkProd(responseText){ 
    ChkLogin1(responseText,"&bookmark=1")
    if (responseText == "reload"){ 
        window.location.reload()
    }else if (responseText == "success"){ 
        document.getElementById("ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_ads_customheader_linkBookmark").title="Already Bookmarked"
        $('#ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_ads_customheader_linkBookmark').removeAttr('href');
         // alreadyBookmark.style.display="block"
    }
}

function ChkLogin1(str,RedirectPage){ 
    if (str == 'login'){ 
      urlredirectTo = "/urlRedirect.php?strurl=" + escape(LoginUrl) + escape(RedirectPage);
      ajaxLogin(urlredirectTo);
    }
}

function BindReason()
{
    $("#dvpopLayer").empty();
    $("#dvRating").empty();
    document.getElementById("dvpopLayer").style.padding="10px";
    $("#dvpopLayer").html('<div id="MsgBox"><div style="display: none;" class="errorbox" id="diverror"><span>Oops! Minimum five and maximum 100 &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;characters required.</span></div><div><textarea id="txtReason" style="width:260px; height:70px; overflow:auto" class="txt_field_black"></textarea><br><div align="center" style="margin-top:10px"><input type="button" value="Submit" onclick="SuspectMember(\'ang_ne\')" class="button_orange"></div></div></div>')
    $("#pptitle").html("Reason");
    document.getElementById("popuplayer1").style.width="290px";
    showcentre('popuplayer1');
    $("#dvPadding").removeAttr('style');
    $('#popuplayer1').css({ top: ($(window).height() - 194 ) / 2 + ($(window).scrollTop() - 70)+ "px",left: ( $(window).width() - 342 ) / 2+$(window).scrollLeft() + "px" }).show();
    closepp("popuplayer1");
}

var objMemProfile = new MemProfObject
function SuspectMember(querystring1)
{
    var strReason = document.getElementById("txtReason").value
    if (strReason.length< 5 || strReason.length > 100)
    {
        //alert("Oops! Minimum five and maximum 100 characters required.");
        document.getElementById("diverror").style.display="block";
        return false;
    }
    objMemProfile.lnksuspect = document.getElementById("ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_lnksuspect")
    var clsAjax = new CreateRequest()
    clsAjax.ajaxObject.onreadystatechange=function (){
    if (clsAjax.ajaxObject.readyState == 4)
        {
            if(clsAjax.ajaxObject.responseText=="success")
            {
                document.getElementById("popuplayer1").style.width="340px";
               $("#dvpopLayer").html('<div style="display: block;" id="sentconfirm" class="confirmationbox"><span style="display:inline-block">Profile Suspected request successfully submitted!</span></div>') 
               document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_lnksuspect').style.display="none";
               document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_lblsuspect').style.display="";
               closepptimeout('popuplayer1',2000)
                tracking1("typ=repr&subtype=repmem&ver=8.7");
            }
            else
            {
                alert("Already reported by you.");
                closepptimeout('popuplayer1',1000)
                document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_lnksuspect').style.display="none";
                document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_lblsuspect').style.display="";
            }
        }
    }
    var strPage = window.location
    var querystring = "Suspect=" + querystring1 + "&page=" + strPage + "&reason=" + strReason + "&rndIe=" + ReturnRndIe()
    clsAjax.ajaxObject.open("GET", "/Account/Profile/profilecommon.aspx?" + querystring, true)
    clsAjax.ajaxObject.send(null)
}
