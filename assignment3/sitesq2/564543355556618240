    <script type="text/javascript">
        function logout1() {
            FB.init({ appId: '146029318804309', status: true, cookie: true, xfbml: true });
            //alert(FB._userID)
            var domainVal = '.mouthshut.com';
            if (domainVal.indexOf(".com") == -1) {
                window.location.href = '/signup/logout/logout.php';
            }
            if (navigator.userAgent.indexOf('irefox/3.6') > -1) {
                window.location.href = '/signup/logout/logout.php?ff=36';
                //return false;
            }
            var img = new Image();
            img.onerror = function() {
                window.location.href = '/signup/logout/logout.php';
            }
            img.src = "http://www.facebook.com/favicon.ico";
            FB.getLoginStatus(function(response) {
                //alert(response.status)
                if (response.status == "connected") {
                    FB.logout(function() {
                        // user is now logged out
                        window.location.href = '/signup/logout/logout.php';
                        return false;
                    });
                }
                else {
                    window.location.href = '/signup/logout/logout.php';
                    return false;
                }
            }, true);
            // When user logs out of his/her fb account
             gapi.auth.signOut();
        }
        var defProdText = "Search product review"
        var searchpage = ''
        var stype = "product"
        var cat = 'product'
        var country = "in"

        if (searchpage.length == 0) {
            if (country == "us") { searchpage = '1'; }
            else { searchpage = '1'; }
        }
        else { searchpage = searchpage; }
