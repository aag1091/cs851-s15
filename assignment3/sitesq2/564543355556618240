function ShowWriteReviewIdle()
    {
    clearTimeout(t);
    clearTimeout(pd);
    document.getElementById('writereview').style.visibility='visible'
    document.getElementById('postdiary').style.visibility='hidden'
    wr = setTimeout('ShowRateReviewIdle()', 10000)
    }

function ShowRateReviewIdle()
    {
    clearTimeout(wr);
    document.getElementById('writereview').style.visibility='hidden'
    document.getElementById('ratereview').style.visibility='visible'
    rr = setTimeout('ShowAddCommentIdle()', 10000)
    }

function ShowAddCommentIdle()
    {
    clearTimeout(rr);
    document.getElementById('ratereview').style.visibility='hidden'
    document.getElementById('addcomment').style.visibility='visible'
    ac = setTimeout('ShowSendGiftIdle()', 10000)
    }

function ShowSendGiftIdle()
    {
    clearTimeout(ac);
    document.getElementById('addcomment').style.visibility='hidden'
    document.getElementById('sendgift1').style.visibility='visible'
    sg = setTimeout('ShowPostDiaryIdle()', 10000)
    }

function ShowPostDiaryIdle()
    {
    clearTimeout(sg);
    document.getElementById('sendgift1').style.visibility='hidden'
    document.getElementById('postdiary').style.visibility='visible'
    pd = setTimeout('ShowWriteReviewIdle()', 10000)
    }
    
