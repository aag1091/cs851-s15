                }
                else
                {
                  
                  document.getElementById("txtproduct").value='';
                  var val= ui.item.value; 
                  val= val.replace(/<strong>/gi, ""); 
                   val= val.replace(/<\/strong>/gi, "");
                  val=proddecode(val).toLowerCase();
