                var isedit = 0

 function AdditionalDetails(v,v1)
 {
     if(document.getElementById(v).style.display=='')
     {
         document.getElementById(v).style.display='none'
         document.getElementById(v1).className='i-expand-new'
     }
     else{document.getElementById(v).style.display=''
         document.getElementById(v1).className='i-expand-new'}
 }
    



function validateform(e1)
{
    var flag = true;
    if(e1=='address' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_address').value;
        pattern = /^([a-zA-Z0-9,.><\s]{0,3000})$/
        if(pattern.test(txt)==false) {
            document.getElementById("adrerr").style.display = "";
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_address').focus();
            flag = false;
        }
        else
            document.getElementById("adrerr").style.display = "none";
       
    }
    
    if(e1=='city' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_city').value;
        pattern = /^([a-zA-Z.\s]{0,50})$/
        if(pattern.test(txt)==false) {
            document.getElementById("cityerr").style.display = "";
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_city').focus();
            flag = false;
        }
        else
            document.getElementById("cityerr").style.display = "none";
       
    }
    
    if(e1=='pincode' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_pincode').value;
        pattern = /^([0-9]{0,10})$/
        if(pattern.test(txt)==false) {
            document.getElementById("pincodeerr").style.display = "";
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_pincode').focus();
            flag = false;
        }
        else
            document.getElementById("pincodeerr").style.display = "none";
       
    }
    
    //website  :,/ and .
    if(e1=='website' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_website').value;
         pattern = /^([a-zA-Z0-9:/.\s]{0,150})$/
         if(pattern.test(txt)==false) {
             document.getElementById("websiteerr").style.display = "";
             document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_website').focus();
            flag = false;
        }
        else
            document.getElementById("websiteerr").style.display = "none";
       
    }
    
    if(e1=='landmark' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_landmark').value;
        pattern = /^([a-zA-Z0-9:,.\s]{0,100})$/
        if(pattern.test(txt)==false) {
            document.getElementById("landmarkerr").style.display = "";
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_landmark').focus();
            flag = false;
        }
        else
            document.getElementById("landmarkerr").style.display = "none";
       
    }
    
    if(e1=='phone' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_phone').value;
        pattern = /^([0-9+-]{0,18})$/
        if(pattern.test(txt)==false) {
            document.getElementById("phoneerr").style.display = "";
            if(pattern.test(txt)==false )
            {  document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_phone').focus(); }
            flag = false;
        }
        else
            document.getElementById("phoneerr").style.display = "none";
       
    }
    
    if(e1=='alternatephone' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_alternatephone').value;
        pattern = /^([0-9+-]{0,18})$/
        if(pattern.test(txt)==false) {
            document.getElementById("phoneerr").style.display = "";
            if(pattern.test(txt)==false )
            {  document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_alternatephone').focus(); }
            flag = false;
        }
        else
            document.getElementById("phoneerr").style.display = "none";
       
    }
    
    if(e1=='mobilephone' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_mobilephone').value;
        pattern = /^([0-9+-]{0,18})$/
        if(pattern.test(txt)==false) {
            document.getElementById("phoneerr").style.display = "";
            if(pattern.test(txt)==false )
            {  document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_mobilephone').focus(); }
            flag = false;
        }
        else
            document.getElementById("phoneerr").style.display = "none";
       
    }
    
    if(e1=='comment' || e1=='all')
    {
        var txt = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_comment').value;
        pattern = /^([a-zA-Z0-9,.><\s]{0,500})$/
        if(pattern.test(txt)==false) {
            document.getElementById("commenterr").style.display = "";
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_comment').focus();
            flag = false;
        }
        else
            document.getElementById("commenterr").style.display = "none";
       
    }
    
      
    return flag;
}

 
function setHeight(height)
{
    document.getElementById('ifrmReview').style.height =  height + 30 + "px"; 
}
function scrollOnTop(val){
    if(val==2){
        var eTop = $('#writereviewpanel2').offset().top; window.scroll(eTop,eTop); }
    else {
        var eTop = $('#writereviewpanel').offset().top; window.scroll(eTop,eTop);}
}        

 
function setHeight2(height)
{
    document.getElementById('ifrmReview2').style.height =  height + 30 + "px"; 
}
var isbot='0'
 function customSpecification(catid,maincat,subcat)
 {
     var dvSpecification=document.getElementById('dvSpecification');
     if(dvSpecification.style.display=='none')
     { dvSpecification.style.display='';
         dvSpecification.style.marginTop='10px';
         //dvSpecification.style.border='1px solid #CCCCCC';
         var eTop = $('#dvSpecificationa').offset().top -120;
         window.scrollTo(eTop,eTop);

     }
     else if((type==0 || type==2) && (dvSpecification.style.display=='block' || dvSpecification.style.display=='')){
         dvSpecification.style.display='none';
         window.scrollTo(0,0);
     }
   
     var parameter="";
     if (isbot=='0')
     {
         parameter = "PType=spec" + "&id="+ catid + "&CatName=" + CatName + "&maincat=" + maincat + "&subcat=" + subcat;
         if (dvSpecification.innerHTML=="")
         {
      
             var xmlhttp = new CreateRequest()
             xmlhttp.ajaxObject.onreadystatechange=function ajaxResponse() {
                 if (xmlhttp.ajaxObject.readyState==4){   
                     dvSpecification.innerHTML =xmlhttp.ajaxObject.responseText
                 }
             }    
             xmlhttp.ajaxObject.open("GET", '/Comparison/compareproducts.php?' + parameter,true);
             xmlhttp.ajaxObject.send(null)
         }
     }

 }

 function Specification(type,parent)
 {
     var dvSpecification=document.getElementById('dvSpecification');
     if(dvSpecification.style.display=='none')
     { dvSpecification.style.display='';
         dvSpecification.style.marginTop='10px';
         //dvSpecification.style.border='1px solid #CCCCCC';
         var eTop = $('#dvSpecificationa').offset().top -120;
         window.scrollTo(eTop,eTop);

     }
     else if((type==0 || type==2) && (dvSpecification.style.display=='block' || dvSpecification.style.display=='')){
         dvSpecification.style.display='none';
         window.scrollTo(0,0);
     }
   
     var parameter="";
     if (isbot=='0')
     {
         if (type==1)
         {
             $("#dvSpecification").html('');
             parameter ="PType=spec" + "&id="+ parent + "&CatName=" + CatName;
         }
         else if(type==2)
         {
             parameter ="PType=info" + "&id=" + CatId + "&CatName=" + CatName.replace("&","**");
         }
         else
         {
             parameter ="PType=spec" + "&id="+ parent + "|1|" + CatId + "&CatName=" + CatName;
        
         }
         if (dvSpecification.innerHTML=="")
         {
      
             var xmlhttp = new CreateRequest()
             xmlhttp.ajaxObject.onreadystatechange=function ajaxResponse() {
                 if (xmlhttp.ajaxObject.readyState==4){   
                     dvSpecification.innerHTML =xmlhttp.ajaxObject.responseText
                 }
             }    
             xmlhttp.ajaxObject.open("GET", '/Comparison/compareproducts.php?' + parameter,true);
             xmlhttp.ajaxObject.send(null)
         }
     }
     else{
         if (type==1)
         {
             $("#dvSpecification").html('');
             //parameter ="PType=spec" + "&id="+ parent + "&CatName=" + CatName;
         }
         else
         {
             //parameter ="PType=spec" + "&id="+ parent + "|1|" + CatId + "&CatName=" + CatName;
        
         }
         //    if (dvSpecification.innerHTML=="")
         //    {
         //  
         //    var xmlhttp = new CreateRequest()
         //    xmlhttp.ajaxObject.onreadystatechange=function ajaxResponse() {
         //            if (xmlhttp.ajaxObject.readyState==4){   
         //            dvSpecification.innerHTML =xmlhttp.ajaxObject.responseText
         //                    }
         //        }    
         //    xmlhttp.ajaxObject.open("GET", '/Comparison/compareproducts.php?' + parameter,true);
         //    xmlhttp.ajaxObject.send(null)
         //    }
     }
 } 

