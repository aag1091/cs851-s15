                     var dds='';
                     if (ddSP!=null)
                     {
                     dds='&amp;p='+ ddSP.options[ddSP.selectedIndex].value;
                     }
                     if (msgdlength>4) 
                      window.location.href = mshost+'search/prodsrch.aspx?data='+ encodeURIComponent(this.value).replace(/%20/g,'+') + decodeURIComponent('%26type=blogs'); 
                    else {
                         
                        
                        var url2 = '%26o=1%26cx=016292610696678199915:bdiiuuck2qo%26cof=FORID:11%26ie=UTF-8%26q=' + encodeURIComponent(this.value).replace(/%20/g,'+') + '%26data=' + encodeURIComponent(this.value).replace(/%20/g,'+') + '%26type=blogs';
