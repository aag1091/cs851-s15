         function GetSliderData() {
            var tsTimeStamp= new Date().getTime();
            var pgCatid='925008794';
            var pgParentId='7';
            var pglevel1='926065';
            var pgParent2='169';
            var pgRevid='739888';
            var pgMergePar='0';
            if (pgMergePar!=0)
            {
                pgCatid=pgMergePar;
            }
            var sliderCatIds=slideCats.split(',');
            var sliderRevIds=slideRevs.split(',');
            var ajxBool=false;
            $.each(sliderCatIds, function(index, value) { 
              //alert(index + ': ' + value); 
              if(pgCatid==value || pgParentId==value || pglevel1==value || pgParent2==value)
              {
                ajxBool=true;
                return false;
              }
            });
            if(ajxBool==false)
            {
                $.each(sliderRevIds,function(index,value)
                {
                    if(pgRevid==value)
                    {
                        ajxBool=true;
                        return false;
                    }
                });
            }
            if(ajxBool==true)
            {            
                $.get('/product/slider_ajax.aspx',{catid:'mntptttuos',parentid:'7',catid2:pgCatid,pg:'rr',timenow:tsTimeStamp}, function (data) {
                //alert(data)
                if (data == '0')
                {      $('#slidebox').html(''); 
                       $('#slidebox').hide();      
                }
                else{
                    var re = new RegExp("<input[^>]*id=\"__VIEWSTATE\"[^>]*>", "g");
                    data = data.replace(re,'');
                    $('#dvSlider').html('');
                    $('#dvSlider').html(data);}
                });
            }
            else
            {
                    $('#slidebox').html(''); 
                    $('#slidebox').hide();              
            }
        }
        GetSliderData();
        
     var v1=0;
     $( function() {
         $( window ).scroll( function( event ) {
         var bottomoffsettop = $('.new-bottombar').offset().top;
         var st = $(window).scrollTop();
         var wH = $(window).height();
         if(bottomoffsettop > (wH + st)){
                 $( '#slidebox' ).stop(true).animate({'right': '-800px'}, 100);
         }
         else {
             $( '#slidebox' ).animate({'right':'0px'}, 200);
              
         }
   /*      
         if ( $(this).scrollTop() < v1 && v1 !=0 )
         {v1=$( this ).scrollTop();
         $( '#slidebox' ).animate({'right': '-800px'}, 100);
         }
            else if ( $(this).scrollTop() >  + 200  ) {
            v1=$( this ).scrollTop();
                $( '#slidebox' ).animate({'right':'0px'}, 100);
            }
            
            else {v1=$( this ).scrollTop();
                $( '#slidebox' ).animate({'right': '-800px'}, 100);
            }*/
        } );
    } );
    
