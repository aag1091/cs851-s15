         <script type="text/javascript">
          
     function signinCallback(authResult) {
        if (authResult['status']['signed_in']) {
    // Update the app to reflect a signed in user
    // Hide the sign-in button now that the user is authorized, for example:
     gapi.client.load('plus','v1', loadProfile); 
            /*var code=authResult['code'];
            var clientid=authResult['client_id'];
            var accesstoken=authResult['access_token'];
            var idtoken=authResult['id_token'];
             gapi.client.load('plus','v1', loadProfile);
            window.document.forms[0].action='http://www.mouthshut.com/facebooklogin/Glogged.aspx?access_token='+accesstoken;
             window.document.forms[0].submit();*/
                //document.getElementById('signinButton').setAttribute('style', 'display: none');
        } else {
            console.log('Sign-in state: ' + authResult['error']);
  }
}


/**
   * Uses the JavaScript API to request the user's profile, which includes
   * their basic information. When the plus.profile.emails.read scope is
   * requested, the response will also include the user's primary email address
   * and any other email addresses that the user made public.
   */
      function loadProfile(){
    var request = gapi.client.plus.people.get( {'userId' : 'me'} );
    request.execute(loadProfileCallback);
  }

  /**
   * Callback for the asynchronous request to the people.get method. The profile
   * and email are set to global variables. Triggers the user's basic profile
   * to display when called.
   */
  function loadProfileCallback(obj) {
    profile = obj;

    // Filter the emails object to find the user's primary account, which might
    // not always be the first in the array. The filter() method supports IE9+.
    email = obj['emails'].filter(function(v) {
        return v.type === 'account'; // Filter out the primary email
    })[0].value; // get the email from the filtered results, should always be defined.
    displayProfile(profile);
  }

  /**
   * Display the user's basic profile information from the profile object.
   */
  function displayProfile(profile){
    /*document.getElementById('name').innerHTML = profile['displayName'];
    document.getElementById('pic').innerHTML = '<img src="' + profile['image']['url'] + '" />';
    document.getElementById('email').innerHTML = email;*/
      window.document.forms[0].hdnId.value = profile['id'];
      window.document.forms[0].hdnEmail.value = email;
      window.document.forms[0].hdnName.value= profile['displayName'];
      window.document.forms[0].hdnfname.value = profile['givenName'];
      window.document.forms[0].hdnlname.value = profile['familyName'];
      if(profile['birthday'])
      {
      window.document.forms[0].hdndob.value=profile['birthday'];}
      if( profile['gender'])
      {window.document.forms[0].hdngen.value = profile['gender'];}
       window.document.forms[0].hdnimgsrc.value=profile['image']['url'];
       if(profile['placesLived'])
       {window.document.forms[0].hdnHmeCty.value= profile['placesLived'][0].value;}
      window.document.forms[0].action='http://www.mouthshut.com/facebooklogin/gplusverify.aspx?l=1&host=web';
      window.document.forms[0].submit();
  }

  /**
   * Utility function to show or hide elements by their IDs.
   */
  
  function assignGplus()
  {
     var myParams = {
            'clientid' : '770711875108-gs3p2lvr2clie10akt4gkdsq5apmskto.apps.googleusercontent.com',
            'cookiepolicy' : 'none',
            'callback' : 'signinCallback',
            'scope' : 'https://www.googleapis.com/auth/plus.login   https://www.googleapis.com/auth/plus.profile.emails.read',
            'requestvisibleactions' : 'http://schemas.google.com/AddActivity'};
            gapi.auth.signIn(myParams);
            tracking1("typ=ml&subtype=gplus&ver=9.0");
  }
           
  </script>

  <script>
      $(document).ready(function(){

      $(".ms-browse-category").mouseover(function() {

          $(".ms-main-nav").css('display', 'block');

      });
      $("#dvMain,#dvFixed").mouseover(function() {
          $(".ms-main-nav").css('display', 'none');
      });

          $('.ms-category-list-slide li').mouseover(function(){
              $(this).addClass('active');
          });
          $('.ms-category-list-slide li').mouseout(function(){
              $(this).removeClass('active');
          });
          $(',.ms-navigation .ms-navigation-wrapper .ms-browse-category').mouseover(function() {
              $('.ms-navigation .ms-navigation-wrapper .ms-browse-category a').css('font-weight','bold');
          }); 
          $(',.ms-navigation .ms-navigation-wrapper .ms-browse-category').mouseout(function() {
              $('.ms-navigation .ms-navigation-wrapper .ms-browse-category a').css('font-weight','normal');
          });

      });
</script>
    	
