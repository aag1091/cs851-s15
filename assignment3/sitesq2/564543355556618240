<script type="text/javascript" >
    var fbloggedin = 0;
   
       
       $(document).ready(function(){
  function facebookReady(){
    FB.init({
      appId  : '146029318804309',
      status : true,
      cookie : true,
      xfbml  : true
    });
    
    FB.getLoginStatus(function(response) {
            //                     alert(response.status)
            if (response.status == "connected") {
                // logged in and connected user, someone you know
                //login(response.authResponse.accessToken,response.authResponse.expiresIn);
                if (window.location.href.indexOf('logout.php') > -1) {
                    FB.logout(function() {
                        window.location.href = 'http://www.mouthshut.com/';
                    });
                    return false;
                }
                else {
                    fbloggedin = 1;
                    FB.api('/me?access_token=' + response.authResponse.accessToken, function(response) {
                        if (response != null) {
                            login(response.id, response.email)
                        }
                        else {
                            timedMsg();
                        }
                    });
                }
            }
            else if (response.status == "not_authorized") {
                if (window.location.href.indexOf('logout.php') > -1) {
                    window.location.href = 'http://www.mouthshut.com/';
                    return false;
                }
                else {
                    fbloggedin = 1;
                    var show = '';
                    //alert(show);
                    if (show != "True") {
                        $("#fbLoginLayer").css({ width: "413px" });
                        execthis = 1;
                        //                            $('#crop_black_overlay').css({visibility:"visible"});
                        //                            $('body').css({overflow:'hidden'});
                        //                            $('#fbLoginLayer').show();
                    }
                }
            }
            else if (response.status == "unknown") {
                if (window.location.href.indexOf('logout.php') > -1) {
                    window.location.href = 'http://www.mouthshut.com/';
                    return false;
                }
                else {
                    fbloggedin = 0;
                }
            }
        }, true);
                
    $(document).trigger("facebook:ready");
  }
    if(window.FB) {
    facebookReady();
  } else {
    window.fbAsyncInit = facebookReady;
  }
});

   (function() {
            var e = document.createElement('script');
            e.type = 'text/javascript';
            e.src = document.location.protocol +
                    '//connect.facebook.net/en_US/all.js#xfbml=1';
            e.async = true;
            document.getElementById('fb-root').appendChild(e);
        } ());
        
        
    function login(token, expires) {

        $.post('http://www.mouthshut.com/fbajax.aspx', { uid: token, hdnEmail: expires }, function(data) {
            if (data == "login") {
                window.location.reload();
            }
        });
    }
    function closeFblogin() {
        $.post('http://www.mouthshut.com/fbajax.aspx', { close: 1 }, function(data) {
            $("#crop_black_overlay").css({ visibility: "hidden" });
            $("#fbLoginLayer").hide();
        });
    }


</script>

     

    

