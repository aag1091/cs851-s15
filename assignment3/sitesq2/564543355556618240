function redirectosrch(){
srctype=''; 
if(srctype=="product") srctype = "";
var val = document.getElementById("txtproduct").value;
if($.trim(val)!="" && val != "Search Products, Reviews and Ratings") {
 if (msgdlength>4)      
     window.location = mshost+'search/prodsrch.aspx?data=' + encodeURIComponent(val).replace(/%20/g,'+') + decodeURIComponent('%26type=' + srctype + '%26p=' + srchcat);   
else

sleep(10)
var url1 ='gsearch=1%26o=1%26cx=016292610696678199915:kd00y2t1ifm%26cof=FORID:11%26ie=UTF-8%26q=' + encodeURIComponent(val).replace(/%20/g,'+') + '%26data=' + encodeURIComponent(val).replace(/%20/g,'+') + '%26type=' + srctype;
if(srctype=='') window.location = mshost+'search/prodsrch.aspx?data=' + encodeURIComponent(val).replace(/%20/g,'+') + decodeURIComponent('%26type=' + srctype + '%26p=' + srchcat);    //mshost+'search/prodsrch.aspx?' + decodeURIComponent(url1);
else if (srctype == 'blogs') window.location = mshost+'search/prodsrch.aspx?gsearch=1' + decodeURIComponent('%26o=1%26cx=016292610696678199915:bdiiuuck2qo%26cof=FORID:11%26ie=UTF-8%26q=' + encodeURIComponent(val).replace(/%20/g,'+') + '%26data=' + encodeURIComponent(val).replace(/%20/g,'+') + '%26type=' + srctype);
else window.location = mshost+'search/prodsrch.aspx?data='+ encodeURIComponent(val).replace(/%20/g,'+') + decodeURIComponent('%26type=' + srctype + '%26p=' + srchcat);   

}
}
function sleep(delay)
{
    var start = new Date().getTime();
    while (new Date().getTime() < start + delay);
}
function singleSelect(cb){
var i=0;
var tot = 21;


if (cb.checked==true){
    for(i=0;i<=tot;i++){
     if (document.getElementById("a" + i) !=undefined) {
      document.getElementById("a" + i).checked = false; 
      }
    }
   cb.checked=true ;
   srchcat = cb.value;
}
else {
    document.getElementById("a0").checked = true;
    srchcat = 0;
}

}

function ToShowLayer()
{ var layer = 'False'
  var show='';
    if (layer=="True")
    {
        if (show=="")
        {   $('html,body').scrollTop(0);
            googleLayer();
        }
    }
}
