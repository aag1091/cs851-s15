function WriteReview()
{
    document.getElementById('dvSpecification').style.display='none';
    if(document.getElementById('writereviewpanel').style.display=='none')
    {document.getElementById('writereviewpanel').style.display='';
        if (document.getElementById('dvreview')){document.getElementById('dvreview').style.display='none';} 
        if(document.getElementById('dvcurvedtab')){document.getElementById('dvcurvedtab').style.display='none';}  
        if (cidOfuserreview1!=cidOfuserreview)
        { 
            cidOfuserreview=cidOfuserreview1;
        }
        if (document.getElementById('ifrmReview2').src=="")
        {              
        
            $('.product-title').hide();
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_top_gads_TopGads').style.display='none';
            document.getElementById('ifrmReview2').src='/review/writereview_readall.aspx?cid=' + cidOfuserreview;
        
        }
        else if (edit_ver_rev==1 && document.getElementById('ifrmReview2').src!="") 
        {
            document.getElementById('ifrmReview2').src='/review/writereview_readall.aspx?cid=' + cidOfuserreview + '&edit_rev=1';
        }
        $('.product-title').hide();
        document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_top_gads_TopGads').style.display='none';
        window.location.href='#writereviewpanela';
    }
        
    else{document.getElementById('writereviewpanel').style.display='none';
        if(browser != "Microsoft Internet Explorer" && browser != "Netscape")
        {   
            window.location.href="";              
        }
        document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_top_gads_TopGads').style.display='block';
        $('.product-title').show();
        if (document.getElementById('dvreview')){document.getElementById('dvreview').style.display='';} 
        if(document.getElementById('dvcurvedtab')){document.getElementById('dvcurvedtab').style.display='';}  
    }
        
    $("#hints").load('/review/writereviewhints.aspx?iscustom=1&cid=925008794');
       AdditionalDetails('hints','expandcollapse5')
       
   }

 
    
   function AdditionalhighlightsDetails()
   {
       $("#dvpopLayer").empty();
       $("#pptitle").html("Additional details");
       //$(".bbg_grey").hide();
