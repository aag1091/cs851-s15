    <script type="text/javascript">
function closeWall()
{
 var clsAjax = new CreateRequest()
    clsAjax.ajaxObject.onreadystatechange=function (){
        if (clsAjax.ajaxObject.readyState == 4){
            document.getElementById('floatdiv').style.display='none'; 
                     
        }
    }
    clsAjax.ajaxObject.open("POST", "/WalStreetSlider.aspx", true)
    clsAjax.ajaxObject.send(null)
  }
    function CallOverlay()
  {
 //  var popuplayer1= document.getElementById("popuplayer1");
 $("#dvpopLayer").empty();
 $("#dvRating").empty();
    $("#dvRating").removeClass('confirmationbox');
    document.getElementById("popuplayer1").style.display="block";
    document.getElementById("popuplayer1").style.width="341px"; 
    $("#dvpopLayer").append($("#mobiledetails").html());   
    document.getElementById("dvpopLayer").style.display="block";
    document.getElementById("pptitle").innerHTML="Please enter mobile number to get Product Details";
    showcentre('popuplayer1');
    //document.getElementById('black_overlay').style.height = document.body.clientHeight + 'px'
    }
 

function MM_showHideLayers() { //v9.0
  var i,p,v,obj,args=MM_showHideLayers.arguments;
  for (i=0; i<(args.length-2); i+=3) 
  with (document) if (getElementById && ((obj=getElementById(args[i]))!=null)) { v=args[i+2];
    if (obj.style) { obj=obj.style; v=(v=='show')?'visible':(v=='hide')?'hidden':v; }
    obj.visibility=v; }
    if(args[0] == 'black_overlay') 
    {
        if(args[2] == 'show'){
            document.getElementById('black_overlay').style.visibility='visible'
            document.getElementById('black_overlay').style.height = document.body.clientHeight + 'px'
            document.getElementById('black_overlay').style.width = (document.body.clientWidth + 30) + 'px'
        }
        else
        {
            document.getElementById('black_overlay').style.visibility='hidden'
            document.getElementById('black_overlay').style.height = document.body.clientHeight + 'px'
            document.getElementById('black_overlay').style.width = (document.body.clientWidth + 30) + 'px'
        }
    }
    
         
}

  function isPhoneNumber() 
{

     // Check for correct phone number
     var s;
     s = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_mob').value
     rePhoneNumber = new RegExp(/^\d{10}$/);

     if (!rePhoneNumber.test(s)) {
          document.getElementById("showerror").style.display = ""
          return false;
     }

return true;
}


