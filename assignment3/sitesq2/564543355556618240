}

var isdocloaded = 0;
function assignimg(path){
    if(path.indexOf('/925008794')>-1)
    {
        path= imgPathForLayer;
    }
    var dtObj = new Date();
    var timee = dtObj.getTime();
    path= path + '?p=' + timee ;
    $('img#ph1').attr('src', path)
              .load(function() { //console.log("loading : " + new Date().getTime()); 
                  //clearTimeout(intt); 
                  document.getElementById("ph1").height=getImgSize(path);
                  document.getElementById("ph1").style.maxWidth="800px";
                  document.getElementById("ph1").style.maxHeight="460px";
                  document.getElementById("photolarge").style.display = "";
                  document.getElementById("photolarge").style.visibility = "visible";
                  document.getElementById("photolarge").focus();
                  showcentre('photolarge');
                  //   showcentre('photolarge');
              });
  
}


function editlisting(){

    var state1 = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_state').value.replace(/^\s+/,'') ;
    var country1 = document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_txtcountry').value.replace(/^\s+/,'') ;
    if(state1 !="" && country1 !=""){
       
        if(document.getElementById('tmc').checked == true){
                
        }
        else {
            document.getElementById('star').style.display= ""
            return false;
        }
    }
    else {
        if(state1==""){
            document.getElementById("stateerr").style.display = "";
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_state').focus();
            }
            else {
                document.getElementById("stateerr").style.display = "none";
            }
            if(country1==""){
                document.getElementById("countryerr").style.display = "";
                document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_txtcountry').focus();
            }
            else {
                document.getElementById("countryerr").style.display = "none";
            }
            return false;
        }

        var parameters = "address=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_address').value) + "&city=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_city').value) + "&state=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_state').value) + "&county=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_txtcountry').value) + "&pincode=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_pincode').value) + "&website=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_website').value) + "&landmark=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_landmark').value) + "&phone=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_phone').value) + "&alternatephone=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_alternatephone').value)  + "&mobilephone=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_mobilephone').value) + "&comment=" + UrlEncode(document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_customheader_comment').value) + "&catname=" + UrlEncode('Little Italy - Juhu - Mumbai') + "&cid=" + 925008794
    var clsAjax = new CreateRequest()
    clsAjax.ajaxObject.onreadystatechange=function (){
        if (clsAjax.ajaxObject.readyState == 4)
        {
            var restxt =  clsAjax.ajaxObject.responseText
            if(restxt=="done") {
                var  ref = $("#Div1").html(); 
                $("#dvpopLayer").empty();
                $("#dvpopLayer").append(ref);
                $("#pptitle").append("&nbsp;");
                $("#popuplayer1").show();
                $("#popuplayer5").hide();
                showcentre('popuplayer1');
            }
            if(restxt=="fail") 
            {
                document.getElementById("dverror1").style.display="block"
            }
        }
    }
    clsAjax.ajaxObject.open("POST", "/sms.aspx?"+parameters, true)
    clsAjax.ajaxObject.setRequestHeader("Content-type", "application/x-www-form-urlencoded")
    clsAjax.ajaxObject.setRequestHeader("Content-length", parameters.length)
    clsAjax.ajaxObject.setRequestHeader("Connection", "close")
    clsAjax.ajaxObject.send(parameters)
}

