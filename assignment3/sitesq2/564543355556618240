	function getproductrating(rate){
	var strrate;
	var Drate = Math.floor(rate);
	var decpart =  rate-Drate
	if(rate>Drate && rate >2 && decpart >= 0.25 && decpart <= 0.75)
	{Drate=Drate+0.5} else if(decpart > 0 && Drate == 0){Drate=Math.ceil(rate)} else if(decpart > 0.75){Drate=Math.ceil(rate)}
    switch(Drate){
    case 0:
      strrate='<span class="smallratenone"></span><span class="smallratenone"></span><span class="smallratenone"></span><span class="smallratenone"></span><span class="smallratenone"></span>'
      break;
    case 1:
      strrate='<span class="smallratefull"></span><span class="smallratenone"></span><span class="smallratenone"></span><span class="smallratenone"></span><span class="smallratenone"></span>'
      break;
    case 2:
     strrate='<span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratenone"></span><span class="smallratenone"></span><span class="smallratenone"></span>'
      break;
    case 2.5:
     strrate='<span class="smallratefull"></span><span class="smallratefull"></span><span class="smallerratefull_half"></span><span class="smallratenone"></span><span class="smallratenone"></span>'
      break;
    case 3:
      strrate='<span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratenone"></span><span class="smallratenone"></span>'
      break;
   case 3.5:
      strrate='<span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratefull"></span><span class="smallerratefull_half"></span><span class="smallratenone"></span>'
      break;
   case 4:
      strrate='<span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratefull"></span><span class="smallratenone"></span>'
