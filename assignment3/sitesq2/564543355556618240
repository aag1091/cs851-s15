<script type ="text/javascript" >
                            function SaveToList(cid,cname)
                            {   var purl=encodeURI(document.URL);
                                var clsAjax = new CreateRequest()
                                clsAjax.ajaxObject.onreadystatechange=function (){
                                    if (clsAjax.ajaxObject.readyState==4){
                                        comparequeue('get','','')
                                            if(document.getElementById(cid)!=null)
                                            {
                                         var ele =document.getElementById(cid).parentNode.parentNode;
                                         if(ele!=null){ele.style.display='none';} 
                                            }
                                                                               
                                    }
                                }
                                clsAjax.ajaxObject.open("GET", '/comparison/savetolist.php?cid=' + cid + '&cname=' + cname + '&URL=' + purl + '&ran=' + Math.random(),true);
                                clsAjax.ajaxObject.send(null)
                            }
                            </script>

     
