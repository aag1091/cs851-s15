        clsAjax.ajaxObject.onreadystatechange=function (){ if (clsAjax.ajaxObject.readyState == 4)
        { var rt = clsAjax.ajaxObject.responseText;
          if (rt!="empty"){
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_nearby').innerHTML = rt;
          }
          else {
            document.getElementById('ctl00_ctl00_ctl00_ContentPlaceHolderHeader_ContentPlaceHolderFooter_ContentPlaceHolderBody_nearby').style.display ='none';
          }
        }
        }
