<script type="text/javascript">
    var broswer;
    browser = navigator.appName; 
    var CatName = "Little Italy - Juhu - Mumbai";
var isver_corp="0";
    var edit_ver_rev="0";

    var CatId = "925008794";
    var cidOfuserreview = "925008794";
    var cidOfuserreview1 = "925008794";
    function UrlEncode(str)
    { str = str.replace(/\+/g, "%2b")
        str = escape(str)
        return str
    }
    var ddlYear
    function trckd(campid, url){
        $.post('/tracking.aspx',{ typ: "campaign", subtype:campid}, function(data) 
        {});
        if(url.indexOf("http://")== -1 && url.indexOf("https://")==-1){
            window.open( 'http://' + url,"_blank");   }else{window.open( url,"_blank");}
    }

    function showRatings(res, txt){
        document.getElementById('divRecentRatings').innerHTML = res;
        document.getElementById('pptitle').innerHTML = "Showing rating for " + txt;
        $("#dvpopLayer").empty(); var  ref = $("#divRecentRatings").html();  $("#dvpopLayer").append(ref); $("#popuplayer1").show(); 
        //window.scroll(0,0);
        showcentre('popuplayer1');
    }

    function displayLastRatings(ddlYrs,lblval,mergedprods){
        // ddlYear = ddlYrs
        // yrs =ddlYrs.value
        var txt = lblval
        var pid = "925008794"
    if (ddlYrs >= 0){
        var clsAjax = new CreateRequest()
        clsAjax.ajaxObject.onreadystatechange=function (){
            if (clsAjax.ajaxObject.readyState == 4)
            {
                showRatings(clsAjax.ajaxObject.responseText, txt)
            }
        }
        clsAjax.ajaxObject.open("GET", "/review/reviewratings_ajax.aspx?pid=" + pid + "&year=" + lblval + "&range=" + ddlYrs + "&rndIe=" + ReturnRndIe() + "&mergedprods=" + mergedprods , true)
        clsAjax.ajaxObject.send(null)   
        return false
    }
}


