<script type="text/javascript">
   function MoreCities()
{
document.getElementById('morelink').style.visibility='hidden'
document.getElementById('morecities').style.display=''
}
  
function LessCities()
{
document.getElementById('morelink').style.visibility=''
document.getElementById('morecities').style.display='none'
}
 function fnRedirect()
 {
    if (document.getElementById("selCntry").value==1)
    {
        window.location='http://mouthshut.com';
    }
  
 }
 
 
function showdvcountryoption()
{
    document.getElementById('dvcountryoption').style.display='block';
} 

function showdvcountryoption1()
{
    document.getElementById('dvcountryoption').style.display='none';
} 
