	$(function() {
		//Product Search Starts
		var xhr1;
        $( "#txtproduct" ).autocomplete({
			source: function( request, response ) {
			var txt = request.term;
			txt = stripHTML(txt);
			request.term = txt;
			if(txt.replace(/[^\w\s]/gi, '')==""){
			return;
			}

			    var key;
			    srctype='';  
			    key=srchcat;  
			    txtId="txtproduct";
			    
			    var parameters = "{contextKey:'"+key+"|0||',count:10,prefixText:'" + txt + "',type:''}"
			    
			    if(prevparameters==parameters) dupreq = true;
			    prevparameters = parameters;
				if(dupreq==true && prevmsgd!=undefined){
				        var msgd = prevmsgd;
				        msgdlength = msgd.length;
				         response( $.map( msgd, function(item) {
				          if(item!=null){
					    var ele=item.split("|")
					    var lblstr;
					    
					    if(ele[1]==0 || ele[1]==1 || ele[1]==2 || ele[1]==3)
					    {
					    return {
					        label:  '<b style="padding-left: 7px;">' + ele[0].replace("repme",stripHTML($('#txtproduct').val())) + '</b>' + '  ' ,
						    value: ele[1]
						    }
						}
						else
						{
