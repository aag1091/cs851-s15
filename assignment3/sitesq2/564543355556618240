    function headover(n){
        for (i=1;i<=2 ;i++ ){
            if (i==n){
                show('link_hide'+i);
            }else{
                hide('link_hide'+i);
            }
        }
    }

    function headout(e,no){
        var p = drop_div(e);
        if (p.id != 'link_hide'+no){
            hide('link_hide'+no);
        }
    }
    function show(divnm){
        d_g_id(divnm).style.display="block";
    }

    function hide(divnm){
        d_g_id(divnm).style.display="none";
    }
    function d_g_id(elem){
        return document.getElementById(elem);
    }
    
    function toggle(divnm){
        if (d_g_id(divnm).style.display=="none" || d_g_id(divnm).style.display=="" || d_g_id(divnm).style.display==null || d_g_id(divnm).style.display==undefined){
            show(divnm);
        }else{
            hide(divnm);
        }
    }
    function drop_div(e) {
        if (!e) var e = window.event;
        var relTarg = e.relatedTarget || e.toElement;
        return relTarg;
    }
    function showDiscreation(rid)
    {    
        document.getElementById('hidReviewId').value = rid;
        //document.getElementById('tblDiscretion').style.display='';
        document.getElementById('btnSubmitDisc').style.display='';
        $("#dvinner").empty();
        //    $("#dvRating").empty();    
        //    $("#dvRating").attr('class','');      
        //    $("#pptitle").html("<strong id='titletext'>The ingenuousness of this review appears doubtful.<br />Do you think this review is genuine?</strong>");
        var  ref = $("#descrition").html();
        $("#dvinner").append(ref);
        $("#AjxLayrFake").width(430);
        //$("#descrition").html("");
        showcentre('AjxLayrFake');
        closepp("AjxLayrFake");   
    }
    
    function setFocus()
    {

        //document.getElementById('txtDName').value='' 
        //document.getElementById('txtDEmail').value='' 
        //
        ////document.getElementById('txtDMsid').readOnly=false;
        ////document.getElementById('txtDEmail').readOnly=false;
        //
        document.getElementById('txtDAreaComm').focus();
    }
    function validate()
    {
        var name = '';
    var msid = 'Guest';
    var emai = '';
    var comm = document.getElementById("txtDAreaComm").value.trim();
    var gfval;
    //document.getElementById('dvError').className ="";
    //document.getElementById('dvError').innerHTML="";
    var err="";
    var emailerr="";
    if(name=="")
    {
        err="Name can not be blank " + '\n';
    }
    if(document.getElementById('roundedOne').checked != true && document.getElementById('roundedTwo').checked != true)
    {
        err=err + "Please select any one option " + '\n';
    }
    else
    {
        if(document.getElementById('roundedOne').checked == true)
        {
            gfval= 0;
        }
        else
        {
            gfval= 1;
        }
    }
    if (emai=="")
    {
        err=err + "Email can not be blank " + '\n';
    }
    else
    {
        if (validateEmail(emai)==false)
        {
            emailerr=emailerr + "Invalid E-mail ID"+ '\n';
        }
    }
    if (comm=="")
    {
        err=err + "Your Comments can not be blank " + '\n';
    }
    if (err=="" && emailerr == "")
    {
        SendEmail("name=" + name + "&msid=" + msid + "&email=" + emai + "&comment=" + comm + "&rid=" + document.getElementById('hidReviewId').value + "&gfval=" + gfval);
    }
    else if (emailerr!="")
    {
        //   document.getElementById('dvError').className="errorbox";
        //   document.getElementById('dvError').innerHTML = "<span>" + emailerr + "</span>"
    }
    else
    {
        alert(err);
        //   document.getElementById('dvError').className="errorbox";
        //   document.getElementById('dvError').innerHTML = "<span>* marked fields are mandatory</span>"
    }
}
function SendEmail(parameters)
{
    var clsAjax = new CreateRequest()
    clsAjax.ajaxObject.onreadystatechange=function (){
        if (clsAjax.ajaxObject.readyState == 4)
        {
            //            document.getElementById('tblDiscretion').style.display='none';
            document.getElementById('btnSubmitDisc').style.display='none';
            //            document.getElementById('dvError').className="confirmationbox";   
            document.getElementById('optionpopup').innerHTML = "<span>Thank you for writing to us. Our Support Team shall look into this and get back to you soon.</span>"
            closepptimeout('AjxLayrFake',2000);
        }
    }
    clsAjax.ajaxObject.open("POST", "/review/DescritionAjax.aspx", true)
    clsAjax.ajaxObject.setRequestHeader("Content-type", "application/x-www-form-urlencoded")
    clsAjax.ajaxObject.setRequestHeader("Content-length", parameters.length)
    clsAjax.ajaxObject.setRequestHeader("Connection", "close")
    clsAjax.ajaxObject.send(parameters)
}
