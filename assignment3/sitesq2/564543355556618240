<script type="text/javascript">
    var arrImg_mp,arrImgCaption_mp
    var k, k_menu,k_mp, type1
    var readAlType = "restaurant"
var totalImages, totalImages_menu,totalImages_mp
if(readAlType=="movie")
{
    var imgServerPath='http://image3.mouthshut.com/images/MoviesMusic/photo/'
    var imgServerPath_menu='http://image3.mouthshut.com/images/MoviesMusic/photo/'
    var imgServerPath_mp='http://image3.mouthshut.com/images/ImagesR/'
}
else 
{
    var imgServerPath='http://image3.mouthshut.com/images/Restaurant/Photo/'
    var imgServerPath_menu='http://image3.mouthshut.com/images/Restaurant/Menu/'
    var imgServerPath_mp='http://image3.mouthshut.com/images/ImagesR/'
}
 


               
        var arrImg = new Array("925008794") 

totalImages=1
               
          
        
var imgPathForLayer = 'http://image3.mouthshut.com/images/imagesp/925008794s.jpg' 
            
        
function clicktozoom1(photoid,idx,type) {
    if(isdocloaded==1){
        document.getElementById("formp").style.display="none";
        if (type=="prop") {
            if(arrImg[idx].indexOf("|")==0){
                assignimg(imgServerPath_mp + arrImg[idx].substring(1,arrImg[idx].length)); 
            }
            else {
                assignimg(imgServerPath + arrImg[idx]);     
            }
            type1="prop"
            k=idx;
            if(k==0)
                document.getElementById("pre").setAttribute('class', 'btn-prev-disabled')
            else {
                document.getElementById("pre").setAttribute('class', 'btn-prev')
                document.getElementById("pre").setAttribute('onclick', 'previous()')
                document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
            }
            if(k==(totalImages-1))
                // FOR LAST IMAGE : next button disabled
                document.getElementById("nxt").setAttribute('class','btn-next-disabled');
            else {
                document.getElementById("nxt").setAttribute('class','btn-next');
                document.getElementById("nxt").setAttribute('onclick', 'next1()');
                document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
            }
             //IF THERE IS ONLY ONE IMAGE  
            //if((k==0) && (k==(totalImages-1)))
            //    objDiv.style.display="none"
            //else
            //    objDiv.style.display=""
        }
        else if(type=="menu")
        {
            assignimg(imgServerPath_menu + arrImg_menu[idx]);  
            type1="menu"
            k_menu=idx;
            if(k_menu==0)
                document.getElementById("pre").setAttribute('class', 'btn-prev-disabled')
            else {
                document.getElementById("pre").setAttribute('class', 'btn-prev')
                document.getElementById("pre").setAttribute('onclick', 'previous_menu()')
                document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
            }
            if(k_menu==(totalImages_menu-1))
                document.getElementById("nxt").setAttribute('class', 'btn-next-disabled')
            else {
                document.getElementById("nxt").setAttribute('class', 'btn-next')
                document.getElementById("nxt").setAttribute('onclick', 'next_menu()')
                document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>" }
            // IF THERE IS ONLY ONE IMAGE  
            //if((k_menu==0) && (k_menu==(totalImages_menu-1)))
            //    objDiv.style.display="none"
            //else
                //objDiv.style.display=""
        }
        else if (type=="ad") {
            if(arrImg_ad[idx].indexOf("|")==0){
                assignimg(imgServerPath + arrImg_ad[idx].substring(1,arrImg_ad[idx].length)); 
            }
            else {
                assignimg(imgServerPath + arrImg_ad[idx]);     
            }
            type1="ad"
            k=idx;
            if(k==0)
                document.getElementById("pre").setAttribute('class', 'btn-prev-disabled')
            else {
                document.getElementById("pre").setAttribute('class', 'btn-prev')
                document.getElementById("pre").setAttribute('onclick', 'previous_ad()')
                document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
            }
            if(k==(totalImages_ad-1))
                // FOR LAST IMAGE : next button disabled
                document.getElementById("nxt").setAttribute('class','btn-next-disabled');
            else {
                document.getElementById("nxt").setAttribute('class','btn-next');
                document.getElementById("nxt").setAttribute('onclick', 'next_ad()');
                document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
            }
            // IF THERE IS ONLY ONE IMAGE  
            //if((k==0) && (k==(totalImages_ad-1)))
            //    objDiv.style.display="none"
            //else
                //objDiv.style.display=""
        }
        else if(type=="mp")
        { 
            assignimg(imgServerPath_mp + arrImg_mp[idx]);
            document.getElementById("formp").style.display="";
            document.getElementById("Desc").innerHTML = arrImgCaption_mp[idx]
            type1="mp"
            k_mp=idx;
            if(k_mp==0)
                document.getElementById("pre").setAttribute('class','btn-prev-disabled')
            else {
                document.getElementById("pre").setAttribute('class','btn-prev')
                document.getElementById("pre").setAttribute('onclick','previous_mp()')
                document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
            }
            if(k_mp==(totalImages_mp-1))
                // FOR LAST IMAGE : next button disabled
                document.getElementById("nxt").setAttribute('class','btn-next-disabled')
            else
            {
                document.getElementById("nxt").setAttribute('class','btn-next')
                document.getElementById("nxt").setAttribute('onclick','next_mp()')  
                document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
            }
            // IF THERE IS ONLY ONE IMAGE  
            //if((k_mp==0) && (k_mp==(totalImages_mp-1)))
            //    objDiv.style.display="none"
            //else
                //objDiv.style.display=""
        }

        //MM_showHideLayers('black_overlay','','show')

    }
}

function closethis(){
    document.getElementById("pre").setAttribute('class','btn-prev');
    document.getElementById("nxt").setAttribute('class','btn-next');
    document.getElementById("pre").setAttribute('onclick','previous()');
    document.getElementById("nxt").setAttribute('onclick','next_menu()');
    document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>";
    document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>";
    document.getElementById('photolarge').style.display = 'none'; 
    //  MM_showHideLayers('black_overlay','','hide');
}

function next1()
{
    if(k==(totalImages-1))
        return
    if(k==(totalImages-2))
    {  document.getElementById("nxt").setAttribute('class','btn-next-disabled');}
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next1()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
    if(arrImg[k+1].indexOf("|")==0){
        assignimg(imgServerPath_mp + arrImg[k+1].substring(1,arrImg[k+1].length));
					
    }
    else {
        assignimg(imgServerPath + arrImg[k+1]);
					
    }        
          
    k=k+1;
    if(k>0) {
        document.getElementById("pre").setAttribute('onclick','previous()')
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
        objPrev.style.display=""
    }
}     
        
function previous()
{
    if(k==0)
        return
    if(k<(totalImages))
        objNext.style.display=""
    var imgsrc;
    if(arrImg[k-1].indexOf("|")==0){
        assignimg(imgServerPath_mp + arrImg[k-1].substring(1,arrImg[k-1].length)); 
    }
    else { assignimg(imgServerPath + arrImg[k-1]);
    }
                             
    k=k-1

    if(k==0)
        document.getElementById("pre").setAttribute('class','btn-prev-disabled')
           
    else {
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").setAttribute('onclick','previous()')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
    }
    if(k==(totalImages-1))
        document.getElementById("nxt").setAttribute('class','btn-next-disabled')
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next1()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
		
}
       

function next_ad()
{
    if(k==(totalImages_ad-1))
        return
    if(k==(totalImages_ad-2))
    {  document.getElementById("nxt").setAttribute('class','btn-next-disabled');}
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next_ad()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
    if(arrImg_ad[k+1].indexOf("|")==0){
        assignimg(imgServerPath + arrImg_ad[k+1].substring(1,arrImg_ad[k+1].length));
					
    }
    else {
        assignimg(imgServerPath + arrImg_ad[k+1]);
					
    }        
          
    k=k+1;
    if(k>0) {
        document.getElementById("pre").setAttribute('onclick','previous_ad()')
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
        objPrev.style.display=""
    }
}     
        
function previous_ad()
{
    if(k==0)
        return
    if(k<(totalImages_ad))
        objNext.style.display=""
    var imgsrc;
    if(arrImg_ad[k-1].indexOf("|")==0){
        assignimg(imgServerPath + arrImg_ad[k-1].substring(1,arrImg_ad[k-1].length)); 
    }
    else { assignimg(imgServerPath + arrImg_ad[k-1]);
    }
                             
    k=k-1

    if(k==0)
        document.getElementById("pre").setAttribute('class','btn-prev-disabled')
           
    else {
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").setAttribute('onclick','previous_ad()')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
    }
    if(k==(totalImages_ad-1))
        document.getElementById("nxt").setAttribute('class','btn-next-disabled')
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next_ad()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
		
}


function previous_menu()
{
    if(k_menu==0)
        return
    if(k_menu<(totalImages_menu))
        objNext.style.display=""
    assignimg(imgServerPath_menu + arrImg_menu[k_menu-1]);
			 
    k_menu=k_menu-1
    if(k_menu==0)
        document.getElementById("pre").setAttribute('class','btn-prev-disabled')
    else {
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").setAttribute('onclick','previous_menu()')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
    }
    if(k_menu==(totalImages_menu-1))
        document.getElementById("nxt").setAttribute('class','btn-next-disabled')
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next_menu()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
}

function next_menu()
{
    if(k_menu==(totalImages_menu-1))
        return
    if(k_menu==(totalImages_menu-2))
        document.getElementById("nxt").setAttribute('class','btn-next-disabled')
    else {
        MM_preloadImages(imgServerPath_menu + arrImg_menu[k_menu+2]) 
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next_menu()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
    assignimg(imgServerPath_menu + arrImg_menu[k_menu+1]);
		   
    k_menu=k_menu+1
    if(k_menu>0) {
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").setAttribute('onclick','previous_menu()')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
        objPrev.style.display=""
             
    } 
}
       
function next_mp()
{
    if(k_mp==(totalImages_mp-1))
        return
    if(k_mp==(totalImages_mp-2))
        document.getElementById("nxt").setAttribute('class','btn-next-disabled')
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next_mp()') 
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
    assignimg(imgServerPath_mp + arrImg_mp[k_mp+1]);
		   
    document.getElementById("formp").style.display="";
    document.getElementById("Desc").innerHTML = arrImgCaption_mp[k_mp+1]
    k_mp=k_mp+1
    if(k_mp>0) {
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").setAttribute('onclick','previous_mp()')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
        objPrev.style.display="";          
    } 
}

function previous_mp()
{
    if(k_mp==0)
        return
    if(k_mp<(totalImages_mp))
        objNext.style.display=""
    assignimg(imgServerPath_mp + arrImg_mp[k_mp-1]);
		   
    document.getElementById("formp").style.display="";
    document.getElementById("Desc").innerHTML = arrImgCaption_mp[k_mp-1];    
    k_mp=k_mp-1;
    if(k_mp==0)
        document.getElementById("pre").setAttribute('class','btn-prev-disabled')
    else {
        document.getElementById("pre").setAttribute('class','btn-prev')
        document.getElementById("pre").setAttribute('onclick','previous_mp()')
        document.getElementById("pre").innerHTML="<a id='btnprevious' href='javascript:void(0);'></a>"
    }
    if(k_mp==(totalImages_mp-1))
        document.getElementById("pre").setAttribute('class','btn-prev-disabled')
    else {
        document.getElementById("nxt").setAttribute('class','btn-next')
        document.getElementById("nxt").setAttribute('onclick','next_mp()')
        document.getElementById("nxt").innerHTML="<a id='btnnext' href='javascript:void(0);'></a>"
    }
}
        
function MM_preloadImages() { 
    var d=document; if(d.images){ if(!d.MM_p) d.MM_p=new Array();
        var i,j=d.MM_p.length,a=MM_preloadImages.arguments; for(i=0; i<a.length; i++)
            if (a[i].indexOf("#")!=0){ d.MM_p[j]=new Image; d.MM_p[j++].src=a[i];}}
}
     
function getImgSize(imgSrc)
{
    var curHeight=460; var newHeight=0; 
    var newImg = new Image(); 
    newImg.src = imgSrc; 
    curHeight = newImg.height; 
    if (curHeight >= 460 )
    { newHeight = 460;}
    else
    { newHeight= curHeight;}
    if(newHeight == 0) newHeight = 460;
    return newHeight
}

function getImgSize_width(imgSrc)
{
    var curWidth=475; var newWidth=0; 
    var newImg = new Image(); 
    newImg.src = imgSrc; 
    curWidth = newImg.width; 
    //        if (curWidth >= 475 )
    //        { newWidth = 475;}
    //        else
    //        { newWidth= curWidth;}
    if(curWidth == 0) newWidth = 475;
    else newWidth= curWidth;
    return newWidth
}
                
