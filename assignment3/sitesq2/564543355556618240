<script type="text/javascript">
function trackshare()
{
    var strSession='0';
    FB.ui(
  {
    method: 'share',
    href: 'http://www.mouthshut.com/review/Little-Italy-Juhu-Mumbai-review-mntptttuos',
    app_id: '146029318804309'
  },
  function(response) {
    if (response && !response.error_code) 
    {
        if (strSession>0)
        {
        if (window.location.href.indexOf("-review-")>0 || window.location.href.indexOf("thankyou.php")>0) 
        {
         $.get(mshost+'tracking/hits.php?sharecnt=f&key=http://www.mouthshut.com/review/Little-Italy-Juhu-Mumbai-review-mntptttuos&user=' + strSession,function(res)
            {
                
            });
         }
        }
    } 
    else {
      //alert('Error while posting.');
    }
  }
 );
}

function tracktweet()
{
    var strSession='0';
    if (strSession>0)
        {
        if (window.location.href.indexOf("-review-")>0 || window.location.href.indexOf("thankyou.php")>0)
        {
         $.get(mshost+'tracking/hits.php?sharecnt=t&key=http://www.mouthshut.com/review/Little-Italy-Juhu-Mumbai-review-mntptttuos&user=' + strSession,function(res)
            {
                return true;
            });
         }
        }
        return true;
}
