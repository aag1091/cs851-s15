<script type="text/javascript" >
var user='';
function popupwindow(unm,img)
{
    if(document.getElementById("dvpopLayer").innerHTML != '' && unm !='')
    {
    //document.getElementById("hunm").value=unm;
    $("#dvRating").attr("class","");
    document.getElementById("dvRating").innerHTML = "";
    user=unm;    
    document.getElementById("popuplayer1").style.display ="block";
    document.getElementById("popuplayer1").style.width ="360px";
    document.getElementById("pptitle").innerHTML ="Send Message to " + unm ;
    document.getElementById("Msg").style.display="block";  
    document.getElementById("userImage").src = img;
    showcentre('popuplayer1')
    }  
    else if (document.getElementById("dvpopLayer").innerHTML == '' && unm !='')
        {            
           var  ref = $("#M2Mbdy").html();
           $("#dvpopLayer").html(ref);  
           popupwindow(unm,img);    
        }
}
function clearMsg(field)
{
if (field.defaultValue == field.value)
{ 
    field.value = '';
    //field.setAttribute('style','color:#000000;padding: 4px;vertical-align: middle;width: 200px;');
}
else if (field.value == '') {field.value = field.defaultValue;}
}
function validateM2M()
{
    var txtMailM2M = document.getElementById("txtMailContent").value;
    var txtSubjectM2M = document.getElementById("txtSubject").value;   
    if (txtMailM2M.trim() == "" || txtSubjectM2M.trim() == "" || txtSubjectM2M.trim() == 'Subject' ||  txtMailM2M.trim() == 'Message' )
    {      
    var dvRating=document.getElementById("dvRating");
    dvRating.style.display="block";
    dvRating.className = "errorbox";
    dvRating.innerHTML="<span>Subject or Message cannot be blank.</span>";
    }
    else
    {  
        document.getElementById("btnSndM2M").disabled=true;
        if ('1'=='1')
        {
           AjaxPostM2M(M2MPostHP, "user=" + user + "&m2m=1&MSSupport=1&cat_id=925008794&m2mSub=" + escape(txtSubjectM2M.trim()) + "&m2mMsg=" + UrlEncode(txtMailM2M.trim()))
        }
        else
        {
                AjaxPostM2M(M2MPostHP, "user=" + user + "&m2m=1&m2mSub=" + escape(txtSubjectM2M.trim()) + "&m2mMsg=" + UrlEncode(txtMailM2M.trim()))
        }
       
    }
    return false
}

function UrlEncode(str)
{ 
    str = str.replace(/\r\n/g, '<br> ');
    str = str.replace(/\n/g, '<br> ');
    str = str.replace(/\r/g, '<br> ');
    str = str.replace(/\&/g, '%26');
    return str
}

function BlockNoneLayers() { 
  var i,p,v,obj,args=BlockNoneLayers.arguments;
  for (i=0; i<(args.length-2); i+=3) 
  with (document) if (getElementById && ((obj=getElementById(args[i]))!=null)) { v=args[i+2];
    if (obj.style) { obj=obj.style; v=(v=='show')?'block':(v=='hide')?'none':v; }
    obj.display=v; }
}
