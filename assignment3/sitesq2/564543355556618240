function ExpandReview(comtxt,morereviewcontent,linkcollapse,linkexpand)
{
var htm = document.getElementById(comtxt).innerHTML;
htm = htm.substring(0,htm.length-2);
document.getElementById(comtxt).innerHTML = htm;
document.getElementById(linkexpand).style.display='none'
document.getElementById(morereviewcontent).style.display=''          
document.getElementById(linkcollapse).style.display=''
}
function CollapseReview(comtxt,morereviewcontent,linkcollapse,linkexpand,ratingradio)
{
var htm = document.getElementById(comtxt).innerHTML;
htm = htm + "..";
document.getElementById(comtxt).innerHTML = htm;
document.getElementById(linkexpand).style.display=''
document.getElementById(morereviewcontent).style.display='none' 
document.getElementById(linkcollapse).style.display='none'
}


function showHideonClick(name)
    {
        document.getElementById('writereview').style.visibility='hidden'
        document.getElementById('ratereview').style.visibility='hidden'
        document.getElementById('postdiary').style.visibility='hidden'
        document.getElementById('addcomment').style.visibility='hidden'
        document.getElementById('sendgift1').style.visibility='hidden'
        
        clearTimeout(t);         
        clearTimeout(wr);
        clearTimeout(rr);
        clearTimeout(ac);
        clearTimeout(sg);
        clearTimeout(pd);
        
        if (name == "writereview")
        {
            window.open('http://www.mouthshut.com/review/write_review.php?cid=925008794');
        }
        if (name == "ratereview")
        {
            window.location.href='#tblRateButtons';
        }
        if (name == "addcomment")
        {
            window.location.href='#txtComment';
        }
        if (name == "giftshop")
        {
           if ('0' == 0)
            {
                ajaxLogin('/gifts/sendGiftPP.php?uid=' + 'ang_ne');
            }
            else
            {
                window.open('http://www.mouthshut.com/' + 'gifts/sendGiftPP.php?uid=' + 'ang_ne','sg','scrollbars=yes,width=805,height=600');
            }    
        }
        if (name == "postdiary")
        {
             window.open('http://www.mouthshut.com/' + 'blog/write_blog.php');
        }
        
        var clsAjax = new CreateRequest()
        clsAjax.ajaxObject.onreadystatechange=function (){
        if (clsAjax.ajaxObject.readyState == 4){
                     
        }
    }
    clsAjax.ajaxObject.open("POST", "/review/reviewratings_ajax.aspx?message=1", true)
    clsAjax.ajaxObject.send(null)
        
    }
    
    function likeComment(comment_id,likeid,likeCnt,liketxt)
    {
        var querystring = "comment_id=" + comment_id + "&type=0"
        var clsAjax = new CreateRequest()
        clsAjax.ajaxObject.onreadystatechange=function ()
        {
            if (clsAjax.ajaxObject.readyState == 4)
                {
                      document.getElementById(likeid).style.display='none';
                      document.getElementById(liketxt).style.display='';
                    var count = document.getElementById(likeCnt).innerHTML;
                    document.getElementById(likeCnt).innerHTML = parseInt(count) + 1;
                   
                }
        }
       clsAjax.ajaxObject.open("GET", "/review/reviewajax.php?" + querystring, true)
        clsAjax.ajaxObject.send(null)
    }
    
    function BindlikeComment(comment_id)
    {
        var querystring = "comment_id=" + comment_id + "&type=0"
        $("#dvpopLayer").empty();
        $("#dvRating").empty();
        $("#dvRating").attr('class','');
        $('#dvpopLayer').html('<br><strong style="font-size:18px;">  Loading...</strong><br><br>');
        $("#dvpopLayer").load("/comment/LikeComment.aspx?" + querystring);        
        $("#pptitle").html("People who like this");
        document.getElementById("popuplayer1").style.width="300px";
        showcentre('popuplayer1');
        $("#dvPadding").removeAttr('style');
        $('#popuplayer1').css({ top: ($(window).height() - 194 ) / 2 + ($(window).scrollTop() - 70)+ "px",left: ( $(window).width() - 342 ) / 2+$(window).scrollLeft() + "px" }).show();
        closepp("popuplayer1");
    }
    
