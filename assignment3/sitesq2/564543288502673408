[ 10, function() {
  var Y = Naiad.Y;

  Naiad.MMM.Init({ photo:'#mmm_iframe_wrapper',video:'#mmm_iframe_wrapper_video'}, 'display.naiadmmm.com');

  var img_enterchat = Y.one('#img_enterchat');
