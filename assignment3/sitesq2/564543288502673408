  <script type="text/javascript" charset="utf-8">
  var oRefreshIframe;
  function switchMultimedia(new_tab) {
    var tabs = ['about', 'photos', 'videos', 'mmm-videos-free', 'comments', 'schedule'], e, i, mmmIframe;
    for (i in tabs) {
      if( tabs[i] != new_tab ) {
        e = document.getElementById('bio-'+tabs[i]+'-container'); if( e ) e.style.display = 'none';
        e = document.getElementById('bio-'+tabs[i]+'-tab'); if( e ) e.className = '';
      }
    }
    e = document.getElementById('bio-'+new_tab+'-container'); if( e ) e.style.display = 'block';
    e = document.getElementById('bio-'+new_tab+'-tab'); if( e ) e.className = 'active';

    if( new_tab == 'photos' ) {
      mmmIframe = document.getElementById('mmm_iframe_wrapper');
    } else if( new_tab == 'mmm-videos-free' ) {
      mmmIframe = document.getElementById('mmm_iframe_wrapper_video');
    }
    if( mmmIframe ) {
      /**
       * if we're refreshing the iframe, the resize works correctly anyway
       * if not, we need to fire a resize event because the thing was hidden originally
       */
      if( oRefreshIframe == new_tab ) {
        mmmIframe.src = mmmIframe.src;
      } else {
        //the MMM iframe is 2 layers deep.  Also, using * because we don't care if someone else fires a resize event
        mmmIframe.contentWindow.document.getElementsByTagName('iframe')[0].contentWindow.postMessage('forceResize', '*');
      }
    }
    oRefreshIframe = new_tab;
  }
  
  function showHideDiv( id ) {
    d = document.getElementById(id);
    if( d ) {
      d.className = ( d.className == 'hide') ? d.className = 'show' : d.className = 'hide';
    }
  } 
</script> 

