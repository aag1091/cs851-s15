        e.preventDefault();
        AVChat.Show();
        AVChat.RemoveLoadFromLanguageLinks();
      });
  }

  var btn_enterchat = Y.one('#btn_enterchat');
  if (btn_enterchat)
  {
    btn_enterchat.on('click', function(e) {
      e.preventDefault();
      AVChat.Show();
      AVChat.RemoveLoadFromLanguageLinks();
    });
  }

  var status_enterchat = Y.one('#status_enterchat');
  if (status_enterchat)
