function checkSub(){
var chkForm = document.getElementById('chkForm');

if (chkForm["fields_email"].value=="") {
alert("Please enter in your valid Email address.")
chkForm["fields_email"].focus();
return false}


 if(chkForm["fields_email"].value!="")
 {
  var str = chkForm["fields_email"].value;
  var reg1 = /(@.*@)|(\.\.)|(@\.)|(\.@)|(^\.)/; 
  var reg2 = /^.+\@(\[?)[a-zA-Z0-9\-\.]+\.([a-zA-Z]{2,3}|[0-9]{1,3})(\]?)$/;
  var reg3 = /\+/;
  if (!reg1.test(str) && reg2.test(str) && !reg3.test(str)){ 
  }else{
  alert ("Please Enter Proper Email Address");
  chkForm["fields_email"].focus();
  return false;
  }
 }


if (chkForm["fields_fname"].value == "") {
alert("Please enter in your Name.")
chkForm["fields_fname"].focus();
return false}
}
</script>



