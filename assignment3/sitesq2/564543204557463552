					<script>
					
					// Show chat controls
					$('.btnControls').click(function(e){
						
						e.preventDefault();
						
						var controlBtn		= $(this);
						var allControlBtns	= $('.btnControls');
						var controlName 	= controlBtn.attr('data-chat-controls');
						var controlDivId 	= $('#' + controlName);
						
						if(controlDivId.hasClass('show')){
							allControlBtns.removeClass('active');
							$('.chatToolWrap').removeClass('show');
						}else{
							allControlBtns.removeClass('active');
							controlBtn.addClass('active');
							$('.chatToolWrap').removeClass('show');
							controlDivId.addClass('show');
						}
						
						
					});
					
					$('.closeChatControls').click(function(){
						
						$('.btnControls').removeClass('active');
						$('.chatToolWrap').removeClass('show');
					
					});
					
					
										
					
					</script>
					
					
				
						
				
				</div><!-- /chatWrapper -->
				
				
				
	
				
	
	
