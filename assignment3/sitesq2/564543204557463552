				</style>
				<script type="text/javascript">
				
					$(document).ready(function() {				
					
						//
						//	Set interval to rotate gifts
						//
						var rotateSeconds	= 60000;
						var giftsTimer		= setInterval(rotategifts, rotateSeconds);
						
						//
						//	Add paging logic to gift rotater
						//				
						$('.giftsPaging div').click(function(e)
						{
							e.preventDefault();
							
							var pagingNum = $(this).attr('id').split('_', 2);
							
							$('#gifting_under_chat_area').removeClass();
							$('#gifting_under_chat_area').addClass('giftsSet_' + pagingNum[1]);
							
							clearInterval(giftsTimer);
							
							giftsTimer	= setInterval(rotategifts, rotateSeconds);
						
						});
						
						function rotategifts() {
					
							var numSets					= 4;
							var	giftContainer			= $('#gifting_under_chat_area');
							var currentGiftSetNumber	= giftContainer.attr('class').split('_', 2);
							var giftSets 				= $('.giftContainer');
														
							//
							//	Determine next set
							//
							var nextGiftSet	= Number(currentGiftSetNumber[1]) + 1;
							
							if( nextGiftSet > numSets ) {
							
								//
								//	Reset
								//
								nextGiftSet = 1;
							
							}
							
							//
							//	Display next gifts set
							//
							var newGiftSet	= 'giftsSet_' + nextGiftSet;
							
							giftContainer.removeClass();
							giftContainer.addClass(newGiftSet);
						
						}
					
					});
				
				</script>
				
				
			
