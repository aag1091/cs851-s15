<script type="text/javascript">
  jQuery(document).ready(function(){

    jQuery("input.mceemail").click(function(){
      if(jQuery(this).val() == "Your email"){
        jQuery(this).val("");
      }
    });
    jQuery("input.mceemail").blur(function() {
      if(jQuery(this).val() == ""){
        jQuery(this).val("Your email");
