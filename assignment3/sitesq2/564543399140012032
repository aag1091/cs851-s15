                            <p class="date"><a class="delcmt" cid="<#= d.cid #>">delete</a></p>
                        <# }else{ #>
                            <p class="date"><#= d.date #></p>
                        <# } #>
                    </div>
                    <p class="content"><#= d.text #></p>
                </div>
                <# } #>


            <# } #>
        </div>
    </script>
    <div id="ctl00_ContentRightColumn_RightColumn_pnlRightColumn" class="rightcolumnsidebar">
	<div id="ctl00_ContentRightColumn_RightColumn_PremiumAd1_ucBanMan_BanManAd" class="banmanad" style="height:60px;min-height:60px;">
