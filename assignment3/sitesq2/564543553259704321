		function amcn_pixeldrop(o) {
			if (!o.ok) {
				var oImg = document.createElement('img');
				var src = '';
				switch (o.isp) {
					case 'time-warner':
					case 'brighthouse':
						src = "http:\/\/domdex.com\/con?image=true&s=50ed9a20242dfb636300000d";
						break;
					case 'rogers':
						src = "";
						break;
					case 'mediacom':
						src = "";
						break;
					case 'cable-one':
						src = "";
						break;
				}
				if (src) {
					oImg.setAttribute('src', src);
					oImg.setAttribute('alt', 'na');    
					oImg.setAttribute('height', '0px');
					oImg.setAttribute('width', '0px');
					document.body.appendChild(oImg);
				}
			}
		}
		</script>
