function loginFB(){
	
	includePrototype();
FB.login(function(response) {
		if (response.session) {
			if (response.perms) {
				// user is logged in and granted some permissions.
				// perms is a comma separated list of granted permissions
			   // Ajax Request To FbLogin.
		
		
		new Ajax.Request('../fblogintobe.php',{method:'get',onSuccess : getResponse });
		//jQuery.ajax({url:"fblogintobe.php",success:getResponse});
			} else {
			
				
			}
		} else {
			// user is not logged in
		
		}
	}, {perms:'email,publish_stream'});

	
	

}

function includePrototype()
{
	var headID = document.getElementsByTagName("head")[0];         
var newScript = document.createElement('script');
newScript.type = 'text/javascript';
newScript.src = '/js/prototype.js';
headID.appendChild(newScript);
	//alert("included");
	}
	
function removePrototype(filename, filetype){
	filename="prototype.js";
	filetype="js";
	
 var targetelement=(filetype=="js")? "script" : (filetype=="css")? "link" : "none" //determine element type to create nodelist from
 var targetattr=(filetype=="js")? "src" : (filetype=="css")? "href" : "none" //determine corresponding attribute to test for
 var allsuspects=document.getElementsByTagName(targetelement)
 for (var i=allsuspects.length; i>=0; i--){ //search backwards within nodelist for matching elements to remove
  if (allsuspects[i] && allsuspects[i].getAttribute(targetattr)!=null && allsuspects[i].getAttribute(targetattr).indexOf(filename)!=-1)
   allsuspects[i].parentNode.removeChild(allsuspects[i]) //remove element by calling parentNode.removeChild()
 }
}






function getResponse(transport,json){
//alert(transport.responseText);
	result = JSON.parse(transport.responseText);
	
	switch(result.status)
	{
    case '5':
    jQuery.facebox({ iframe: '../request_username.php?',height:325,width:650});
	break;
	
	case '4': // Mapped Existing VbUser Reload the page
	doLoginRedirect();
	break;
	
	case '3': // Logged In Connected User
	doLoginRedirect();
	break;
	}

}

function closeBox()
{

jQuery.facebox.close();	
	doLoginRedirect();
}

function doLoginRedirect()
{
jQuery.facebox.close();	

window.location="http://www.letsbeef.com/forums/index.php";
}



function changeCategory(url) {
  location.href = url;
}
 
