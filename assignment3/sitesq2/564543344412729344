<script>
require(["janrain", "janrain-logger"], function() {
    GLOBAL.JANRAIN.authState = GLOBAL.JANRAIN.USER.AUTH;
    GLOBAL.JANRAIN.flowState = GLOBAL.JANRAIN.FLOW.SOCIALCONNECT;
    GLOBAL.JANRAIN.subflowState = GLOBAL.JANRAIN.SUBFLOW.NOTAPPLICABLE;
    JANRAIN.janrainDefaultSettings(GLOBAL);
    janrain.settings.capture.registerFlow = 'socialRegistration';
    JANRAIN.janrainInitLoad();
    janrain.ready = true;
});
var missionsMomentsTemplate = {
  backgroundImageMobile: '',
  backgroundImageTablet: '',
  backgroundImageDesktop: '',
  configureImageVars: function() {
    missionsMomentsTemplate.backgroundImageMobile = missionsMomentsTemplate.backgroundImageMobile !== '' ? missionsMomentsTemplate.backgroundImageMobile : null;
    missionsMomentsTemplate.backgroundImageTablet = missionsMomentsTemplate.backgroundImageTablet !== '' ? missionsMomentsTemplate.backgroundImageTablet : null;
    missionsMomentsTemplate.backgroundImageDesktop = missionsMomentsTemplate.backgroundImageDesktop !== '' ? missionsMomentsTemplate.backgroundImageDesktop : null;
  },
  adjustBackgroundImage: function() {
    if (missionsMomentsTemplate.backgroundImageDesktop && componentGlobalVars.windowSize == 'desktop') {
      $('.missionMomentsTemplate_BackgroundWrapper').css('background-image', 'url(' + missionsMomentsTemplate.backgroundImageDesktop +')');
    } else if (missionsMomentsTemplate.backgroundImageTablet && componentGlobalVars.windowSize == 'tablet') {
      $('.missionMomentsTemplate_BackgroundWrapper').css('background-image', 'url(' + missionsMomentsTemplate.backgroundImageTablet +')');
    } else if (missionsMomentsTemplate.backgroundImageMobile && componentGlobalVars.windowSize == 'mobile') {
      $('.missionMomentsTemplate_BackgroundWrapper').css('background-image', 'url(' + missionsMomentsTemplate.backgroundImageMobile +')');
    } else {
      if (missionsMomentsTemplate.backgroundImageDesktop) {
        $('.missionMomentsTemplate_BackgroundWrapper').css('background-image', 'url(' + missionsMomentsTemplate.backgroundImageDesktop +')');
      } else if (missionsMomentsTemplate.backgroundImageTablet) {
        $('.missionMomentsTemplate_BackgroundWrapper').css('background-image', 'url(' + missionsMomentsTemplate.backgroundImageTablet +')');
      } else if (missionsMomentsTemplate.backgroundImageMobile) {
        $('.missionMomentsTemplate_BackgroundWrapper').css('background-image', 'url(' + missionsMomentsTemplate.backgroundImageMobile +')');
      }
    }
  }
}
$(document).ready(function() {
  missionsMomentsTemplate.configureImageVars();
  if(GLOBAL.pageWidth < 714) {
        templateVariables.windowSize = 'mobile';
        missionsMomentsTemplate.adjustBackgroundImage();
  }
  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
      templateVariables.windowSize = 'tablet';
      missionsMomentsTemplate.adjustBackgroundImage();
  }
  if(GLOBAL.pageWidth > 1065) {
      templateVariables.windowSize = 'desktop';      
      missionsMomentsTemplate.adjustBackgroundImage();
  }
});
$(window).resize(function() {
  if (GLOBAL.pageWidth < 714) {
    if (templateVariables.windowSize != 'mobile') {
        templateVariables.windowSize = 'mobile';
        missionsMomentsTemplate.adjustBackgroundImage();
    }
  }
  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
    if (templateVariables.windowSize != 'tablet') {
      templateVariables.windowSize = 'tablet';
      missionsMomentsTemplate.adjustBackgroundImage();
    }
  }
  if(GLOBAL.pageWidth > 1065) {
    if (templateVariables.windowSize != 'desktop') {
      templateVariables.windowSize = 'desktop';
      missionsMomentsTemplate.adjustBackgroundImage();
    }
  }
});
</script>








</body>
</html>