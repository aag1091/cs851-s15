  if ($.cookie('analyticsSessionId') !== undefined) {
    analyticsSessionId = $.cookie('analyticsSessionId');
    console.log('analyticsSessionId:- ' + analyticsSessionId + ',  TimeStamp:- ' + $.now());
  } else {
    analyticsSessionId = generateUUID();
