	<script>
	// EventHandler on click for remindMe button and for interacting with AJAX
	jQuery('#contact_submit').click(function(){ 
		
		var button = jQuery(this);
		button.disabled = true;
		setTimeout(function(){
			button.disabled = false;
		}, 3000);
		
		var form = button.parent().parent();
		
		var username = form.find('#contact_name').val();
		var userchoise = form.find('#email_sel').find('#contact_subject').attr('value');
		var usermail = form.find('#contact_email').val();
		var userphone = form.find('#contact_phone').val();
		var usermessage = form.find('#contact_message').val();
		var usersubscribtion = form.find('#contact_newsletters:checked').val();

		var re = new RegExp("[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?");
		if(username.length>=2 && usermessage.length>0)
		{
			if (usermail.match(re)) {
				jQuery.post(
						'http://www.carolines.com/wp-admin/admin-ajax.php',
						{  
							action: 'contact_us_ajax',  
							data: "username="+username+"&userchoise="+userchoise+"&usermail="+usermail+"&userphone="+userphone+"&usermessage="+usermessage+"&usersubscribtion="+usersubscribtion,    
						},
						function(html){  
								form.find('#contact_response').html(html);  
							});
				jQuery('#contact_submit').css('display','none');
			}
			else
			{
				form.find('#contact_response').html("Please check email address!"); 
			}
		}
		else
		{
			form.find('#contact_response').html("Please check your info!"); 
		}
	});
	</script>
	
	
	
	
