<script>
jQuery(document).ready(function () {
var displayRelated = false;
if(jQuery('#related-content .headline-list').text().trim() != "") {
displayRelated = true; }
if (jQuery('.story-mini-gallery').text().trim() != "") {
displayRelated = true;
}
// And now actually show the related content, if we should.
if (displayRelated) {
jQuery('#related-content.pull-left').css({'width':'55%'});
jQuery('#story-content .heading-inverted, .story-embed .heading-inverted').show();
jQuery('#related-content').show();
jQuery('#related-content-xs').show();
} else {
jQuery('#related-content').attr('style','display:none!important;');
}
});
</script>
