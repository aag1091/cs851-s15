<script type="text/javascript" charset="utf-8">
  $(document).ready(function(){
    $("a[rel^='prettyPhoto']").prettyPhoto({
	horizontal_padding: 20, 
	social_tools: ''	
	});
	
	/* code added by kkk */
	
	if($("#btn_bottom_chunk").length>0){
		$("#btn_bottom_chunk").click(function(){
			if($("form#user_registration_form").length>0){
				$("form#user_registration_form").submit();
			}
		});
	}
	
	if($("#btn_upper_chunk").length>0){
		$("#btn_upper_chunk").click(function(){
			if($("div#upper_chunk").length>0){
				$("div#upper_chunk").addClass("hidden_cont");				
			}
			if($("div#bottom_chunk").length>0){
				$("div#bottom_chunk").removeClass("hidden_cont");
			}
		});
	}
	
	if($("#back_btn").length>0){
		$("#back_btn").click(function(){
			if($("div#upper_chunk").length>0){
				$("div#upper_chunk").removeClass("hidden_cont");				
			}
			if($("div#bottom_chunk").length>0){
				$("div#bottom_chunk").addClass("hidden_cont");
			}
		});
	}
	
	
  });
</script>
