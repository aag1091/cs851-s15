<script type="text/javascript" src="/cms/includes/jquery.pager.js"></script>
<script type="text/javascript">
	/* <![CDATA[ */
	var totalPages = 1;
	var currentCategory = 0;
	var siteId = 0;
    var count = 0;

function initializePager(pg) {
	$('#videoPager').pager({ pagenumber: pg, pagecount: totalPages, buttonClickCallback: VideoPageClick });
}

function getVideos(pg,searchTerms){

	if (!searchTerms && currentCategory == 0 && count == 0){
     searchTerms = getQuerystring('s');
     count = 1;}
     else{
     if (!searchTerms) searchTerms = "";}

	$.ajax({
		type: "POST",
		url: "/ajax/videos/ajax.aspx?mode=VideoPage",
		data: "CategoryId=" + currentCategory + "&pg=" + pg + "&searchTerms=" + searchTerms + "&SiteId=1" ,
		dataType: "json",
		success: videoPageResponse
	});

}

function videoTracking(videoId) {
	if (!videoId) return;

	//Now playing overlay
	mMnowPlaying('v' + videoId);

	$.ajax({
		type: "POST",
		url: "/ajax/videos/ajax.aspx?mode=ViewCount",
		data: "VideoId=" + videoId
	});
}

function VideoPageClick(pg) {
	initializePager(pg);

	var searchTerms = $("#mMSearchText").val();
	getVideos(pg,searchTerms);
}

function getQuerystring(key, default_)
{
  if (default_==null) default_="";
  key = key.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
  var regex = new RegExp("[\\?&]"+key+"=([^&#]*)");
  var qs = regex.exec(window.location.href);
  if(qs == null)
    return default_;
  else
    return qs[1];
}

function selectVideoCategory(id) {
	$("#mMSearchText").val("");
	currentCategory = id;
	getVideos(1);
}

selectVideoCategory(getQuerystring('VideoCategoryId'));

function doVideoSearch() {

	var searchTerms = $("#mMSearchText").val();
	getVideos(1, searchTerms);
}

function videoPageResponse(resp) {
	$("#mMAjaxContent").html(resp.html);
	$("#mMRecordCount").html(resp.recordCount);
	$("#mMStartingRow").html(resp.startingRow);
	$("#mMEndingRow").html(resp.endingRow);

	totalPages = resp.totalPages;
	initializePager(resp.currentPage);
}

function mMnowPlaying(vid) {
	$(".mMIndex li .mMThumbOverlay").hide("fast");
	$("#" + vid).find(".mMThumbOverlay").show("fast");
	return false;
};

function mMdeselectAll() {
	$(".mMIndex li .mMThumbOverlay").hide("fast");
	return false;
};
/* ]]> */

</script>


<script type="text/javascript">
	/* <![CDATA[ */
	$(document).ready(function(){        

		$(".mMCats ul.sf-menu").superfish({
			animation: { height: 'show' },
			speed: 200,
			dropShadows: false
		}).find("ul").bgIframe();

		$(".mMCats ul.sf-menu > li > a span").remove();

		$(".mMCats ul.sf-menu a").click(function(event) {
			event.preventDefault();
		});

		$(".mMCats ul.sf-menu ul a").click(function() {
			$(".mMCats ul.sf-menu").superfish();
			$(".mMCats ul.sf-menu > li > a span").remove();
		});

	});
	/* ]]> */
</script>

