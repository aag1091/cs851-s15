<script type="text/javascript">
function setNumMensajescomentarios(num) {    
  document.getElementById("num_comentarios_noticia_1423492837-6861ddeebd478473136f8e9e8969f92a").innerHTML = "<a href='#EnlaceComentarios'>"+num+"</a>";
  if ( num > 0){
    document.getElementById("num_cabecera_comentarios_noticia_1423492837-6861ddeebd478473136f8e9e8969f92a").innerHTML = '<span class="numero">'+num+'</span> comentario';
    if (num > 1) {
        document.getElementById("num_cabecera_comentarios_noticia_1423492837-6861ddeebd478473136f8e9e8969f92a").innerHTML += 's';
    }
  }
} 
ESKUP.setOnReceiveNumMensajes("comentarios_noticia_1423492837-6861ddeebd478473136f8e9e8969f92a", setNumMensajescomentarios); 
</script> 
