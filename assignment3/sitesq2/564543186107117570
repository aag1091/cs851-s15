};
var checker = 0;
function jqueryLoaded() {
clearInterval(checker);
window.CEM();
}
function checkJquery() {
if (window.jQuery) {
jqueryLoaded();
}
if(checker == 0) {
checker = window.setInterval(checkJquery, 100);
}
}	
