<script>
    $('.donateButton').click(function () {
                var url = "https://www.gofundme.com/lj5no4/donate";
        if ($(this).data("wl") !== undefined) {
            url += "&wl=" + $(this).data("wl");
        } else if ($(this).data("amt") !== undefined) {
            url += "&amt=" + $(this).data("amt");
        }
        try {
            window.top.location.href = url;
        } catch (e) {
            location.href = url;
        }
                return false;
    });
    $('.share-fb').click(function () {
        $(".followContinue").show();
        $('.followLoading').hide();
        try {
            GFM.Follower.post2feedAB.apply($(this));
        }
        catch (notready) {
            setTimeout(function () {
                GFM.Follower.post2feedAB.apply($(this))
            }, 1000);
        }
        return false;
    });
