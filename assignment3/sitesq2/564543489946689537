        $(function(){
                $(".openContactLb").click(function(){
                        $("#landingContact").show();
                        $("#contactLbForm, #contactThankYou, .gfm_lb").hide();
                        $("#contactLb").showAsLightbox(true);
                        return false;
                });

                $(".closeContact").click(function(){
                        $("#contactLb").closeLightbox();
                        return false;
                });

                $(".contactUser").click(function(){
                                                var captcha_image = '//www.gofundme.com/mvc.php?route=controls/securimageshow&sid=' + Math.random();
                        $('.contact_captcha').attr('src', captcha_image);
                        
                        $("#landingContact").slideUp();
                        $("#contactLbForm").slideDown();
                        return false;
                });

                $(".backToLanding").click(function(){
                        $("#landingContact").slideDown();
                        $("#contactLbForm").slideUp();
                        return false;
                });

                $("#contactLbSend").click(function(){
                        fns.handlers.submit($("#contactForm"));
                        return false;
                });
        });

        function contactsent(){
                $("#contactLb").scrollTo();
                $("#contactThankYou").slideDown();
                $("#contactLbForm").slideUp();
                $(".error").removeClass("error");
                $("input, textarea").val("");
        }

        fns.errors.signupsubmiterror = function(error,form){
                werror=error;
                wform = form;

                $(".error").removeClass("error");
                $('#input_'+error.field).addClass("error");
                $('.label_'+error.field).addClass("error").scrollTo();

                if (error.field == "captcha"){
                        $('.contact_captcha').attr('src','/mvc.php?route=controls/securimageshow&sid=' + Math.random());
                }
        }
