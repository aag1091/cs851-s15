                                            function monster_apply_callback(data){
                                                console.log(data);
                                                if (data.event == "success") {
                                                    console.log("success!");
                                                    var monster_api_key_field = document.getElementById('id_monster_api_key'); 
                                                    monster_api_key_field.value = monster_api_key_field.value.split('').reverse().join('');

                                                    var monster_request_url_field = document.getElementById('id_request_url'); 
                                                    console.log(monster_request_url_field);
                                                    console.log(data.req.requestUrl);
                                                    monster_request_url_field.value = data.req.requestUrl;
                                                    document.getElementById("monster_form").submit();

