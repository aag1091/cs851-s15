    (function (d,s,id) {
        var t, js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js=d.createElement(s); js.id=id;
        js.src="//player.ooyala.com/v3/MDYzZmYzZjIwNTA0YjI4Y2YyM2JmNTgw?platform=html5-fallback";
        js.onload = js.onreadystatechange = function(){
            var ev;

            js.onreadystatechange = js.onload = null;

            try {
                ev = new Event('ooyalaLoaded');
            }
            catch(e) {
                ev = document.createEvent('Event');
                ev.initEvent('ooyalaLoaded', true, true);
            }

            document.dispatchEvent(ev);
        };
        fjs.parentNode.insertBefore(js, fjs);
    }(document, "script", "ooyala-pscript"));
</script>


