<script type="text/javascript">

	$(document).ready(function(){

							   

		// function for mousehover and mouseout dropdown menu and click even for mobile versions

		t_mnu = null;

		$('.navbar-collapse ul li').mouseenter(function(){

			myel = $(this);

			myel.siblings().find('.submenu-dropdown').hide();

			myel.parent().find('li').removeClass('active');

			clearTimeout(t_mnu);

			t_mnu = setTimeout(function(){

				myel.addClass('active');

				myel.find('.submenu-dropdown').show();						

			},250);

			

		}).mouseleave(function(){

			clearTimeout(t_mnu);

			myel = $(this);

			t_mnu = setTimeout(function(){

				myel.parent().find('li').removeClass('active');

				myel.find('.submenu-dropdown').hide();						

			},250);

		});

		if($('body').innerWidth() < 768){

			$('.navbar-collapse ul li').unbind('mouseenter mouseleave');

			$('.navbar-collapse ul li').each(function(){

				if($(this).find('ul.submenu-dropdown').length > 0 ){

					$(this).find('a:first').attr('href','javascript:void(0)');

				}

			});

			trigclick();

		}

	

		function trigclick(){

			$('.navbar-collapse ul li').click(function(){

				myel = $(this);

				myel.toggleClass('active').find('.submenu-dropdown').toggle();

			});   

		}

		// menu function ends here

		

	});



</script>

<script>
  if(window.location.pathname.indexOf('report') >= 0){var reqvalid = true;}else{var reqvalid = false;}
  $(document).ready(function() {
    if(true && reqvalid) {
      _gaq.push(['_trackEvent', 'article', 'viewed', 'article-pageviews']);
      $('a').mousedown(function (element) {
        var thisclass = $(this).attr('class');
        var cltrclass = thisclass.indexOf('cltr') >= 0;
        var linkid = $(this).attr('id');
        var linkclasses = $(this).attr('class');
        var res = null;
        var linkclass = null;
        var res = linkclasses.split(" ");
        for(i=0;i<res.length;i++) {
          if(res[i].indexOf('cltr_') >= 0) {
            linkclass = res[i];
          }
        }
        if(cltrclass && linkclass) {
          var ga_arr = linkclass.split("_");
          ga_cat = ga_arr[1];
          ga_lbl = ga_arr[2];
          _gaq.push(['_trackEvent', ga_cat, 'clicked', ga_lbl,, 'true']);
        }
      });
    }
  });
</script>




