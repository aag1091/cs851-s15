				<script type="text/javascript">
					function tvpub(){
						var tv = "4397";
						var div_tv = jQuery('#left');
						jQuery.ajax({
							type:"POST",
							url:gameslivetvref,
							data:'tv='+tv,
							cache: false,
							success:function (msg){
								if (msg=='refresh') {
									div_tv.html(div_tv.html());
								} 
							}
						});
						setTimeout(tvpub, 29600);
						return false;
					}
		
					setTimeout(tvpub, 5000);
					setTimeout(refpub, 1000);
					
					function refpub(){
						var tv = "4397";
						jQuery.ajax({
							type:"POST",
							url:gameslivetvbtn,
							data:'tv='+tv,
							cache: false,
							success:function (msg){
								if (msg=='ok') {
									//afficher
									$('.ajoutpub').show();
									$('.ajoutpub').attr("onClick","ajoutpub();");
									$('.ajoutpub').removeAttr("disabled");
									$('.ajoutpub').text("Lancer une publicité");
								} else {
									$('.ajoutpub').removeAttr("onClick");
									$('.ajoutpub').text("Vous pouvez lancer une publicité toutes les 10 minutes");
									$('.ajoutpub').attr('disabled', 'disabled');
								}
							}
						});
						setTimeout(refpub, 300000);
						return false;
					}
					
					function hidebtn(){
						$('.ajoutpub').removeAttr("onClick");
						$('.ajoutpub').text("Vous pouvez lancer une publicité toutes les 10 minutes");
						$('.ajoutpub').attr('disabled', 'disabled');
					}
					
					function ajoutpub(){
						$.ajax({
							type:"POST",
							url:gameslivetvadd,
							data:'user=4397',
							success:function (msg) {
								console.log(msg);
							}
						});
						hidebtn();}
				</script>

