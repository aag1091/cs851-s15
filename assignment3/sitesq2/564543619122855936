<script type="text/javascript">

//get distance scrolled (scrOfY)
document.onscroll = getScrollY;
function getScrollY() {
var scrOfY = 0;
if(typeof(window.pageYOffset)=='number') {scrOfY = window.pageYOffset;} 
else if(document.body && (document.body.scrollLeft || document.body.scrollTop)) {scrOfY=document.body.scrollTop;}
else if(document.documentElement && (document.documentElement.scrollTop)) {scrOfY = document.documentElement.scrollTop;}

//get window height (WinY)
//modified from Andy Langton's script: http://bit.ly/uhVzr5
var WinY;
if (typeof window.innerWidth != 'undefined') { WinY = window.innerHeight }
else if (typeof document.documentElement != 'undefined' && typeof document.documentElement.clientHeight != 'undefined' && document.documentElement.clientHeight != 0) { WinY = document.documentElement.clientHeight }
else { WinY = document.getElementsByTagName('body')[0].clientHeight}

//get page height (DocY)
//modified from James Padolsey's script: http://bit.ly/XxWM41
var DocY = Math.max(document.body.scrollHeight, document.documentElement.scrollHeight, document.body.offsetHeight, document.documentElement.offsetHeight, document.body.clientHeight, document.documentElement.clientHeight);

//convert distance as % of page height to distance as % of window height
var pageY = (scrOfY/DocY)*(WinY-32); //value in px

//apply vertical position to scrolling hook
document.getElementById("hook").style.marginTop = pageY+"px";

//apply vertical position to weight
document.getElementById("weight").style.marginBottom = pageY+"px";
document.getElementById("weightshadow").style.marginBottom = pageY+"px";

//apply chain movement
document.getElementById("mfchainright").style.backgroundPosition = "center "+(pageY*0.41655)+"px";
document.getElementById("mfchainleft").style.backgroundPosition = "center "+(-pageY*0.41655)+"px";

//calculate rotation value for cog1
var r1 = scrOfY*1.6*0.4686 + 15;

//apply chain spin
document.getElementById("mfmiddle").style.mozTransform = "rotate("+r1+"deg)";
document.getElementById("mfmiddle").style.webkitTransform = "rotate("+r1+"deg)";
document.getElementById("mfmiddle").style.oTransform = "rotate("+r1+"deg)";
document.getElementById("mfmiddle").style.msTransform = "rotate("+r1+"deg)";
document.getElementById("mfmiddle").style.transform = "rotate("+r1+"deg)";

//apply cog1 spin
document.getElementById("cog1a").style.mozTransform = "rotate("+r1+"deg)";
document.getElementById("cog1a").style.webkitTransform = "rotate("+r1+"deg)";
document.getElementById("cog1a").style.oTransform = "rotate("+r1+"deg)";
document.getElementById("cog1a").style.msTransform = "rotate("+r1+"deg)";
document.getElementById("cog1a").style.transform = "rotate("+r1+"deg)";

//calculate rotation value for cog2
var r2 = -(scrOfY*1.6*0.4686) + 6;

//apply cog2 spin
document.getElementById("cog2a").style.mozTransform = "rotate("+r2+"deg)";
document.getElementById("cog2a").style.webkitTransform = "rotate("+r2+"deg)";
document.getElementById("cog2a").style.oTransform = "rotate("+r2+"deg)";
document.getElementById("cog2a").style.msTransform = "rotate("+r2+"deg)";
document.getElementById("cog2a").style.transform = "rotate("+r2+"deg)";

document.getElementById("cog2c").style.mozTransform = "rotate("+r2+"deg)";
document.getElementById("cog2c").style.webkitTransform = "rotate("+r2+"deg)";
document.getElementById("cog2c").style.oTransform = "rotate("+r2+"deg)";
document.getElementById("cog2c").style.msTransform = "rotate("+r2+"deg)";
document.getElementById("cog2c").style.transform = "rotate("+r2+"deg)";

//calculate rotation value for cog3
var r3 = scrOfY*0.8*0.4686 + 10;

//apply cog3 spin
document.getElementById("cog3a").style.mozTransform = "rotate("+r3+"deg)";
document.getElementById("cog3a").style.webkitTransform = "rotate("+r3+"deg)";
document.getElementById("cog3a").style.oTransform = "rotate("+r3+"deg)";
document.getElementById("cog3a").style.msTransform = "rotate("+r3+"deg)";
document.getElementById("cog3a").style.transform = "rotate("+r3+"deg)";

//calculate rotation value for cog4
var r4 = scrOfY*0.8*0.4686 + 8;

//apply cog4 spin
document.getElementById("cog4").style.mozTransform = "rotate("+r4+"deg)";
document.getElementById("cog4").style.webkitTransform = "rotate("+r4+"deg)";
document.getElementById("cog4").style.oTransform = "rotate("+r4+"deg)";
document.getElementById("cog4").style.msTransform = "rotate("+r4+"deg)";
document.getElementById("cog4").style.transform = "rotate("+r4+"deg)";

document.getElementById("cog4shadow").style.mozTransform = "rotate("+r4+"deg)";
document.getElementById("cog4shadow").style.webkitTransform = "rotate("+r4+"deg)";
document.getElementById("cog4shadow").style.oTransform = "rotate("+r4+"deg)";
document.getElementById("cog4shadow").style.msTransform = "rotate("+r4+"deg)";
document.getElementById("cog4shadow").style.transform = "rotate("+r4+"deg)";

return true;
}

</script>



