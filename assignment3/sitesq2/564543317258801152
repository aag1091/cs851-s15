require(['loaders/kaltura-kwidget'], function(loader) {
  var partnerId = '1656501';
  var uiconfId = '24997472';

  loader.load(partnerId, uiconfId).then(function(kWidget) {
    kWidget.embed({
      'targetId': 'kaltura_player_0_1hnu27gk',
      'wid': '_' + partnerId,
      'uiconf_id': uiconfId,
      'cache_st': '1415138000',
      'entry_id': '0_1hnu27gk',
      'flashvars': {
        
          'adzone': 'tpxnoads',
        
        'externalInterfaceDisabled': false,
        'titleLabel': {
          'plugin': false
        }
      },
      'params': {'wmode': 'transparent'}
    });
  });
});
</script>

