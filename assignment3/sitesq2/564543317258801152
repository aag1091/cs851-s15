<script>
// todo: review, copy to clipboard is buggy
require(['jquery'], function($) {
    var clip = $('#zeroclipboard-button');
    clip.zclip({
        path: 'http://cdn-assets.ellentube.com/ellentube.com/main/default/cachejs/zclip/1.1.2/ZeroClipboard.v2014_11_17_160658.swf',
        copy: $('#zeroclipboard-field').val(),
        afterCopy: function() {
            clip
                .attr('data-original-title', 'Copied!')
                .tooltip('fixTitle')
                .tooltip('show');
        }
    });

    clip
        .attr('data-placement', 'top left')
        .attr('title', 'Copy to clipboard')
        .tooltip()
        .on('mouseover', function() {
            $(this)
                .attr('data-original-title', 'Copy to clipboard')
                .tooltip('fixTitle');
        })
        .on('mouseout', function() {
            $(this)
                .attr('data-original-title', 'Copy to clipboard')
                .tooltip('fixTitle');
        });

    $('.st_embed_custom').on('click', function() {
        $('#video-embed-code').slideToggle('fast');
        if (!$('#video-embed-code').hasClass('open')) {
            $('#video-embed-code').addClass('open');
            $(this).find('.glyphicon').addClass('glyphicon-minus').removeClass('glyphicon-plus');
        } else {
            $('#video-embed-code').removeClass('open');
            $(this).find('.glyphicon').addClass('glyphicon-plus').removeClass('glyphicon-minus');
        }
    });

    $(document).on('focusin', '#zeroclipboard-field', function() {
        var t;
        return t=this, setTimeout(function() {
            return $(t).select();
        }, 0);
    });

    $('.st_share_custom').on('click', function() {
        $('#share-embed-code').slideToggle('fast');
        if (!$('#share-embed-code').hasClass('open')) {
            $('#share-embed-code').addClass('open');
        } else {
            $('#share-embed-code').removeClass('open');
        }
    });
});
</script>
                        
