        <script type="text/javascript">

            function fcGoToPage(page, petitionId) {
                var spinner = new fcAjaxSpinner('#fc-signatures-' + petitionId);
                jQuery.post('http://forcechange.com/wp-admin/admin-ajax.php',
                    {
                        page: page,
                        petition_id: petitionId,
                        action: "fc_go_to_page"
                    },
                    function(response){
                        if(response) {
                            jQuery('#fc-signatures-' + petitionId).html(response);
                            spinner.destroy();
                        }
                    }
                );
            }

            function fcAjaxSpinner(elem) {

                this.elem = elem;
                this.image = '<img src="http://forcechangecom.c.presscdn.com/wp-content/plugins/forcechange/images/ajax-loader.gif" />';

                this.init = function() {
                    this.spinner = jQuery(this.image);
                    jQuery(this.elem).after(this.spinner);
                    return this;
                }

                this.destroy = function() {
                    jQuery(this.spinner).remove();
                }

                return this.init();
            }

        </script>

    		
		
