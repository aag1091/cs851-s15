 <!-- story table -->
 
 <h4>Wednesday, October 2, 2002</h4>
 <h2><MCC HEAD>Fashion show to benefit NNMC Fisher Houses</MCC HEAD></h2>
  
 <!-- story info -->
 <div class="story-info">
 	<div class="links">		
							 		<span class="links-border"><a href="http://www.morriscomm.com/cgi-bin/sendit.pl"><img src="http://www.gazette.net/images/icons/email2.gif" alt="" width="25" height="23" border="0"> E-Mail This Article</a></span>
 		<a href="javascript:window.print();"><img src="http://www.gazette.net/images/icons/printer2.gif" style="margin:7px 0 0 2px;" alt="" border="0" height="23" width="25"> Print This Story</a>
 	</div>
 	<div class="byline">
 		<div class="byline1"><MCC BYLINE1><B>Deanna Wharwood</I></MCC BYLINE1></div>
 	</div>
 </div>
<MCC PHOTOTABLE>
 	<table cellpadding="0" cellspacing="0" border="0" width="198" class="float-right photo">
 		<tr>
 			<td>
