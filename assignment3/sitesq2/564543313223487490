            (function($, undefined) {
                $(document).onClientContextReadyFast(function() {
                    var store = this;

                    var name = store.getProperty('firstName');
                    if (name == 'anonymous' || name == '') {
                        $('.logged-out').removeClass('hide');
                    } else {
                        $('.logged-in').removeClass('hide');
                        $('#nickname').html(name);
                    }
                });
