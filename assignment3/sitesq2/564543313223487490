	this.frameworkMappings.push({scVar:"prop3",cqVar:"pagesvisited.previous",resourceType:"chelsea/components/content/personalization/contextstores/pagesvisited"});
	this.frameworkMappings.push({scVar:"event36",cqVar:"eventdata.events.gigyaRewardsInstances",resourceType:"chelsea/components/content/social/gigya"});
