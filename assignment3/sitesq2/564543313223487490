            $(function () {
                $('.nav-el.search .nav-a').click(function () {
                    $('input#q')[0].focus();
                });

                $.ajax('/content/cfc/en/shared-parts/header/megamenu/news/jcr:content.popular.json', {
                    dataType: 'json',
                    success: function (popularTerms) {
                        if (popularTerms.length > 0) {
                            var $popularSearch = $('.popular-search');
                            $popularSearch.append('<div class="h2-container">')
                                          .append('<h2>Popular searches</h2>');
    
                            var $popularSearchTermsList = $('<ul class="popular-searches" id="popularSearchesList"></ul>');
    
                            $.each(popularTerms, function (index, term) {
                                $popularSearchTermsList.append("<li><a href=\"/content/cfc/en/homepage/search.html?query=" + term + "\"><span class=\"magglass\">search for </span>" + term + "</a></li>");
                            });
    
                            $popularSearch.append($popularSearchTermsList );
                        }
                    }
                });
//                $('input#q').typeahead({
