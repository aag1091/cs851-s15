                showCounts: 'right',
                containerID: 'likeReaction',
                reactions: myReactions,
                userAction: act,
                showSuccessMessage: true,
                noButtonBorders: true,
                promptShare: false,
                cancelReactions: false
            }
        }
    </script>
    <div id="likeReaction"></div>
    <script type="text/javascript">
        if ( CFC.Settings.gamificationOn ) {
            gigya.socialize.showReactionsBarUI(showReactionsBarUI_params);
            $(function() {
                var getPoints = function() {
                        gigya.gm.getChallengeStatus({
                            includeChallenges: "_default"
                            , details: "full"
                            , callback: function( response ) {
                                var pointsCurrent = 0;
                                if ( response.errorCode === 0 ) {
                                    if ( null !== response.achievements[0] && response.achievements.length ) {
                                        pointsCurrent = response.achievements[0].pointsCurrent;
                                        $( '.UserPoints' ).html( pointsCurrent );
                                    }
                                    // todo muz sitecat action: _reaction
                                } else {
                                    console.log( 'Gigya error:', response );
                                }
                            }
                        });
                    }
                    $('td.gig-button-td').on('click', function() {
                        setTimeout( function() { 
                            getPoints()
                        }, 1000);
                    });
            });
        }                             
    </script>
</div>
</div>

                     </div>

                </article>
            </div>
