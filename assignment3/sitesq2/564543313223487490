<script>
    $(document).ready(function() {
        gigya.gm.notifyAction({
            action: "viewPageArticle",
            callback: function( response ) {
                if ( response.errorCode === 0 ) {
                    console.log( 'Article viewed', response );
                    if ( CFC.Settings.gamificationOn ) {
                        gigya.services.gm.getChallengeStatus({
                            includeChallenges: '_default'
                            , details: "full"
                            , callback: function( response ) {
                                var pointsCurrent = 0;
                                if ( response.errorCode === 0 ) {
                                    if ( null !== response.achievements && response.achievements.length > 0 ) {
                                        for( var i = 0; i < response.achievements.length; i++ ){
                                            pointsCurrent = response.achievements[i].pointsCurrent;
                                            GigyaCummulativePoints = response.achievements[i].pointsTotal;
                                        }
                                    }
                                    // todo muz sitecat
                                } else {
                                    console.log( 'Gigya error:', response );
                                }
                                $( '.UserPoints' ).html( pointsCurrent );
                            }
                        });
                    }
                } else {
                    console.log( 'Gigya error:', response );
               }
            }
        });
    });
</script>

            

