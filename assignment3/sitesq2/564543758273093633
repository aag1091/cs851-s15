    CN.stats.omniture.setMobileView( ( CN.isMobile ) ? 'Mobile' : 'Non-Mobile' );

    if ( CN.cookie.get('amg_user_partner') != '' ) {
        CN.stats.omniture.setCustomProp( "52", CN.cookie.get('amg_user_partner') );
    }
    // keep
    CN.stats.omniture.setContentType( "homepage" );

    if ( CN.stats.omniture.setReferrer !== undefined && document.referrer.match( CN.site.tld ) ) {
        var referrer = document.referrer;
        referrer=referrer.split("?")[0];
        CN.stats.omniture.setReferrer( referrer );
    }

    if ( CN.stats.omniture.setKeywords !== undefined ) {
        CN.stats.omniture.setKeywords( "staging" );
    }
    */
    var repo = {"zone1":"trends;","zone2":"fashion","keywords":["trends","fashion","2015","street-style-rihanna"],"type":"blogs","meta":{"title":"Rihanna - Street Style, Grammy Awards","description":"Wouldn’t we all love to spend a day living like Rihanna?","keywords":"fashion trends, style trends"},"json":"{\"zone1\":\"trends;\",\"zone2\":\"fashion\",\"keywords\":[\"trends\",\"fashion\",\"2015\",\"street-style-rihanna\"],\"type\":\"sub-index\",\"meta\":{\"title\":\"Fashion Trends - Style.com\",\"description\":\"The latest fashion and runway trends on Style.com\",\"keywords\":\"fashion trends, style trends\"}}","template":"article","pubDate":1423398600000}, canonicalURL = window.location.pathname.split('/');
    CN.stats.omniture.setContentType(repo.type);

    var previousUrl = document.referrer;
    if( previousUrl != "" ) {
        CN.stats.omniture.setCustomProp(68, previousUrl);
    } else {
        CN.stats.omniture.setCustomProp(68, null);
    }

    if( repo.zone1 === 'fashion-shows;' ) {
        CN.stats.omniture.setCustomProp('20', repo.zone2);
        if( repo.template != 'seasons-landing' ) {
            CN.stats.omniture.setCustomProp('21', repo.brand);
        } else {
            CN.stats.omniture.setCustomProp('21', null);
        };
    }

    if( repo.template === 'fashionshows-slideshows' ) {
        var url,
            host = window.location.origin,
            keywords = [],
            lastKeyword = repo.keywords.length - 1,
            slide = parseInt(canonicalURL[canonicalURL.length - 1]);

         if( isNaN(slide) === false ) {
            for( var i = 0; i < lastKeyword; i++ ) {
                keywords[i] = repo.keywords[i];
            };
            CN.stats.omniture.setCustomProp("32", "slide" + canonicalURL[canonicalURL.length - 1]);
            if( slide === repo.slideshowLength ) {
                CN.stats.omniture.setCustomProp("32", "slide_end");
            }
        } else {
            keywords = repo.keywords;
            CN.stats.omniture.setCustomProp("32", "slide1");
        };
        keywords = keywords.join('/');
        url = host + '/' + keywords;

        CN.stats.omniture.setPageName(url);
        CN.stats.omniture.setCustomProp('6', "fashion-shows");
        CN.stats.omniture.setCustomProp('7', "fashion-shows/" + repo.zone2);
        CN.stats.omniture.setCustomProp('60', repo.slideshowLength);
        CN.stats.omniture.setCustomProp('20', repo.zone2);
        CN.stats.omniture.setCustomProp('21', repo.brand);
    }

    if( repo.template === 'slideshows' ) {
        var url,
            host = window.location.origin,
            keywords = [],
            lastKeyword = repo.keywords.length - 1,
            slide = parseInt(canonicalURL[canonicalURL.length - 1]);

        if( isNaN(slide) === false ) {
            CN.stats.omniture.setCustomProp("32", "slide" + canonicalURL[canonicalURL.length - 1]);
            if( slide === repo.slideshowLength ) {
                CN.stats.omniture.setCustomProp("32", "slide_end");
            }
        } else {
            CN.stats.omniture.setCustomProp("32", "slide1");
        };
        for( var i = 0; i < lastKeyword; i++ ) {
            keywords[i] = repo.keywords[i];
        };
        keywords = keywords.join('/');
        url = host + '/' + keywords;

        CN.stats.omniture.setPageName(url);
        CN.stats.omniture.setCustomProp('6', repo.zone1);
        CN.stats.omniture.setCustomProp('7', repo.zone1 + '/' + repo.zone2);
        CN.stats.omniture.setCustomProp('60', repo.slideshowLength);
    }

    if( repo.template === 'article' ) {
        var today = new Date(),
            diff = today.valueOf() - repo.pubDate;
            diff = Math.floor(diff / 1000);
        var days = Math.floor((diff % 31536000) / 86400),
            hours = Math.floor(((diff % 31536000) % 86400) / 3600),
            pubDate = hours + 'h|' + days + 'd';
        CN.stats.omniture.setCustomProp('61', pubDate);
    }

    if( repo.template != 'slideshows' && repo.template != 'fashionshows-slideshows' ) {
        CN.stats.omniture.setCustomProp('60', null);
        CN.stats.omniture.setCustomProp("32", null);
    }

    if( repo.zone1 != 'fashion-shows;' ) {
        CN.stats.omniture.setCustomProp('20', null);
        CN.stats.omniture.setCustomProp('21', null);
    };

    if( repo.template != 'article' ) {
        CN.stats.omniture.setCustomProp('61', null);
    }




    
    // Run page tracking (also calls .init() again )
    CN.stats.omniture.doPageTracking();
</script>
