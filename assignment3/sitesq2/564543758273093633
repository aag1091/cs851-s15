    <script>
        try {
            //due to chrome unexpected behaviour with back button after pushState
            var forceReload = function (href) {
                document.location.href = href;
            };
            var getHash = function() {
                var match = window.location.href.match(/#!\/(.*)$/);
                return match ? match[1] : '';
            };
            var popstateHandler = function (ev) {
                var href = document.location.href, pathname = document.location.pathname;
                if (pathname.length > 1) {
                    forceReload(document.location.href);
                }
            };
            window.onpopstate = popstateHandler;
            var hash = getHash();
            if (hash && hash.length > 0) {
                forceReload("/"+hash);
            }
        } catch (error) {}
    </script>

