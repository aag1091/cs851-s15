<script type="text/javascript">

    function alertboxShowMsg(title, message, autoHideDelay) {

        if (!autoHideDelay || autoHideDelay == null) { autoHideDelay = 300; }

        var opts = {};

        opts.classes = ["simple"];
        opts.autoHide = true;
        opts.prepend = true;
        opts.keepOnHover = true;
        opts.autoHideDelay = autoHideDelay;
        opts.hideStyle = {
            opacity: 0,
            
        };
        opts.showStyle = {
            opacity: 1,
            left: 0
        };

        $("#alertbox").freeow(title, message, opts);

    }

  
</script>

