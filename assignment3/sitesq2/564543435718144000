	<script type="text/javascript">

	    $(window).load(function () {

	        // Not mobile
	        if ($("div.navigation").width() != 280) {

	            $('body').click(function (event) {
	                if (!$(event.target).closest('#popup_login').length && !$(event.target).closest('#lnkShowPopupLogin').length && $('#popup_login').is(':visible')) {
	                    $('#popup_login').fadeOut(500);
	                };
	                if (!$(event.target).closest('#contenttypefilterpopup').length && !$(event.target).closest('#changecontentfilter').length && $('#contenttypefilterpopup').is(':visible')) {
	                    changecontentfilter_popup_hide();
	                };

	            });

	            $('#lnkShowPopupLogin').click(function () {
	                $('#popup_login').fadeIn(500, function () {
	                    $('#txtPopupLoginUsername').focus();

	                })
	                return false;
	            });

	            $('#changecontentfilter').click(function () {
	                $('#contenttypefilterpopup').fadeToggle(500);
	            });

	            /*--------- Popup Change Content Filter START -------- */

	            /*--------- Popup Change Content Filter START -------- */
	            $('#changecontentfilter').click(function (event) {
	                if (!$('#contenttypefilterpopup').is(':visible')) {
	                    changecontentfilter_popup_show();
	                }
	            });

	            function changecontentfilter_popup_show() {
	                $('#contenttypefilterpopup').fadeIn(500);
	            };

	            function changecontentfilter_popup_hide() {
	                $('#contenttypefilterpopup').fadeOut(500);
	            };

	            /*--------- Popup Change Content Filter END -------- */


	            $('#txtSearch').watermark('Search for videos and galleries');

	            $(".ellipsis").dotdotdot({
	                watch: 'window'
	            });

	            function closeWindow() {
	                setTimeout(function () {
	                    window.open('', '_self', '');
	                    window.close();
	                }, 100);
	                window.location.href = "http://www.google.co.uk";
	            }



	            // Age verification
	            $('.popupEnter').click(function () {
	                var success = acceptage();
	                if (success = 1) {
	                    $('#pnlAgeCheck').hide();
	                }
	                else {
	                    alert('Redirect to google.com');
	                }
	            });

	            function acceptage() {
	                var success = 0;
	                $.ajax({
	                    type: 'POST',
	                    url: '/WebServices/WebServices.asmx/AcceptAge',
	                    contentType: 'application/json; charset=utf-8',
	                    dataType: 'json',
	                    success: function (result) {
	                        if (result.d) {
	                            var results = eval('(' + result.d + ')');
	                            success = results;
	                        }
	                    },
	                    error: function (err) {
	                        alert('error');
	                    }
	                });

	                return success;
	            }

	        }

	    });

	</script>


