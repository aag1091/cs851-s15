				$FR.register('paymentUI', FundRazr.module.PaymentUI, {
					campaign			: campaign,
					customerData		: '',
					paymentContainer	: '#paypal-payment-box',
					targetMode			: 'standalone'
				});
	    	}
	       	$FR.register('clipboard', FundRazr.module.Clipboard, {});
	    	$FR.register('widget'	, FundRazr.module.CampaignWidget, {
	    		isMobileClientIgnoreParam : false
	    	});
	    	$FR.register('ownerinfo', FundRazr.module.OwnerInfo);
	    	$FR.register('share'	, FundRazr.module.CampaignShare, {
  		    	campaign			: campaign,
  		    	recaptchaKey		: '6LekPuQSAAAAAEvETwoCUGinw0SmItKa7JdKeVaR'
	    	});
	    	$FR.register('highlightFeed', FundRazr.module.CampaignFeed, {
 		    	container			: "feed-item-box-1",
 		    	campaign			: campaign,
				loadOnInit			: true,
		    	activityType		: 'highlight',
		    	currentDonation		: currentDonation,
 		    	isEmbedded			: false,
		    	infiniteScroll		: infiniteScroll
		    });
	    	$FR.register('storyFeed', FundRazr.module.CampaignFeed, {
		    	container			: "feed-item-box-2",
		    	campaign			: campaign,
				loadOnInit			: true,
		    	activityType		: 'story',
		    	isEmbedded			: false,
		    	infiniteScroll		: infiniteScroll
		    });
	    	$FR.register('donationFeed', FundRazr.module.CampaignFeed, {
		    	container			: "feed-item-box-4",
		    	campaign			: campaign,
				loadOnInit			: true,
		    	activityType		: 'all',
		    	currentDonation		: currentDonation,
		    	isEmbedded			: false,
		    	infiniteScroll		: infiniteScroll
		    });
	    	if (spotlightFeed.activities) {
		    	$FR.register('spotlightFeed', FundRazr.module.CampaignFeed, {
			    	container			: "activity-spotlight-feed",
			    	campaign			: campaign,
			    	activities			: spotlightFeed.activities,
			    	activityType		: 'highlight',
			    	isEmbedded			: false
			    });
	    	}
	    	$FR.register('storyUpdateWebcam', FundRazr.module.Webcam, {
			    apiKey: '45096292',
			    sessionId: '',
			    token: '',
				previewContainer: '#record-video-preview',
				dimensions: {
					height: 236,
					width: 420
				}
			});
  		    $FR.register('commentUI', FundRazr.module.CommentUI, {
  		    	campaign			: campaign,
		    	storyUpdateWebcam   : 'storyUpdateWebcam'
		    });
	    	$FR.register('activity'	, FundRazr.module.Activity, {
		    	shareOnLoad			: shareOnLoad,
		    	currentDonation		: currentDonation,
		    	action				: action,
		    	isEmbedded			: false
		    });
	    	$FR.register('facebook'	, FundRazr.module.Facebook, {
		    	parentShareId		: parentShareId,
		    	campaignId			: 'c3f054c323794edba086b4a831b26fbb'
		    });
   	    	$FR.register('gplus'	, FundRazr.module.GooglePlus, {
		    });
			$FR.register('linkedin', FundRazr.module.LinkedIn);
	    	$FR.register('likes', FundRazr.module.Likes, { campaign: campaign });
		    $FR.register('oembed', FundRazr.module.OEmbed);
		    $FR.register('follow', FundRazr.module.Follow, {
		    	campaign: campaign,
		    	follow: (!!followProxy ? followProxy.follow : undefined)
		    });
	    	$FR.register('facebookRequest'	, FundRazr.module.FacebookRequest, {
		    	campaign: campaign
		    });
	    	if (campaign.isSupporterLeaderboardEnabled) {
		    	$FR.register('referralLeaderboard', FundRazr.module.ReferralLeaderboard, {
			    	campaign: campaign
			    });
	    	}
	    	$FR.register('referrals', FundRazr.module.Referrals, {
		    	campaign: campaign
		    });
	       	$FR.register('mediaGallery', FundRazr.module.Gallery, {
	    		campaign: campaign,
		    	isMobileClientIgnoreParam: false
		    });
			
			if (true) {
				$FR.register('inactivityPopup', FundRazr.module.InactivityPopup, { delay: 15000 });
				$FR.start('inactivityPopup');
			}
			
	    	$FR.start('auth');
	    	$FR.start('mediaGallery');
	    	$FR.start('gplus');
	    	$FR.start('linkedin');
	    	$FR.start('clipboard');
	    	if (!isMobileView) {
				$FR.start('paymentUI');	    		
	    	}
	    	$FR.start('widget');
	    	$FR.start('ownerinfo');
	    	$FR.start('share');
    		$FR.start('likes');
		    $FR.start('oembed');
		    $FR.start('commentUI');
	    	$FR.start('activity');
	    	$FR.start('facebook');
	    	$FR.start('follow');
	    	$FR.start('facebookRequest');
	    	$FR.start('referrals');
	    	
	    	if (true)
	    		$FR.start('highlightFeed');
	    	
	    	if (spotlightFeed.activities)
	    		$FR.start('spotlightFeed');
	    	
	    	if (false) {
				$FR.register('contactImporter', FundRazr.module.ContactImporter, {
	  		    	recaptchaKey: '6LekPuQSAAAAAEvETwoCUGinw0SmItKa7JdKeVaR'
		    	});
		    	$FR.start('contactImporter');
				
		    	$FR.register('wysiwyg', FundRazr.module.wysiwyg, {
		    		selector: '#wysiwyg-editor',
					placeholderSelector: '#story-placeholder',
		    		userId: '',
		    		cachebreaker: '32'
				});
	    	}
	    	
	    	// Init feed whenever the user accesses the tab for the first time
	    	$('#feed-nav').find('a[data-toggle="tab"]').on('shown', function() {
	    		var feedName = $(this).data('feed');
    			if (feedName && !$FR.isModuleRunning(feedName))
	    			$FR.start(feedName);
	    	})
	    	$('#feed-nav li.active').find('a[data-toggle="tab"]').trigger('shown');
	    });
	/*]]>*/
</script>
	
	<script src="//www.google.com/recaptcha/api/js/recaptcha_ajax.js"></script>







<script src="//cf4all.com/cf4all/plugins/crowdxlog.js#plat_id=30f6dac6-5e6c-11e3-8aae-00155d062a12"></script>
	
	
	<script>
		
		$(document).ready(function() {
			var triggerPoint = $("#feed-panel").position().top;
			$(window).scroll(_.debounce(function() {
				if ($(window).scrollTop() >= triggerPoint ) {
					$("#flydown-navbar").fadeIn(1000, 'easeOutExpo');
				}
				else {
					$("#flydown-navbar").fadeOut(1000, 'easeOutExpo');
				}
			}, 100));
		});
			
		$('.sponsor-banner .accordion-trigger').click(function() {
			$('.sponsor-banner .accordion-trigger.opened').toggle();
			$('.sponsor-banner .accordion-trigger.closed').toggle();
			$('.sponsor-banner').toggleClass('opened');
		});
		
	</script>
	
</body>
	
</html>