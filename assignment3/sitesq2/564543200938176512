	<form id="pageloader" action="#" method="POST">
	</form>
	<script type="text/javascript">
		/*<![CDATA[*/
		var loadPage = function(page, target, params, method) {
			if (page.indexOf('/') == 0) {
				var uriBase = $('meta[name="run-location"]').attr('content') === 'facebook' ? $FR.config.fbAppCanvasURL : $FR.config.applicationURL;
				// Prevent double slash in URI path
				if (uriBase.lastIndexOf('/') == uriBase.length - 1)
					uriBase = uriBase.slice(0, -1);
				page = uriBase + page;
			}
			
			var url		= (page.indexOf("http") === 0 || page.indexOf('//') === 0) ? page : ($.fixProtocol($FR.config.applicationURL) + page),
				target	= target || '_top',
				params	= params || { },
				method	= method || 'GET';
			
			if (method.toUpperCase() == 'GET') {
				// Merge URL parameters
				var urlParams = $.getQueryParams(url);
				$.extend(params, urlParams);
				// Remove query string
				var urlVals = url.split('?');
				if (urlVals.length > 1)
					url = urlVals[0];
			}
			
			var fields = [], field;
			for (var key in params) {
				if (params.hasOwnProperty(key)) {
					field = $('<input>')
						.attr({
							type: 'hidden',
							name: key
						})
						.val(params[key])
					fields.push(field);
				}
			}
			for (var i = 0; i < fields.length; i++)
				$('#pageloader').append(fields[i]);
			
			$('#pageloader')
				.attr('action', url)
				.attr('method', method)
				.attr('target', target)
				.submit();
		}

		var emptyPageloaderParams = function() {
			$('#pageloader').empty();
		}
  		/*]]>*/
	</script>
