<script>
$(window).scroll(function() {
    if($(this).scrollTop() > 300) {
        $('#kaitop').fadeIn();
    } else {
        $('#kaitop').fadeOut();
    }
});

$('#kaitop').hide().click(function() {
    $('html, body').animate({scrollTop:0}, 900);
    return false;
});
</script>
