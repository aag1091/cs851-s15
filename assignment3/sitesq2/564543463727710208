<script type="text/javascript">

var SERVER_NAME = "d3al.org";
var countryId = "101";
var docRoot = "/var/www/vhosts/d3al.org/httpdocs/";

var localDealsWasClicked = false;

function showDailyDealResult(merchant_id,locate,cat,show, rego, srchPhrase)
{
	srchPhrase = (typeof srchPhrase === "undefined") ? '' : srchPhrase;
	$('#ajaxImage').css('display', 'block');
	
	$('#ajaxImage').css('display', 'none');
	if(rego == 1)
		hideShowNational_C(show, 1);
	else if(rego == 2)
		catTail_C(cat);
	else if(rego == 3)
		merTail_C(merchant_id);
	else if(rego == 4)
		locTail_C(locate);
}
function searchbox(location){
	location = (typeof location === "undefined") ? '' : location;
	searchText = $("#inputbox").val();
	if(searchText == '' || searchText == null || searchText == "KEYWORD / DEAL ID" ){
		alert("Please enter valid keywords or deal id to search!");
	}
	else{
		
		if(/^([1-9]\d*)$/.test(searchText)){
			window.location.href = 'http://'+window.location.hostname+"/content/deal-page.php?E="+searchText;
		}
		else if(isNaN(searchText)){
			
			if(/([\%\=\?\-\\\/\#\@\^\&\*\(\)\{\}\[\]\:\;\>\<\,\.\`\~\+\!])/.test(searchText)){
				alert("Please put valid characters!");
			}
			else{
				searchText = searchText.replace(/\s/g,'-');
				if(location != '' && location != null)
					window.location.href = 'http://'+window.location.hostname+WEB_ROOT+location+"-Daily-Deals"+"/"+searchText;
				else
					window.location.href = 'http://'+window.location.hostname+"/"+searchText;
			
			}
		}
		else{
			alert("Please enter valid keywords or deal id (positive number with no leading 0) to search!");	
		}
	}
}
//not in use
function getCityName(city_id){
	$.ajax({
		   type: "GET",
		   url: WEB_ROOT + "deals/ajax-getcityname.php?cityId="+city_id,
		   success: function(data){
				return data;
		}
	});
}
function showTopDealResult(city_id)
{
	$.ajax({
		   type: "GET",
		   url: WEB_ROOT + "deals/ajax-topdeal.php?cityid="+city_id,
		   success: function(data){
				$('#promoted_deal').html(data);
		}
	});
}
function showDeal(){
	innerVal = '<img src="'+WEB_ROOT+'images/next.png"/><a href="#" onclick="showDeal();return false;">CLICK HERE TO VIEW MORE DEALS</a><img src="'+WEB_ROOT+'images/prev.png" />';
	$('#moreDealsBar').html('<img src="'+WEB_ROOT+'images/ajax-loader-img.gif" border=0 height="22" width="22" />');
	$.ajax({
		   type: "GET",
		   url: WEB_ROOT + "deals/ajax-loadnextdeals.php",
		   success: function(data){
			   		$('#moreDealsBar').html(innerVal);			  
					$('#dealContainer').append(data);
					ttDeal();			
		}
	});
	
}

function ttDeal(){
	
	$.ajax({
		   type: "GET",
		   url: WEB_ROOT + "deals/ajax-totaldeals.php",
		   success: function(data){
			   if(data == "no"){
					$('#moreDealsBar').html("No more deals are available");
		   	   }
		   }
	});
}
function hideShowNational(par, pLoad){
	if(pLoad != 1){
		if (par == 0) {
			localDealsWasClicked = true;
			$('#localWasClicked').val(1);			
		}
		else {
			localDealsWasClicked = false;
			$('#localWasClicked').val(0);			
		}

		reg = 1;
		loc = $("#locationId").val();
		mer = $("#merchantId").val();
		cat = $("#categoryId").val();
		
		var urlstring = '';
		 			urlstring = WEB_ROOT + "deals/pass-params.php?mer_in_homepage=" + mer+"&loc="+loc +"&cat_in_homepage="+cat+"&reg="+reg+"&local=" + localDealsWasClicked;
				
			$.ajax({
			   type: "GET",
			   url: urlstring,
			   success: function(data){
				   window.location.reload();
				}
			});

		//showDailyDealResult(mer,loc,cat,par,reg);
	}
	return false;
}
function hideShowNational_C(par, kty){
	if(par == 1){
		htmlstr = '<div class="nationalDealTab1">Local+National Deals</div><div class="nationalDealTab" onclick="hideShowNational(0,0);return false;">Local Deals Only</div>';
		$('#optionsNT').html(htmlstr);
		$("#nationalDeal").val(1);
	}
	else{
		htmlstr = '<div class="nationalDealTab1">Local Deals Only</div><div class="nationalDealTab" onclick="hideShowNational(1,0);return false;">Local+National Deals</div>';
		$('#optionsNT').html(htmlstr);
		$("#nationalDeal").val(0);
	}
	if(kty == 1){
		if($('#moreDealsBar').html() == "No more deals are available"){
			innerVal = '<img src="'+WEB_ROOT+'images/next.png"/><a href="#" onclick="showDeal();return false;">CLICK HERE TO VIEW MORE DEALS</a><img src="'+WEB_ROOT+'images/prev.png" />';
			$('#moreDealsBar').html(innerVal);
		}
	}
}
function catTail(catId){
	reg = 2;
	loc = $("#locationId").val();
	mer = $("#merchantId").val();
	nat = $("#nationalDeal").val();

	if ($('#localWasClicked').val() == 1)
		localDealWasClicked = true;
	else
		localDealWasClicked = false;
			
	
	//showDailyDealResult(mer,loc,catId,nat,reg);
	/*$("#optionsDT").ajaxComplete(function(event, XMLHttpRequest, settings) {
		cUrl = WEB_ROOT + "deals/ajax-dailydeals-list.php";
		//alert(settings.url );
		if (settings.url == cUrl) {
			loadUrl = WEB_ROOT+"deals/ajax-load-category.php";
        	$(this).load(loadUrl, {"catg":catId});
		}
		$("#categoryId").val(catId);
    });*/
	var urlstring = '';
	var reloadurl = '';
	 		urlstring = WEB_ROOT + "deals/pass-params.php?mer_in_homepage=" + mer+"&loc="+loc +"&cat_in_homepage="+catId+"&reg="+reg+"&local=" + localDealWasClicked;
		
	$.ajax({
			   type: "GET",
			   url: urlstring,
			   success: function(data){
				   window.location.reload();
				}
			});

	
	$("#categoryId").val(catId);
	return false;
}
function catTail_C(catId){
	loadUrl = WEB_ROOT+"deals/ajax-load-category.php";
    $("#optionsDT").load(loadUrl, {"catg":catId}, function(){
		$("#categoryId").val(catId);
		if($('#moreDealsBar').html() == "No more deals are available"){
			innerVal = '<img src="'+WEB_ROOT+'images/next.png"/><a href="#" onclick="showDeal();return false;">CLICK HERE TO VIEW MORE DEALS</a><img src="'+WEB_ROOT+'images/prev.png" />';
			$('#moreDealsBar').html(innerVal);
		}
		if($("#sDT").html() == "Less"){
			$("#sDT").html("more");
			$("#iDT").attr('src', WEB_ROOT+'images/ddl.png');
		}
	});
}
function merTail(merId){
	reg = 3;
	loc = $("#locationId").val();
	cat = $("#categoryId").val();
	nat = $("#nationalDeal").val();
	if ($('#localWasClicked').val() == 1)
		localDealWasClicked = true;
	else
		localDealWasClicked = false;
			
	//showDailyDealResult(merId,loc,cat,nat,reg);
	$("#merchantId").val(merId);
	var urlstring = ''
	 		urlstring = WEB_ROOT + "deals/pass-params.php?mer_in_homepage=" + merId+"&loc="+loc +"&cat_in_homepage="+cat+"&reg="+reg+"&local=" + localDealWasClicked;
		
	$.ajax({
			   type: "GET",
			   url: urlstring,
			   success: function(data){
				   window.location.reload();
				}
			});
	
	return false;
}

function merTail_C(merId){
	loadUrl = WEB_ROOT+"deals/ajax-load-provider.php";
    $("#optionsPT").load(loadUrl, {"merg":merId}, function(){
		$("#merchantId").val(merId);
		if($('#moreDealsBar').html() == "No more deals are available"){
			innerVal = '<img src="'+WEB_ROOT+'images/next.png"/><a href="#" onclick="showDeal();return false;">CLICK HERE TO VIEW MORE DEALS</a><img src="'+WEB_ROOT+'images/prev.png" />';
			$('#moreDealsBar').html(innerVal);
		}
		if($("#sPT").html() == "Less"){
			$("#sPT").html("more");
			$("#iPT").attr('src', WEB_ROOT+'images/ddl.png');
		}
	});
}

//function for location base deals on category and merchant page
function locTail(locationId){
	reg = 4;
	cat = $("#categoryId").val();
	nat = $("#nationalDeal").val();
	mer = $("#merchantId").val();
	if ($('#localWasClicked').val() == 1)
		localDealWasClicked = true;
	else
		localDealWasClicked = false;
			

	//showDailyDealResult(mer,locationId,cat,nat,reg);
	$("#locationId").val(locationId);
	var urlstring  = '';
			urlstring = WEB_ROOT + "deals/pass-params.php?mer=" + mer+"&loc=0&loc_in_merchant="+locationId+"&cat_in_merchant="+cat+"&reg="+reg;
		
		
		$.ajax({
			   type: "GET",
			   url: urlstring,
			   success: function(data){
				   window.location.reload();
				}
			});

	return false;
}

function locTail_C(cityId){
	loadUrl = WEB_ROOT+"deals/ajax-load-city.php";
			var external_link = 1;
			
	$.getJSON(loadUrl, {"locg":cityId, "external":external_link}, function(data){
		$("#optionsLT").html(data.locList);
		/*if(data.nationalId == "yes")
			$("#optionsNT").html('<div class="inactiveNatDiv">Local Deals Only</div><div class="inactiveNatDiv">Local+National Deals</div>');
		else if($("#optionsNT").children("div:first").attr("class") == "inactiveNatDiv"){
			$currNat = $("#nationalDeal").val();
			 hideShowNational_C($currNat, 0)
			
		}*/
		 $("#locationId").val(cityId);
		
		if($('#moreDealsBar').html() == "No more deals are available"){
			innerVal = '<img src="'+WEB_ROOT+'images/next.png"/><a href="#" onclick="showDeal();return false;">CLICK HERE TO VIEW MORE DEALS</a><img src="'+WEB_ROOT+'images/prev.png" />';
			$('#moreDealsBar').html(innerVal);
		}
		if($("#sLT").html() == "Less"){
			$("#sLT").html("more");
			$("#iLT").attr('src', WEB_ROOT+'images/ddl.png');
		}			
		
	});
  /* $("#optionsLT").load(loadUrl, {"locg":cityId}, function(){
		$("#locationId").val(cityId);
		
		if($('#moreDealsBar').html() == "No more deals are available"){
			innerVal = '<img src="../images/next.png"/><a href="#" onclick="showDeal();">CLICK HERE TO VIEW MORE DEALS</a><img src="../images/prev.png" />';
			$('#moreDealsBar').html(innerVal);
		}
	});*/
}
// function for subscribing mail from signup box
function dosubsValidate1() {
	frm = document.subscribe_form1;
	if (tfv_validate(frm)) 
		{
			
			$('#btnSubs1').hide();
			$('#imgSubs1').show();
			
			$.ajax({
			   type: "GET",
			   url: WEB_ROOT+"deals/ajax-subscribe-to-deals-constant.php?subsEmail=" + frm.email1_v1_v9_.value +"&city_id=" + frm.choose_city1_v6_[frm.choose_city1_v6_.selectedIndex].value+"&country_id="+countryId+"&refrer=",
			   success: function(data){
				    $("#emailId").val(frm.email1_v1_v9_.value);
					$('#imgSubs1').hide();
					$('#btnSubs1').show();
				   	var arr = data.split("|");
					var flag = arr[1].replace(/^\s+|\s+$/g,"");
					var msg = arr[0].replace(/^\s+|\s+$/g,"");
					$("#msgArea").html(msg);					
					if(flag == "no"){
						$('#subsToNat').css('display', 'inline');
						$("#linkSubs").trigger('click');
					}
					else{
						$('#subsToNat').css('display', 'none');
						$("#linkSubs").trigger('click');
					}
				}
			});
		}
		else 
		{
			return false;
		}

}
function dosubsValidate2() {
	frm = document.subscribe_form2;
	if (tfv_validate(frm)) 
		{
			
			$('#btnSubs2').hide();
			$('#imgSubs2').show();
			
			$.ajax({
			   type: "GET",
			   url: WEB_ROOT+"deals/ajax-subscribe-to-deals-constant.php?subsEmail=" + frm.email2_v1_v9_.value +"&city_id=" + frm.choose_city2_v6_[frm.choose_city2_v6_.selectedIndex].value+"&country_id="+countryId+"&refrer=",
			   success: function(data){
				   $("#emailId").val(frm.email2_v1_v9_.value);
					$('#imgSubs2').hide();
					$('#btnSubs2').show();
				   	var arr = data.split("|");
					var flag = arr[1].replace(/^\s+|\s+$/g,"");
					var msg = arr[0].replace(/^\s+|\s+$/g,"");
					$("#msgArea").html(msg);					
					if(flag == "no"){
						$('#subsToNat').css('display', 'inline');
						$("#linkSubs").trigger('click');
					}
					else{
						$('#subsToNat').css('display', 'none');
						$("#linkSubs").trigger('click');
					}
				}
			});
		}
		else 
		{
			return false;
		}

}

function HideMoreInfo(par){
	if(par == 1){
		$("#moreInfoText1").slideUp("normal","", function(){
			$("#moreHide1").css('display', 'none');
			$("#moreInfo1").css('display', 'inline');
		});
		
	}
	else if(par == 2){
		$("#moreInfoText2").slideUp("normal","", function(){
			$("#moreHide2").css('display', 'none');
			$("#moreInfo2").css('display', 'inline');
		});
	}
	
}

function ShowMoreInfo(par){
	if(par == 1){
		$("#moreInfoText1").slideDown("normal","", function(){
			
			$("#moreHide1").css('display', 'inline');
		});
		$("#moreInfo1").css('display', 'none');	
		
	}
	if(par == 2){
		$("#moreInfoText2").slideDown("normal","", function(){
			
			$("#moreHide2").css('display', 'inline');
		});
		$("#moreInfo2").css('display', 'none');	
		
	}
}

function dosubsValidate(){
	$("#msgArea").html("");
	$('#subsToNat').css('display', 'none');
	$("#ajaxNat").css('display', 'inline');
	
	var cntryid = $("#deshId").val();
	var citid = $("#natoId").val();
	var email = $("#emailId").val();
	
	$.ajax({
		   type: "GET",
		   url: WEB_ROOT+"deals/ajax-subscribe-to-deals-constant.php?subsEmail=" +email+"&city_id=" +citid+"&country_id="+cntryid+"&refrer=",
		   success: function(data){
				var arr = data.split("|");
				var msg = arr[0].replace(/^\s+|\s+$/g,"");
				$("#ajaxNat").css('display', 'none');	
				$("#msgArea").html(msg);				
	
			}
		});
}

function testAttribute(element, attribute)
{
  var test = document.createElement(element);
  if (attribute in test)
    return true;
  else
    return false;
}

if (!testAttribute("input", "placeholder"))
{
  window.onload = function()
  {
    var demo = document.getElementById("inputbox");
    var text_content = "KEYWORD / DEAL ID";

    demo.style.color = "gray";
    demo.value = text_content;

    demo.onfocus = function() {
    if (this.style.color == "gray")
    { this.value = ""; this.style.color = "black" }
    }

    demo.onblur = function() {
    if (this.value == "")
    { this.style.color = "gray"; this.value = text_content; }
    }
  }
}
</script>
