	function cookieShow() {
		jQuery('.cookie_msg').floatingMessage("",{  
	        position : "bottom-right" 
    	}); 
	}
			jQuery(document).ready(function() {
			jQuery('.cookie_msg').show();
			cookieShow();	
					
		});
	
		setTimeout(function() {
		    jQuery('.cookie_msg').hide();
		}, 9000);  
		
			
   $('#inputbox').keypress(function(e){
      if(e.keyCode==13)
      $('#searchbtn').click();
    });
});
