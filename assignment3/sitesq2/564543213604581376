<script type="text/javascript">
  // load publisher mininbanner, if available
  _ready.push(function($){
    $( '.article .publication h2 .publisher-name' ).each(function(){
      var $container = $(this),
          imgSrc = $container.data('imgSrc'),
          bgImg;
      if ( imgSrc ) {
        bgImg = new Image;
        $(bgImg).load(function(){
          var width = bgImg.width,
              height = bgImg.height;
          $container.width( width );
          $container.height( height );
          $container.addClass( 'publisher-logo' );
          $container.removeClass( 'publisher-name' );
        });
        bgImg.src=imgSrc;
      }
    });
  });

  _ready.push(function($){
    var $els,
        activateAsideAds = $( '.aside:visible' ).length;

    if ( activateAsideAds ) {
      $( '#right-top-anchor .outer-ad-container:hidden' ).show();
      $( '#right-top-anchor .outer-ad-container' ).find('.google-ad.inactive').removeClass('inactive').addClass('activated');
    }

    $( '.article .story-body .outer-ad-container' ).find('.google-ad.inactive').removeClass('inactive').addClass('activated');
    fetchDFPAds( $( '.article .story-body .outer-ad-container' ).find('.google-ad.activated') );

    if ( $( '.column.main > .content' ).height() < 2000 ) {
      $( '.aside .section.headlines.publisher-headlines, .aside .section.headlines.top-headlines' ).addClass('short');
    }

    availableHeight = $('.column.aside').height() - $('#right-top-anchor').height() - $('#right-bottom-anchor').height();
    // we always activate right-a if the column is visible

    // if we have enough space to show the related-links list as a
    // buffer between the ads, activate the ad in the bottom anchor
    // otherwise, show the related-links
    if ( availableHeight >= (5+114+25) ) {
      $( '#related-links-container' ).show( 200 );
      availableHeight = availableHeight - (5+114+25);
    }

    if ( availableHeight >= (250+30) && activateAsideAds ) {
      $( '#right-bottom-anchor .outer-ad-container:hidden' ).show();
      // add a class on which we can depend to determine 'activated'
      // state, since :visible dependings on show() delay and
      // client-side rendering
      $( '#right-bottom-anchor .outer-ad-container').find('.google-ad.inactive').removeClass('inactive').addClass('activated');
      // adjust the value statically, since there is a delay in show()
      // which makes height() return unreliable values
      availableHeight = availableHeight - (250+30);
    }

    if ( availableHeight >= (5+70+25) ) {
      $( '#connect-container' ).show( 200 );
      availableHeight = availableHeight - (5+75+25);
    }

    $('#right-mid-flex .flex-item-container:hidden').each(function(){
      var $container = $(this);
      if ( availableHeight > $container.height() ) {
        // TODO: find more succint method for determining whether we
        // are dealing with an ad container
        if ( $container.find('.google-ad.inactive') ) {
          if ( activateAsideAds ) {
            availableHeight -= $container.height();
            $container.show();
            $container.find('.google-ad.inactive').removeClass('inactive').addClass('activated');
          }
        } else {
          availableHeight -= $container.height();
          $container.show( 600 );
        }
      } else {
        return false;
      }
    });
    if ( availableHeight > 280 + 275 && activateAsideAds ) {
      $( '#right-c-container:hidden' ).show();
      // add a class on which we can depend to determine 'activated'
      // state, since :visible dependings on show() delay and
      // client-side rendering
      $( '#right-c-container' ).find('.google-ad.inactive').removeClass('inactive').addClass('activated');
      $( '#tf-container:hidden' ).show( 600 );
    } else {
      if ( availableHeight > 275 ) {
        $( '#tf-container:hidden' ).show( 600 );
      }
    }

    // expand the headline boxes to fill the gap
    if ( availableHeight > 0 ) {
      $els = $('.aside .section.headlines.top-headlines > .content, .aside .section.headlines.publisher-headlines > .content');
      $els.each(function(){
        var $this = $(this),
        targetHeight = $this.height() + parseInt( availableHeight / $els.length );
        if ( targetHeight > 282 ) {
          targetHeight = 282;
        }
        $this.css( 'max-height', targetHeight );
      });
    }

    fetchDFPAds( $('.aside .outer-ad-container').find('.google-ad.activated') );

  });
</script>

