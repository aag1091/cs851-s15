<script type="text/javascript">
  var dataLayer = {
    push: function () {
      var obj = arguments[0];
      //console.log( JSON.stringify( obj, '', '  ' ) );
      if ( ! obj.event ) return;
        switch (obj.event) {
          case "experiment":
            __aaEventTracker.trackEvent(obj.nameSpace, obj.testName, obj.cohort + ' | Total', undefined, true);
            break;
          case "event":
            __aaEventTracker.trackEvent(obj.eventCategory, obj.eventAction, obj.eventLabel + ' | ' + obj.eventName, undefined, false);
              break;
          case "ScrollDistance":
            //console.log( JSON.stringify( obj, '', '  ' ) );
            if ( obj.eventAction == 'Elements' ) {
              // register interaction when user scrolls through entire story
              if ( obj.eventLabel == '#aans-body-end' ) { obj.eventNonInteraction = false }
            }
            __aaEventTracker.trackEvent(obj.eventCategory, obj.eventAction, obj.eventLabel, obj.eventValue, obj.eventNonInteraction);
              break;
          default:
            console.log( 'uncaught event in dataLayer.push(): ' + JSON.stringify( obj, '', '  ' ) );
        }
    }
  };

  _ready.push(function($) {
    if ( $('body.story').length > 0 && $('body.story.restricted .inline-story-login').length == 0 ) {
      $.scrollDepth({
        elements: ['#aans-body-mid', '#aans-body-end', '#category-spotlight-grid', '#story-bottom-most-read', '#story-bottom-trending-tf', '#footer-spotlight-grid', '#standard-footer' ],
        percentage: false,
        userTiming: false,
        pixelDepth: false,
        nonInteraction: true
      });
    }
  });

  _ready.push(function($){
    var $adContainers = $( '.subnav .section.ads:visible, .bottom .section.ads:visible, .column.main .section.ads:visible, .column.aside:visible' );
    $adContainers.find('.google-ad.inactive').removeClass('inactive').addClass('activated');
    fetchDFPAds( $adContainers.find('.google-ad.activated') );
    $( '.ad-container.google-ad.anchor-a:visible, .ad-container.google-ad.pixel-a:visible, .ad-container.google-ad.pixel-b:visible' ).removeClass('inactive').addClass('activated');
    fetchDFPAds( $( '.ad-container.google-ad.anchor-a.activated, .ad-container.google-ad.pixel-a.activated, .ad-container.google-ad.pixel-b.activated' ) );
  });
</script>

