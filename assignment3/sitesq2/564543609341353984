       function setReadyForSocialReferral(ready, clid, stid, pride, localeid,button,fn,ln) {
            SR_READY = ready;
            SR_PRIDE = pride;
            SR_STID = stid;
            SR_CLID = clid;
            SR_LOCALE = localeid;
            SR_FN = fn;
            SR_LN = ln;
            if (SR_READY == "yes") {
                LaunchSocialReferralMenu(document.getElementById(button));
            }
        }
        function LaunchSocialReferralMenu(button) {
            var jobstr = document.getElementById("jobid").value;
            var urlstring = "../tgnewui/referral/home/referralworkflowoptions?siteid=" + SR_STID + "&partnerid=" + SR_CLID + "&localeid=" + SR_LOCALE + "&bruid=" + SR_PRIDE + "&jobids=" + jobstr+ "&fn=" + SR_FN + "&ln=" + SR_LN
            window.dialogBuilder.showDialog(button, urlstring);

        }     
        function ApplyNow()
	    {			    
			var ApplyURL = "-1";
			if (ApplyURL=="-1" ) ApplyURL= "";
			if (ApplyURL.length>0 )
			{				
			    window.location.href=ApplyURL; 
			}
			else
			//In case GTG if the job siteid is different than the current siteid
			//display warning pop-up to confirm site switched to job site
			{			   
				ApplyJobNow('1033');
			}
		}		
			
		function ApplyJobNow(strLocale)
        {
	        var strGQId;
	        var iLen, iIndx;
	        var arrGQId;
	        var arrGQIds;
	        var lngNewSelection = 0;
	        var lngSiteLocale = 1033;
	        var strSwitchSiteQS;
	        strGQId = "0";	// from Querystring
	        strSwitchSiteQS = "";

	        //var lngSelect = 1;
	        var lngSelect = 1;
	        
	        if  (strGQId == '')
	        {	
	            strGQId= document.frmJobDetail.JobInfo.value;
			    arrGQId = (strGQId.substring(2, strGQId.length - 2)).split("%%");
			    iLen = arrGQId.length;
			    arrGQIds = arrGQId[lngSelect-1].split('|');
			    if (arrGQIds[2] != 0)
			    {
				    if (strLocale == lngSiteLocale)
				    {
				        if (js_LaunchGQ_DotNet.toUpperCase() == 'YES')
				        {
				            openWindow11("gqlogin.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&fjd=true&referer=&gqid="+ arrGQIds[2] + "&jobinfo=__" + arrGQId[lngSelect-1] + "__&applycount=1&type=search_jobdetail");
				        }
				        else
				        { 
					        openWindow11("https://sjobs.brassring.com/" + strLocale + "/asp/tg/GQLogin.asp?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&fjd=true&referer=&gqid="+ arrGQIds[2] + "&jobinfo=__" + arrGQId[lngSelect-1] + "__&applycount=1&type=search_jobdetail");
					    }
				    }
				    else
				    {
				        document.frmMassApply.GQID.value = arrGQIds[2];
				        if(strSwitchSiteQS == '')
				        {
					        document.frmMassApply.action= "apply.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&src=jd&qs=partnerid%3d25649%26siteid%3d5465%26areq%3d9055BR";
					    }
					    else
					    {
					        document.frmMassApply.action= "apply.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&src=jd&qs=";
					    }
					    document.frmMassApply.submit();
				    }
			    }
			    else
			    {
				    document.frmMassApply.action= "apply.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj";
				    document.frmMassApply.submit();
			    }	        
			        		        
	        }
	        else
	        {
	           	
		        if (strGQId != 0)
		        {
			        if (strLocale == lngSiteLocale)
			        {
			            if (js_LaunchGQ_DotNet.toUpperCase() == 'YES')
				        {
				            openWindow11("gqlogin.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&fjd=true&referer=&gqid=" + document.frmMassApply.GQID.value + "&jobinfo=&applycount=1&type=search_jobdetail");
				        }
				        else
				        { 
				            openWindow11("https://sjobs.brassring.com/" + strLocale + "/asp/tg/GQLogin.asp?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&fjd=true&referer=&gqid=" + document.frmMassApply.GQID.value + "&jobinfo=&applycount=1&type=search_jobdetail");
				        }
			        }
			        else
			        {
			            if(strSwitchSiteQS == '')
				        {
				            document.frmMassApply.action= "apply.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&src=jd&qs=partnerid%3d25649%26siteid%3d5465%26areq%3d9055BR";
				        }
				        else
				        {
				            document.frmMassApply.action= "apply.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj&src=jd&qs=";
				        }
				        document.frmMassApply.submit();
			        }
 	            }
 	            else
 		        {
 			        document.frmMassApply.action = "apply.aspx?SID=^imhM8s9JfqPBrKL4Asb0iH37fHC8KMXUj/mWggL_slp_rhc_NYKJ406jH47Rt0QVhMVmZDwj";
			        document.frmMassApply.submit();
 		        }
 	        }
        }
        
