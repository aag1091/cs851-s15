<script type="text/javascript" src="//extend.schoolwires.com/creative/scripts/creative/rotate/creative.GalleryRotate-V3.jQuery.min.js"></script>
<script type="text/javascript" src="/cms/lib07/MA01906464/Centricity/Template/9/scripts/jflickrfeed.min.js"></script>
<!-- END PLUGINS -->
<script type="text/javascript">
	$(window).load(function(){
		if($(window).width() >= 320 && $(window).width() <= 639){
			calendarListView();
		}
		$(".icon-font").css("visibility","visible");
		searchText();
	});

	$(function(){
		// SET IE CLASSES TO BODY
		if(isMSIE){ $("body").addClass("msie"); }
		if(isMSIE7){ $("body").addClass("msie7"); }
		if(isMSIE8){ $("body").addClass("msie8"); }
		if(isMSIE9){ $("body").addClass("msie9"); }
		if(isMSIE10){ $("body").addClass("msie10"); }
	
		$(window).bind("resize", function(){ pgScreenSize(); });
		
		// RESPONSIVE MENU
		$("body").creativeResponsiveMenu({
			"breakPoint"		: 1023, // SYSTEM BREAK POINTS - 1023, 767, 639, 479
			"folderURL"		: "/cms/lib07/MA01906464/Centricity/Template/9/rs-menu/",
			"useDefaultCSS"		: "yes",
			"slideDirection"	: "top-to-bottom", // OTHER OPTION IS 'left-to-right'
			"menuPosition"		: "absolute", // OTHER OPTION IS 'relative'
			"menuButtonParent"	: "#sw-mystart-left",
			"showMenuBtnText"	: "yes",
			"menuBtnText"		: "MENU",
			"showMenuInfoText"	: "yes",
			"menuInfoText768"	: "Access information and school sites",
			"menuInfoText640"	: "Access information and school sites",
			"menuInfoText480"	: "Access information and school sites",
			"menuInfoText320"	: "Access information and school sites",
			"showSearch"		: "yes",
			"showMyAccount"		: "yes",
			"showSiteManager"       : "yes",
			"showDistrictHome"	: "yes",
			"showSchoolList"	: "yes",
			"showChannelList"	: "yes",
			"showTranslate"		: "yes",
			"translateId"           : "none",
			"extraMystartBtns"	: []
		});
		
		pgScreenSize();
		useChannelForSection();
		addFramesetTranslate();
		dynamicContent();
		
	});	
	
	function pgScreenSize(){
		if(isMSIE8 = 1){
			var getWidth = $(window).width();
		} else {
			var getWidth = screen.width;
		}
		if (getWidth >= 1024) { // 960+
			photoGallery();
		} if (getWidth >= 768 && getWidth < 1024) { // 768+
			photoGallery();
		} if (getWidth >= 640 && getWidth < 768) { // 640+
			photoGallery();
		} if (getWidth >= 480 && getWidth < 640) { // 480+
			photoGallery();
			calendarListView();
			appView();
		} if (getWidth >= 320 && getWidth < 480) { // 320+
			calendarListView();
			appView();
		}
	}
	
	function flickrFeed(){
		if("yes" == "yes"){
			$('#flickr-list').jflickrfeed({
				limit: 9,
				qstrings: {
					id: '109433059@N08'
				},
				useTemplate: false,
				itemCallback: function(item){
					$(this).append("<div class='thumbnail-outer'><div class='thumbnail-inner'><img src='" + item.image_m + "' alt=''/></div></div>");
					if($("#large-image img").hasClass("first-image")){
						if("yes" == "yes"){
							loadLargeImage();
						}
					}
				}
			});
			
			$(".thumbnail-inner").live('click', function(){
				$("#large-image").empty();
				var imgURL = $("img", this).attr("src");
				var imgTitle = $("img", this).attr("title");
				var imgURLArray = imgURL.split("_");
				var largeImg = imgURLArray[0]+"_"+imgURLArray[1]+".jpg";
				
				$("<img src='"+largeImg+"' title='"+imgTitle+"'/>").load(function(){
			        	$("#large-image").append(this);
			        });
			});
			
			$("#flickr-container-outer").removeClass("ui-helper-hidden");
		}
	}
	
	function loadLargeImage(){
		var imgURLFirst = $(".thumbnail-inner:eq(0) img").attr("src");
		var imgTitleFirst = $(".thumbnail-inner:eq(0) img").attr("title");
		
		var imgURLFirstArray = imgURLFirst.split("_");
			var largeImgFirst = imgURLFirstArray[0]+"_"+imgURLFirstArray[1]+".jpg";
			
			$("#large-image img").attr("src", largeImgFirst).removeClass("first-image");
		        
	}
	
	function photoGallery(){
		if(!$("#sw-content-container10.region.ui-hp .gallery-container").length){
			// PHOTO GALLERY CONFIGURATION
			$("#sw-content-container10.ui-hp div.ui-widget.app.gallery").creativeGalleryRotate({
				"imageHeight"		: 334,
				"imageControls"		: "on",
				"animateType"		: "fade", // CHOICES - "slide"
				"animateSpeed"		: 3,
				"pauseHover"		: ['all'], // CHOICES - ['title'], ['overlay'], ['caption'], ['all']
				"autoRotate"		: "on",
				"useTitles"		: "on",
				"useCaptions"		: "on",
				"useOverlay"		: "off",
				"randomOrder"		: "on",
				"linkedElement"		: ['picture'], // CHOICES - ['picture'], ['title'], ['overlay']
				"itemizedBullets"	: "off",
				"thumbnails"		: "off",
				"thumbnailWidth"	: 50,
				"thumbnailHeight"	: 32,
				"thumbnailControls"	: "off",
				"thumbnailLimit"	: 3,
				"addClass"		: "unclassed",
				"moreLinkText"		: "off",
				'onImageLoad'		: function(){ },
				'onTransitionStart'	: function(){
								var title = jQuery.trim($(".title h1 span").text());
								if(title.indexOf("[hide]") == -1){
									$("#sw-content-container10.region.ui-hp .gallery-desc").show();
								} else {
									$("#sw-content-container10.region.ui-hp .gallery-desc").hide();
								}
							},
				'onTransitionEnd'	: function(){ } 
			});
			
			var title = jQuery.trim($(".title h1 span").text());
			if(title.indexOf("[hide]") == -1){
				$("#sw-content-container10.region.ui-hp .gallery-desc").show();
			} else {
				$("#sw-content-container10.region.ui-hp .gallery-desc").hide();
			}
		}
	}
	
	function appView(){
		//DOC make homepage apps collapsible for small mobile devices
		if(!$("div.ui-widget.app.active").length){
			$("#hp-content div.ui-widget.app:first").addClass("active");
		}
		$("div.ui-hp div.ui-widget-header").click(function(){
			var clickedApp = $(this).parent().parent().attr("id");
			
			$("div.ui-widget.app").removeClass("active");
			$(this).parent().addClass("active");
			
			$("html, body").animate({
				scrollTop: $("#" + clickedApp).offset().top
			}, 500, function(){
				$("html, body").stop(true, false);
			});
		});
	}
	
	
	function useChannelForSection(){
		//DOC use channel name for pagelistnavigation header if one is not present
		if(!$("div.sp.column.one .ui-widget-header h1").length > 0) {
			$("div.sp.column.one .ui-widget-header").append("<h1></h1>");
		}
        }
        
	function dynamicContent(){
		// ********** MYSTART ********** //
		//DOC replace mystart dropdown arrows
		$(".sw-myaccount-chevron").html("<span class='icon-font' style='font-size:10px;'>8</span>");
		$(".sw-dropdown-arrow").html("<span class='icon-font' style='font-size:10px;'>8</span>");
		$("#sw-search-button span").addClass("icon-font").text("7");
		$("#sw-mystart-left").prepend("<div id='home-icon' class='hide768'><a href='/'><span class='icon-font'>1</span></a></div>");
		$(".sw-mystart-button.home").clone().addClass("show768").prependTo("#sw-mystart-right");
		$("#sw-mystart-inner").show();
		
		// ********** HEADER ********** //
		$(".sw-mystart-dropdown.schoollist").prependTo("#gb-search-dropdowns");
		$("#sw-mystart-search").addClass("ui-clear").prependTo("#gb-search-dropdowns");
		
		//SITE NAMES
		switch("home"){
		case "home":
			//NO NAME FOR THE DISTRICT, JUST LOGO
		break;
		case "educatoreffectiveness":
			//NO SITE NAME, JUST LOGO
		break;
		case "oiit":
			//NO SITE NAME, JUST LOGO
		break;
		default:
			var siteNameText = "Boston Public Schools";
			var headerArray = siteNameText.split(" ");
			$("#gb-sitename h1 a").html(headerArray[0]);
			
			for(i = 1;i <= headerArray.length - 1;i++){
				$("#gb-sitename h2 a").append(headerArray[i]+" ");
			}
		}
		
		// ********** CHANNEL BAR ********** //
		//DOC add first class to first channel item
		$("li.sw-channel-item").first().addClass("first");
		//DOC add last class to last channel item
		$("li.sw-channel-item").last().addClass("last");
		
		// ********** BODY ********** //
		//DOC initialize modEvents()
		$(".ui-hp .upcomingevents").modEvents({
			columns:"yes"
		});
		eventsByDay(".ui-hp .upcomingevents .ui-articles");
		//DOC move headlines h1 to top of the article
		$(".headlines .ui-article").each(function(){
			var headlineTitle = $(this).find("h1.ui-article-title");
			$(this).prepend(headlineTitle);
		});
		//DOC copy headlines more link to the header
		$(".ui-hp .headlines").each(function(){
			$("a.more-link",this).html("<span>View All</span>").addClass("view-all hide480").removeClass("more-link").appendTo($("div.ui-widget-header", this));
		});
		$("a.more-link").text("View All");
		
		//INITIALIZE FLICKR FEED
		if($(".ui-hp").length){
			flickrFeed();
		}
		//DOC rename first breadcrumb
		$("li.ui-breadcrumb-first > a > span").text("Home");
		//DOC auto-focus username signin field
		$("#swsignin-txt-username").focus();
		//DOC center signed out message and signin button
        	if($("div.ui-spn > div > p > span").text() == "You have been signed out."){
        		$("div.ui-spn > div").css({"text-align" : "center", "padding" : "50px 0px 50px 0px"});
        		//DOC add signed out breadcrumb
        		$(".ui-breadcrumbs").append("<li>Signed Out</li>");
        	}
        	//DOC add sign in breadcrumb
        	if($("#swlogin").length){
        		$(".ui-breadcrumbs").append("<li>Sign In</li>");
        	}
		
		// ********** FOOTER ********** //
		//DOC get Schoolwires footer links
		var feedbackURL = $("div#sw-footer-links li:eq(0) a").attr("href");
		var termsURL = $("div#sw-footer-links li:eq(2) a").attr("href");
		var policyURL = $("div#sw-footer-links li:eq(1) a").attr("href");
		
		$("a#feedback").attr("href", feedbackURL);
		$("a#terms").attr("href", termsURL);
		$("a#policy").attr("href", policyURL);			
	}
	
	function searchText() {
		//DOC custom mystart search placeholder
	        //SEARCH TEXT
	        $("div#sw-mystart-search input#sw-search-input").removeAttr("placeholder");
		$("div#sw-mystart-search input#sw-search-input").attr("value", "Search This Site...");
		$("div#sw-mystart-search input#sw-search-input").focus(function() {
			if($(this).val() == "Search This Site...") {
				$(this).val("");
			}
		});
		$("div#sw-mystart-search input#sw-search-input").blur(function() {
			if($(this).val() == "") {
				$(this).val("Search This Site...");
			}
		});
	}
	
	function calendarListView(){
		//DOC auto click function for schoolwires calendar list view for small mobile devices
		if(!$("a.ui-btn-toggle.list").hasClass("active")){
			$("a.ui-btn-toggle.list").click();
		}
	}
	
	function eventsByDay(container) {
		var countLi = 0;
		$(".ui-article", container).each(function(){
			if (!$(this).find("h1.ui-article-title.sw-calendar-block-date").size()){
				var moveArticle = $(this).html();
				$(this).parent().prev().children().children().next().append(moveArticle);
				$(this).parent().remove();
			};
		});
	}
	
	function addFramesetTranslate(){
		var pageURL = window.location.href;
		var translateDropdown = '<div class="sw-mystart-dropdown translate">' +
							'<div class="selector">Translate this page...</div>' +
							'<div class="sw-dropdown" style="display: none;">' +
								'<div class="sw-dropdown-selected">Translate this page...</div>' +
								'<ul class="sw-dropdown-list">' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|es&hl=en&anno=0&ie=UTF8">English to Spanish</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ar&hl=en&anno=0&ie=UTF8">English to Arabic</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|zh-CN&hl=en&anno=0&ie=UTF8">English to Chinese (Simplified)</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|zh-TW&hl=en&anno=0&ie=UTF8">English to Chinese (Traditional)</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|fr&hl=en&anno=0&ie=UTF8">English to French</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|de&hl=en&anno=0&ie=UTF8">English to German</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|it&hl=en&anno=0&ie=UTF8">English to Italian</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ja&hl=en&anno=0&ie=UTF8">English to Japanese</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ko&hl=en&anno=0&ie=UTF8">English to Korean</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|pt&hl=en&anno=0&ie=UTF8">English to Portuguese</a>' +
									'</li>' +
									'<li>' +
										'<a href="http://translate.google.com/translate?u=' + pageURL + '&langpair=en|ru&hl=en&anno=0&ie=UTF8">English to Russian</a>' +
									'</li>' +
								'</ul>' +
							'</div>' +
							'<div class="sw-dropdown-arrow"></div>' +
						'</div>';
		var pageHost = window.location.host;
		if(pageHost != "translate.googleusercontent.com")
		{				
			$("#gb-search-dropdowns").append(translateDropdown);
			$(".sw-mystart-dropdown.translate").click(function()
			{
				$(".sw-dropdown",this).show();
			});
			$(".sw-mystart-dropdown.translate").hover(function()
			{
			},
			function()
			{
				$(".sw-dropdown",this).hide();
			});
		}
			
	}
	
</script>

