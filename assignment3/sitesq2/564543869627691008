$('.hostGoal .indicator').each(function(){
    var range = $(this).attr('data-range');
    var reward = $(this).attr('data-reward');
    $(this).qtip({

        content: '<div class="rw range">'+range+' in sales</div><div class="rw reward">' +reward+ '</div>',
        show: 'mouseover',
        hide: 'mouseout',
        position: {
          corner: {
             target: 'bottomMiddle',
             tooltip: 'topLeft'
          }
        },
        style: {
            background: '#444',
          width: 200,
          padding: 2,
          textAlign: 'center',
          border: {
             width: 4,
             radius: 2,
             color: '#444'
          },
          tip: 'topLeft'
        }

    });
});



