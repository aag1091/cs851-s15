    <script type="text/javascript">
        $(function(){
            var $body = $('body');
            $body.attr('id','shop');
            $body.addClass('presenterOwned');

            $(".changemarket").on({
                click: function(e) {
                    e.stopPropagation();
                    var $target = $(e.currentTarget);
                    var $languageOption = $target.find('a');
                    var market = parseInt($target.closest('li').find('input').val());
                    var overrideLanguage = '';
                    var userLanguage = overrideLanguage || '';
                    var defaultLanguage = userLanguage || 'en_US';
                    var languageSetting = "";
                    API.call("/user/setMarket/"+market, {}, function(result) {
                        if(market != 1) {
                            defaultLanguage = 'en_US';
                        }
                        else if($languageOption.length > 0 && $languageOption.hasClass('marketLanguageOption')) {
                            defaultLanguage = $languageOption.attr('id');
                            languageSetting = "override";
                        }
                        API.call("/user/setLocale/" + defaultLanguage + "/" + languageSetting, {}, function(languageResult) {
                            window.location.reload();
                        });
                    });
                }
            });
        });
	</script>
