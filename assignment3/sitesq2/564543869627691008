       $(".posttowall").click(function(e) {
           e.preventDefault();
           var post = {
             method: 'feed',
             name: "Jenna Greene&#039;s Younique Virtual Party",
             link: window.location.href+"?autoplay=1#mediaDisplay",
             caption: "Wow!  I just attended a Younique Virtual Party",
             description: "So quick and easy!  Check it out here!"
           };
                      post.picture = "http://www.youniqueproducts.com/img/party/partypage_default.jpg";
                      FB.ui( post, function(response) {
                 if (response && response.post_id) {
                  $('#shareResp').fadeIn('fast').delay(4000).fadeOut(500);
                 }
               }
           );
       });

       $("body").on("facebookloaded", function() {
           FB.api("//attending", 'get', function(resp) {
               $(".greeting .rsvpList").find("img").remove();
               $(".rsvpList h4").show();
               var count = 0;
               $.each(resp.data, function(i,e) {
                   count++;
                   if(count <= 20) {
                       var img = $('<a href="https://facebook.com/'+e.id+'" target="_blank"><img width="32" height="32" src="https://graph.facebook.com/'+e.id+'/picture?type=square"/></a>');
                       img.attr("alt", e.name);
                       img.attr("title", e.name);
                       $(".greeting .rsvpList p").before(img);
                   }
               });
           });
       });

       $(".invitefriends").click(function(e) {
           e.preventDefault();
           FB.login(function(response) {
                if (response.authResponse) {
                    FB.api("//attending/", 'post', {}, function(resp) {
                           FB.ui(
                               {
                                 method: 'apprequests',
                                 data: "party=1331522",
                                 message: 'Invite others to join this party'
                               },
                               function(response) {
                                   //Now lets invite them to the event as well
                                    FB.api("//invited/", 'post', {users: response.to.join(",")}, function(resp) {
                                        //alert("You have invited " + response.to.length.toString() + " friends");
                                        var inviteMsg = "<span class=\"icomoon\">c</span> Thank you! You invited "+response.to.length.toString()+ " friends";
                                        $('#inviteResp').html(inviteMsg).fadeIn('fast').delay(4000).fadeOut(500);
                                    });
                               }
                             );
                       });
                } else {
                    alert("Please grant our application permission in order to invite others to this party");
                }
           }, {scope: "rsvp_event,create_event"});
       });


       

   });
</script>

