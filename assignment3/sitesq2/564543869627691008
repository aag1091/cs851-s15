<div id="fb-root"></div>

<script type="text/javascript">

var requireFacebookPermissions = function(requiredPermissions, callback,forceLogin) {
	if(forceLogin !== false) {
		forceLogin = true;
	}
    FB.getLoginStatus(function(response) {
    	if (response.status !== 'connected') {
        	if(forceLogin) {
	    		FB.login(function(response) {
		    		API.calllocal("/account/signinapi", {}, function() {
		    			callback(response);
		    		}, function() {
			    		callback(response);
		    		});
				}, {scope: requiredPermissions.join(",")});
        	}
    	} else if(requiredPermissions.length) {
    		FB.api('/me/permissions',function(resp) {
        		var permissions = resp.data[0];
        		var hasMissingPermissions = false;
        		for(var i = 0; i < requiredPermissions.length; i++) {
            		var perm = requiredPermissions[i];
            		if(perm != '' && permissions[perm] != 1) {
            			hasMissingPermissions = true;
            			break;
            		}
        		}
        		if(hasMissingPermissions) {
        			FB.login(function(response) {
                		callback(response);
            		}, {scope: requiredPermissions.join(",")});
        		} else {
        			callback(response);
        		}
    	    });
    	} else {
    		API.calllocal("/account/signinapi", {}, function() {
    			callback(response);
    		}, function() {
	    		callback(response);
    		});
    	}
	});
};
var checkHasFacebookPermission = function(requiredPermissions, callback) {
	FB.api('/me/permissions',function(resp) {
		if(resp.error) {
			callback(false);
		} else {
			var permissions = resp.data[0];
			var hasMissingPermissions = false;
			for(var i = 0; i < requiredPermissions.length; i++) {
	    		var perm = requiredPermissions[i];
	    		if(permissions[perm] != 1) {
	    			hasMissingPermissions = true;
	    			break;
	    		}
			}
			callback(!hasMissingPermissions);
		}
    });
};
window.fbAsyncInit = function() {
    // init the FB JS SDK
    FB.init({
//      appId      : '179262732208465', // App ID from the App Dashboard
		appId      : '179262732208465', // App ID from the App Dashboard
      status     : true, // check the login status upon init?
      cookie     : true, // set sessions cookies to allow your server to access the session?
      xfbml      : false  // parse XFBML tags on this page?
    });

    	    // Additional initialization code such as adding Event Listeners goes here
	    var scope = '';
	    scope = $.trim(scope);

	    requireFacebookPermissions(scope.split(','), function() {
		    $("body").trigger("facebookloaded");
	    }, false);
	  };

  // Load the SDK's source Asynchronously
  (function(d, debug){
     var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement('script'); js.id = id; js.async = true;
     js.src = "//connect.facebook.net/en_US/all" + (debug ? "/debug" : "") + ".js";
     ref.parentNode.insertBefore(js, ref);
   }(document, /*debug*/ false));
