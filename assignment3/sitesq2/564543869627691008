  $(document).ready(function(){
    $('#promoteTrig').click(function(event){
        event.preventDefault();
        getPromoteParty();
        $('#promotePartyPanel').slideDown('fast');
        $('.promoteBtn').fadeOut('fast');
        $('#closePromote').slideDown('fast');
    });
    $('#closePromote').click(function(e) {
       e.preventDefault();
       $('#promotePartyPanel').html('');
       $(this).slideUp('fast');
       $('.promoteBtn').slideDown('fast');
    });

    // promote the party:
    function getPromoteParty() {
         $.ajax({
           url: "/OpLashBlast/modals/promoteparty/1331522",
           cache: false
         }).done(function( html ) {
           $("#promotePartyPanel").html(html);
         });
    }
  });
</script>
