  <script type="text/javascript">
	var songs_played = 0;

    var sc_key = 'cca2dd1559b220dff02f959ee90cd731';

    SC.initialize({
      client_id: sc_key,
    });

    //maybe we need to conditionally load the youtube api?


    //show loading gif until youtube api is ready

    /*function onYouTubePlayerAPIReady() {
      //handle loading gif

      //set sc to fake iframe
      var sc = SC.Widget(jQuery('<iframe />').get(0));
      //wheen souundcloud finishes loading
      sc.bind(SC.Widget.Events.READY, function(player, data){play_current();});
    }*/

    jQuery('div.html5_player img.play_img').bind('click', function(){
      play_current();
      update_ads();
    });

    jQuery('ol.html5_playlist').on('mouseenter', 'li .cover_img_div', function() {
    	jQuery(this).find('.cover_img_square').hide();
    	jQuery(this).find('.cover_img_play').show();
    });

    jQuery('ol.html5_playlist').on('mouseleave', 'li .cover_img_div', function() {
    	jQuery(this).find('.cover_img_play').hide();
    	jQuery(this).find('.cover_img_square').show();
    });

    jQuery('ol.html5_playlist').on('click', 'li .cover_img_div', function() {

      var current = jQuery('ol.html5_playlist li.current_song');

      if (current.length > 0) {

        //stop current song that is playing
        stop_song();
        //remove class
        jQuery('ol.html5_playlist li.current_song').removeClass('current_song');

      }

      //add class to this li
      jQuery(this).parents('li:first').addClass('current_song');
      //play current
      play_current();
      update_ads();
    });

  function update_song_info(song_info) {

    //remove top div
    jQuery('div.about-song').css('display', 'none');

    //grab current small info object
    var small_info_block = jQuery('#html5_playlist_small_info');

    small_info_block.find('span.small_info_title').html(song_info.songTitle);
    small_info_block.find('img.cover_img_thumb').attr('src', "http://static.djbooth.net" + song_info.imageThumb);
    //jQuery('#conttitle').html(song_info.songTitle);

      var content_buffer =  'Artist: <a href="/index/artists/info/' + song_info.artistUrl + '" target="blank">' + song_info.artistName + '</a>' +
                  '<br />' +
                  'Song: <a href="/index/tracks/review/' + song_info.songUrl + '" target="_blank">' + song_info.songName + '</a>' +
                  '<br />' +
                  'Producer: ' + song_info.songProducer + '';

      switch (song_info.downloadType) {
        case 'itunes':
          content_buffer += ' | ' +
                    '<strong><a href="' + song_info.songItunes + '" target="_blank">Buy on iTunes</a></strong><br />';
        break;
        case 'mp3':
          content_buffer += ' | ' +
                    '<strong><a href="/index/tracks/review/' + song_info.songUrl + '" target="_blank">Download MP3</a></strong><br />';
        break;
        //false is also the default case, security
        case 'false':
        default:
          content_buffer += '<br />';
        break;
      }

      content_buffer += '<i>Published ' + song_info.entryMonth + '/' +song_info.entryYear + '</i> - ' + song_info.songDescription;

      small_info_block.find('div.small_info_content').html(content_buffer);
      small_info_block.css('display','block');

  }

  /*
   * Old update
   *
    function update_song_info(song_info) {
      jQuery('#contcover img').attr('src', "http://static.djbooth.net" + song_info.coverImage);
      jQuery('#conttitle').html(song_info.songTitle);
      var content_buffer = '<div class="overview">' +
                  '<p>Artist: <a href="/index/artists/info/' + song_info.artistUrl + '" target="blank">' + song_info.artistName + '</a>' +
                  '</p>' +
