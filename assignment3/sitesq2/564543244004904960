				<script>
					jQuery(document).ready(function() {
						
						var star_rating_clicked = false;
						
						jQuery('div.star-rating-form div.star-img').on('mouseenter', function() {
							
							if (!star_rating_clicked) {
								//color stars
								jQuery('div.star-rating-form div.star-img.full').removeClass('full');
								jQuery(this).prevAll().addClass('full');
								jQuery(this).addClass('full');
							}
							
							//below is for if you change your mind after deciding on a rating, rebinds mouseout, nulls rating
							//jQuery('div.star-rating-form input[name="rating"]').val('');
						});
						
						jQuery('div.star-rating-form div.star-img').on('mouseout', function() {						
							if (!star_rating_clicked) jQuery('div.star-rating-form div.star-img.full').removeClass('full');
						});
						
						jQuery('div.star-rating-form div.star-img').on('click', function() {
							
							star_rating_clicked = true;
							
							//color stars
							jQuery('div.star-rating-form div.star-img.full').removeClass('full');
							jQuery(this).prevAll().addClass('full');
							jQuery(this).addClass('full');
							
							//update hidden input
							//ways to get number of stars - 1, 1st will be ALL on page, 2 will be only those in parent container
							//console.log(jQuery(this).index('.star-img'));
							//console.log(jQuery(this).prevAll().length);
							jQuery('div.star-rating-form input[name="rating"]').val(jQuery(this).prevAll().length);
						});
						
						jQuery('div.star-rating-form button.submit').on('click', function(e) {
							e.preventDefault();

							if (jQuery('div.star-rating-form input[name="rating"]').val()) {
								
								jQuery.ajax({
									type : "POST",
									url : jQuery('div.star-rating-form form').attr('action'), // <= Providing the URL
									data : jQuery('div.star-rating-form form').serialize(), // <= Providing the form data, serialized above
									success : function(results) {
										//console.log(results);
										
										if (results === 'Your rating has been successfully submitted.') {
											jQuery('div.star-rating-form').html('Your rating has been successfully submitted.');
										} else {
											console.log(results);
											alert('An error has occurred. Please refresh and try again.');
										}
										
									},
									error : function(results) {
										// What to do when the ajax fails.
										console.log("ERROR");
										alert('An error has occurred. Please refresh and try again.');
									}
								});
								
							} else {
								alert('You have left your rating empty. Please select a rating and try again.');
							}
						});
						
					});
				</script></form>
