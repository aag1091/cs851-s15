  <script type="text/javascript">

    function expand_small_info() {
      var small_info = jQuery('#html5_playlist_small_info');

      small_info.animate({'max-height':'700px'});
      small_info.find('span.small_info_more_less').html('See Less');
      //small_info.find('div.small_info_content').animate({'margin-left':'0'});
    }

    function shrink_small_info() {
      var small_info = jQuery('#html5_playlist_small_info');

      small_info.animate({'max-height':'100px'});
      small_info.find('span.small_info_more_less').html('See More');
      //small_info.find('div.small_info_content').animate({'margin-left':'100px'});
    }
    //make click expand
    jQuery('#html5_playlist_small_info').bind('click', function(){
      var small_info_max_height = jQuery(this).css('max-height');

      if (small_info_max_height === '100px') {expand_small_info();} else {shrink_small_info();}

    });
  </script>

