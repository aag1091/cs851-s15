        break;
        //false is also the default case, security
        case 'false':
        default:
        break;
      }
      content_buffer += '</div><p>' + song_info.songDescription + '</p>';
      jQuery('#contdescr').html(content_buffer);
    }*/

    function play_current(){
      var player  = jQuery('div.html5_player');
      player.show('blind');

      var current = jQuery('ol.html5_playlist li.current_song');
      //if there is no current song, make it the first in the list
      if (current.length === 0) {
        jQuery('ol.html5_playlist li:first').addClass('current_song');
        current = jQuery('ol.html5_playlist li.current_song');
      }

      var classes = current[0].classList;
      var type  = classes[0];

    //shrink_small_info();
      var song_info = current.find('input[name="song_info"]').data();
      update_song_info(song_info);

      //clear out contents of player
      player.empty();

      //if 3 songs played, show video add, then after video ad, continue playing
      /*
      if (songs_played >= 3) {
      	show_video_ad();
      	return;
      } else {
      	//add to songs played variable
      	songs_played++;
      }
      */

      //figure out how to play this type
      switch (type) {

        case 'youtube':

          //grab video id
          var yt_id = current.find('input[name="source_val"]').val();

          //inject div with id into object
          player.html('<div id="'+yt_id+'"></div>');

          //create yt player using api and previously created div
          var yt_player = new YT.Player(yt_id, {
            height: '343',
            width: '609',
            videoId: yt_id,
            playerVars: {
              //controls: 0,
              wmode:'transparent'
            },
            /*autoPlay: true,*/
            events: {
              //onready, play
              'onReady': function(event){event.target.playVideo();},
              //on statechange, if end, NEXT
              'onStateChange': function(event){
                if (event.data === 0) {next_song();}
              }
            }
          });

        break;

        case 'mp3':

          if (check_html_audio_support() === true) {

            var mp3_url = current.find('input[name="source_val"]').val();

            player.html(''+
                '<audio controls>'+
                  '<source src="' + mp3_url +'" type="audio/mpeg">'+
                  'Your browser does not support the audio tag.'+
                '</audio>');

            var audio = jQuery('div.html5_player').find('audio');

            //play
            //audio.get(0).play();

            //load will autoplay hopefully
            audio.get(0).load();

            audio.bind("canplay", function(){audio.get(0).play();});

            //bind ended event
            audio.bind("ended", function(){next_song();});

            jQuery('div.html5_player').append('<img class="cover_img" src="http://static.djbooth.net' + song_info.coverImage + '" width="300" height="300" />');

          } else {
            jQuery('div.html5_player').append('Your browser does not support HTML5 Audio. Please <a href="http://chrome.google.com">update your browser</a>.');
          }
        break;

        case 'soundcloud':

          if (check_html_audio_support() === true) {

            var sc_id = current.find('input[name="source_val"]').val();
            var stream_url = '';

            jQuery.getJSON('http://api.soundcloud.com/tracks/' + sc_id + '.json?consumer_key=' + sc_key, function(track) {
              //console.log(track.stream_url);
              stream_url = track.stream_url;

              player.html(''+
                '<audio controls>'+
                  '<source src="' + stream_url + '?consumer_key='+sc_key+'" type="audio/mpeg">'+
                  'Your browser does not support the audio tag.'+
                '</audio>');

              var audio = jQuery('div.html5_player').find('audio');

              //play
              //audio.get(0).play();

              //load will autoplay
              audio.get(0).load();

              audio.bind("canplay", function(){audio.get(0).play();});

              //bind ended event
              audio.bind("ended", function(){next_song();});

              jQuery('div.html5_player').append('<img class="cover_img" src="http://static.djbooth.net' + song_info.coverImage + '" width="300" height="300" />');
              jQuery('div.html5_player').append('<span class="via_soundcloud"> <a target="_blank" href="' + track.permalink_url + '"><img src="http://developers.soundcloud.com/assets/logo_white.png" /></a> </span>');

            });

          } else {
            jQuery('div.html5_player').append('Your browser does not support HTML5 Audio. Please <a href="http://chrome.google.com">update your browser</a>.');
          }

        break;
        
        case 'audiomack':
			
			if (check_html_audio_support() === true) {

	            var am_url = current.find('input[name="source_val"]').val();
	            
	            jQuery.ajax({
					url: '/audiomack-mp3.php',
					type: 'POST',
					data: { 'audiomack_url' : am_url },
					success: function(audiomack_src) {
						player.html(''+
							'<audio controls>'+
								'<source src="' + audiomack_src + '" type="audio/mpeg">'+
								'Your browser does not support the audio tag.'+
			                '</audio>');
			
						var audio = jQuery('div.html5_player').find('audio');
			
			            //play
			            //audio.get(0).play();
			
			            //load will autoplay
			            audio.get(0).load();
			
			            audio.bind("canplay", function(){audio.get(0).play();});
			
			            //bind ended event
			            audio.bind("ended", function(){next_song();});
			
			            jQuery('div.html5_player').append('<img class="cover_img" src="http://static.djbooth.net' + song_info.coverImage + '" width="300" height="300" />');
			            jQuery('div.html5_player').append('<span class="via_soundcloud"> <a target="_blank" href="http://audiomack.com/song/' + am_url + '"></a> </span>');
					},
					error: function(msg) {
						console.log('Error getting audiomack MP3: ' + msg);
					}
				});
				
          } else {
            jQuery('div.html5_player').append('Your browser does not support HTML5 Audio. Please <a href="http://chrome.google.com">update your browser</a>.');
          }
          
        break;

        default:
        break;

      }

      jQuery('#html5_playlist_previous').show();
      jQuery('#html5_playlist_next').show();

      //update_ads();

    }

    function stop_song() {
      var player  = jQuery('div.html5_player');
      var current = jQuery('ol.html5_playlist li.current_song');
      var classes = current[0].classList;
      var type  = classes[0];

      switch (type) {

        case 'youtube':
          //grab video id
          var yt_id = current.find('input[name="source_val"]').val();
          jQuery('#'+yt_id).remove();
        break;

        case 'mp3':
        case 'soundcloud':
        case 'audiomack':
          var audio = player.find('audio').get(0);

          if (player.find('audio').length) {
	          if (!audio.paused || typeof audio == 'undefined') {
		          audio.pause();
		          audio.currentTime = 0;
		      }
		  }
        break;

        default:
        break;

      }
    }

    function next_song(){
      var current = jQuery('ol.html5_playlist li.current_song');
      var classes = current[0].classList;
      var type  = classes[0];

      var next  = current.next('li');
      if (typeof next.html() == 'undefined') var next = jQuery('ol.html5_playlist li:first');

      stop_song();

      current.removeClass('current_song');
      next.addClass('current_song');

      play_current();

    }

    function previous_song(){
      var current = jQuery('ol.html5_playlist li.current_song');
      var classes = current[0].classList;
      var type  = classes[0];

      var prev  = current.prev('li');
      if (typeof prev.html() == 'undefined') var prev = jQuery('ol.html5_playlist li:last');

      stop_song();

      current.removeClass('current_song');
      prev.addClass('current_song');

      play_current();
    }

    function update_ads() {
      var $tag = $("#cmn_ad_tag_head");
      $tag.css('min-height', $tag.height());
      $tag = $(".boxad:eq(0)");
      $tag.css('min-height', $tag.height());
      $tag = $(".160adunit-songs:eq(0)");
      $tag.css('min-height', $tag.height());
      writeCaptureRefresh();
    }

    function show_video_ad() {
		jQuery('#html5_playlist_small_info').hide();
		jQuery('#html5_playlist_previous').hide();
		jQuery('#html5_playlist_next').hide();
		jQuery('.html5_player').html('<p align="center" style="color:#FFFFFF;margin-top:15px;font-size:12px;">Please disable AdBlock.</p>');

		cmnOverlay.showPreroll(cmnvideotagdefault(), 609, 343);

		document.addEventListener("cmnPrerollComplete", function(e) {after_video_ad();});

		document.addEventListener("cmnHidePreroll", function(e) {after_video_ad();});

	}

	function after_video_ad() {
		jQuery('.html5_player').html('');
		jQuery('#html5_playlist_small_info').show();
		jQuery('#html5_playlist_previous').show();
		jQuery('#html5_playlist_next').show();

		songs_played = 0;

		play_current();
	}

    function check_html_audio_support() {
      var player = undefined;

      try {
        player = new Audio();
      } catch(e) {
        // HTML5 audio not supported :(
          return false;
      }

      if(!player.canPlayType("audio/mpeg;").replace(/no/, false)) {
        // MP3 format not supported
        return false;
      }

      return true;
    }
  </script>

