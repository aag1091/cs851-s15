	*/
 	$(".youtube").colorbox({iframe:true, innerWidth:640, innerHeight:390}); 
});

function setMenuCookie(menuId){
	jQuery.ajax('includes/setmenu.php?menu=' + menuId);
}

function initialiseAnimatedImages() {
    $("img.animated").each(function (Index) {
        var interval = 60;
        $(this).attr("index", "1");
        var intervalID = setInterval(function () { animateImage(Index); }, interval);
        $(this).attr("intervalid", intervalID);
		var pattern = "images/switch#.png";
		var src = pattern.replace("#", 2);
            $(this).attr("index", 2);
            $(this).attr("src", src);
    });
}
function animateImage(Index) {
    $("img.animated:eq(" + Index + ")").each(function (Index) {
        if (!$(this).is(":visible")) {
            var intervalID = parseInt($(this).attr("intervalid"));
            clearInterval(intervalID);
        } else {
            var max = 9;
            var pattern = "images/switch#.png";
            var index = parseInt($(this).attr("index"));
			if (index > 1) {
				index++;
			}
            if (index > max) {
                index = 1;
            }
            var src = pattern.replace("#", index);
            $(this).attr("index", index);
            $(this).attr("src", src);
        }
    });
}
function toggleMenuClicked(link){
	var item = $(link).parent();
	if(item.hasClass('open')){
		item.removeClass('open');
	}else{
		item.addClass('open');
	}
}
function closeMenuClicked(link){
	var item = $(link).parent();
	item.removeClass('open');
}
</script>


