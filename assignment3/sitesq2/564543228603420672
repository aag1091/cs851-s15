var images = ['toppic1.jpg', 'toppic2.jpg', 'toppic3.jpg', 'toppic4.jpg', 'toppic5.jpg', 'toppic6.jpg', 'toppic7.jpg', 'toppic8.jpg'];
$(document).ready(function(){
	for (var i=0; i<5; i++) {
		var rannum = Math.floor(Math.random() * images.length);
		var imgsrc = "images/"+images[rannum];
		$(".image"+(i+1)).attr("src", imgsrc);
		images.splice(rannum,1);
	}
  /*
	$("#beginnerlink").click(function(){
	$("#beginner").fadeIn("slow");
    $("#expert").hide();
    $("#beginnerlink").hide();
    $("#expertlink").show();
	initialiseAnimatedImages();	
		setMenuCookie('1');
	});
  $("#expertlink").click(function(){
    $("#beginner").hide();
	$("#expert").fadeIn("slow");
    $("#beginnerlink").show();
    $("#expertlink").hide();
	initialiseAnimatedImages();	
		setMenuCookie('2');
