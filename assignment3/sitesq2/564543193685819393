<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_912085").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/912085?view=json", function(json){
				jQuery("#upv_912085").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_912085").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_912085").append("Not available");
				}
			});
		}
	}
</script>
