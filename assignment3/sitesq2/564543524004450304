(function(){

  
  window._sf_async_config={
    uid: '43610',
    domain: 'skysports.com',
    sections: 'type:interactive commentary,live blog,football',
    authors: '',
    useCanonical: true
  };

  
  window._sf_endpt = new Date().getTime();
  var script = document.createElement('script');
  script.src = (document.location.protocol === 'https' ? 'https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/' : 'http://static.chartbeat.com/') + 'js/chartbeat.js';
