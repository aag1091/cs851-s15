    I18n.defaultLocale = "en";
    I18n.locale = "en";

      $(document).ready(function() {
        igg.site.onDocumentReady();
      });

    $.cloudinary.config({"api_key":"754759674861391",
      "cloud_name": "indiegogo-media-prod-cld",
      "cname": ""
    });

      igg.externalService(function() {
    ga('set', 'contentGroup1', 'Home');
  });
  igg.site.YouTube.onReady(function() {
  new igg.site.HomepageVideo({
  source : 'https://www.youtube.com/embed/u4tZWpYTso4?enablejsapi=1&amp;origin=https://www.indiegogo.com',
  el     : $('.js-homepage-video'),
  mobile : false
  });
  });
  new igg.site.CampaignDropdown({el: $('.i-dropdown-mobile')});
  new igg.site.Carousel({el: $(".i-home-featured-carousel"), addDots: true, stretchItemToCarousel: true, onItemShown: function($li) {
    $li.find('.i-home-button').on('click', function() {
      var picture = 'click - ' + $(this).parents('.i-content').siblings('[data-picture]').data('alt');
      igg.externalService(function() {
        ga('send', 'event', 'Hero Button', 'click', picture);
      });
    });
    window.picturefill();
  }}).start();

  new igg.site.CampaignDropdown({el: $('.i-dropdown-mobile')});

  if ($(window).width() >= 768) {
    new igg.site.Carousel({el: $(".i-home-trendingNow"), itemsPerPage: 5, incrementBy: 4, carouselArrowClass: 'black', onItemShown: function($li) {
      $li.on('click', function() {
        var label = $(this).find('.i-title').text();
        var action = 'Homepage Baseball Card - ' + $(this).find('.project-card-with-friend-list').data('slot');
        igg.externalService(function() {
          ga('send', 'event', 'Homepage Baseball Cards (via feed)', action, label);
        });
      });
      igg.site.refreshProjectCard($li.find(".i-project-card"));
    }}).start();
  }

  </script>

  

  

  
