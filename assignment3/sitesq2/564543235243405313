function recscallback(data) {
	cX.library.renderTemplate('cxense-queue-template', 'cxense-queue-target', data);
}

// Function for taking the category name off the heading.
function trimHeading(heading) {
	// Sometimes the heading is a list, not a string.
	if (typeof heading!="string") {
		heading = heading[0]
	}
	return heading.substr(0, heading.indexOf(" - "));
}

var recs_siteid = '9222289450909506884';

var recs_params = {
	p_aq: 'query(og-type:"*article*")',
	p_rs: '{"fl":["click_url","url","heading","dominantimage","description"]}',
	p_callback: 'recscallback',
	media: 'json',
	p_dr: 'contenthash',
	ctx: 'http://www.london24.com/news/missing_sophia_dietrich_15_from_clapham_1_3948072',
	p_c: 5,
	matching_mode: 'trend^100'
}

var recs_url = '//recs.cxense.com/api/recommendation/'+recs_siteid+'?'+jQuery.param(recs_params)+'&_nocache';

cX.library.loadScript(recs_url);


