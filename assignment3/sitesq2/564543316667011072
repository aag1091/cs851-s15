<script type='text/javascript'>
      //<![CDATA[
      jQuery(document).ready(function() {
        $("#back-top").hide();
        // fade in #back-top
        $(function () {
          $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
              $('#back-top').fadeIn();
            } else {
              $('#back-top').fadeOut();
            }
          });
          // scroll body to 0px on click
          $('#back-top a').click(function () {
            $('body,html').animate({
              scrollTop: 0
            }, 800);
            return false;
          });
        });
        $(".feature").click(function() {
          var search_val = this.value;
          var search_str = $(location).attr('href') + "&features[]=" + search_val;
          if(search_val.length > 0 ) {
            window.location = search_str;
          }
        });
        // event search check box (already checked)
        $(".feature_remove").click(function() {
          var search_val = "&features[]=" + this.value;
          var search_str = $(location).attr('href');
          search_str = search_str.replace(search_val, "");
          if(search_val.length > 0 ) {
            window.location = search_str;
          }
        });
        $('#page').bind('click', function(e) {
          e.preventDefault(); 
        });
        $("input.gsc").focus(function() {
          //console.log(this.value.length);
          if(this.value.length == 0 ) {
            $(this).css('background-image', 'none');}
          if(this.value.length != 0 ) { 
            var cx = '017228705479884060727:sv8dskeqtrq';
            window.location = "/p/search.html?cx="+cx+"&q=" + this.value;}
        }).blur(function(){
          if(this.value.length == 0 ) {
            $(this).css('background-image', 'url(http://www.heyyou-app.com/google_custom_search_watermark.gif)');
          }		
        });
        $(".entry input[type=text]").focus(function() {
          if( this.value == this.defaultValue ) {
            this.value = "";
          }
        }).blur(function() {
          if( !this.value.length ) {
            this.value = this.defaultValue;
          }
        });
        $("a.search_toggle").click(function(e) {
          $("div.nav_search").toggle();
          /*alert(this);*/
          e.preventDefault();
          // return false;
          $("input.gsc").focus();$("input.gsc").blur();
        });
   
      });
      //]]></script>
