        jQuery.sharedCount = function(url, fn) {
          url = location.href.split('?')[0];
          //url = encodeURIComponent(url || location.href);
          url = encodeURIComponent(url);
          //console.log(url);
          var arg = {
            data: {
              url : url,
              apikey : "1e1e1ed48929297419b3e9a2c98b45aa8e9eeb51" //API KEY HERE
            },
            url: "//" + (location.protocol == "https:" ? "sharedcount.appspot" : "free.sharedcount") + ".com/",
            cache: true,
            dataType: "json"
          };
          if ('withCredentials' in new XMLHttpRequest) {
            arg.success = fn;
          }
          else {
            url.split('?')[0];
            var cb = "sc_" + url.replace(/\W/g, '');
            window[cb] = fn;
            arg.jsonpCallback = cb;
            arg.dataType += "p";
          }
          return jQuery.ajax(arg);
        };
        sharedCountSingle("");
      });
