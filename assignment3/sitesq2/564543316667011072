<script type='text/javascript'>
      //<![CDATA[
   
      var numposts = 8;
      var showpostthumbnails = true;
      var displaymore = false;
      var displayseparator = true;
      var showcommentnum = false;
      var showpostdate = false;
      var showpostsummary = false;
      var numchars = 0;
      var labeltrends = function() {
        var elem = document.createElement('script');
        elem.src = '/feeds/posts/default/-/Standout?alt=json-in-script&callback=labeltrendy&orderby=published';
        document.getElementsByTagName('head')[0].appendChild(elem);
      };
      var labeltrendy = function(json) {
        var topics = [];
        var html = '<div class="widget-content" style="height:40px;"><ul>';
        total = numposts;
        for(var i=0;i<numposts;i++){
          var entry=json.feed.entry[i];
          var posttitle=entry.title.$t;
          var posturl;
          if(i==json.feed.entry.length)break;
          for(var k=0;k<entry.link.length;k++)
          {if(entry.link[k].rel=='alternate'){
            posturl=entry.link[k].href;break;}}
          var thumburl;
          var post_title_arr = posttitle.split("|");
          var arr = topics.filter( function( el ) {
            return !!~el.indexOf( post_title_arr[1] ); });
          if(!arr[0]){topics.push([post_title_arr[1],posturl]);} }
        for(i=0;i<topics.length;i++){
          html+= '<li><a href="'+topics[i][1]+'" >'+topics[i][0]+'</a></li>';
        }
        html+= '</ul>';
        document.getElementById('PageList1').innerHTML = html;
      };
      labeltrends();
      //]]>
</script>
<script type='text/javascript'>
        //<![CDATA[ 
        function strip_tags(input, allowed) {
          //  discuss at: http://phpjs.org/functions/strip_tags/
          // original by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
          // improved by: Luke Godfrey
          // improved by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
          //    input by: Pul
          //    input by: Alex
          //    input by: Marc Palau
          //    input by: Brett Zamir (http://brett-zamir.me)
          //    input by: Bobby Drake
          //    input by: Evertjan Garretsen
          // bugfixed by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
          // bugfixed by: Onno Marsman
          // bugfixed by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
          // bugfixed by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
          // bugfixed by: Eric Nagel
          // bugfixed by: Kevin van Zonneveld (http://kevin.vanzonneveld.net)
          // bugfixed by: Tomasz Wesolowski
          //  revised by: Rafał Kukawski (http://blog.kukawski.pl/)
          //   example 1: strip_tags('<p>Kevin</p> <br /><b>van</b> <i>Zonneveld</i>', '<i><b>');
          //   returns 1: 'Kevin <b>van</b> <i>Zonneveld</i>'
          //   example 2: strip_tags('<p>Kevin <img src="someimage.png" onmouseover="someFunction()">van <i>Zonneveld</i></p>', '<p>');
          //   returns 2: '<p>Kevin van Zonneveld</p>'
          //   example 3: strip_tags("<a href='http://kevin.vanzonneveld.net'>Kevin van Zonneveld</a>", "<a>");
          //   returns 3: "<a href='http://kevin.vanzonneveld.net'>Kevin van Zonneveld</a>"
          //   example 4: strip_tags('1 < 5 5 > 1');
          //   returns 4: '1 < 5 5 > 1'
          //   example 5: strip_tags('1 <br/> 1');
          //   returns 5: '1  1'
          //   example 6: strip_tags('1 <br/> 1', '<br>');
          //   returns 6: '1 <br/> 1'
          //   example 7: strip_tags('1 <br/> 1', '<br><br/>');
          //   returns 7: '1 <br/> 1'
          allowed = (((allowed || '') + '')
                     .toLowerCase()
                     .match(/<[a-z][a-z0-9]*>/g) || [])
          .join(''); // making sure the allowed arg is a string containing only tags in lowercase (<a><b><c>)
          var tags = /<\/?([a-z][a-z0-9]*)\b[^>]*>/gi,
              commentsAndPhpTags = /<!--[\s\S]*?-->|<\?(?:php)?[\s\S]*?\?>/gi;
          return input.replace(commentsAndPhpTags, '')
          .replace(tags, function($0, $1) {
            return allowed.indexOf('<' + $1.toLowerCase() + '>') > -1 ? $0 : '';
          });
        }
        var titel = document.title;
        var href = location.href;
        var tempArray = titel.split('|');
        var searchNews = function(searchterm) {
          var elem = document.createElement('script');
          elem.src = 'http://ajax.googleapis.com/ajax/services/search/news?v=1.0&q='+searchterm+'&callback=displayNews&hl=en';
          document.getElementsByTagName('head')[0].appendChild(elem);
        };
        var displayNews = function(news) {
          var html = '',
              items = news.responseData.results,
              total = items.length;
          if (total>0) {
            for (var i=0; i<total; i++) {
              var item = items[i];
              /*html+= '<article><a href="'+item.unescapedUrl+'"><h1>'+item.titleNoFormatting+'</h1></a><p>'; */           
              // console.log(item.content);
              item.content = strip_tags(item.content);
              item.content = item.content.split(/\.{3,}/g)[0];  
              item.content = item.content.replace(/ *\([^)]*\) */g, "");      
              /* item.content = item.content.split(/["']?[A-Z][^.?!]+((?![.?!]['"]?\s["']?[A-Z][^.?!]).)+[.?!'"]+/g); */
              /*item.content = item.content.split(/["']?[A-Z][^.?!]+((?![.?!]['"]?\s["']?[A-Z][^.?!]).)+[.?!'"]+/)[0];*/
              //item.content = item.content.match(/["']?[A-Z][^.?!]+((?![.?!]['"]?\s["']?[A-Z][^.?!]).)+[.?!'"]+/g);
              item.content = item.content.match( /[^\.!\?]+[\.!\?]+/ );
              //console.log(item.content[0]);
              // console.log("no:"+i+" - " +item.content);
              //if(item.content.indexOf('.') !== -1){
              //item.content = item.content.split(/(?<=[a-z|0-9][!?.)(?:$|\s+(?=\p{Lu}\p{Ll}*\b))/g)[0];
              //}
              //item.content =  item.content.match(/\(?[A-Z][^\.]+[\.!\?]\)?/g)[0]+' ';
              // item.content = item.content.split(/\b\?{1,}\W{1,}/g)[0]+'? ';
              //  item.content = item.content.split(/\b\!{1,}\W{1,}/g)[0]+'! ';
              /*         
item.content = item.content.split(/\b\.{1,}\W{1,}/g)[0]+'. ';
item.content = item.content.replace(/\s{1,}\W{1,}\s{1,}/g,' ');
*/    
              if (item.content != null) {
                html+= item.content+" ";    
              }
              /* */
              //html+= item.content + ". ";
              /*html+= '</p>';
      html+= '</article>';*/
            }
            html = html.replace(/ *\([^)]*\) */g, "");    
            html = html.replace('--',"");   
            document.getElementById('newsresults').innerHTML = html;
          }
        };
        var term= tempArray[1];
        //if (document.documentElement.clientWidth > 640) {
        searchNews(tempArray[1]);
        //}
        //]]>
</script>
