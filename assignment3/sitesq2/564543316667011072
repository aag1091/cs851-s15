                      var title=document.title;
                      var keyword=title.split("|")[1];
                      var stats='';
                      var values = [];
                      google.setOnLoadCallback(drawChart);
                      var rows;
                      var total;
                      function drawChart() {
                        var query = new google.visualization.Query(
                          'http://www.google.com/trends/fetchComponent?hl=en-US&cmpt=geo&cid=TOP_ENTITIES_0_0&export=3&w=300&h=20&q='+keyword);
                        query.send(handleQueryResponse);
                        //  console.log(query);
                      }
                      var values = [];
                      function handleQueryResponse(response) {if (response.isError()) {   /* alert('Error in query: ' + response.getMessage() + ' ' + response.getDetailedMessage());return;  */}
                                                              var data = response.getDataTable();
                                                              var array1 = JSON.stringify( data );
                                                              var jsonObj = JSON.parse(array1);
                                                              rows = $.parseJSON(jsonObj);
                                                              total = rows['rows'].length;
                                                              stats+= "<dl style='width: 300px;margin:0px;'>";
                                                              for (i=0;i< rows['rows'].length;i++)
                                                              {
                                                                //  console.log(rows['rows'][i]['c'][2]['v']);
                                                                var query =  rows['rows'][i]['c'][2]['v'].split('+-+')[0];
                                                                //console.log(query);  
                                                                query = query.replace(/\#/g,'');
                                                                query = query.replace(/%22/g,'');
                                                                query = query.replace(/\+/g,' ');
                                                                query = query.replace('&cmpt=geo','');
                                                                stats+="<span style='width:100%'><a href='/p/search.html?"+ query +"' target='_blank'>";
                                                                stats+="<div id='data-"+i+"' class='bar' style='width:100%'>"+rows['rows'][i]['c'][0]['v'].substring(0,54); //split('+-+')[0];
                                                                stats+="<span style='float:right'>"+rows['rows'][i]['c'][1]['v']+"%</span></a>";
                                                                stats+="<div style='border-bottom:3px solid #3f85f2;width: "+rows['rows'][i]['c'][1]['v']+"%;'></div>";
                                                                stats+="</div></span>";
                                                              }
                                                              stats+="</dl>";
                                                              document.getElementById('datastats').innerHTML = stats;
                                                             }
                      /*
