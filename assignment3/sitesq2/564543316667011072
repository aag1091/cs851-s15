                    var q;var title=document.title;var titleArr=title.split(" | ");
                    q = getUrlVars()["q"];
                    //document.write(titleArr[0]);
                    if(titleArr[1] !== undefined){document.write('<h2>'+titleArr[1]+' Rising searches</h2>');}
                                                                 //if(q !== undefined){q=decodeURIComponent(q);document.write('');}</script>
</div>
<center>
<div id='datastats2' style='float: right; width: 100%;margin:13px 0px 0px 0px;'>
<script type='text/javascript'>
                      //<![CDATA[
                      var title2=document.title;
