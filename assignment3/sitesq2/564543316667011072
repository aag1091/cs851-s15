                      var rows2;
                      var total2;
                      var value;var value2;
                      function drawChart2() {
                        var query2 = new google.visualization.Query(
                          'http://www.google.com/trends/fetchComponent?hl=en-US&tz&cmpt=q&content=1&cid=RISING_QUERIES_0_0&export=3&w=300&h=420&q='+keyword2);
