<script language="javascript">

// Check to see if user was viewing interactive map on last visit
var x, y, iMapcookies = document.cookie.split(";");
for (i = 0; i < iMapcookies.length; i++) {
    // after the 1st cookie, the name has a space as the 1st character
    if (i == 0) {
        x = iMapcookies[i].substr(0, iMapcookies[i].indexOf("="));
    }
    else {
        x = iMapcookies[i].substr(1, iMapcookies[i].indexOf("=") - 1);
    }
    y = iMapcookies[i].substr(iMapcookies[i].indexOf("=") + 1);
    if (x == 'serviceUpdates' && y != 'off') {
	//	alert(y);
        toggleAlertsTab(document.getElementById(y), y);
    }
}
</script>


