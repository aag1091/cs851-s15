    var path = '/rider_tools/transit_updates';
    var definedMode = '';

    // Used for setting zoom and center on GMap
    var routeCookie = definedMode;

    function loadGoogleMaps() {
        checkloadjscssfile("/lib/css/station_popup.css", "css");
        //dynamically load and add this .js file
        checkloadjscssfile("https://maps.googleapis.com/maps/api/js?v=3&client=gme-massachusettsbay&sensor=false&callback=loadGoogleLibs", "js");
    }
    function loadGoogleLibs() {
        checkloadjscssfile("http://google-maps-utility-library-v3.googlecode.com/svn/tags/infobox/1.1.9/src/infobox_packed.js", "js");
        var loadMbtaScriptsCheckInterval = 60; //tries for 30 seconds
        var loadMbtaScriptsCheck = setInterval(function(){
            if (typeof InfoBox === 'function'){
                clearInterval(loadMbtaScriptsCheck);
                loadMbtaScripts();
            }
            //Interval termination conditions
            if (loadMbtaScriptsCheckInterval < 1 || loadMbtaScriptsCheckInterval > 60){
                clearInterval(loadMbtaScriptsCheck);
                console.log('Map loading failed: Stopped after ' + (60 - loadMbtaScriptsCheckInterval) +  ' attempts (2/second).');
            } else {
                loadMbtaScriptsCheckInterval--;
            }
        }, 500);
    }
    function loadMbtaScripts () {
    //Load scripts required for the page
    
    }
    
    //http://www.javascriptkit.com/javatutors/loadjavascriptcss.shtml
    var filesadded=""; //list of files already added

    function checkloadjscssfile(filename, filetype, callback){
        if (filesadded.indexOf("["+filename+"]")==-1){
            loadjscssfile(filename, filetype, callback);
            filesadded+="["+filename+"]"; //List of files added in the form "[filename1],[filename2],etc"
        }
    }
    function loadjscssfile(filename, filetype, callback){
        if(filetype=="js"){ //if filename is a external JavaScript file
            var fileref=document.createElement('script');
            fileref.setAttribute("type","text/javascript");
            fileref.setAttribute("src", filename);
        }
        else if (filetype=="css"){ //if filename is an external CSS file
            var fileref=document.createElement("link");
            fileref.setAttribute("rel", "stylesheet");
            fileref.setAttribute("type", "text/css");
            fileref.setAttribute("href", filename);
        }
        if (typeof fileref!="undefined"){
            document.getElementsByTagName("head")[0].appendChild(fileref);
        }
        if (typeof callback !== "undefined"){
            callback();
        }
    }
    function initMbtaMaps (){
        var countdown = 60; //tries for 30 seconds
        var scripts_loaded = setInterval( function (){
            var pre_countdown = countdown;
            if (false){

                mbta.routes.getRoutesByMode(definedMode);
                setTimeout(function (){if(typeof mbta.maps.showMap === 'function'){mbta.maps.showMap(definedMode);}}, 300);

                clearInterval(scripts_loaded);
            }
            if (countdown < 1 || countdown > 60){
                clearInterval(scripts_loaded);
                console.log('Map loading failed: Stopped after ' + pre_countdown +  ' attempts (2/second).');
            } else {
                countdown--;
            }
        }, 500);
    }
