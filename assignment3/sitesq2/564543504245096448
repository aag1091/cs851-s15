<script type="text/javascript">
        (function($){
          $(document).ready(function(){

            Reverb.Facebook.initialized().done(function() {
              try{
                $('.js-rsvp-link').fbGoing();
              }catch(e){
                Reverb.error(e);
                // h[k.attr("data-fb-event-id")] is undefined
              }
            });


            var setMarkerIcon = function(marker, color){
              marker.setIcon("//maps.google.com/mapfiles/ms/icons/"+color+".png")
            }, last_showed_item

            $("#artist_profile_shows_map_more").bind("click", function(e){
              e.preventDefault()
              if( last_showed_item ){
                var address = last_showed_item.attr("data-addr"), lat = last_showed_item.attr("data-lat"), long = last_showed_item.attr("data-long")
                window.open("//maps.google.com/?q="+address+"&sll="+[lat, long].join(","), '_blank')
              }
            })

            $("#artist_profile_shows_container").flyout("li")

            var has_shown_google_map = false;
            function show_google_map() {
              if (has_shown_google_map) return;
              has_shown_google_map = true;
              Reverb.GMaps.ready(function(maps) {
                var selector = "li[data-long][data-lat]", cnt = $("#artist_profile_shows_container"), markers = {}

                var map = new maps.Map( document.getElementById("artist_profile_shows_map") , {
                  center: new maps.LatLng(0,0),
                  mapTypeId: maps.MapTypeId.ROADMAP,
                  zoom: 10
                })

                var lats = [], longs = [], lat_sum=0, long_sum=0

                var items = cnt.find(selector).each(function(){
                  var self = $(this).identify(), lat = parseFloat(self.attr("data-lat")), long = parseFloat(self.attr("data-long"))
                  var marker = new maps.Marker({
                    position: new maps.LatLng(lat, long)
                  })
                  markers[self.attr("id")] = marker
                  marker.setMap(map)
                  setMarkerIcon(marker, "orange")
                  lats.push(lat)
                  longs.push(long)
                  lat_sum += lat
                  long_sum += long
                })



                var showMarkerOnMap = function(item){

                  if(last_showed_item){
                    setMarkerIcon(markers[last_showed_item.attr("id")], "orange")
                    last_showed_item.removeClass("active")
                  }

                  last_showed_item = item
                  setMarkerIcon(markers[item.attr("id")], "green")

                  map.setCenter( new maps.LatLng( item.attr("data-lat"), item.attr("data-long")) )
                  map.setZoom(15)
                  $("#artist_profile_shows_map_more").css("visibility","visible")
                }

                var selectItem = function(item){
                  if(!item.hasClass("active")){
                    item.addClass("active")
                    showMarkerOnMap(item)
                  }
                }

                var hoverTimeout

                cnt.delegate(selector+":not(.active)", "mouseover", function(){
                  var self = $(this)
                  window.clearTimeout(hoverTimeout)
                  hoverTimeout = window.setTimeout(function(){
                    self.addClass("active")
                    showMarkerOnMap(self)
                  }, 300)
                }).bind("mouseout", function(){
                  window.clearTimeout(hoverTimeout)
                })

                if(items.length > 0){

                  var lat = lat_sum/lats.length, long = long_sum/longs.length, math = Math
                  if(items.length > 1 && !isNaN(lat) && !isNaN(long)){
                    var bounds = new maps.LatLngBounds( new maps.LatLng(math.min.apply(math, lats), math.min.apply(math, longs) ), new maps.LatLng(math.max.apply(math, lats), math.max.apply(math, longs) ) )
                    map.fitBounds( bounds )
                  }else{
                    map.setZoom(15)
                  }
                  if (!isNaN(lat) && !isNaN(long)) {
                    map.setCenter( new maps.LatLng(lat, long) )
                  }

                }

              })
            }

            if (navigator && navigator.userAgent && navigator.userAgent.match && navigator.userAgent.match(/googlebot/i)) {
              $('#artist_profile_shows_map, #artist_profile_shows_map_more').hide();
            } else {
              $('#artist_profile_shows_map').appear();
              $('#artist_profile_shows_map').on('appear', function() {
                show_google_map();
              });
            }
          })
        })(jQuery)

      </script>
