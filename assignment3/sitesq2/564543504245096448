  jQuery(function(){
    jQuery( document ).ajaxSuccess(function( event, request, settings ) {
      registerNewSplitTests(request.getResponseHeader('RN-NEW-SPLIT-TESTS'));
    })
    Ajax.Responders.register({  onComplete: function(ev, request) {
      registerNewSplitTests(request.getHeader('RN-NEW-SPLIT-TESTS'));
    }});

    function registerNewSplitTests(new_split_tests){
      if(!new_split_tests)
        return
      try{
        Reverb.Mixpanel.register(jQuery.deparam(new_split_tests));
      }catch(err){
        logTime("Error parsing new split test"+new_split_tests);
      }
    }
  })
</script>


