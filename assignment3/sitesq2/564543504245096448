<script type="text/javascript">
  function load_retargeting_pixels() {
    if (window.Reverb && Reverb.include_pixel) {
      drop_retargeting_pixels();
    } else {
      jQuery.getScript("/javascripts/tracking/retargeting_pixels.js").done(drop_retargeting_pixels);
    }
    
    function drop_retargeting_pixels() {
      if (Reverb.log) {
        Reverb.log("Dropping pixels: 630210, 405193, 465567");
      }

      setTimeout(function() {Reverb.include_pixel("49767", "artist_1128625_pixel_for_direct_fan", "&lt;!-- Segment Pixel - artist_1128625_pixel_for_direct_fan - DO NOT MODIFY --&gt;&lt;script src=&quot;http://ib.adnxs.com/seg?add=630210&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");}, 100);
      setTimeout(function() {Reverb.include_pixel("21705", "genre_pixel_for_hip_hop", "&lt;!-- Segment Pixel - genre_pixel_for_hip_hop - DO NOT MODIFY --&gt;&lt;script src=&quot;http://ib.adnxs.com/seg?add=405193&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");}, 200);
      setTimeout(function() {Reverb.include_pixel("29474", "genre_pixel_for_hip_hop", "&lt;!-- Segment Pixel - genre_pixel_for_hip_hop - DO NOT MODIFY --&gt;&lt;script src=&quot;http://ib.adnxs.com/seg?add=465567&amp;t=1&quot; type=&quot;text/javascript&quot;&gt;&lt;/script&gt;&lt;!-- End of Segment Pixel --&gt;");}, 300);
    };
  };
  
  jQuery('#pixel_auto_load_marker_49767_21705_29474').ready(load_retargeting_pixels);
</script>
