    rcel.src = "//api.revcontent.com/respond/serve.js.php?t="+rcel.id+"&c="+(new Date()).getTime()+"&width="+(window.outerWidth || document.body.clientWidth)+"&"+rc;
    rcel.async = true;
    var rcds = document.getElementById("rcjsload_4aa9ec"); rcds.appendChild(rcel);
})();
</script>
</div>
