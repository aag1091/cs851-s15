<script>
	var tribune_get_rtp_targeting = function ( ad_slot, mode ) {
		if ( 'undefined' === typeof rtp || 'undefined' === typeof rtp[ad_slot] )
			return '';

		switch ( mode ) {
			case 'key':
				return 'tier';

			case 'value':
				return parseInt( rtp[ad_slot].tier );

			default:
				return '';
		}
	}
</script>
