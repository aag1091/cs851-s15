        <script type="text/javascript">
            var ua = navigator.userAgent.toLowerCase();
            var isAndroid = ua.indexOf("android") > -1; //&& ua.indexOf("mobile");
            var isIpad = ua.indexOf("ipad") > -1; //&& ua.indexOf("mobile");
            var isIphone = ua.indexOf("iphone") > -1; //&& ua.indexOf("mobile");
            var isIpod = ua.indexOf("ipod") > -1; //&& ua.indexOf("mobile");
            var isAppletv = ua.indexOf("appletv") > -1; //&& ua.indexOf("mobile");
			
			var isAndroidMobile = ua.indexOf("android") > -1 && ua.indexOf("mobile");
			
			if(isAndroid) {    window.location = 'androidTablet.html'; }
			if(isAndroidMobile) {    window.location = 'android.html'; }

            //if(isAndroid) {    window.location = 'android.html'; }
			
			
            if(isIphone || isIpod || isAppletv) {    window.location = 'ios.html'; }
            if(isIpad) {    window.location = 'ipad.html'; }
        </script>

