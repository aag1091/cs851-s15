<script type="text/javascript">

var known = $.cookie('__siren') ? '1' : '0' ;

function filterArray(arr) {
    var retArr = [];
    $.each(arr, function(i, part) {
        if (part !== '') {
            retArr.push(part);
        }
    });

    return retArr;
}

function addToTargeting(targeting, data) {

    $.each(data, function (key, value) {
        if (!value) {
            return;
        }
        targeting[key] = value;
    });
    return targeting;
}


