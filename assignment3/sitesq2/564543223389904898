(window.AmazonUIPageJS ? AmazonUIPageJS : P).when('cf').execute(function() {
  (window.AmazonUIPageJS ? AmazonUIPageJS : P).load.js('http://z-ecx.images-amazon.com/images/G/01/AUIClients/BooksDetailPageJavascript-7f9baa002df2662a06ce6de1ad826b570113af4b.min._V2_.js');
