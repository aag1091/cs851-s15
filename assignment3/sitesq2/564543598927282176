         if (w.xtparam === undefined || w.xtparam === null) {
            w.xtparam = "";
         }
         w.xtparam += "&ac=" + w.xt_ac + "&an=" + w.xt_an + w.xt_multc;
         loadJs(lmd.conf.medias.location.base_url_version + "/js/lib/xiti/4.4.007/xtclicks.js", loadCore);
      }
   };
})(this);

