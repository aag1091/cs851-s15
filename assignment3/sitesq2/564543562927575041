    <script>
        $(function() {
            var oAuthToken = $.cookie('tncms-authtoken');
            var oServiceToken = $.cookie('tncms-services');
            if (oAuthToken){
                $(".authorized-user").show();
                $(".unauthorized-user").hide();
                if (oServiceToken){
                    $(".user-no-services").hide();
                    $(".user-has-services").show();
                }else{
                    $(".user-no-services").show();
                    $(".user-has-services").hide();
                }
            } else{
                $(".authorized-user").hide();
            }
        });
    </script>


    
        

