    <script>
  // Sticky share links for online deal show
  $(function(){
    $(document).scroll(function () {
      var y = $(document).scrollTop();
      var onlineShareLinks = $('.share-links');

      if (y > 214) {
        onlineShareLinks.addClass("sticky");
      } else {
        onlineShareLinks.removeClass("sticky");
      }
    });
  });

  // Behaviour for right rail buy box
  (function ($) {
    var dataKey = "tipTimeout";
    var tipSelector = ".tip";
    $.fn.tip = function () {
      this.each(function () {
        $(this).
          on("mouseenter",function () {
            var target = $(this);
            target.
              find(tipSelector).
                show();
            clearTimeout(target.data(dataKey));
            target.removeData(dataKey);
          }).
          on("mouseleave",function () {
            var target = this;
            $(this).
              data(dataKey,setTimeout(function () {
                $(target).
                  find(tipSelector).
                    hide();
              },300));
          }).
          on("click",".close",function () {
            var target = $(this);
            target.
              closest(tipSelector).
                hide();
            clearTimeout(target.data("tipTimeout"));
            target.removeData(dataKey);
          });
      });
    };
  })(jQuery);

  $(".deals_online_ab .quick-and-easy-refunds").
    tip();

  $(".ls-rail").
    on("click.slideout",".deals_online_ab .shipping",function (event) {
      event.preventDefault();
      var slideout = $(this).
        find(".slideout");

      if (!slideout.is(":visible")) {
        slideout.
          slideDown();
      } else {
        slideout.
          slideUp();
      }
    });
</script>



    <script type="text/javascript" charset="utf-8">
$(function() {
  window.CommissionJunction = {
    hide_coupons: function(){
      if (this.cj_ref_code_matches()) {
        $('[href$="/coupons"]').parent().hide();
      }
    },
    cj_ref_code_matches: function() {
      var cookie, ref_code;
      cookie = document.cookie.match(/ref_code=([^;]*)/);
      ref_code = cookie ? cookie[1] : "";
      return ref_code.match(/^XUA0000COMJ000000000000ENUS0000000000000021052012/);
    }
  }
  CommissionJunction.hide_coupons();
});
</script>

