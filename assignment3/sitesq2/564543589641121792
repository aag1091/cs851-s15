  Number.prototype.formatted = function(delimiter, separator) { //

    var string_with_desired_precision = function() {
      // There are cents. Format (and ROUND) accordingly.
      if(this % 1 > 0) { return this.toFixed(2) }
      else             { return this.toFixed(0) }
    }

    if (!separator) { separator = "."; }
    var number = string_with_desired_precision.call(this), delimiter = delimiter || ',';
    var split = number.split('.');
    split[0] = split[0].replace( /(\d)(?=(\d\d\d)+(?!\d))/g, '$1' + delimiter );
    return split.join(separator);
  };

  Deal = {
    id: 1388538,
    value: "\u00a335.99",
    option_price: 10.0,
    preview: false,
    options_count: 1,
    max_purchases: 3,
    locale: "en-GB",
