    <script src="https://a1.lscdn.net/deals/system/javascripts/urgency-b2e2d7d8873e79421bc4ea79bd7fb4ce.js" type="text/javascript"></script>
    <script>
      (function(){
        function showUrgency() {
          // Show urgency if eligible

          function getDuration(){
            // TODO pull this info from setupData
            if (storage.data && storage.data.duration) {
              return storage.data.duration;
            } else {
              return 10*60*1000; // 10 min
            }
          }

          function getContainerHeight(){
            var dealUrgencyHeight = $('#deal-urgency-header').outerHeight(),
            dealDetailsHeight = $('#deal-details').height(),
            containerHeight = ( dealUrgencyHeight > dealDetailsHeight ? dealUrgencyHeight : dealDetailsHeight );
            return containerHeight;
          }

            function setupUrgency(){
              $('#ls-wrap').addClass('deal-urgency');
              $("#urgency-pricing").hide().fadeIn();
              $("#pricing").fadeOut();
              $(".ls-deal_buy_box .ls-deal_buttons").fadeOut();
              $(".ls-deal_buy_box .urgency-buy-buttons").hide().fadeIn();

              $('#ls-wrap').addClass('deal-urgency');
              var timer = new CountdownTimer(storage.data.start, getDuration());
              var dealDetailsHeight = $('#deal-details').height();
              var beforeHeight = $('#deal-urgency-header').height();
              trackWithGAQ();
              // set shipping truck image to white
              $('.truck').attr('src', '//a5.lscdn.net/imgs/633c00c8-f058-4b7a-ae51-da5bd9e46a8e/q100_.png');
              $('.urgency-banner,#deal-urgency-content, #promo-message').hide().slideDown(500);
              $(".deals_online_ab .shipping").tip();

              $(timer).on('counter_zero',function(){
                teardownUrgency();
              });
            }
            function teardownUrgency(){
              window.setTimeout(function(){
                $("#urgency-pricing").fadeOut();
                $("#pricing").hide().fadeIn();
                $('#ls-wrap').removeClass('deal-urgency');
                $('#deal-details-container-v2').animate({
                  height: _conditionalHeight()
                },500)
                $("#deal-details").fadeIn(500);
                // set shipping truck image back to normal color
                $('.truck').attr('src', '//a5.lscdn.net/imgs/28505de1-9cd9-410e-9609-67ed85d4330d/q100.gif');
                $('.urgency-banner,#deal-urgency-content, #promo-message').fadeOut(500,function(){
                  $(this).remove();
                });
              },500);
            }

          function trackWithGAQ(){
            dataLayer.push({
              'event': 'urgency',
              'event-category': 'Deal',
              'event-action': 'deals/show',
              'event-label': 'Urgency Promotion',
              'pageurgency': 'true'
            });
            _gaq.push(['_trackEvent', 'Deal', 'deals/show', 'Urgency Promotion']);
          }

          function _conditionalHeight() {
            var sfl =  $('#save-for-later'),
                details = $('#deal-details').height();

            if (sfl.length > 0) {
              return details + 17;
            } else {
              return details;
            }
          }

          var storageName = "livingsocialUrgency"
          var setupData = $('#urgency-setup-data').data();
          if (setupData.validUrgencyDeal) {
            $.ajax({
              url: "/api/v4/enticements/urgency_v2/available.json?offer_id=" + setupData.dealId + "&id_space=VOUCHER_OFFER",
              type: 'GET',
              dataType: 'json',
              success: function(res) {
                var storage = window.storage = new UrgencyStore(storageName, setupData.dealId);

                // reset deal id and on-screen timer if previous deliveries have expired and this is a new delivery
                if (res.available) {
                  storage.set({ id: setupData.dealId, start: new Date().getTime(), duration: setupData.urgencyDuration});
                }

                if(storage.data && new Date().getTime() < storage.data.start + getDuration()){
                  window.setTimeout(setupUrgency,3000);
                  $('.dfp-promo-banner').remove();
                }
                // Hide the Urgency banner on top the deal image after 3 seconds.
                $('#deal-urgency-content').on('click',function(e){
                  e.preventDefault();
                  $('#deal-urgency-content').fadeOut(500);
                })
              }
            })
          }
        }

        $(function(){
          showUrgency();
        });

        }());
    </script>
  <script type="text/javascript">
  (function() {
    var sharePurchaseData = function(data) {
      var data = data.body;
      var $deal_meta = $("#share_meta_data");
      var deal_id = $deal_meta.data("deal-id");
      var purchases = [];
      if (data && data.deals && data.deals[deal_id]){
      var purchases = data.deals[deal_id];
      }
      var best_matching_purchase = function() {
        if(purchases.length == 0){
          return false;
        }
        var best_count, best_index = 0;
        for(var idx=0; idx < purchases.length; ++idx) {
          var ref_count = purchases[idx][2];
          if (ref_count > best_count) { 
            best_count = ref_count;
            best_index = idx;
          }
        }

        return purchases[best_index][0];
      }

      if (best_matching_purchase()) { 
        $deal_meta.data("purchase-id", best_matching_purchase());
      }
    }

    $(function() { MeInfo.bind(sharePurchaseData); }); 
  })();
  </script>
