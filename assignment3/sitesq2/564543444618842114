					<script>
					jQuery(document).ready(function(){
						
						var theMenu = jQuery('.site-header');
						var menuHeight = theMenu.height();
						
						stickyMenu( menuHeight );
						
						jQuery(window).scroll(function(){
							stickyMenu( menuHeight );
						});
						
						jQuery('.site-header .search').click(function(){
							jQuery('.site-header .search').removeClass('active');
							jQuery(this).toggleClass('active');
							jQuery('.site-header .search-wrap').toggle();
						});
					
						jQuery(document).click(function(){
							jQuery('.site-header .search-wrap').hide();
							jQuery('.site-header .search').removeClass('active');	
						});
					
						jQuery('.site-header .search-wrap, .site-header .search').click(function(e){
							e.stopPropagation();
						});
					
					});
					
					function stickyMenu( a ){
						var windowTop = jQuery(window).scrollTop();					
						if( windowTop > a ){
							jQuery('.site-header').addClass('sticky');
						}else{
							jQuery('.site-header').removeClass('sticky');
						}
					}
