b_simple_weekdays: ['Mo','Tu','We','Th','Fr','Sa','Su'],
b_simple_weekdays_for_js: ['Mon','Tue','Wed','Thu','Fri','Sat','Sun'],
b_long_weekdays: ['Monday','Tuesday','Wednesday','Thursday','Friday','Saturday','Sunday']
}
