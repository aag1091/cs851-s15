<script type="text/javascript">

    var banners = ["\u003cdiv class=\"celeb_ttl\"\u003e\u003cp\u003e\u003ca href=\"/celeb\"\u003eCeleb Event\u003c/a\u003e\u003c/p\u003e\u003c/div\u003e\u003cdiv class=\"banner\"\u003e\u003ca href=\"/celeb\" \u003e\u003cimg border=\"0\" src=\"http://j3.gatewayinterface.com/App_Themes/Celebs/sashaheart555/template2/celebeventcontent_offline_2_left.jpg?v=0104\" alt=\"\"/\u003e\u003c/a\u003e\u003c/div\u003e"];
    var items = banners.length;
    var counter = 0;
   
    $(document).ready(function () {

        if (items == 0) {
            $(".celeb").html("");
            $(".celeb").hide();
        }
        else {
            StartBanner();            
            if (items > 1) {
                setInterval("selectBanner()", 5000);
            }
        }   
    });


    function StartBanner() {
        $('.celeb').html(banners[counter++]);
        $(this).fadeIn();
    }

    function selectBanner() {

        if (counter == items)
            counter = 0;


        $(".banner").fadeOut("slow", function() {
            $('.celeb').html(banners[counter++]);
            $(this).fadeIn();

        });

    }

</script>
    

