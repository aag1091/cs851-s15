    <script>
        var pageTracker;
        var analyticsId='UA-44679150-1';
      
        var _gaq = _gaq || [];
            _gaq.push(['_setAccount', analyticsId]);
            _gaq.push(['_trackPageview']);
            _gaq.push(['_trackPageLoadTime']);


            (function() {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
       

        function TrackEvent(category, action, label) {
            _gaq.push(['_setAccount', analyticsId]);
            _gaq.push(['_trackEvent', category, action, label]);
        }

        ///sets the sent event first before all binded events stack !!! only works with CLICK events
        ///adds delay of 100 ms to a second event to let the analitycs to send an event 
        $.fn.bindFirst = function (name, fn) {
            try {



                var handlers = this.data('events')[name.split('.')[0]];
                var handlersArray = new Array();

                $.each(handlers, function (index, handler) {
                    handlersArray.push(handler.handler);
                });

                this.unbind('click');
                this.bind(name, fn); // bind first event;

                var $this = this;
                $.each(handlersArray, function (index, item) {
                    if (index == 0) {
                        $this.click(function () {
                            setTimeout(function () {
                                item();
                            }, 100);// adds  delay -  to analytics to send request
                        });
                    }
                    else {
                        $this.click(function () { item(); });
                    }
                });
            } catch (e) {

            }
        };


    </script>




