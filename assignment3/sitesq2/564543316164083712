<script type="text/javascript">
  
    function SendMobileEmail() {
        var email = $("#qr_email").val();
        var EmailFormat = new RegExp(/^([a-z0-9]+(?:(\.|_|-)[a-z0-9]+)*@(?:[a-z0-9]+(?:(-)[a-z0-9]+)?\.)+[a-z0-9](?:[a-z0-9]*[a-z0-9])?)$/i);
        if (!EmailFormat.test(email)) {
            $("#ErrorMobileEmail").show();
            return;
        }
        $.ajax({
            url: '/Services/GeneralData/qrcode.ashx?cmd=sendmail&email=' + encodeURIComponent(email),
            success: function (data) {
                if (data == "OK") {
                    var emailsCounter = $.cookie('NumEmailsSent') == null ? 1 : parseInt($.cookie('NumEmailsSent'))+1;
                    $.cookie('NumEmailsSent', emailsCounter, { expires: 1, path: '/' });
                    
                    var QRformHtm = $('.QRform').html();
                    var beforeHtml = $('#popUpDefult').html();
                    var afterHtm = $('#AfterEmailSend2').html();
                    $('#popUpDefult').html(afterHtm);
                    if (emailsCounter < 3) {
                        $('.QRform').html('<div class="sndAgain">' + $("#AfterEmailSend").html() + '</div>');
                    } else {
                        $('.QRform').html('');
                       // $('#popUpDefult').append(EmailLimitReached);---todo: not yet requeired but ready if it will be. need to create the resource.
                    }
                    //openMobilePopup();
                    $('.sndAgain').click(function () {
                        $('#popUpDefult').html(beforeHtml);
                        $('.QRform').html(QRformHtm);
                    });
                }
            }
        });

    }

    function openMobilePopup() {
        var qr_url = '/Services/GeneralData/qrcode.ashx?url=' + escape('http://?wid=126014142915&linkid=0');
        if ($(".QRcodePopup #popUpDefult img").length == 0)
            $(".QRcodePopup #popUpDefult").append('<img class="QRcodeImg" alt="" src="' + qr_url + '"/>');
        $(".QRcodePopup").modal(
            {
                closeHTML: ('<span></span>'),
                overlayId: 'forgetpass-overlay',
                containerId: 'step24-container',
                onClose: function () {
                    $.modal.close();
                }
            });
        $(".QRclose").show();
    }

    $(document).ready(function () {
        // use document.location.pathname instead of document.location.href to prevent wrong html rendering in all domains starting from http://mobile...
        if (document.location.pathname.indexOf('/mobile') > -1) {
            var qr_url = '/Services/GeneralData/qrcode.ashx?url=' + escape('http://?wid=126014142915&linkid=0');
            if ($(".QRcodePopup #popUpDefult img").length == 0)
                $(".QRcodePopup #popUpDefult").append('<img class="QRcodeImg" alt="" src="' + qr_url + '"/>');
            $(".QRcodePopup").show();
        }
        $(".QRclose").click(function () {
            $.modal.close();
            $(".QRform").next().remove();
        });
        var emailsCounter = $.cookie('NumEmailsSent') == null ? 1 : parseInt($.cookie('NumEmailsSent')) + 1;
        if (emailsCounter >= 3) {
            $('.QRform').html('');
        }
    });

</script>

