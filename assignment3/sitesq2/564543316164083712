        {
            hlLng = val;
            hlPagerMin = 0; hlPagerMax = 0; hlDirection = 0; hlShift = 0; hlPage = 1;
            if (!reset_all)
            {
                getHLPage();
            }
            if (val == 0) text = '';
            setLangNiche(text);
        });
