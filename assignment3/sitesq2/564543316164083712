<script type="text/javascript">
    var newulv3_ids;
    var isForgotPassDivShow = false;
    var tabsByFeatureLoginParams = $("#TabsByFeatureLoginParams");

    $(function ()
    {
        if (tabsByFeatureLoginParams.length > 0)
        {
            isForgotPassDivShow = $("#TabsByFeatureLoginParams").data("is-forgot-pass-div-show") == true;
        }

        if (isForgotPassDivShow)
        {
            $('#lnkForgotPassword').click(function (e)
            {
                e.preventDefault();
                $(".forgotPass").show();
            });

            $('#emailForgotPassInput').first().keyup(function ()
            {
                var $email = this.value;
                if (checkEmail($email))
                {
                    $('#emailForgotPassButton').removeAttr("disabled");
                }
                else
                {
                    $('#emailForgotPassButton').attr("disabled", true);
                }
            });

            $('#emailForgotPassButton').click(function (e)
            {
                e.preventDefault();

                var emailAddress = $("#emailForgotPassInput").val();
                if (emailAddress.trim().length > 0)
                {
                    $('#emailForgotPassButton').attr("disabled", true);
                    $.ajax({
                        type: "POST",
                        url: "/webapi/Login/SendEmail",
                        dataType: 'json',
                        contentType: 'application/json',
                        data: JSON.stringify({
                            emailAddress: emailAddress
                        }),
                        success: function (data, response)
                        {
                            if (response == "success")
                            {
                                if (data == "Email sent")
                                {
                                    $("#emailForgotPassErrorDiv").hide();
                                    $("#emailForgotPassSuccessDiv").fadeIn();
                                }
                                else if (data == "Email Not Found")
                                {
                                    $("#emailForgotPassErrorDiv").fadeIn();
                                    $('#emailForgotPassButton').removeAttr("disabled");
                                }
                                else
                                {
                                    //if there is error, print the message here
                                }
                            }
                            else
                            {
                                $('#emailForgotPassButton').removeAttr("disabled");
                            }
                        },
                        error: function (response)
                        {
                            $('#emailForgotPassButton').removeAttr("disabled");
                        }
                    });
                }
            });
        }
        else
        {
            $('#lnkForgotPassword').click(function (e)
            {
                e.preventDefault();
                $.modal('<div class="forgotPassContainer" style="border:solid 2px white;"><iframe src="/forgotpassword/?modal=1" id="iframeForgotPass" name="iframeForgotPass" height="250" width="319" scroll="0" scrolling="no" frameborder="0" style="border:0" /></div>',
        {
            closeHTML: ("<a class='modalCloseImg' title='Close'></a>"),
            position: ["20%", "40%"],
            overlayId: 'forgetpass-overlay',
            onOpen: function (dialog)
            {
                dialog.overlay.fadeIn('fast', function ()
                {
                    dialog.container.show();
                    dialog.data.slideDown('normal');
                });
            }
        });
            });
        }


        GetFieldsVals();
        $(newulv3_ids.username)
            //.watermark({ watermarkCssClass: "userNameInputs t-x-t-w" })
            .keyfilter(/[a-z0-9]/i, submitFrm);
        $(newulv3_ids.password)
            //.watermark({ watermarkCssClass: "t-x-t-w" })
            .keyfilter(/[A-Za-z0-9!@#$%^&*()_+\-=\[\]{};':&quot;\\|,.<>\/?]/i, submitFrm);

        $('#id-ForgotPassword').click(function (e)
        {
            e.preventDefault();
            $.modal('<div class="forgotPassContainer"><iframe src="/forgotpassword/?modal=1" id="iframeForgotPass"  name="iframeForgotPass" height="250" width="319" scroll="0" scrolling="no" frameborder="0" style="border:0" /></div>',
            {
                closeHTML: ("<a class='modalCloseImg' title='Close'></a>"),
                position: ["20%", "40%"],
                overlayId: 'forgetpass-overlay',
                onOpen: function (dialog)
                {
                    dialog.overlay.fadeIn('fast', function ()
                    {
                        dialog.container.show();
                        dialog.data.slideDown('normal');
                    });
                }
            });
        });
        //$('input[id*="UpperLogin_txtTabsUsername_watermark"]').click(function (e) { $(newulv3_ids.username).focus(); });
        // $('input[id*="UpperLogin_txtTabsPassword_watermark"]').click(function (e) { $(newulv3_ids.password).focus();  });

    });


    function checkEmail()
    {
        var emailAddress = $("#emailForgotPassInput").val();
        if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(emailAddress))
            return true;
        else
            return false;
    }



    function GetFieldsVals()
    {
        newulv3_ids = {
            username: '#dynControl_42986_ctl00_UpperLogin_txtTabsUsername',
            password: '#dynControl_42986_ctl00_UpperLogin_txtTabsPassword',
            btn: '#dynControl_42986_ctl00_UpperLogin_loginTabsSubmit'
        };
    }

    var ulv3_res = {
        missingUsername: 'Please enter a Username.',
        missingPassword: 'Please enter a Password.',
        ok: 'OK'
    };

    function submitFrm()
    {
        $(newulv3_ids.btn).click();
    }

    function submitTabsUpperLogin()
    {
        GetFieldsVals();
        var val = $.trim($(newulv3_ids.username).val());
        if (val.length == 0)
        {
            doAlert("missingUsername",
                ulv3_res.missingUsername,
                { css: "bo msgBox" },
                [{ id: "okBtn", css: "buttonRed", style: "width:100px", value: ulv3_res.ok, click: "$.modal.close();" }]);
            $(newulv3_ids.username).focus();
            return false;
        }

        val = jQuery.trim($(newulv3_ids.password).val());
        if (val.length == 0)
        {
            doAlert("missingPassword",
                ulv3_res.missingPassword,
                { css: "bo msgBox" },
                [{ id: "okBtn", css: "buttonRed", style: "width:100px", value: ulv3_res.ok, click: "$.modal.close();" }]);
            $(newulv3_ids.password).focus();
            return false;
        }
        return true;
    }
