<script language="javascript" type="text/javascript">
    var maxsrv = 4;
	var isMultiDomainImageSrv = true;
    var _top = true;
    var StopFading = false;
    var useFading = true;
    var loaded = false;
    var FeatureIsOn = false;    
    function getdomain(url, mainimg){
	    if(!isMultiDomainImageSrv){ return url;}
	    url = url.split('/');
	    var ind = mainimg.charAt(1) / 1;
	    if(isNaN(ind)){ ind = 0;}
	    url[2] = "i" + (ind) + ".gatewayinterface.com";
	    return url.join('/');
	}
    function GetText(seo,lt,id,name,img,HpURL)
    {

        var retval = "<div class='LatestActivitiesInfo'>";
        var lImage = getdomain('http://i1.gatewayinterface.com/imagesrv/imp_getimage?cctrl=public,max-age=518400&quality=77&imagesource=IMLFOH&mark=1&watermark=0&width=250&height=200&filename=', img) + img;
        var _chat = '/waitingroom';
        var profile = HpURL;

        var snapshotsLink = profile +'/' +  name + '/snapshots';        
        var videosLink = profile +'/' +  name + '/videos-main-list';
         profile+= '/' +  name;
        retval += "<img border='0' seo='" + profile +"'  class='LiveFeedImage' width='62px' height='42px' src='" + lImage + "'>";
        var realName;
        
        var cr = getCurrRoom();
        try {
            //if (cr == 0)
                cr = 10;
        }
        catch(e)
        {
        }
        
        var isGay = (cr == "53" || cr == "54");
        
        realName = name;
        if(name.length > 11 ) //  VERYLONGWWWNAME 
        {
            realName = name.substring(0,11) + "...";
            realName = realName.replace("_86", "");
        }
        
        retval +="<a title='" +  name + "' alt='" +  name + "' href='"+ _chat + "/" + name + "'>" + realName + "</a> ";
        
        switch(lt)
        {
            
            case "HappyHour":
                retval += "has started a discounted show – <strong>don’t miss it!</strong>";                 
                break;
            case "NewHost": 
                if (isGay) {
                    retval +="is a new host – ";
                    
                    retval += ' ' + '<a href="' + _chat + "/" + name + '">' + 'Check out his profile' + '</a>';
                    
                }
                else {
                    retval +="is a new host – ";
                    
                    retval += ' ' + '<a href="' + _chat + "/" + name + '">' + 'Check out her profile' + '</a>';
                    
                }
                break;
            case "NudeNow": 
                retval +="has just entered a <b>Private Sex Chat!</b>";
                retval += ' ' + '<a href="' + _chat + "/" + name + '">' + 'Take a peek!' + '</a>';
                break;
            case "IsFavorite": 
                retval +="is now online!";
                retval += ' ' + '<a href="' + _chat + "/" + name + '">' + 'Drop in and say Hi' + '</a>';
                break;
            case "NewPhotos": 
                retval +='has added <strong>new photos</strong>.';
                if (isGay)
                    retval += ' ' + '<a href="' + snapshotsLink + '">' + 'View his gallery' + '</a>';
                else
                    retval += ' ' + '<a href="' + snapshotsLink + '">' + 'View her gallery' + '</a>';
                break;
            case "NewVideos": 
                retval +='has just uploaded a <strong>video</strong>.';
                retval += ' ' + '<a href="' + videosLink + '">' + 'Watch it now!' + '</a>';
                break;
        }
        retval += "</div>";
        
        return retval;
    }
    var LiveFeedArray = '';
    function lfCallBack(data)
    {
        if(data.length > 0)
        {
            LiveFeedArray = data;  
            DrawMe();
        }
             
    }
    function DrawMe()
    {
        var length = 5;
        var start=0;
        var end = length;
        if(LiveFeedArray.length < length)
            length = LiveFeedArray.length;
        end =  LiveFeedArray.length; 
        _top = !_top;
        if(_top && LiveFeedArray.length > length)
        {
            start = length;
        }
        else
        {
            start = 0;
            if(LiveFeedArray.length > length)
                end =length;
        }
        var innerHTML = '';
        for(i=start;i<end;i++)
        {
            innerHTML+= GetText(LiveFeedArray[i].RoomID,LiveFeedArray[i].FT,LiveFeedArray[i].id,LiveFeedArray[i].name,LiveFeedArray[i].img,LiveFeedArray[i].HpUrl);
        }
        document.getElementById("LatestActivities").innerHTML = innerHTML;
        
        $(".LatestActivitiesCon").bind("mouseover",function(){ StopFading = true;});
        $(".LatestActivitiesCon").bind("mouseleave",function(){StopFading = false;});
        
        if (useFading) {
            $(".LiveFeedImage").bind("mouseenter",function()
            {
                StopFading = true;
                var o = $("#DivLargImage");          
			    $("#iLargImage").attr("src",$(this).attr("src"));
			    var pos = $(this).offset();               
			    var header_top = $('#body').length > 0? $('#body').offset().top : 0;			    
			    o.css("top", pos.top - header_top);
			    			    
			    var profileUrl = $(this).attr("seo");
			    o.click(function ()
			        {
                        
			            document.location.href = profileUrl ;
			        });
			    setTimeout(ShowBigImage,500);    			
		    });
		} else {
		    $(".LiveFeedImage").bind("click", function() { document.location.href = $(this).attr("seo");  });
		    $(".LiveFeedImage").attr("style", "cursor:pointer;");
		}		
    }
    function ShowBigImage()
    {
        var o = $("#DivLargImage");  
        o.show("slow");			
	    o.bind("mouseleave",function()
	    {
	        HideBigImage();
	    });   
	    
	    setTimeout(truckPosition, 500);
    }
    
    function truckPosition(){
        var o = $("#DivLargImage");
        if(o.is(":visible")){  
            var pos = o.offset();
            $().mousemove(function(e){
                if( e.pageX < pos.left || 
                    e.pageX > pos.left + 250 || 
                    e.pageY > pos.top + 200 ||
                    e.pageY < pos.top){
                    
                    HideBigImage();
                    $().unbind("mousemove");
                }
            });
        }
    }
    
    function HideBigImage()
    {
         var o = $("#DivLargImage");  
         o.hide();          
    }
    
    function LoadData()
    {        
        loaded = true;
//        var rid = getCurrRoom();
//        if(rid == 0) 
//        {
           rid= '10';
      // } 

        $.getJSON("/services/LiveFeed.ashx?roomId=" + rid + "&r=" + Math.round(Math.random() *10000),'',lfCallBack);
    }
    function fade()
    {
       if(!StopFading)
       {
            $(".LatestActivities").fadeOut("slow");
            setTimeout(_fadein,1000);
       }
       else
       {
         setTimeout(fade,2000);
       }
    }
    function _fadein()
    {
        DrawMe();
        $(".LatestActivities").fadeIn("slow");
        setTimeout(fade,10000);
    }
    function LoadWhenEmpty()
    {
        if(!loaded)
            LoadData();
    }
    
    try
    {
        var Timer;
        Timer = setInterval(LoadData,180000);        
        DrawMe();
        fade();
    }
    catch(e)
    {}

    $().ready(function(){
		setCurrentRoomChangedListener(LoadData);
	    setTimeout(LoadWhenEmpty,2500);
	});
