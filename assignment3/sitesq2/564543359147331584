// function to keep two (or more) input elements in sync. Accepts jQuery identifiers as parameters
function syncInputs(input1, input2) {
	jQuery(input1).change(function(){jQuery(input2).val(jQuery(this).val());});
	jQuery(input2).change(function(){jQuery(input1).val(jQuery(this).val());});
}
