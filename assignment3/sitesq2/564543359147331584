<script type="text/javascript">

var pu_url = "";
var awePuShown = true;


//jfrench: Add this to an element's "onclick" to suppress popup for that click.
//Use this for login prompts, TOS links, etc.
function suppress_pop() {
	if (!awePuShown) {
		setTimeout(function() {
			awePuShown = false;
		},1000);
	}
	
	awePuShown = true;
}

function aweDoOpen(url) {
	if ( awePuShown === true ) return true;

	//jfrench: Logic is slightly modified with respect to this
	//awePuShown variable. Original version checks return value of
	//window.open before setting awePuShown to true. Doing it this
	//way appears to fix a duplicate popup bug in Firefox 3.6.6
	awePuShown = true;

        window.popunder = [
            [url, {window: {height:780, width:1024, toolbar:0, scrollbars:1, location:1, statusbar:1, menubar:0, resizable:1}, blocktime: 1}],
        ];

        jQuery.popunder(window.popunder);
}

function aweSetCookie(name, value, time) {
	var expires = new Date();

	expires.setTime( expires.getTime() + time );
	document.cookie = name + "=" + value + "; path=/; expires=" + expires.toGMTString() + "; path=/";
}

function aweGetCookie(name) {
	//return null; //Uncomment this line to get a popunder with every pageload.
	var cookies = document.cookie.toString().split('; ');
	var cookie, c_name, c_value;

	for (var n=0; n<cookies.length; n++) {
		cookie  = cookies[n].split("=");
		c_name  = cookie[0];
		c_value = cookie[1];

		if ( c_name == name ) return c_value;
	}

	return null;
}

function aweCheckTarget(e) {
	//Check if this is a no_popunder link
	if (!e) e = window.event;
	var target;
	if (e.srcElement) target = e.srcElement;
	else target = e.target;
	if (target.className.indexOf("no_popunder") >= 0) {
		//Do nothing
	}else{	
		//Check the cookie
		var cookieValue = aweGetCookie("popundr");

		if ( cookieValue === null ) {
			//jfrench: We only want to set this cookie if we're actually showing a popunder
			if (!awePuShown) aweSetCookie("popundr", 1, 24*60*60*1000);
			aweDoOpen(pu_url);
		}
	}
}

function aweInitPu() {
	if ( document.attachEvent ) document.attachEvent( "onclick", aweCheckTarget );
	else if ( document.addEventListener ) document.addEventListener( "click", aweCheckTarget, false );

	return true;
}

aweInitPu();

</script>
	
