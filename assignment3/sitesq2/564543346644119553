var userAuthtoken = $.cookie('tncms-authtoken');
if (userAuthtoken){
    $(".authtoken-req").show();
    popUserName();
}
else{
    $(".authtoken-alt").show();
}
function popUserName(){
    var avatar = '';
    var screenname = $.cookie('tncms-screenname');
    var avatarurl = $.cookie('tncms-avatarurl');


    if (avatarurl){
        avatar = '<img src="'+avatarurl+'" alt="'+screenname+'" class="user-avatar">';
    }else{
                avatar = '<img src="http://bloximages.newyork1.vip.townnews.com/timesdaily.com/content/tncms/live/editorial/editorial-core-base/resources/images/no_avatar.png?_dc=1424861690" alt="'+screenname+'" class="user-avatar">';
        }
    if (screenname != null){
        $("#side-panel .screenname").html('<p>'+avatar+'<strong>'+screenname+'</strong></p>');
    }
}
