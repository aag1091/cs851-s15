    <script>
    $(document).ready(function() {
    // Gather cookie info
        var bloxAdInter = $.cookie('BLOX-interstitial');
        // Is ad populated?
        var isEmpty = $("#ad-mobile-phone-interstitial div.tncms-region-ads").children(':not("script")').length==0
          if(isEmpty){
            // Ad is not populated
              $.cookie("BLOX-interstitial", "EMPTY", {path: '/'});
          }else{
            // Ad is populated
            // Has the interstitial WAIT cookie expired yet?
                if(bloxAdInter == null){
                // Set expiration date
                    var date = new Date();
                    var minutes =1;
                    date.setTime(date.getTime() + (minutes * 60 * 1000));

                // Create interstitial cookie
                    $.cookie("BLOX-interstitial", "WAIT", {path: '/', expires: date});
                    var newCookie = $.cookie('blox-interstitial');
                // Pop up interstitial
                    setTimeout(function(){
                        // jQM can't initialize quickly enough to use it's own open function...
                        // So we auto click a button instead.

						$( "#interstitialAd" ).bind({
							popupbeforeposition: function(event, ui) {
								$('#interstitialAd').removeClass('ui-corner-all');
								$('#interstitialAd').find('.interstitial').removeClass('ui-content').css({'border':'none'});
								$('#interstitialAd .ad-position').css({'margin':0});
								$('#interstitialAd .ad-position .tncms-region-ads img').css({'margin':'4px 0 0 0'});
							}
						});
                        $("#interstitialTrigger").click();
                    },750);
                } // end bloxAdInter wait check
          } // end else populated
     }); // end onready
    </script>
