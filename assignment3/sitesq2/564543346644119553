<script>
    $(document).unbind("pageshow").bind("pageshow", function(event, data) {
        //console.log('pageshow fired');
        var fourZeroOne = $.cookie("blox-401");
        //console.log('Check 401 cookie');
        if (fourZeroOne){
            //console.log('401 cookie found');
            $("#errorDialog").show().popup("open");
            //console.log('open popup, remove cookie');
            $.cookie("blox-401", null, {path:"/", expires: -10});
        }
    });

    $(document).unbind("pageloadfailed").bind("pageloadfailed", function(event, data) {
        //console.log("\n");
        //console.log("Page Load Failed!!!");

        //console.log(data.errorThrown);

        if ((data.errorThrown) && (data.errorThrown.indexOf('Authorization Required') > -1)){
            event.preventDefault();
            data.deferred.reject();
            //console.log("\n Let me handle this 401. \n Prevent default and reject deferred.");

            //console.log('blox-401 cookie created');
            $.cookie("blox-401", 1, {path:"/"});

            // manage 401 fail by going ahead to the clicked link
            var sURL = data.absUrl;
            //console.log("Clicked link \n"+ sURL);

            var nRandCacheBuster = Math.random() * 1000000000000;
            if (sURL.indexOf('?') > -1) {
                sURL += '&_dc=' + nRandCacheBuster;
            } else {
                sURL += '?_dc=' + nRandCacheBuster;
            }

            //console.log("Go to: "+ sURL);

            if (window.location.replace) {
                //javascript: console.log('...using location.replace');
                window.location.replace(sURL);

             } else if (window.location.assign) {
                 //javascript: console.log('...using location.assign');
                 window.location.assign(sURL);
             } else {
                 //javascript: console.log('...using location.href');
                 window.location.href(sURL);
             }

        }

    });
    </script><script>
