if(typeof(tncmsServicesTracker) == "undefined"){ tncmsServicesTracker = new Object(); }


tncmsServicesTracker.getConfig = function(){
    tncmsServicesTracker.assetUuid = "c7e9b11c-af40-11e4-b862-c37bda6966db";
    tncmsServicesTracker.groups = [{
                weight:1,
                reg:10,
                sub:0}];
                
                
        tncmsServicesTracker.disable = false,

        tncmsServicesTracker.forceExpirationDate ="01/14/2014",
        tncmsServicesTracker.checkResurrection = false;
        tncmsServicesTracker.checkVerified = false;
        
        tncmsServicesTracker.socialMedia = [""];
}

tncmsServicesTracker.getConfig();
$(document).trigger("trackerLoaded");

// run mobile tracker
$("#c7e9b11c_af40_11e4_b862_c37bda6966db_jqm").die("pageinit").live("pageinit",function(){
        if((!tncmsServicesTracker.checkVerified)){
            if(typeof(ninjaLog)!="undefined"){ ninjaLog.Update({text:"Tracker: .eval() not run, running .eval() from pageInit"}); }
            tncmsServicesTracker.eval();
        } else {
            if(typeof(ninjaLog)!="undefined"){ ninjaLog.Update({text:"Tracker: pageInit event, checks already run or no dom elements not loaded."}); }
        }
    setTimeout(function(){tncmsServicesTracker.eval();},500);
    
    if(window.sessionStorage){
        if(sessionStorage.getItem('__tnt.metering.wall.subscription')){
            var sAuthToken = jQuery.cookie('tncms-authtoken');

            if(sAuthToken!=null&&sessionStorage.getItem('__tnt.metering.wall.subscription')&&!sessionStorage.getItem('__tnt.metering.wall.login')){
                $.fn.googleEvents('tnt-metering', 'wall action', 'logged in after encountering wall', 1);
                sessionStorage.removeItem('__tnt.metering.wall.subscription');
                sessionStorage.setItem('__tnt.metering.wall.login',1);
            }
        }
    }
});
</script>

<script>
$(document).ready(function() {
    $("a").click(function(event) {
        if  ( $(this).attr('href').indexOf('#') == -1
                && !($(this).attr('data-rel')) ){
                event.preventDefault();
                var sHref = ($(this).data('href') || $(this).attr('href'));
                location.href = sHref;
        }
    });
});
