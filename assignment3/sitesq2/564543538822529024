<script>
function record_facebook_like() {
    new Ajax.Request('/ajax/record_social_click.php', {
	method: 'get',
	parameters: {
		id: window.product.id,
		event_type: 1
	}
    });
}

window.fbAsyncInit = function () {
	FB.init({xfbml: true});
	try { 
		FB.Event.subscribe('edge.create', function(targetUrl) {
			record_facebook_like();
			if ( _gaq != undefined ) {
				_gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]);
			}
		});
		
		FB.Event.subscribe('edge.remove', function(targetUrl) {
			// a current facebook bug means that likes are showing as dislikes..
			record_facebook_like();
			if ( _gaq != undefined ) {
				_gaq.push(['_trackSocial', 'facebook', 'unlike', targetUrl]);
			}
		});
		
		FB.Event.subscribe('message.send', function(targetUrl) {
			if ( _gaq != undefined ) {
				_gaq.push(['_trackSocial', 'facebook', 'send', targetUrl]);
			}
		});
	} catch (e) {
		
	}
};
</script>

<div id="light_box_bg_fade2"></div>
<div id="light_box_container_container2">
    <div id="light_box_container2">
	<div id="light_box_inner_container">
	    <div id="light_box_container_header2">
		<a id="light_box_close2" class="button_style" href="#">
		    <i class="icon-2x icon-remove"></i>
		</a>
	    </div>
	    
	    <div id="light_box_container_text2">
	    </div>
	    <div id="light_box_container_name2"></div>
	</div>
    </div>
</div>

<script type="text/javascript">
    // Nice jQuery for doing social bits
    (function($) {
	$(function() {
	    
	    // Just for facebook likes..
	    var lightbox2 = {
		init: function() {
		    var scope = this;
		    
		    this.$container = $("#light_box_container2");
		    this.$outercontainer = $("#light_box_container_container2");
		    this.$text = $("#light_box_container_text2");
		    this.$background = $("#light_box_bg_fade2");
		    
		    $("#light_box_close2").click(function() {
			scope.close();
			return false;
		    });
		    
		    
		},
		close: function() {
		    // Instantly close
		    this.$background.css('opacity',0).hide();
		    this.$outercontainer.hide();
		},
		open_likes: function(url, name, image) {
		    
		    var image_html = '';
		    if (typeof image != "undefined") {
			image_html = '<img src="'+ image + '" alt="' + name + '" width="100" height="100" style="width:100px;height:100px;float:left;margin-right:5px;"/>';
		    }
		    var fb_html = '<iframe src="http://www.facebook.com/plugins/like.php?href=' + url + '&amp;send=false&amp;layout=standard&amp;width=320&amp;show_faces=true&amp;action=like&amp;colorscheme=light&amp;font=lucida+grande&amp" scrolling="no" frameborder="0"  allowTransparency="true" style="clear:both;max-width:100%;margin:auto;height:100px;"></iframe>';
		    
		    this.$text.html(image_html + '<h1 class="product_title">Like ' + name + ' on Facebook</h1><div style="clear:both;height:10px;">&nbsp;</div>' + fb_html);
		    
		    // Don't go wider than the screen
		    var width = $(window).width() - 8;
		    if (width > 425) {
			width = 425;
		    }
		    this.$container.css('width', width + 'px');
		    
		    this.$background.show().animate({
			opacity: 0.6
		    },500,function() {
			// done
		    });
		    
		    this.$outercontainer.fadeIn();
		    
		    
		    // Vertically center it..
		    var big = parseInt(this.$outercontainer.height() / 2 );
		    var small = parseInt(this.$container.height() / 2 );

		    if (small > 60){
			if ((big-small)>0){
			    this.$container.css('marginTop',(big - small) + "px");
			} else{
			    this.$container.css('marginTop',"0px");				
			}
		    } else{
			this.$container.css('marginTop',"px");
		    }
		    
		    
		}
	    };
	    lightbox2.init();
	    
	    $(".like_link").click(function() {
		var $this = $(this);
		
		lightbox2.open_likes($this.attr('data-url'), $this.attr('data-name'), $this.attr('data-image'));
		
		return false;
	    });
	    
	    $(".pin_link").each(function() {
		var PIN_URL = "https://pinterest.com/pin/create/button";
		var $this = $(this);
		// Use current page URL as default link
		var url = encodeURIComponent($this.attr("data-url") || document.location.href);
		var text = encodeURIComponent($this.attr("data-text") || document.title);
		var media = encodeURIComponent($this.attr("data-media") || "");
		
		var fullurl = PIN_URL + "?url=" + url + "&media=" + media + "&description=" + text;
  	
		var w = 632;
		var h = 270;
		var left = ( $(window).width() /2)-(w/2);
		var top = (  $(window).height() /2)-(h/2);
		
		$this.attr('href',fullurl)
		    .attr('onclick', "var popup = window.open('" + fullurl + "','_blank','status=no,resizable=yes,scrollbars=yes,personalbar=no,directories=no,location=no,toolbar=no,menubar=no,width=" + w + ",height=" + h +",left=" + left + ",top=" + top +"');return false;")
		    .attr('target','_blank');
		
	    });
	    
	    $(".tweet_link").each(function() {
		var TWEET_URL = "https://twitter.com/intent/tweet";
		var $this = $(this);
		// Use current page URL as default link
		var url = encodeURIComponent($this.attr("data-url") || document.location.href);
		var text = encodeURIComponent($this.attr("data-text") || document.title);
		var via = $this.attr("data-via") || "";
		var related = encodeURIComponent($this.attr("data-related")) || "";
		var hashtags = encodeURIComponent($this.attr("data-hashtags")) || "";
		
		var fullurl = TWEET_URL + "?hashtags=" + hashtags + "&original_referer=" +
                encodeURIComponent(document.location.href) + "&related=" + related +
                "&source=tweetbutton&text=" + text + "&url=" + url + "&via=" + via;
		
		$this.attr('href',fullurl).attr('target','_blank');
		
	    });
	    
	    
	});
    })(jQuery);
</script>


   


