    <div class="wide_page">
       	<h2 class="product_2014_heading">Customer Reviews</h2>
       	<div class="gapbox"><div class="gap"></div></div>

				<div id="prev_review"><i class="icon-chevron-left"></i></div><div id="next_review"><i class="icon-chevron-right"></i></div>
		<script type="text/javascript">
			var current_review = 1;
			var batch_offset = 0;
			var loaded_reviews = 7;
			var last_review = 7;

			$('#next_review').click(function(event){
		        event.preventDefault();
				$('.product_2014_review').hide();
				current_review++;
				if (current_review > last_review){
					current_review = 1;
				}
				if (current_review > loaded_reviews){
					batch_offset ++;
					load_reviews_via_ajax(1);
				}else{
					$('#review_' + current_review).fadeIn();
				}
			});
			$('#prev_review').click(function(event){
		        event.preventDefault();
				$('.product_2014_review').hide();
				current_review--;
				if (current_review < 1){
					current_review = last_review;
				}
				if (current_review > loaded_reviews){
					batch_offset ++;
					load_reviews_via_ajax(loaded_reviews);
				}else{
					$('#review_' + current_review).fadeIn();
				}				
			});
			function load_reviews_via_ajax(backup_plan){
				$("#review_loading").show();
				$.ajax({
			        url: '/ajax/get_reviews.php',
			        type: 'POST',
			        dataType: 'json',
			        data: {
			            offset: batch_offset,
			            num_per_batch: 10,
			            product_id: 6558			        }
			    }).done(function(data) {
					try {
						if (data) {
							$("#all_reviews").append(data.reviewhtml);
							loaded_reviews += data.loaded_count;
							if(current_review <= loaded_reviews){
								$('#review_' + current_review).fadeIn();
								$("#review_loading").hide();
							}else{
								batch_offset ++;
								load_reviews_via_ajax(backup_plan);
							}
						}else{
							$("#review_loading").hide();
		                    current_review = backup_plan;
		                    $('#review_' + current_review).fadeIn();
						}
					} catch(err) {
						$("#review_loading").hide();
						current_review = backup_plan;
						$('#review_' + current_review).fadeIn();
					}
				}).fail(function() {
					$("#review_loading").hide();
					current_review = backup_plan;
					$('#review_' + current_review).fadeIn();
				});
			}
		</script>
