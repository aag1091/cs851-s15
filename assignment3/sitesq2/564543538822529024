<script type='text/javascript'>
$j(document).ready(function(){
   $window = $j(window);
   $j('i.icon-heart').each(function(){
    var $heart = $j(this); 
    var scrollcheck = $window.scroll(function() { 
      if($window.scrollTop() > $heart.offset().top - $window.height()){
		 $heart.replaceWith($heart.clone(true));
		 $(window).off("scroll", scrollcheck);
	  }
    }); 
  });  
}); 
</script>





