    <script type='text/javascript'>
  
	$j(function() {

	    $j('.buysection select').on('change', function() {
	    	var sku_id = this.value;
		    if (sku_id != $j("#main_image").attr('data-sku')){
				var $thumbnailimg = $j('.image_thumb_container img[data-' + this.value +'="true"]');
				if ($thumbnailimg.length > 0) {
				    $j('#extra_image' + sku_id).remove();
				    $j('#extra_image').before('<div id="extra_image' + sku_id +'" style="display:none;" class="extra_image"></div>');
				    $j('#extra_image' + sku_id).html("<img src='" + $thumbnailimg.attr('data-image') + "' alt='"+$thumbnailimg.attr('data-caption')+"'/>");
					$j('#extra_image' + sku_id).fadeIn();
				}else{
				    $j('#extra_image' + this.value).remove();
				    $j('#extra_image').before('<div id="extra_image' + sku_id +'" style="display:none;" class="extra_image"></div>');
