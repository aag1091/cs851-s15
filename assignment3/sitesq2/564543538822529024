<script type='text/javascript'>
var scrollbarwidth = 0;
var Browser = ["",0];
$(document).ready(function(){
    Browser = BrowserCheck();
    resize_stuff();
    window.onresize = function() {
      resize_stuff();
    }
    $("select").blur(function() {
      resize_stuff();
    });
});
function resize_stuff(){
  $window = $j(window);
  w =$j(window).width();
  if ($("#product_2014_main_image").css("display") == "table-cell"){
    $("#product_2014_main_image").width((w/2) + "px");
    $("#product_2014_buyarea").width((w/2) + "px");
    if($("#product_2014_main_image").width()>648){
      $("#product_2014_buyarea").width("auto");
      $("#product_2014_main_image").width(648 + "px");
    }
    $("#product_2014_buyarea").height("auto");
    min_height = 348 * Math.min(1,w/1200) + 300;
    if($("#product_2014_buyarea").height() < min_height){
      $("#product_2014_buyarea").height(min_height);
    }
    if($("#product_2014_main_image").width()<$("#product_2014_main_image").height()){
      $("#product_2014_main_image img").height("100%");
      if($("#product_2014_main_image img").height()<$("#product_2014_main_image").height()){
        $("#product_2014_main_image img").height($("#product_2014_main_image").height());
      }
      $("#product_2014_main_image img").width("auto");
    }else{
      $("#product_2014_main_image img").width($("#product_2014_main_image").width());
      $("#product_2014_main_image img").height("auto");
    }
    if($("#product_2014_main_image_fade").height()<$("#product_2014_top").height()){
      $("#product_2014_main_image_fade").height($("#product_2014_top").height());
    }
  }else{
    $("#product_2014_buyarea").height("auto");
    $("#product_2014_main_image").width("100%");
    $("#product_2014_main_image").height("auto");

    $("#product_2014_main_image img").width("100%");
    $("#product_2014_main_image img").height("auto");
  }

  if(Browser[0].toLowerCase() == "firefox" && parseFloat(Browser[1]) < 30){
    $("#product_2014_top").addClass("shitfirefox");
  }
}
function BrowserCheck()
{
    var N= navigator.appName, ua= navigator.userAgent, tem;
    var M= ua.match(/(opera|chrome|safari|firefox|msie|trident)\/?\s*(\.?\d+(\.\d+)*)/i);
    if(M && (tem= ua.match(/version\/([\.\d]+)/i))!= null) {M[2]=tem[1];}
    M= M? [M[1], M[2]]: [N, navigator.appVersion,'-?'];
    return M;
}
</script>
