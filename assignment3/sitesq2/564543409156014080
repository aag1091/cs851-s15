            holder.currentPostId = 8948096094162483608
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/317474431038303966/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/317474431038303966/comments/default";
          
          holder.currentBlogUrl = "http://www.my-family-ties.com/";
          holder.currentBlogId = "317474431038303966";
        
        return holder;
      });
    }
  </script>
