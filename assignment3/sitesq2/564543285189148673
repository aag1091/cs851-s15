<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
<script>!window.jQuery && document.write('<script src="http://icra2015.org/templates/ras-conference/js/libs/jquery-1.8.2.min.js"><\/script>')</script>
<script src="http://icra2015.org/templates/ras-conference/js/libs/jquery-picture.min.js"></script>
<script src="http://icra2015.org/templates/ras-conference/js/libs/jquery-retina.js"></script>
<script src="http://icra2015.org/templates/ras-conference/js/scripts.js"></script>
<script>
jQuery(document).ready(function() {
//SSO check
	//var requestUrl = 'http://oamssoqae.ieee.org/ieeevendorsso/ssocookievalidator'; 
	var requestUrl = 'https://securesso.ieee.org/ieeevendorsso/ssocookievalidator'; 
	jQuery.ajax({ url: requestUrl, type: 'GET', dataType: 'jsonp', success: function(data){ var cidCount = data.userIdentity.indexOf(","); var cid = data.userIdentity.substr(4,cidCount-4);
		if (cid) { //cookie is valid and has a user logged in
			jQuery('.sso-sign-in').show(); jQuery('.sso-login').hide();			
			//auto-log in the user by submitting the form
			document.getElementById('login-form-member').submit();
		} else {
			jQuery('.sso-sign-in').hide(); jQuery('.sso-login').show();	
		}
	  }, 
	  error: function(data){ 
		alert('There was an error in the request'); 
	  } 
	});
});
</script>

</body>
