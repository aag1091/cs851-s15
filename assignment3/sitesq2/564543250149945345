                if (userOS === 'iOS' && Number(userOSver.charAt(0)) === 5) {
                    window.open("maps://maps.google.com/maps?daddr=5110+Old+Chapel+Hill+Road+Durham,+NC+27707&amp;ll=", '_blank');
                } else if (userOS === 'iOS' && Number(userOSver.charAt(0)) >= 7) {

                    window.location = "maps://maps.apple.com/?daddr=5110+Old+Chapel+Hill+Road+Durham,+NC+27707";
                }
                else if (userOS === 'iOS' && Number(userOSver.charAt(0)) >= 6) {
                    window.open("maps://maps.apple.com/?daddr=5110+Old+Chapel+Hill+Road+Durham,+NC+27707", '_blank');
                } else
