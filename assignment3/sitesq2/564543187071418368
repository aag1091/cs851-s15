            function generateId(url) {
                var urlRegexp;
                if (url.indexOf("https:") === 0) {
                    urlRegexp = /https:\/\/[^\/]*\/([^?#]*).*/i;
                } else {
                    urlRegexp = /http:\/\/[^\/]*\/([^?#]*).*/i;
