                <li>
                  <a data-confirm="Do you want to delete this post?" data-method="delete" data-remote="true" href="/posts/delete/{{id}}" rel="nofollow">Remove</a>
                </li>
              {{/if}}

              {{#if current_user_id}}
                {{#unless can_report}}
                  <li>
                    <a href="/posts/report_menu/{{id}}" class="report fancybox.ajax">Report</a>
                  </li>
                {{/unless}}
              {{else}}
                "Report"
              {{/if}}
            </ul>
          </li>
        {{/unless}}
      </ul>

      <ul class="posts_stats">
        {{#unless is_share}}
          {{#if has_been_shared}}
            <li style="margin-left: 5px;">
              <span class="share_stats_span {{#if has_shared}}share{{else}}not-share{{/if}}" style="float: left;">
                {{share_count}}
              </span>
            </li>
          {{/if}}
        {{/unless}}

        {{#unless disable_comments}}
          {{#if has_comments}}
            <li>
              <span class="comment_stats_span evac_comments_count_{{id}}", style: "float: left">
                {{comment_count}}
              </span>
            </li>
          {{/if}}
        {{/unless}}

        <li class="fb_show_likes" style="position:relative;" title="{{like_list_user_fullnames}}">
          <span class="like_stats_span {{#if has_liked}}like{{else}}not-like{{/if}}" style="float: left; cursor: pointer; {{#unless has_likes}}display: none;{{/unless}}">
            <a href="/likes/loadmore_likes/post/{{id}}" class="ec_user_profile_summary fancybox.ajax">
              {{like_count}}
            </a>
          </span>
          </span>
        </li>
      </ul>
    </div>

    {{#unless disable_comments}}
      <div class="comment_{{id}}">
        {{#if show_more_comments_link}}
          <div class="load_more_post_comments post_comment">
            <img src="/assets/comment.png" style="float: left; margin-right: 6px; margin-top: 3px; opacity: 0.8;"
                 height="11" width="11">
            <a href="#">view previous comments...</a>
          </div>
        {{/if}}

        <div class="post_write_comment">
          <div>
            {{#current_user}}
              {{> profile_picture}}
            {{/current_user}}
          </div>

          <form accept-charset="UTF-8" action="/comments/evac_comment" id="create_comment_form" method="post"><div style="display:none"><input name="utf8" type="hidden" value="&#x2713;" /><input name="authenticity_token" type="hidden" value="W8+w989UVSFm+0cB4D03Ob6SoH1CBQ/+RT5/hKYlzx8=" /></div>
            {{#if is_native_ad}}
              <input id="aid" name="aid" type="hidden" value="{{ad_id}}">
            {{/if}}
            <input id="post_id" name="post_id" type="hidden" value="{{id}}">

            <div class="expandingText_parent">
              <textarea class="expanding evac_comment_textarea" id="comment_text" name="text"
                        placeholder="Write a comment..."></textarea>
            </div>
</form>        </div>
      </div>
    {{/unless}}
    </div>
  </div>
</script>

