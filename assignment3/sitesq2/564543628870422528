<script type="text/javascript">
jQuery.noConflict();

jQuery(document).ready(function(){

	// Tabs initialization
	jQuery('div.tabs > ul.menu > li:first-child').addClass('current');
	jQuery('div.tabs > ul.content > li:visible').hide();
	jQuery('div.tabs > ul.content > li:first-child').show();
	
	// Tabs action

	jQuery('div.tabs ul.menu li a').on('click', function(event){
		event.preventDefault();
		
		console.log('in');
		
		var parent = jQuery(this).parents('div.tabs');
		var index = jQuery(this).parents('div.tabs').find('a').index(this);
		jQuery(parent).find('ul.menu > li').removeClass('current');
		jQuery(parent).find('ul.menu > li:eq('+index+')').addClass('current');
		jQuery(parent).find('ul.content > li:visible').fadeOut(100, function(){
			jQuery(parent).find('ul.content > li:eq('+index+')').fadeIn(100);
		});
	});
	
});
</script>
