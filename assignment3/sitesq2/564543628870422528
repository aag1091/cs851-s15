		<script>		
		jQuery(document).ready(function() {
	
			var stop = jQuery('.container.end').attr('id');
			
			if(window.location.hash) {
				if(window.location.hash=="#all"){
					jQuery('#mh_gallery > div.container').addClass('current');
					jQuery('#gallery_all').html('VIEW SHORT');
					jQuery('#mh_gallery .container a.hidden').fadeIn();
				}else{
					jQuery('#mh_gallery > div.container:lt('+stop+')').addClass('current');
				}
				//googletag.pubads().refresh([slot1, slot2]);
			} else {
				jQuery('#mh_gallery > div.container:lt('+stop+')').addClass('current');
			}
			
			jQuery('#gallery_all').bind('click', function(e){
				e.preventDefault();
				
				if(jQuery('#gallery_all').html()=="VIEW ALL")
				{
					window.location.hash = "all";
					jQuery('#gallery_all').html('VIEW SHORT');
					jQuery('#mh_gallery .container a.hidden').fadeIn();
					jQuery('html, body').animate({scrollTop:jQuery('a.picture.hidden:first-child').position().top}, 'fast');
					//googletag.pubads().refresh([slot1, slot2]);
				}
				else
				{
					//window.location.hash = "";
					jQuery('#mh_gallery > div.container').removeClass('current');
					jQuery('#mh_gallery > div.container:lt('+stop+')').addClass('current');
					jQuery('#gallery_all').html('VIEW ALL');
					jQuery('#mh_gallery .container a.hidden').fadeOut();
					jQuery('html, body').animate({scrollTop:jQuery('a.picture:nth-child(13)').position().top - 100}, 'fast');
					//googletag.pubads().refresh([slot1, slot2]);
				}
			});
		});
		</script>
		
    		
