        <script type="text/javascript">
        // Check if it is a Number
        function isNumber(n)
        {
          return !isNaN(parseFloat(n)) && isFinite(n);
        }
        
        // Login Form Control
        function checkLoginSubmit(){
                            
            var vb_login_password = jQuery('#vb_login_password').val();
            var vb_login_username = jQuery('#vb_login_username').val();
            
            var data = "jb_password="+vb_login_password+"&jb_username="+vb_login_username;
                
                jQuery.get('http://www.joblo.com/joblo-mfc-bridge.php', data, function(response) {
                    
                    if(response != 'false' && isNumber(response))
                    {
                        
                        jQuery("#user_id").val(response);
                        //jQuery("#jb-comments-login").hide();
						jQuery("div.welcome-logged").html('Welcome '+vb_login_username+'! <a style="cursor:pointer;" id="jb_logout" onclick="javascript:logoutOfthat();">Log Out</a>');
                        //jQuery("#jb-comments-form").slideDown("slow");
						jQuery('.post-reply-placeholder').each(function(index) {
							jQuery(this).html('<img onclick="javascript:postAReply('+jQuery(this).attr('id')+');" id="post-reply'+jQuery(this).attr('id')+'" class="reply_to_post" alt="post a reply" src="/img/btn/post-reply.png">');
						});
						jQuery('.reply-form form .user_id').each(function(index) {
							jQuery(this).val(response);
						});
						
						location.reload();
                    }
                    else
                    {
                        alert("Sorry, Username/Password didn't match.\nPlease provide a valid username and password");
                    }
                });
                
            return false;
        
        }
		
		function logoutOfthat() {
			FB.getLoginStatus(function(response) {
			  if (response.status === 'connected') {
				// the user is logged in and has authenticated your
				// app, and response.authResponse supplies
				// the user's ID, a valid access token, a signed
				// request, and the time the access token 
				// and signed request each expire
				// var uid = response.authResponse.userID;
				// var accessToken = response.authResponse.accessToken;
				FB.logout(function(responseFb) {
				  // user is now logged out
				  jQuery.get('http://www.joblo.com/joblo-mfc-bridge.php', 'logout=yes', function(responseJb) {
						window.location.reload();
					});
				});

			 // } else if (response.status === 'not_authorized') {
				// the user is logged in to Facebook, 
				// but has not authenticated your app
			  } else {
				// the user isn't logged in to Facebook.
				jQuery.get('http://www.joblo.com/joblo-mfc-bridge.php', 'logout=yes', function(responseJb) {
						window.location.reload();
					});
			  }
			 });
			 
			 // in case
			 jQuery.get('http://www.joblo.com/joblo-mfc-bridge.php', 'logout=yes', function(responseJb) {
						window.location.reload();
					});
		}
			
		jQuery(document).ready(function($){
			/**/
			$(window).bind("load", function() {
				
				$('#jb_logout').click(function(){logoutOfthat();});
				$('#a_rules').bubbletip($('#guidelines'));
				
			});
			
			
			
		});
        </script>
