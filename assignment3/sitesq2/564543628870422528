<script type="text/javascript">
jQuery(document).ready(function() {  
	jQuery('#printPage').click(function() {  
		window.print();  
		return false;  
	}); 
});

function editComment(newsid,talk_id, comt_num)
{
	var data = "mode=edit&do=show&newsid="+newsid+"&talk_id="+talk_id+"&ts=joblo";
    
	jQuery.post('http://www.joblo.com/joblo-ajax-article.php', data, function(response) {
		jQuery("#cmt_full_"+comt_num).hide();
		jQuery("#cmt_xrp_"+comt_num).show();
		jQuery("#cmt_xrp_"+comt_num).html(response);
	});
}

function editReply(newsid,talk_id, rpl_num)
{
	var data = "mode=edit&do=show&newsid="+newsid+"&talk_id="+talk_id+"&ts=joblo";
    
	jQuery.post('http://www.joblo.com/joblo-ajax-article.php', data, function(response) {
		jQuery("#rpl_full_"+rpl_num).hide();
		jQuery("#rpl_xrp_"+rpl_num).show();
		jQuery("#rpl_xrp_"+rpl_num).html(response);
	});
}

function showHideComments()
{
	// jQuery('.all-comments-container').slideToggle(300);
	jQuery('.comment_excerpt').hide();
	jQuery('.show_full_comment').hide();
	jQuery('.comment_full').show();
	jQuery('.comment').show();
	jQuery('.reply-container').show();
	return false;
}

function showHideReply(n)
{
	jQuery('#reply-container'+n).slideToggle(300);
	return false;
}

function showFullContent(n)
{
	jQuery('#cmt_xrp_'+n).hide();
	jQuery('#cmt_full_'+n).show();
	jQuery('#a_cmt_full_'+n).hide();
	return false;
}

function showFullContentReply(n)
{
	jQuery('#rpl_xrp_'+n).hide();
	jQuery('#rpl_full_'+n).show();
	jQuery('#a_rpl_full_'+n).hide();
	return false;
}

function postAReply(tid)
{
	jQuery('#reply-form'+tid).show();
	jQuery('#post-reply'+tid).hide();
	return false;
}

function voteComment(a, b, c, d) {
	var e = jQuery(a).parent().get();
	var d = 'joblo';
	
	if(jQuery(a).attr('title') == 'voted')
	{
		alert("You've already voted on this comment!")
	}
	else
	{
		jQuery.get("http://www.joblo.com/tb_vote.php", {
			talk_id: b,
			type: c,
			site: d
		}, function (a) {
			if (a >= 0) {
				a = "+" + a
			}
			jQuery(e).find(".counter").html(a)
		});
		jQuery(e).find("a.up").attr({title: "voted"});
    	jQuery(e).find("a.down").attr({title: "voted"})
	}
}

function submitReply(a) {
	var b = jQuery(a).parent().get();
	var c = jQuery(b).find("input.talk_id").val();
	var d = jQuery(b).find("input.user_id").val();
	var e = jQuery(b).find("input.news_id").val();
	var f = 'joblo'; // jQuery(b).find("input.site").val()
	var g = jQuery(b).find("textarea.comment_reply").val();
	var h = jQuery(a).parent("form").serialize();
	jQuery.ajax({
		type: "POST",
		url: "/tb_reply_dev.php",
		contentType: "application/x-www-form-urlencoded",
		data: h+'&site='+f,
		success: function (a) {
			if (a == "") {
				alert("Comment submit failed")
			} else {
				alert(a);
				window.location.reload()
			}
		}
	})
}
</script>
<script type="text/javascript">
(function(jQuery) {
	jQuery(function() {
            var offset = jQuery("#scrolladd").offset();
            var bottomPadding = 0;
            var topPadding = 80;
            jQuery(window).scroll(function() 
            {
				// console.log('Position Scroll: '+jQuery(window).scrollTop()+' Doc Height: '+jQuery(document).height()+' bottom Pad: '+bottomPadding+' Scrollbar Height: '+jQuery("#scrolladd").height());
				// footer 1210px
            	if(jQuery(window).scrollTop()<jQuery(document).height()-bottomPadding-jQuery("#scrolladd").height()-1410){
            		if (jQuery(window).scrollTop() > offset.top) 
                	{
			    		var ss= jQuery(window).scrollTop() - offset.top + topPadding;
        	            jQuery("#scrolladd").stop().animate({marginTop:ss});
                	} 
                	else {jQuery("#scrolladd").stop().animate({marginTop: 0}); }
                };
            });
        });
})(jQuery);
</script>

