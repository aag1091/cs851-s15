<script>
var ready_add_content = true;
var add_zergnet = true;

function getArticle(type,from,limit) {
	var dynamicData = {};
	
	dynamicData["type"] = type;
	dynamicData["headlinesNb"] = limit;
	
	if ( from == 0 ){
		dynamicData["ajxp_joblo"] 	= 0;
		dynamicData["ajxp_arrow"] 	= 0;
		dynamicData["ajxp_hotties"] = 0;
		dynamicData["ajxp_video"] 	= 0;
	} else {
		dynamicData["ajxp_joblo"] 	= jQuery("form#pagination > input[name$='ajxp_joblo']").val();
		dynamicData["ajxp_arrow"] 	= jQuery("form#pagination > input[name$='ajxp_arrow']").val();
		dynamicData["ajxp_hotties"] = jQuery("form#pagination > input[name$='ajxp_hotties']").val();
		dynamicData["ajxp_video"] 	= jQuery("form#pagination > input[name$='ajxp_video']").val();
	}
	
	return jQuery.ajax({
		url: "http://www.joblo.com/article.json.1.0.0.php",
		type: "get",
		data: dynamicData,
		dataType: 'json',
		beforeSend: function() {
			jQuery('#loading').show();
		},
		success: function(data) {
			jQuery('#loading').hide();
			jQuery("form#pagination > input[name$='ajxp_joblo']").val(data.ajxp_joblo);
			jQuery("form#pagination > input[name$='ajxp_arrow']").val(data.ajxp_arrow);
			jQuery("form#pagination > input[name$='ajxp_hotties']").val(data.ajxp_hotties);
			jQuery("form#pagination > input[name$='ajxp_video']").val(data.ajxp_video);
		}
	});
}

function process(data){
	if(data.view_load_more===false)
		jQuery("#portal_loadmore").hide();
	if(data.html!=null) {
		ready_add_content = true;
		jQuery("div.column.main > div.section.news").append(data.html);
		if(add_zergnet){
			var zerg_num = Math.floor(Math.random()*90000) + 10000;
					
			if(jQuery('#zergnet-widget-22914').length>0){
				jQuery('#zergnet-widget-22914').attr('id','zergnet-widget-'+zerg_num);
			}
			
			jQuery("div.column.main > div.section.news").append('<div class="item"><h3 style="margin: 6px 0 6px 0;">MORE FUN FROM AROUND THE WEB</h3><div id="zergnet-widget-22914"></div>');
	
		    (function() {
		        var zergnet = document.createElement('script');
		        zergnet.type = 'text/javascript'; 
		        zergnet.async = true;
		        zergnet.src = 'http://www.zergnet.com/zerg.js?id=22914';
		        var znscr = document.getElementsByTagName('script')[0];
		        znscr.parentNode.insertBefore(zergnet, znscr);
		    })();
		    add_zergnet = false;
		} else {
			add_zergnet = true;
		}
		
		
		if(typeof googletag !== 'undefined'){
		post_init();
		}
	} else {
		jQuery("#portal_loadmore").fadeOut();
		jQuery('.cmd_portal_loadmore span').text(loadbutton);
	}
	jQuery('.cmd_portal_loadmore span').text('Load more movie news headlines');
}

loadbutton = jQuery(this).find('span').text();

jQuery(window).ready(function( $ ) {
	
	jQuery('.cmd_portal_loadmore span').text('Loading...');
	
	// getArticle("joblo",0,3).done(function(data) {
	// 	process(data);
	// });

	var pageCount = 1;
	
	$(document).scroll(function( ) {

		var docViewTop = $(window).scrollTop();
		var docViewBottom = docViewTop + $(window).height();
		var elemTop = $("div.column.main > div.section.news").offset().top;
		var elemBottom = elemTop + $("div.column.main > div.section.news").height();


		if(docViewBottom >= elemBottom && ready_add_content) {
		
			ready_add_content = false;	
			
			// add Loading text
			jQuery('.cmd_portal_loadmore span').text('Loading...');

			var nbr = jQuery("#cmn_wrap > div.wrapper > div.body > div > div.column.main > div.section.news > div.item").length;
			getArticle("joblo",nbr, 12).done(function(data) {
				process(data);

				var pushurl = 'http://www.joblo.com/movie-news/guy-pearce-mia-wasikowska-join-western-thriller-brimstone-241';
				pushurl = pushurl.replace(/^.*\/\/[^\/]+/, '').substr(1);

				console.log('NOTICE: Pushing a pageview to google. \n -> '+pushurl+'/infinite-scroll-'+pageCount);
				
				ga('send', 'pageview', pushurl+'/infinite-scroll-'+pageCount);

				pageCount++;
			});
			
		}	
	});  
	
	var offset = 220;
    var duration = 500;
    
    jQuery(window).scroll(function() {
        if (jQuery(this).scrollTop() > offset) {
            jQuery('#back-to-top').fadeIn(duration);
        } else {
            jQuery('#back-to-top').fadeOut(duration);
        }
    });
    
    jQuery('#back-to-top').click(function(event) {
        event.preventDefault();
        jQuery('html, body').animate({scrollTop: 0}, duration);
        return false;
    })

	
});
</script>



