<script type="text/javascript">
function pushAdsToSide() {
	// add block pub
	var number = 1 + Math.floor(Math.random() * 10000);
	var dynslot = 'slot_'+number;
	
	if(typeof googletag !== 'undefined'){
	
	googletag.cmd.push(function() {
        dynslot = googletag.defineSlot('/25961103/joblo_side_300x250', [300, 250], 'div-gpt-ad-'+number+'-1').addService(googletag.pubads());
        googletag.pubads().enableSingleRequest();
		googletag.enableServices();
    });

	jQuery("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side").append("<div class='section advertisement'><div class='body' id='right-ad' style='min-height: 250px;'><div id='div-gpt-ad-"+number+"-1' style='width:300px; height:250px;'><sc"+"ript type='text/javascript'>if(typeof googletag !== 'undefined'){ googletag.cmd.push(function() { googletag.display('div-gpt-ad-"+number+"-1'); });}</sc"+"ript></div></div></div>");
	
	googletag.pubads().refresh([dynslot]);
	
	}
}

jQuery(document).ready(function( $ ) {
	
	c_side_add = 0;
	
	$(window).scroll(function( ) {

		var docViewTop = $(window).scrollTop();
		var docViewBottom = docViewTop + $(window).height();
				
		var elemTopSide = $("#cmn_wrap > div.wrapper > div.body > div > div.column.side").offset().top;
		var elemBottomSide = elemTopSide + $("#cmn_wrap > div.wrapper > div.body > div > div.column.side").height();
				// add sidebar
		if(docViewBottom >= elemBottomSide){
			
			console.log(c_side_add);
			
			if(c_side_add==0){
				pushAdsToSide(); // add ads
				c_side_add++;
			} else if(c_side_add==1){
				$("div.body div.inner div.column.side .section.social:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add++;
			} else if(c_side_add==2){
				$("div.body div.inner div.column.side .section.news.most-popular-trailers:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add++;
			} else if(c_side_add==3){
				pushAdsToSide(); // add ads
				c_side_add++;
			} else if(c_side_add==4){
				$("div.body div.inner div.column.side .section.news.most-popular-movie-news:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add++;
				console.log(c_side_add);
			} else if(c_side_add==5){	
				$("div.body div.inner div.column.side .section.links:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add++;
			} else if(c_side_add==6){
				$("div.body div.inner div.column.side .section.upcoming:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add++;
			} else if(c_side_add==7){
				pushAdsToSide(); // add ads
				c_side_add++;
			} else if(c_side_add==8){
				$("div.body div.inner div.column.side .section.posters:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add++;
			} else if(c_side_add==9){
				$("div.body div.inner div.column.side .section.reviews:first").clone(true).appendTo("html.pw-locale-en body.main-template div#cmn_wrap div.wrapper div.body div.inner div.column.side");
				c_side_add = 0;
			}			
		}	
	});  
});
</script>


<div class="column side">
	<div class="section advertisement">
		<div class="body" id="right-ad" style="min-height: 250px;background-color: #262626;">
		
		<!-- Ads Side -->
		                
                
        <!-- cmnUNT | Begin ad tag -->
		<script type="text/javascript">cmnUNT('300x250,300x600', tile_num++, 1);</script>
		<!-- cmnUNT | End ad tag -->
		
						</div>
	</div>
	
	

	
