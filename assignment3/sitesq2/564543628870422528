	<script type="text/javascript">
	jQuery(document).ready(function($){
	
		var top1 = jQuery('#float_nav').offset().top + (244+60);
		
		jQuery(window).scroll(function(){
			if ( jQuery(window).scrollTop() > top1 ) {
				if($.cookie('_sfnc')==undefined||$.cookie('_sfnc')=="open") {
					jQuery('#float_nav').stop().animate({top:'0', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
					jQuery('#nav_opener').stop().animate({top:'-40px', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
				} else {
					jQuery('#nav_opener').stop().animate({top:'0', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
				}
			} else {
				jQuery('#float_nav').stop().animate({top:'-86px', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
				jQuery('#nav_opener').stop().animate({top:'-40px', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
			}
		});
		
		jQuery("#close a").bind('click', function(e){
			e.preventDefault();
			$.cookie('_sfnc', 'close', { expires: 7, path: '/' });
			jQuery('#float_nav').stop().animate({top:'-86px', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
			jQuery('#nav_opener').stop().animate({top:'0', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
		});
		
		jQuery("#nav_opener").bind('click', function(e){
			e.preventDefault();
			$.cookie('_sfnc', 'open', { expires: 7, path: '/' });
			jQuery('#float_nav').stop().animate({top:'0', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
			jQuery('#nav_opener').stop().animate({top:'-40px', display:'block'},{queue:false, duration:600, easing: 'easeOutQuart'});
		});
		
		jQuery(document).on('click', "#scrolladd #close_social_bar", function(e){
			e.preventDefault();
			$.cookie('_ssbc', 'close', { expires: 7, path: '/' });
			jQuery('#scrolladd').css('z-index','-1').stop().animate({left:'-16px'},{queue:false, duration:600, easing: 'easeOutQuart'});
			jQuery( "#scrolladd" ).after( '<div id="scrolladd_opener"><div></div></div>' );
			var top = jQuery('#scrolladd').css('margin-top');
			jQuery('#scrolladd_opener').css('margin-top', top);
		});

		jQuery("#social_bar").before('<div id="close_social_bar"><div>Close</div><div id="arrow-close"></div><br /><hr /></div>');
		
		if($.cookie('_ssbc')!=undefined||$.cookie('_ssbc')=="close"){
			jQuery('#scrolladd').css('z-index','-1').stop().animate({left:'-16px'},{queue:false, duration:600, easing: 'easeOutQuart'});
			jQuery( "#scrolladd" ).after( '<div id="scrolladd_opener"><div></div></div>' );
		}
		if($.cookie('_ssbc')==undefined||$.cookie('_ssbc')=="open"){
			jQuery('#scrolladd').stop().animate({left:'-82px'},{queue:false, duration:600, easing: 'easeOutQuart', complete: function() {
				jQuery('#scrolladd').css('z-index','1');
			}});
		}

		jQuery(document).on( 'click', '#scrolladd_opener', function(e){
			e.preventDefault();
			$.cookie('_ssbc', 'open', { expires: 7, path: '/' });
			jQuery("#scrolladd_opener").remove();
			jQuery('#scrolladd').stop().animate({left:'-82px'},{queue:false, duration:600, easing: 'easeOutQuart', complete: function() {
				jQuery('#scrolladd').css('z-index','1');
			}});
			
			
		});
		
		jQuery(function() {
			var offset = jQuery("#scrolladd").offset();
			var bottomPadding = 0;
			var topPadding = 80;
			if(offset!=undefined){			
				jQuery(window).scroll(function() {
					if(jQuery(window).scrollTop()<jQuery(document).height()-bottomPadding-jQuery("#scrolladd_opener").height()-1410){
						if (jQuery(window).scrollTop() > offset.top) {
							var ss= jQuery(window).scrollTop() - offset.top + topPadding;
							jQuery("#scrolladd_opener").stop().animate({marginTop:ss});
						} else {
							jQuery("#scrolladd_opener").stop().animate({marginTop: 0});
						}
					}
				});
			}
		});      
	});
	</script>
