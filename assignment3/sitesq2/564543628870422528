<script>
jQuery(document).ready(function($){
	$('#comment-lists > div.all-comments-container > div.comment').hide();
	$('#comment-lists > div.all-comments-container > div.comment:lt(5)').show();
	
	if( $('#comment-lists > div.all-comments-container > div.comment').length > 5 ) {
		$("#comments_loadmore").show();
	}
	
	$("#comments_loadmore .cmd_comments_loadmore").bind('click', function(e){
		e.preventDefault();
		if( $( this ).hasClass( "open" ) ) {
			$('#comment-lists > div.all-comments-container > div.comment:gt(4)').slideUp();
			$("#comments_loadmore > .cmd_comments_loadmore").removeClass('open');
			$('#comments_loadmore > .cmd_comments_loadmore > span').text('View All Comments');
		} else {
			$('#comment-lists > div.all-comments-container > div.comment').slideDown();
			$("#comments_loadmore > .cmd_comments_loadmore").addClass('open');
			$('#comments_loadmore > .cmd_comments_loadmore > span').text('Close');
		}
	});

});
</script>
