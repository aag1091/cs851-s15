        <div id="floating_bar_adtech_container">
            <script type="text/javascript">
                (function(){
                    var el = $('.ad_floating_bar');
                    if (el) {
                        var cookie_name = 'sw_no_ad_floating_bar';
                        if (!$.cookie(cookie_name)) {
                            if (typeof floatingBarAd != 'undefined') {
                                var text = floatingBarAd();
                                document.write(text);
                                
                                // only show banner div if Ad content exists
                                setTimeout(function(){
                                	var adExists = false;
                                	var isFlash = false;
                                    
                                	var addContent = $("#floating_bar_adtech_container").children("div");
                                    if (addContent && addContent.length > 0){
                                    	adExists = true;
                                    } else {
                                    	addContent = $("#floating_bar_adtech_container").children("a");
                                    	if (addContent && addContent.length > 0){
                                            adExists = true;
                                        } else {
                                            addContent = $("#floating_bar_adtech_container").children("object");
                                            if (addContent && addContent.length > 0){
                                                adExists = true;
                                                isFlash = true;
                                            }
                                        }
                                    }
                                    //alert('adExists:' + adExists);

                                    if (adExists) {
                                    	// display:none stops flash objects from loading
                                    	// so show if flash, or not a 1x1 Ad 
                                    	if (isFlash || addContent[0].width > 1 || addContent[0].firstChild.naturalWidth > 1) {
                                    		el.css({display:'block'});
                                    		
                                    		// adjust width if iPad mode 
                                        	if (!isDesktopMode()) {
                                        		$('.inner-container').css({width:'660px'});
                                        	}
                                    	}
                                    }
                                },2000); //Adjust the timeout interval to broadcast ads more accurately
                                $('.ad_floating_bar .close').bind('click', function(evt){
                                    el.css({display:'none'});

                                    var date = new Date();
                                    date.setTime(date.getTime()+(24*60*60*1000)); // 24h
                                    document.cookie = cookie_name + "=1; expires=" + date.toUTCString() + "; path=/; domain=.premierleague.com;";
                                    Event.stop(evt);
                                });
                            }
                        }
                    }
                })();
            </script>
