			function isDesktopMode() {
				if (screen.width >= 940) {
					return true;
				}
				return false;	// iPad mode
			}

			function floatingBarAd() {
       			if (window.adgroupid == undefined) {
					window.adgroupid = Math.round(Math.random() * 1000);
				}
       			
       			//alert('isDesktop: ' + isDesktopMode() + ', screenWidth:' + screen.width);
       			
				return '<scr'+'ipt language="javascript1.1" src="http://adserver.adtech.de/addyn|3.0|327|4799947|0|16|ADTECH;cookie=info;kvlayout=' + ((isDesktopMode()) ? 'wide' : 'narrow') + ';kvskin=' + ((isDesktopMode()) ? 'yes' : 'no') + ';loc=100;target=_blank;sub1=[subst];grp='+window.adgroupid+';misc='+new Date().getTime()+'"></scri'+'pt>';
			}
		</script>
