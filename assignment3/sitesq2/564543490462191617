//ajax - xt_at to be dynamically fetched
$.ajax({
	type: 'POST',
	url: 'http://news.asiaone.com/sites/all/libraries/analytics/atinternet/include/header_details.php',
	dataType: 'json',
	cache: true,
	async: false,
	beforeSend: function(){},
	success: function(data) {
		if (data.HTTP_W !== null && data.HTTP_W.length > 1) {
			xt_at = data.HTTP_W;
			var meta = document.createElement('meta');
			meta.name = 'ldapw';
			meta.content = data.HTTP_W;
			document.getElementsByTagName('head')[0].appendChild(meta);			
		}
		else {
			xt_at = "";			
		}

		var script = document.createElement("script");
		script.type = "text/javascript";
		script.text = "xtnv = document;\n" +
			"xtsd = 'http://logw348';\n" +
			"xtsite = '542258';\n" +
			"xtn2 = '2'; // level 2 site ID\n" +
			"xtpage = 'Malaysia::Novelist_sues_flim_director_and_actor_son'; //page name (with the use of :: to create chapters)\n" +
			"xtdi = ''; //implication degree\n" +
			"xt_multc = '&x1=Novelist_sues_flim_director_and_actor_son&x2=http%3A%2F%2Fnews.asiaone.com%2Fnews%2Fmalaysia%2Fnovelist-sues-flim-director-and-actor-son&x3=1&x4=1&x5=pnafilah%40a1&x6=1&x7=1';	 //all the xi indicators (like '&x1=...&x2=....&x3=...')\n" +
			"xt_at = '" + encodeURIComponent(xt_at) + "'; //user ID\n" +
			"xt_ac = ''; //category ID\n" +
			"if (window.xtparam!=null){window.xtparam+='&ac='+xt_ac+'&at='+encodeURIComponent(xt_at)+xt_multc;}\n" +
			"else{window.xtparam='&ac='+xt_ac+'&at='+encodeURIComponent(xt_at)+xt_multc;};\n";

		// Append to body
		document.body.appendChild(script);
		
		$("body").append("<noscript>" +
			"<img width='1' height='1' alt='' src='http://logw348.ati-host.net/hit.xiti?s=542258&s2=2&p=Malaysia::Novelist_sues_flim_director_and_actor_son&di=&at="+encodeURIComponent(xt_at)+"&ac=&x1=Novelist_sues_flim_director_and_actor_son&x2=http%3A%2F%2Fnews.asiaone.com%2Fnews%2Fmalaysia%2Fnovelist-sues-flim-director-and-actor-son&x3=1&x4=1&x5=pnafilah%40a1&x6=1&x7=1'>" +
			"</noscript>");
