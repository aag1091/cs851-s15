$(document).ready(function() {
	/** Clicks **/
	/* Navigation */
	$("#navmenu a").live("click", function() {
		var elem = $(this);

		var parent = $(this)
			.parent() // li
			.parent(); // ul
		var new_parent = parent.parent();

		if (!new_parent.hasClass("has-sub")) {
			xt_click(
				$(this).get(0), // jQuery version of DOM element this
				'C', // Click
				'2', // Level 2 ID
				'Navigation::'+clean_text(elem.text()),
				'N' // Navigation
			);
		}
		else {
			xt_click(
				$(this).get(0), // jQuery version of DOM element this
				'C', // Click
				'2', // Level 2 ID
				'Navigation::'+clean_text(new_parent.find("a:first").text())+'::'+clean_text(elem.text()), // get the menu without the ul submenus
				'N' // Navigation
			);
		}
		return false;
	});
	/* Footer */
	// About us, Contact us, Advertise, Terms of use
	$("#footer_lft").find("a").live("click", function() {
		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'Footer::'+clean_text($(this).text()),
			'N' // Navigation
		);
		return false;
	});
	// navigation
	$("#footer_lft_bot .a1_lft1").find("a").live("click", function() {
		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'Footer::'+clean_text($(this).text()),
			'N' // Navigation
		);
		return false;
	});
	// social
	$(".sphp_bot").find("area").live("click", function() {
		var href = $(this).attr("href");
		var title = "";

		if (-1 !== href.indexOf("facebook")) {
			title = "Facebook";
		}
		else if (-1 !== href.indexOf("twitter")) {
			title = "Twitter";
		}
		else if (-1 !== href.indexOf("rss")) {
			title = "RSS";
		}

		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			title,
			'A' // Action
		);
		return false;
	});
	$(".footer_social").find("a").live("click", function() {
		var href = $(this).attr("href");
		var title = "";

		if (-1 !== href.indexOf("facebook")) {
			title = "Facebook";
		}
		else if (-1 !== href.indexOf("twitter")) {
			title = "Twitter";
		}
		else if (-1 !== href.indexOf("rss")) {
			title = "RSS";
		}

		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			title,
			'A' // Action
		);
		return false;
	});
	$(".footer_social").find("area").live("click", function() {
		var href = $(this).attr("href");
		var title = "";

		if (-1 !== href.indexOf("straitstimes")) {
			title = "TheStraitsTimes";
		}
		else if (-1 !== href.indexOf("stomp")) {
			title = "Stomp";
		}
		else if (-1 !== href.indexOf("razor")) {
			title = "RazorTV";
		}
		else if (-1 !== href.indexOf("businesstimes")) {
			title = "TheBusinessTimes";
		}
		else if (-1 !== href.indexOf("tnp")) {
			title = "TheNewPaper";
		}
		else if (-1 !== href.indexOf("zaobao")) {
			title = "ZaoBao";
		}
		else if (-1 !== href.indexOf("mypaper")) {
			title = "MyPaper";
		}
		else if (-1 !== href.indexOf("omy")) {
			title = "Omy";
		}
		else if (-1 !== href.indexOf("stclassifieds")) {
			title = "STClassifieds";
		}
		else if (-1 !== href.indexOf("stcars")) {
			title = "STCars";
		}
		else if (-1 !== href.indexOf("stproperty")) {
			title = "STProperty";
		}
		else if (-1 !== href.indexOf("stjobs")) {
			title = "STJobs";
		}
		else if (-1 !== href.indexOf("cyberita")) {
			title = "CyBerita";
		}
		else if (-1 !== href.indexOf("tamilmurasu")) {
			title = "TamilMurasu";
		}
		else if (-1 !== href.indexOf("913fm")) {
			title = "91.3";
		}
		else if (-1 !== href.indexOf("radio1003")) {
			title = "1003";
		}
		else if (-1 !== href.indexOf("hardwarezone")) {
			title = "HardwareZone";
		}
		else if (-1 !== href.indexOf("tabla")) {
			title = "Tabla";
		}
		else if (-1 !== href.indexOf("herworldplus")) {
			title = "HerWorldPlus";
		}

		xt_click(
			$(this).get(0), // jQuery version of DOM element this
			'C', // Click
			'2', // Level 2 ID
			'SPH_Websites::'+title,
			'A' // Action
		);
		return false;
	});
	/** Virtual Pageviews **/
	/* Photo Gallery */
	// positioning on a tag causes colorbox to be broken; using li instead
	// photo gallery
	/* // this causes popup to tag twice as the image page itself already has tagging
