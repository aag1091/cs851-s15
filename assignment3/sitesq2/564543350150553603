<script src="//stags.peer39.net/1373/trg_1373.js?c_Metro" id="peer39ScriptLoader"></script>

<script>

	(function(metro, $, window){

		var head = $('head');

		// Add special stylesheet with encoded fonts for Firefox only
		if ($.browser.mozilla) {
			head.append( '<link rel="stylesheet" href="https://s0.wp.com/wp-content/themes/vip/metrouk/css/fonts-base64.css" type="text/css" media="screen and (min-width: 768px)" />' );
		}

		var ua = navigator.userAgent;
		if (ua.indexOf("Android") >= 0) {
			var androidversion = parseFloat(ua.slice(ua.indexOf("Android") + 8));
			if (androidversion < parseFloat(3)) {
				$('.score-bar').css('display', 'none');
			}
		}

		
		// Easter egg functionality
		var metroEgg = {

			stylesheetAppended: false,
			supportsAnimation: false,
			eggContainer: null,
			vendorPrefixes: ["webkit", "Moz", "MS", ""],

			init: function(){

				this.checkSupportsAnimation();

				// Listen for Easter Egg sequence 'metro'
				var keys = [];
				var metroKeySequence = '77,69,84,82,79';

				$(document).keydown(function (e) {
					if (keys.length > 100) keys = [];
					keys.push(e.keyCode);
					if (keys.toString().indexOf(metroKeySequence) >= 0) {
						metroEgg.play();
						keys = [];
					}
				});

			},

			// Append egg HTML to the page
			write: function() {
				var html = '<div id="egg-container" class="egg-container">';
				html += '<div id="egg-cart" class="egg-cart">';
				html += '<img src="https://s0.wp.com/wp-content/themes/vip/metrouk/img/egg/head.png" class="egg-head" />';
				html += '<img src="https://s0.wp.com/wp-content/themes/vip/metrouk/img/egg/box.png" class="egg-box" />';
				html += '<img src="https://s0.wp.com/wp-content/themes/vip/metrouk/img/egg/big-wheel.png" class="egg-big-wheel" />';
				html += '<img src="https://s0.wp.com/wp-content/themes/vip/metrouk/img/egg/small-wheel.png" class="egg-small-wheel" />';
				html += '</div>';
				html += '</div>';
				$('body').append(html);
				this.eggContainer = $('#egg-container');
				if (!this.stylesheetAppended) {
					head.append('<link rel="stylesheet" href="https://s0.wp.com/wp-content/themes/vip/metrouk/css/easter-egg.css" type="text/css" />');
					this.stylesheetAppended = true;
				}
			},

			// Clone an existing egg element to allow restarting of CSS anim in FF
			clone: function() {
				var newEggContainer = this.eggContainer[0].cloneNode(true);
				this.eggContainer[0].parentNode.replaceChild(newEggContainer, this.eggContainer[0]);
				this.eggContainer = $('#egg-container');
			},

			// Check if egg element is present
			isPresent: function(){
				var eggContainer = $('#egg-container');
				if (eggContainer != null) return true;
				return false;
			},

			// Check if browser supports CSS3 animation
			checkSupportsAnimation: function() {
				var animation = false,
					elm = document.getElementById('pageBody');
				if (elm.style.animationName) {
					animation = true;
				}
				if (animation === false) {
					for (var i = 0; i < this.vendorPrefixes.length; i++) {
						if (elm.style[ this.vendorPrefixes[i] + 'AnimationName' ] !== undefined) {
							animation = true;
							break;
						}
					}
				}
				this.supportsAnimation = animation;
			},

			// Activate CSS animation or animate with JS
			play: function(){
				if (this.supportsAnimation) {
					metro.conlog('Supports animation');
					if (this.eggContainer == null){
						this.write();
					} else {
						this.clone();
					}
					this.eggContainer.addClass('animate');
				} else {
					metro.conlog('Doesn\'t support animation');
					if (this.eggContainer == null) this.write();
					this.eggContainer.addClass('animate');
					this.jsAnimate();
				}
			},

			// JS animation for browsers that don't support CSS animation
			jsAnimate: function() {
				var baby = $('#baby'),
					head = baby.find('.baby-head'),
					crown = baby.find('.baby-crown'),
					pram = baby.find('.baby-pram');

				baby.css('left', '-75%');

				var animateCrown = function() {
					crown.animate({
						bottom: '310px'
					}, {
						duration: 450,
						complete: function(){
							crown.css('bottom', '329px');
							animateCrown();
						}
					})
				}

				var animatePram = function() {
					pram.animate({
						bottom: '50px'
					}, {
						duration: 450,
						complete: function(){
							pram.css('bottom', '60px');
							animatePram();
						}
					})
				}

				var animateBaby = function() {
					baby.animate({
						left: '125%'
					}, {
						duration: 9000,
						complete: function() {
							baby.css('left', '-75%')
						}
					})
				}

				animateCrown();
				animatePram();
				animateBaby();

			}

		} // End metroEgg

		metroEgg.init();

		
	})(metro, jQuery, window);

</script>

