<!--
var _unitedCountryId = '471';
var _canadaCountryId = '472';

function ValidateUsaCanadaPostalCode() {
    var _zipCodeValue = document.getElementById('Can_HZip_8102');
    var _CountryValue = document.getElementById('Can_H_CountryFK_8102');
    if (_zipCodeValue == null || _CountryValue == null || _zipCodeValue.value == '') {
        return true;
    }
    var _validationReturn;
    if (typeof checkZipWithRegEx == 'function') {
        _zipCodeValue.onchange = ValidateUsaCanadaPostalCode;
        _CountryValue.onchange = ValidateUsaCanadaPostalCode;
        _country = _CountryValue.value;
        var _validationReturn = checkZipWithRegEx(_zipCodeValue.value, _country);
        if (_validationReturn) {
            _zipCodeValue.style.backgroundColor = '';
            removeZipLabelError(_zipCodeValue);
        }
        else {
            var hasError = false;
            try {
                if (_zipCodeValue.parentNode != null && (_zipCodeValue.parentNode.nodeName == 'TD' || _zipCodeValue.parentNode.nodeName == 'Td' || _zipCodeValue.parentNode.nodeName == 'td')) {
                    removeZipLabelError(_zipCodeValue);
                    writeZipLabelError(_zipCodeValue, '<b>Invalid zipcode/postal code format for this country.</b>');
                }
            } catch (e) {
                hasError = true;
            }
            if (hasError) {
                _zipCodeValue.title = 'Invalid Postal Code';
                _zipCodeValue.style.backgroundColor = 'yellow';
            }
        }
        return _validationReturn;
    }
    return true;
}

function removeZipLabelError(_zipCodeValue) {
    var withSuccess = true;
    try {
        var lenght = _zipCodeValue.parentNode.childNodes.length;
        var index = 0;
        while (index < lenght) {
            var kids = _zipCodeValue.parentNode.childNodes[index];
            if (kids.id == 'spPostalCodeValidationResult') {
                _zipCodeValue.parentNode.removeChild(kids);
                break;
            }
            index++;
        }
    }
    catch (e) { withSuccess = false; }
    return withSuccess;
}

function PerformResetValidation() {
    var withSuccess = true;
    try {
        var _zipCodeValue = document.getElementById('Can_HZip_8102');
        if (_zipCodeValue != null) {
            removeZipLabelError(_zipCodeValue);
            _zipCodeValue.style.backgroundColor = '';
            _zipCodeValue.style.title = 'Postal Code';
        }
    }
    catch (e) { withSuccess = false; }
    return true;
}

function checkZipWithRegEx(zip, countryCode) {
    try {
        switch (countryCode) {
            case '':
                return false;
                break;
            case _canadaCountryId:
                var RE_ZIP = /^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]{1}\d{1}[a-zA-Z]{1} *\d{1}[a-zA-Z]{1}\d{1}$/;
                return RE_ZIP.test(zip);
                break;
            case _unitedCountryId:
                var RE_ZIP = /^\d{5}(-\d{4})?$/;
                return RE_ZIP.test(zip);
                break;
            default:
                return true;
                break;
        }
    } catch (e) {
        return true;
    }
}

function PerformValidate() {
    return ValidateUsaCanadaPostalCode();
}

function writeZipLabelError(_zipCodeValue, errorMsg) {
    if (_zipCodeValue == null) {
        return true;
    }
    var withSuccess = true;
    try {
        if (_zipCodeValue.parentNode != null && (_zipCodeValue.parentNode.nodeName == 'TD' || _zipCodeValue.parentNode.nodeName == 'Td' || _zipCodeValue.parentNode.nodeName == 'td')) {
            removeZipLabelError(_zipCodeValue)
            var objSpanResult = document.createElement('span');
            objSpanResult.id = 'spPostalCodeValidationResult';
            objSpanResult.style.color = 'Red';
            objSpanResult.innerHTML = errorMsg;
            _zipCodeValue.parentNode.appendChild(objSpanResult);
            withSuccess = false;
        }
    }
    catch (e) { withSuccess = false; }
    return withSuccess;
}
//-->
</SCRIPT>
