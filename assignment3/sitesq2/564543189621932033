    <script type='text/javascript'>

        function trackMenu(menu, link)
        {
            trackEvent(menu, $(link).attr('href'));

            delayedLoad(link);

            return false;        
        }
        
        $(function()
        {
            PEPS.rollover.init();

            SetGenreLinks();

            $('a[data-image]').live('click', function()
            {
                if ($(this).attr('href'))
                {
                    trackEvent('Image link', $(this).attr('data-image'), $(this).attr('data-label'));

                    delayedLoad(this);

                    return false;
                }
            });
            
            $('a[data-artist]').live('click', function()
            {
                trackEvent('Artist link', $(this).attr('data-artist'), $(this).attr('data-label'));

                delayedLoad(this);

                return false;
            });
                      
            $('span.TopMenuItem a').live('click', function()
            {
                return trackMenu('Top Menu', this);
            });        

            $('.HeaderMenu a').live('click', function()
            {
                return trackMenu('Header Menu', this);
            });        

            $('.HeaderTopMenu a').live('click', function()
            {
                return trackMenu('Very Top Menu', this);
            });        

            $('.FooterMenu a').live('click', function()
            {
                return trackMenu('Footer Menu', this);
            });        

        });
        
        $( "img.lazy" ).lazyload( { failure_limit: 1000, threshold: 200 } );

    </script>

    

