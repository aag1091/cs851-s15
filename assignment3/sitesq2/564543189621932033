<script>
    $( function ()
    {
        $( '#modalInputContainer .pil-follow' ).click( function ()
        {
            pilFollow( this, false );

            return false;
        } );

        $( '#modalInvite' ).click( function ()
        {
            getGroupInvitationList( $( this ).attr( 'data-imageId' ), $( '#modalInputContainer' ) );

            return false;
        }
        );
    } );

</script>


	<div id='fixedImageDiv'>
		<img id='fixedImage' src='http://pic.pilpix.com/48/48346/635590077438383997-chess-players.jpg' />
	</div>

	<script type='text/javascript'>

		var fixedTimer = 0;
		
		$(function()
		{        
			$( '.TopVotingDiv .pil-follow, .BottomVotingDiv .pil-follow' ).click( function ()
			{
				pilFollow( this, true );

				return false;
			} );

			$('#commentForm').validate();
			//$('.CommentDelay').text('It can take a few minutes for comments to appear on the site.');
			highlightHelp();

			$('#CommentText').change(function() { window.onbeforeunload = warnLeaving; });
			$('.CommentReplyTextarea textarea').live('change', function() { window.onbeforeunload = warnLeaving; });

			// $('.IVoteIt').not('.ModalVote').click(function() { voteIt(this, 399099, false); return false; });
			$('.SpecialFaves').not('.ModalSpecialFaves').click(function() { addToFaves(this, 399099, false); return false; });
			
			setFixedImagePosition();
			
			$(window).resize(function(){ setFixedImagePosition();});

			$(window).scroll(function()
			{
				if ($(window).scrollTop() > $('#image1').position().top + $('#image1').height() && $(window).scrollTop() < $('#commentDiv').position().top - 200 )
				{
					$('#fixedImageDiv').fadeIn('slow');
				}
				else
				{
					$('#fixedImageDiv').fadeOut('slow');                
				}
			});
			
			$('#fixedImage').hover(
			function()
			{
				fixedTimer = setTimeout(function() { fixedTimer = 0;
				$('#fixedImage').css('width', $(window).width() - $('#fixedImage').parent().position().left - 8);                
				}, 300);
			},
			function()
			{
				clearTimeout(fixedTimer);
				fixedTimer = 0;
				$('#fixedImage').css('width', '200px');            
			}
			);
			
		});
		

		function toggleReply(commentId)
		{
			// don't hide if unposted text
	
			commentId = commentId.toString();

			if (!($('#replyText' + commentId).val().length > 0 && $('#saveReply' + commentId).val().indexOf('Post') >= 0))
			{
				$('#reply' + commentId).slideToggle();
			}
		}

		function addToPool(elem, groupId, imageId, approved)
		{
		    $.post('/ajax/addtogroup', { groupId: groupId, imageId: imageId, isAlreadyApproved: (approved || 0) },
                    function (data)
                    {
                        var result = data.split(/\t/);
                        $(elem).hide();
                        $('#addedToPool').show();
                    });
		}

	</script>



            

