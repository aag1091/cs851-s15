<script type="text/javascript">
    $(function()
    {
        $( '#pilpageload' ).text(( ( new Date() ).getTime() - pil_startload ) / 1000 );

        if ($.cookie('gm') != '1')
        {
            $('#footerGold').addClass('GoldHighlight');
