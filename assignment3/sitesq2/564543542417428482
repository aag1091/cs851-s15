								$('#new_bloc_bas_breve #infos .content-bloc').css('float','left').css('width','200px').css('padding-left','10px').css('padding-right','10px');
							}	
					
							$('.wrap .separator').each(function(){
							
							
									var heightprev = $(this).prev().height();
									var heightnext = $(this).next().height();
										$(this).css('height', Math.max(heightprev,heightnext)+'px');
									
							
							
							
							});
							
							
					
					}
					
					


