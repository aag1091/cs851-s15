function init_mobile(){
    var waWidth = screen.width;
    var hash = window.location.hash;
    var wanavagent = navigator.userAgent;
    //var waIsMobile = (isMobile() || isMobile(true));
    var waIsMobile = isMobile();
    var waIsTablet = isMobile(true);
    var cookie     = (getCookie("waIgnoreMobile") && getCookie("waIgnoreMobile")==1);
    var waRedirect = true;
    var excludeLink = /.fr(\/explore\/|\/Quotidien\/|\/club\/compte\/formulaire_newpass|\/premium\/|\/eStore\/|\/[a-z]+\/Article\/|\/concours\/|\/rss\/|\/infogs\/|\/Infog\/|\/hors-normes\/|\/special\/|\/connect\/|\/cdm2014\/pronos\/|\/.*-2ndecran|\/.*native.html)/i;

    var parts = window.location.search.substr(1).split("&");
    var isGoogleMobile = /Googlebot-Mobile/i;
    var isSocialNetwork = /twitter|fbios/i;
    var $_GET = {};
    for (var i = 0; i < parts.length; i++) {
        var temp = parts[i].split("=");
        $_GET[decodeURIComponent(temp[0])] = decodeURIComponent(temp[1]);
    }
    if(cookie || ($_GET['M'] && $_GET['M']==1) || (excludeLink.test(window.location.href)) || (isSocialNetwork.test(wanavagent) && waIsTablet) || !waIsMobile){waRedirect = false;}

    if((isGoogleMobile.test(wanavagent) || (isSocialNetwork.test(wanavagent) && !waIsTablet && waIsMobile)) && !excludeLink.test(window.location.href) && (window.location.href.indexOf('/Infog') == -1)) {waRedirect = true;}

    if(waRedirect && /free.lequipe.fr/i.test(window.location.host)){
        waRedirect = false;
    }
    // redirection vers la sortie webapp pour les relives
    if((waIsMobile || waIsTablet) && /\/Compte-rendu\/[^/]+\/\d+/i.test(window.location.href)){
        waRedirect = true;
    }
    //if(cookie || ($_GET['M'] && $_GET['M']==1) || (excludeLink.test(window.location.href)) || !waIsMobile){waRedirect = false;}
    //if((isGoogleMobile.test(wanavagent) || (isSocialNetwork.test(wanavagent) && waIsMobile)) && !excludeLink.test(window.location.href)) {waRedirect = true;}
    if(hash == '#modemobile'){
        var res = "";
        res = '<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>######################<br>MOBILE MODE<br>#################<br>';
        res += 'Navigator agent : '+wanavagent; 
        res += '<br>Cookie to site version :  '+cookie;
        res += '<br>IsMobile : '+waIsMobile;
        res += '<br>Redirection to mobile version :  '+waRedirect;
        document.write(res);
        return false;
    }
    if (xtref !='' ) { xtref='?xtref='+xtref.replace(/\?/gi,"¬§").replace(/=/gi,"¬ß")+'&'+xtor; }
    //console.log(window.location.href.replace(window.location.host,'m.lequipe.fr')+xtref);
    if (waRedirect){
        var newLocationHref=window.location.href.replace(window.location.host,'m.lequipe.fr')+xtref;
        window.location.href = newLocationHref;
    }
    else if( window.location.href.indexOf('/infogs/Football/statistiques-coupe-du-monde') >0 && waIsMobile)
        {
        
         var newLocationHref=window.location.href.replace(window.location.host,'m.lequipe.fr')+xtref;
         window.location.href = newLocationHref;
        
        }

}
function getCookie(c_name){
    var c_value = document.cookie;
    var c_start = c_value.indexOf(" " + c_name + "=");
    if (c_start == -1){
        c_start = c_value.indexOf(c_name + "=");
    }
    if (c_start == -1){
        c_value = null;
    }else{
        c_start = c_value.indexOf("=", c_start) + 1;
        var c_end = c_value.indexOf(";", c_start);
        if (c_end == -1){
            c_end = c_value.length;
        }
        c_value = unescape(c_value.substring(c_start,c_end));
    }
    return c_value;
}
init_mobile();
