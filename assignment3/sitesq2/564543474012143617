
<script type='text/javascript'>
function resizeFrame(){
	var WX,WY,BX,BY;
	var o=document.getElementById("iFaj6vUmga"),t,d;
	if (!o) return;
	d=o.contentDocument;
	if (!(t=d.getElementById("wrapperXaj6vUmga"))) WX=0;
	else WX=t.value;
	if (!(t=d.getElementById("wrapperYaj6vUmga"))) WY=0;
	else WY=t.value;
	if (!(t=d.getElementById("bannerXaj6vUmga"))) BX=0;
	else BX=t.value;
	if (!(t=d.getElementById("bannerYaj6vUmga"))) BY=0;
	else BY=t.value;
	BX=parseInt(BX)+parseInt(WX);
	BY=parseInt(BY)+parseInt(WY);
	if (BX<0) o.style.width="100%";
	else if (BX>0) o.style.width=BX+"px";
	if (BY<0) o.style.height="100%";
	else if (BY>0) o.style.height=BY+"px";
}
</script>
