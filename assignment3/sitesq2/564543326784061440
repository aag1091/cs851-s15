    <script>
    function show_menu(m) {
        for (var i=0;i<8;i++) {
            if (i!=m) {	hide_menu(i);}
        }
        document.getElementById('submenu_'+m).style.display='block';
        window.setTimeout('hide_menu('+m+')',5000);
    }
    function hide_menu(m) {
        document.getElementById('submenu_'+m).style.display='none';
    }
