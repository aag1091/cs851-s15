    <script>
    function send_2_friend_show(b) {
		if (b) {
			$('#send_2_friend_div').show().center();
			$('#shadow_div').show().css({width:$(document).width(), height:$(document).height()}).css({'opacity':'0.1'});
			$('body').css({'overflow':'hidden'});
		} else {
			$('#send_2_friend_div').hide();
			$('#shadow_div').hide();
			$('body').css({'overflow':''});
		}
    }
    function send_2_friend_check_valid(frm) {
        //var frm = document.forms['form1'];
    
        if (frm.name.value=='') {
            alert('Please enter name');
            frm.name.select();frm.name.focus();return false;
            
        } else if (!isEmail(frm.email.value)) {
            alert('Please enter email address');
            frm.email.select();frm.email.focus();return false;
    
        } else if (frm.sender_name.value=='') {
            alert('Please enter your name');
            frm.sender_name.select();frm.sender_name.focus();return false;
    
        } else {
            return true;
        }
    }
    
    	
    </script>
    