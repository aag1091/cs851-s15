											</div>
					</div>
											<script type="text/javascript">

							jQuery(document).ready(function(){
								var p_length = jQuery( ".main-body-content p" ).length;
								
								var twits = jQuery("#story_content_parts #part1").html();
								jQuery( ".main-body-content p:nth-child(2)" ).after( twits );
								
								var related = jQuery("#story_content_parts #part2").html();
								jQuery( ".main-body-content p:nth-child(3)" ).after( related );
								
								jQuery(".main-story .story-image img").each(function(){
									var width = jQuery(this).width(); 
									if(width <= 480){
											jQuery(".main-story").css({"float":"left", "width":"480"});
										}else{
											jQuery(this).css({"float":"none", "width":"100%", "height":"auto", "margin":"0 auto", "display":"block",});
										}
									});
								jQuery(".main-body-content img").each(function(){
									var width = jQuery(this).width();
									if(width > 480){
										jQuery(this).css({"float":"none", "width":"100%", "height":"auto", "margin":"0 auto", "display":"block",});
									}
								});
							});
