<script>
	require(["janrain", "janrain-logger"], function() {
	    GLOBAL.JANRAIN.authState = GLOBAL.JANRAIN.USER.AUTH;
	    GLOBAL.JANRAIN.flowState = GLOBAL.JANRAIN.FLOW.SOCIALCONNECT;
	    GLOBAL.JANRAIN.subflowState = GLOBAL.JANRAIN.SUBFLOW.NOTAPPLICABLE;
	    JANRAIN.janrainDefaultSettings(GLOBAL);
	    janrain.settings.capture.registerFlow = 'socialRegistration';
	    JANRAIN.janrainInitLoad();
	    janrain.ready = true;
	});
	var challengesTemplate = {
		  backgroundImageMobile: '',
		  backgroundImageTablet: '',
		  backgroundImageDesktop: '',
  		  interactTrackingJSON: {},
		  configureImageVars: function() {
		    challengesTemplate.backgroundImageMobile = challengesTemplate.backgroundImageMobile !== '' ? challengesTemplate.backgroundImageMobile : null;
		    challengesTemplate.backgroundImageTablet = challengesTemplate.backgroundImageTablet !== '' ? challengesTemplate.backgroundImageTablet : null;
		    challengesTemplate.backgroundImageDesktop = challengesTemplate.backgroundImageDesktop !== '' ? challengesTemplate.backgroundImageDesktop : null;
		  },
		  adjustBackgroundImage: function() {
		    if (challengesTemplate.backgroundImageDesktop && componentGlobalVars.windowSize == 'desktop') {
		      $('.challengesTemplate_BackgroundWrapper').css('background-image', 'url(' + challengesTemplate.backgroundImageDesktop +')');
		    } else if (challengesTemplate.backgroundImageTablet && componentGlobalVars.windowSize == 'tablet') {
		      $('.challengesTemplate_BackgroundWrapper').css('background-image', 'url(' + challengesTemplate.backgroundImageTablet +')');
		    } else if (challengesTemplate.backgroundImageMobile && componentGlobalVars.windowSize == 'mobile') {
		      $('.challengesTemplate_BackgroundWrapper').css('background-image', 'url(' + challengesTemplate.backgroundImageMobile +')');
		    } else {
		      if (challengesTemplate.backgroundImageDesktop) {
		        $('.challengesTemplate_BackgroundWrapper').css('background-image', 'url(' + challengesTemplate.backgroundImageDesktop +')');
		      } else if (challengesTemplate.backgroundImageTablet) {
		        $('.challengesTemplate_BackgroundWrapper').css('background-image', 'url(' + challengesTemplate.backgroundImageTablet +')');
		      } else if (challengesTemplate.backgroundImageMobile) {
		        $('.challengesTemplate_BackgroundWrapper').css('background-image', 'url(' + challengesTemplate.backgroundImageMobile +')');
		      }
		    }
		  },
			// this function submits the program name to for interact tracking.
			interactTracking: function() {
				challengesTemplate.interactTrackingJSON.program = dcs.WT.z_program;
				$.ajax({
					type: 'POST',
					url: '/ajax/v1/track/view',
					dataType: 'json',
					contentType: 'application/json; charset=utf-8',
					data: JSON.stringify(challengesTemplate.interactTrackingJSON),
					success: function(response) {
						
					}
				});
			}
		}
		$(document).ready(function() {
			challengesTemplate.interactTracking();
		  	challengesTemplate.configureImageVars();
			if (GLOBAL.pageWidth < 714) {
				componentGlobalVars.windowSize = 'mobile';
				challengesTemplate.adjustBackgroundImage();
			}
			if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
				componentGlobalVars.windowSize = 'tablet';
				challengesTemplate.adjustBackgroundImage();
			}
			if(GLOBAL.pageWidth > 1065) {
				componentGlobalVars.windowSize = 'desktop';
				challengesTemplate.adjustBackgroundImage();
			}
		});
		$(window).resize(function() {
			if (GLOBAL.pageWidth < 714) {
				if (componentGlobalVars.windowSize != 'mobile') {
				  componentGlobalVars.windowSize = 'mobile';
				  challengesTemplate.adjustBackgroundImage();
				}
			}
		  if(GLOBAL.pageWidth > 713 && GLOBAL.pageWidth < 1066) {
		    if (componentGlobalVars.windowSize != 'tablet') {
		      componentGlobalVars.windowSize = 'tablet';
		      challengesTemplate.adjustBackgroundImage();
		    }
		  }
		  if(GLOBAL.pageWidth > 1065) {
		    if (componentGlobalVars.windowSize != 'desktop') {
		      componentGlobalVars.windowSize = 'desktop';
		      challengesTemplate.adjustBackgroundImage();
		    }
		  }
		});
</script>

</body>
