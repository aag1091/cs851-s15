<script type="text/javascript">
function crosscheck() {
	if ($("#step1").is(":visible")) {
		$("#step2").show();
		$("#step1").hide();
		updateForm();
		return false;
	}
	if ($("#checkfs").is(":visible")) {
			return true;
	}
	else {
		return false;
	}
}
var email = '';
function updateForm() {
	email = document.getElementById('email_input').value;	
	document.getElementById('email').value = email;
	$.post("optin.php",
    {
	  "email":email,
	  "lid":"475",
      "CustomFields[12]":"27086",
      "CustomFields[61]":"",
      "CustomFields[67]":"",
      "CustomFields[68]":"",
      "CustomFields[76]":"",
      "CustomFields[15]":"mm4-bobproctor-2q",
      "CustomFields[73]":"NzU4MDQ1428103081"
    },
    function(data,status){
      //alert("Data: " + data + "\nStatus: " + status);
    });
}

</script>
