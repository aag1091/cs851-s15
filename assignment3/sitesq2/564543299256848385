<script type="text/javascript">
    jQuery(document).ready(function () {
        var myForm = jQuery('#searchForm');

        myForm.validate({
            errorClass: 'error',
            validClass: 'valid',
            rules: {
                q: { required: true, minlength: 2 }
            },
            success: function (error) {
                // Use a mini timeout to make sure the tooltip is rendred before hiding it
                setTimeout(function () {
                    myForm.find('#search').qtip('destroy');
                }, 1);
            },
            errorPlacement: function (error, element) {
                // Set positioning based on the elements position in the form
                //                var corners = ['left center', 'right center'],
                //               flipIt = jQuery(element).parents('span.right').length > 0,
                //               position = {
                //                   my: corners[flipIt ? 0 : 1],
                //                   at: corners[flipIt ? 1 : 0]
                //               };

                // Apply the tooltip only if it isn't valid
                jQuery(element).filter(':not(.valid)').qtip({
                    overwrite: false,
                    content: error,
                    position: {
                        corner: {
                            target: 'rightBottom'
                        }
                    },
                    show: {
                        event: false,
                        ready: true
                    },
                    //hide: 'unfocus',
                    hide: { when: { event: 'inactive' }, delay: 2000 },
                    //hide: { when: 'mouseout', fixed: true },
                    style: {
                        textAlign: 'left',
                        //color: 'lightcoral',
                        tip: 'leftTop',
                        name: 'red' // Inherit the rest of the attributes from the preset dark style
                    },
                    api: {
                        onShow: closeLater(element)
                    }
                });
            }
        });


        function closeLater(target) {
            setTimeout(function () {
                jQuery(target).qtip('hide');
            }, 3000);
        }

        jQuery("#helpButton").qtip({
            content: jQuery('#toolTipContent').html(),
            position: {
                corner: {
                    tooltip: 'topRight',
                    target: 'bottomLeft'
                }
            },
            show: { when: { event: 'click' } },
            hide: 'unfocus',
            style: {
                width: 360,
                textAlign: 'left',
                color: 'black',
                tip: 'topRight',
                name: 'cream' // Inherit the rest of the attributes from the preset dark style
            },
            api: {
                onRender: function () {
                    jQuery(".searchTip").click(function () {
                        jQuery("#search").val(jQuery(this).html());
                        jQuery("#search").focus();
                    });
                    jQuery(".searchTip").attr('title', 'Click to try it');
                }
            }
        });

    });
</script>
