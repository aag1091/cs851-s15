<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery("#logonDialogModal").dialog({
            autoOpen: false,
            bgiframe: true,
            modal: true,
            resizable: false,
            width: 960,
            height: 530,
            close: function (event, ui) { },
            open: function () { checkClientCookie(); }
        });
    });

    function showLogonShadowBox(targetUrl) {
        jQuery("#logonDialogModal").dialog('open');
        jQuery("#sbLogon").attr("action", '/Account/LogOn?ReturnUrl=' + targetUrl);
        jQuery("#registerLink").attr("href", '/Account/Register?ReturnUrl=' + targetUrl);
    }

    function showLogonShadowBox(targetUrl, openTargetInNewWindow, onCloseFunction) {
        jQuery("#logonDialogModal").dialog('open');
        var theForm = jQuery("#sbLogon");
        theForm.attr("action", '/Account/LogOn?ReturnUrl=' + targetUrl);
        jQuery("#registerLink").attr("href", '/Account/Register?ReturnUrl=' + targetUrl);
        if (openTargetInNewWindow) {
            theForm.attr("target", "_new");
        }
        jQuery("#logonDialogModal").bind("dialogclose", function (event, ui) {
            onCloseFunction();
        });
    }

</script>
<script type="text/javascript" language="JavaScript">
    function doAjaxLogon() {
        var data = {
            Username: jQuery('#ShadowBoxUserName').val(),
            Password: jQuery('#ShadowBoxPassword').val(),
            RememberMe: jQuery('#RememberMe').is(':checked')
        };
        
        jQuery.ajax({
            url: '/Account/AjaxLogOn',
            type: "POST",
            data: data,
            success: function (result) {
                if (result.success) {
                    jQuery("#sbLogon").submit();
                } else {
                    jQuery("#ajaxLogonMessage").html('*' + result.message);
                }
            },
            error: function (req, status, error) {
                alert("An unexpected error ocurred. Please try again later: " + error);
            }
        });

    }
    

    function checkClientCookie() {
        var cookie = getCookie("checkCookieEnabled");
        if (cookie == null) {
            jQuery("<div>Your browser has cookies disabled, which will prevent you from logging into GETV.<br/><br/><a style='color: #d9d103;' href='/Account/HowToEnableCookies'>Click here to learn how to turn it on.</a><br/><br/></div>").dialog({
                title: "Cookies In Your Browser Are Disabled",
                autoOpen: true,
                bgiframe: true,
                modal: true,
                resizable: false,
                width: 460,
            });
            //alert("No cookies enabled");
        }
    }
    

    if (typeof String.prototype.trimLeft !== "function") {
        String.prototype.trimLeft = function() {
            return this.replace(/^\s+/, "");
        };
    }
    if (typeof String.prototype.trimRight !== "function") {
        String.prototype.trimRight = function() {
            return this.replace(/\s+$/, "");
        };
    }
    if (typeof Array.prototype.map !== "function") {
        Array.prototype.map = function(callback, thisArg) {
            for (var i=0, n=this.length, a=[]; i<n; i++) {
                if (i in this) a[i] = callback.call(thisArg, this[i]);
            }
            return a;
        };
    }
    function getCookies() {
        var c = document.cookie, v = 0, cookies = {};
        if (document.cookie.match(/^\s*\$Version=(?:"1"|1);\s*(.*)/)) {
            c = RegExp.$1;
            v = 1;
        }
        if (v === 0) {
            c.split(/[,;]/).map(function(cookie) {
                var parts = cookie.split(/=/, 2),
                    name = decodeURIComponent(parts[0].trimLeft()),
                    value = parts.length > 1 ? decodeURIComponent(parts[1].trimRight()) : null;
                cookies[name] = value;
            });
        } else {
            c.match(/(?:^|\s+)([!#$%&'*+\-.0-9A-Z^`a-z|~]+)=([!#$%&'*+\-.0-9A-Z^`a-z|~]*|"(?:[\x20-\x7E\x80\xFF]|\\[\x00-\x7F])*")(?=\s*[,;]|$)/g).map(function($0, $1) {
                var name = $0,
                    value = $1.charAt(0) === '"'
                              ? $1.substr(1, -1).replace(/\\(.)/g, "$1")
                              : $1;
                cookies[name] = value;
            });
        }
        return cookies;
    }
    function getCookie(name) {
        return getCookies()[name];
    }
</script>

