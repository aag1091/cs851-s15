		function effectFadeIn(id, repeatCount) {
			if (repeatCount > 0) {
				repeatCount--;
				jQuery("#" + id).fadeOut(1000).fadeIn(2000, effectFadeOut(id, repeatCount));
			}
		}
		function effectFadeOut(id, repeatCount) {
			if (repeatCount > 0) {
				repeatCount--;
				jQuery("#" + id).fadeIn(2000).fadeOut(1000, effectFadeIn(id, repeatCount));
			}
		}

