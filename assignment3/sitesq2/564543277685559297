<script type="text/javascript">
jQuery(document).ready(function($){
$("#frm_form_24_container input[type=submit]").attr('disabled','disabled'); //disable submit button by default

$("input[name^='item_meta[201]']").change(function(){ 
var submit = true;

if($("input[name^='item_meta[201]']:checked").val() == 'Every Year' ||
$("input[name^='item_meta[201]']:checked").val() == 'Bi-Annually' ||
$("input[name^='item_meta[201]']:checked").val() == 'Monthly' ||
$("input[name^='item_meta[201]']:checked").val() == 'At Christmas')
 submit = false;

if(submit){$("#frm_form_24_container input[type=submit]").removeAttr('disabled'); }
else{$("#frm_form_24_container input[type=submit]").attr('disabled','disabled'); }

});
});
</script>

<script type="text/javascript">
jQuery(document).ready(function($){
$("#frm_form_25_container input[type=submit]").attr('disabled','disabled'); //disable submit button by default

$("input[name^='item_meta[206]'], input[name^='item_meta[207]'], input[name^='item_meta[208]']").change(function(){ 
var submit = true;

if($("input[name^='item_meta[206]']:checked").val() == 'no' ||
$("input[name^='item_meta[207]']:checked").val() == 'no' ||
$("input[name^='item_meta[208]']:checked").val() == 'no')

 submit = false;

if(submit){$("#frm_form_25_container input[type=submit]").removeAttr('disabled'); }
else{$("#frm_form_25_container input[type=submit]").attr('disabled','disabled'); }

});
});
</script>
