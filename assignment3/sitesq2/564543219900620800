<script>
if($.cookie('local_site'))
{
	$(".header-search").css("width","25%");	
	$("#localSiteReturn").attr("href", "http://elitetradepainting.com/" + $.cookie('local_site'));
	$("#postalCode").val($.cookie('postal_code'));
	$("#localSiteReturn").show();
}

$(".mainNavItem").hover(
 	 function() {$(this).children(".submenu").show();}, 
	function() {$(this).children(".submenu").hide();}
);

$(".mobile-menu .mainNavItem").hover(
 	 function() {$(this).children(".mobile-submenu").show();}, 
	function() {$(this).children(".mobile-submenu").hide();}
);

$('#postalCode').keyup(function() {getFranchiseInfo($('#postalCode').val());});

$("#postalSearch").click(function() {
	if(typeof franchiseDirectory !== 'undefined')
    {
      $.cookie('local_site', franchiseDirectory);
      $.cookie('postal_code', $('#postalCode').val());
      window.location.href = "http://elitetradepainting.com/" + franchiseDirectory;
    }
    else window.location.href = "http://elitetradepainting.com/locate_us/no_franchise";
});



$(window).load(function() {
    $("#flexiselDemo1").flexisel({
		 visibleItems: 3,
		 responsiveBreakpoints: { 
            portrait: { 
                changePoint:480,
                visibleItems: 1
            }, 
            landscape: { 
                changePoint:640,
                visibleItems: 2
            },
            tablet: { 
                changePoint:1000,
                visibleItems: 3
            }
        }
	});
    
});


</script>

