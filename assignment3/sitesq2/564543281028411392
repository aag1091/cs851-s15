    <script>
        $(document).ready(function(){
            calcTimeline();
        });

        $(window).bind('resize', function(){
            calcTimeline();
        });

        function calcTimeline() {
            var reset_width = $('.content-in').width(),
                persons = $('.person');

            $('.timeline-in').css('width',reset_width);
            if(persons.length) {
                for(p = 0; p <= persons.length; p++) {
                    var ps = $(persons[p]),
                        ps_start = ps.data('start'),
                        ps_end = ps.data('end'),
                        ps_width = ps.data('width'),
                        new_width = reset_width,
                        new_margin = 0;

                    if(ps_start != 0 || ps_end != 100) {
                        new_width = Math.round((reset_width * ps_width) / 100);

                        if(ps_start != 0) {
                            new_margin = Math.round((reset_width * ps_start) / 100);

                            if(new_width+new_margin > reset_width) {
                                new_margin = new_margin-(new_margin+new_width-reset_width);
                            }
                        }
                    }

                    ps.find('a').css({
                        'margin-left': new_margin+'px',
                        'width':new_width+'px'
                    });
                }
            }
        }
    </script>

