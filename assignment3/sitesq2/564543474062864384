<script type="text/javascript">

    /* Utility scripts
    ====================================================================================================*/
    function GetDropDownListSelectedValue(DropDownList) {
        return $.trim($(DropDownList).children('option:selected').val());
    }
    function GetTextBoxText(TextBox) {
        return $.trim($(TextBox).val());
    }
    function GetTextBoxTextLength(TextBox) {
        return GetTextBoxText(TextBox).length;
    }

    /* Global objects
    ====================================================================================================*/
    var Dataware = Dataware || {};
    Dataware.JoinOnline = {
        Startup: function () {

            // Find all donation amount 'options' element.
            $('.donationamounts .options').each(function () {

                // Set the visibility of the 'Other' textboxes for each element.
                Dataware.JoinOnline.SetDonationOtherAmountVisibility($(this));

                // Wire up donation amount changed events.
                $(this).find('input').change(function () {
                    Dataware.JoinOnline.DonationAmountChanged($(this)); // $(this) is the current input.
                });
            });

            // Make sure all currency inputs display as 00.00 on blur.
            $('input.currency').blur(function () {
                var float_OtherAmount = parseFloat($(this).val());
                if (!isNaN(float_OtherAmount)) {
                    $(this).val(float_OtherAmount.toFixed(2));
                }
            });
        },

        // Donation Amounts
        SetDonationOtherAmountVisibility: function ($options) {
            var $fieldset = $options.siblings('fieldset');
            var visible = $fieldset.is(':visible');
            var selectedvalue = $options.find('input:checked').val();

            if (selectedvalue == 999 && !visible) { // Other amount and other fieldset not visible - show fieldset.
                $fieldset.show();
            }
            else if (selectedvalue != 999 && visible) { // 'Standard' amount and other fieldset is visible - hide fieldset.
                $fieldset.hide();
            }
        },
        DonationAmountChanged: function ($option) {
            var $options = $option.parents('.options');
            Dataware.JoinOnline.SetDonationOtherAmountVisibility($options);
        }
    }

    /* Validation scripts
    ====================================================================================================*/

    // Member Details
    function customvalidator_MemberDetails_MembershipType_ClientValidate(sender, args) {

        var $selectedMembership = $('.membershiptype .RadioButton input').filter(':checked');

        // Ensure a Membership Type is selected.
        if ($selectedMembership.length < 1) {
            sender.errormessage = "Please select a Membership Type.";
            args.IsValid = false;
            return;
        }
        
        // Check various conditions per selection.
        var var_Value = $selectedMembership.parent().data('radio-value').toLowerCase();
        switch (var_Value) {
            //case 'armedforces': {
            //    // If Armed Forces selected, ensure a Service No. is entered.
            //    if (GetTextBoxTextLength($('.membershiptype input.serviceno')) < 1) {
            //        sender.errormessage = "Please enter your Service No.";
            //        args.IsValid = false;
            //        return;
            //    }
            //} break;
            case 'under22': {
                // If Under 22 selected, ensure DOB has been entered and is valid.
                var var_DOB = GetTextBoxText($('input.dob'));
                if (var_DOB.length < 1) {
                    sender.errormessage = "Please enter your Date of Birth.";
                    args.IsValid = false;
                    return;
                }

                // Validate entry.
                if (!moment(var_DOB, 'DD-MM-YYYY').isValid()) {
                    sender.errormessage = "Please enter a valid Date of Birth.";
                    args.IsValid = false;
                    return;
                }

                // Check that the applicant is under 22 years old.
                var now = moment();
                var dob = moment(var_DOB, 'DD-MM-YYYY');
                var var_diff = now.diff(dob, 'years'); // returns a rounded down number.
                if (var_diff >= 22) {
                    sender.errormessage = "You must be 22 or under to apply for 'Under 22' membership.";
                    args.IsValid = false;
                    return;
                }
            } break;
            case 'other': {
                // If Other selected, ensure a valid monetary amount is entered.
                var var_OtherAmount = GetTextBoxText($('.membershiptype input.otheramount'));
                var float_OtherAmount = parseFloat(var_OtherAmount);

                if (isNaN(float_OtherAmount) || float_OtherAmount < 15) {
                    sender.errormessage = "Please enter at least &pound;15.00 for your membership.";
                    args.IsValid = false;
                    return;
                }
            } break;
        }

        // Valid.
        args.IsValid = true;
    }
    function customvalidator_MemberDetails_VoluntaryDonation_ClientValidate(sender, args) {

        var $selectedOption = $('.donationamounts .options input').filter(':checked');

        if ($selectedOption.val() == "999") {
            // If Other selected, ensure a valid monetary amount is entered.
            var var_OtherAmount = GetTextBoxText($('.donationamounts input.otheramount'));
            var float_OtherAmount = parseFloat(var_OtherAmount).toFixed(2);

            if (isNaN(float_OtherAmount) || float_OtherAmount <= 0) {
                sender.errormessage = "Please enter a valid donation amount.";
                args.IsValid = false;
                return;
            }
        }

        // Valid.
        args.IsValid = true;
    }

    // Patron Details
    function customvalidator_PatronDetails_AdditionalOptions_ClientValidate(sender, args) {

        var $selectedOption = $('.additionaloptions .RadioButton input').filter(':checked');

        // Ensure an Additional Option is selected.
        if ($selectedOption.length < 1) {
            sender.errormessage = "Please select a Membership Option.";
            args.IsValid = false;
            return;
        }

        // Check various conditions per selection.
        var var_Value = $selectedOption.parent().data('radio-value').toLowerCase();
        if (var_Value == 'existingmember') {
            // If Existing Member selected, ensure a Membership No. is entered.
            if (GetTextBoxTextLength($('.additionaloptions input.membershipno')) < 1) {
                sender.errormessage = "Please enter your Membership No.";
                args.IsValid = false;
                return;
            }
        }

        // Valid.
        args.IsValid = true;
    }    

    // Donation panel validation scripts
    function customvalidator_DonationDetails_DonationOptions_ClientValidate(sender, args) {

        console.log('validating');

        var $selectedOption = $('.donationoptions .RadioButton input').filter(':checked');

        // Ensure an option is selected.
        if ($selectedOption.length < 1) {
            sender.errormessage = "Please select if you are a UKIP Member or not.";
            args.IsValid = false;
            return;
        }

        // Check various conditions per selection.
        var var_Value = $selectedOption.parent().data('radio-value').toLowerCase();
        if (var_Value == 'existingmember') {
            // If Existing Member selected, ensure a Membership No. is entered.
            if (GetTextBoxTextLength($('.donationoptions input.membershipno')) < 1) {
                sender.errormessage = "Please enter your Membership No.";
                args.IsValid = false;
                return;
            }
        }

        // Valid.
        args.IsValid = true;
    }
    function customvalidator_DonationDetails_DonationAmount_ClientValidate(sender, args) {

        var $selectedOption = $('.donationamounts .options input').filter(':checked');

        // Ensure an option is selected.
        if ($selectedOption.length < 1) {
            sender.errormessage = "Please select or enter a donation amount.";
            args.IsValid = false;
            return;
        }

        if ($selectedOption.val() == "999") {
            // If Other selected, ensure a valid monetary amount is entered.
            var var_OtherAmount = GetTextBoxText($('.donationamounts input.otheramount'));
            var float_OtherAmount = parseFloat(var_OtherAmount).toFixed(2);

            if (isNaN(float_OtherAmount) || float_OtherAmount <= 0) {
                sender.errormessage = "Please enter a valid donation amount.";
                args.IsValid = false;
                return;
            }
        }

        // Valid.
        args.IsValid = true;
    }

    // Checkboxes panel validation scripts
    function customvalidator_Checkboxes_Terms_ClientValidate(sender, args) {
        // Ensure that terms have been agreed.
        args.IsValid = $('.termsagree input').filter(':checked').length > 0;
    }

</script>

