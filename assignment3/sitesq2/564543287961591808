<script>
$(document).ready(function(e) {

  $( "#email" ).on( "keydown", function(event) {
      if(event.which == 13) 
	  trylogin();
    });
	  $( "#password" ).on( "keydown", function(event) {
      if(event.which == 13) 
	  trylogin();
    });
$("#loginbutton").click( function(){
	$("#widget_headerbox_buttons").hide();
	$("#widget_headerbox_login").show();	
});

$("#loginbuttoncancel").click( function(){
$("#widget_headerbox_buttons").show();
	$("#widget_headerbox_login").hide();
});

$("#createnewaccount").click( function(){
window.location="/newaccount";
});

$("#loginbuttonsubmit").click( function(){
trylogin();
});
function trylogin(){

	var email=$("#email").val();		
	var password=$("#password").val();			
	if(email.length<4){
	alert( "Invalid Email address");
		return false;	
	}
		if(password.length<5){
	alert( "Very short password ");
		return false;	
	}
$("#widget_headerbox_buttons").hide();
$("#widget_headerbox_login").hide();
$("#widget_headerbox_loggingin").show();

	$.post( "/ajax/users/login.php", { email: email, password: password }).done(function( data ) { 
//	 alert( "Data Loaded: " + data ); 
//return "emailused" "shortpassword" invalidemail" "ok";
if(data=="wrong"){
	alert("Wrong Email/password combination please try again.");
$("#widget_headerbox_login").show();
$("#widget_headerbox_loggingin").hide();

		return false;

}
if(data=="blocked")
{
	alert("Your IP is blocked because you entered too may wrong email/password combination, Please try again in 10 minutes.");	
$("#widget_headerbox_login").show();
$("#widget_headerbox_loggingin").hide();
	return false;
}
	if(data=="ok"){

    location.reload();
	return false;
}

alert(data);
});
}



});
</script>
