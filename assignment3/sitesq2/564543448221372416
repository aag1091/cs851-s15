<script language="javascript">
function TrackLinkAtlas(LinkDesc, destURL, ProspectID, PromoID, newWindow)
{
	var imgPixel = new Image( 1, 1 );
	imgPixel.src = "https://view.atdmt.com/action/" + LinkDesc + "/v3/atz." + ProspectID + "/atc1." + PromoID + "";
	if(destURL != null)
	{
		if (newWindow)
		var cmd = "window.open('" + destURL + "')";
		else
		var cmd = "location.href='" + destURL + "'";
		var t = setTimeout( cmd, 900 );
	}
}
</script>
