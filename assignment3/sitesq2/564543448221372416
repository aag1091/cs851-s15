}
		
function TrackPixel(pixID)
{
	var r = Math.random() * 100000000;
	var imgPixel = new Image( 1, 1 );    

	szPixSrcURL = "//citi.bridgetrack.com/track/?id=" + pixID + "&r=" + r;
	imgPixel.src = szPixSrcURL; 
			
	var cmd = "";
	var t = setTimeout( cmd, 900 );
}
</script>
