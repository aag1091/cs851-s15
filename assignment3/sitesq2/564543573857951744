<script>

$('div.view-song > a').click(function(e) {
	e.stopPropagation();
});

$('a.player-favorite').click(function(e) {
	e.preventDefault();
	var el = $(this);
	var suffix = ' Album';
	$.ajax({ url: el.attr('href'), data: {ajax : true},
		success: function(result) {
			if (result.is_favorite) {
				$('a.player-favorite').attr('title', 'Un-Favorite' + suffix)
				  .tooltip('hide')
				  .attr('data-original-title', 'Un-Favorite' + suffix)
				  .tooltip('fixTitle')
				  .tooltip('show');
				$('a.player-favorite').addClass('active').blur();
			} else {
				$('a.player-favorite').attr('title', 'Favorite' + suffix)
				  .tooltip('hide')
				  .attr('data-original-title', 'Favorite' + suffix)
				  .tooltip('fixTitle')
				  .tooltip('show');
				$('a.player-favorite').removeClass('active').blur();
			}

		}
	});
});
</script>					</div><!--/.player-->
