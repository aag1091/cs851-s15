								<script>
								$('a.download').on('click', function() {
									var dl = $(this).data('url');
									ga('send', 'event', { eventCategory: 'Downloads', eventAction: $(this).attr('href').split('?')[0], eventLabel: window.location.href});
									setTimeout(function() {recordDl(dl)}, 250);
								});
								</script>

