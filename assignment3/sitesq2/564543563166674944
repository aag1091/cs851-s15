<section id="mediacontentstory" class="yom-mod yom-card-main yom-article" data-uuid="76c14f3d-2b4f-30ef-8345-d1f8f27c6d8a" data-type="story" data-ylk="t1:a3;t2:ct-mod;sec:ct-mod;itc:0;rspns:nav;">
