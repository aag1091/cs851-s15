<script type="text/javascript">
DuplicateSubmitProtection = {};
DuplicateSubmitProtection.protectedTags = {};
DuplicateSubmitProtection.protect = function(tag) {
  var submitted = this.protectedTags[tag];
  this.protectedTags[tag] = true;
  return submitted;
};
DuplicateSubmitProtection.reset = function(tag) {
  delete this.protectedTags[tag] ;
};

// Fix for double submits
jQuery(document).ready(function(){
  jQuery('form[data-dup-safe=true]').submit(function(e){
    var form = jQuery(this);
    if (form.data('submitted')) {
      return false;
    } else {
      form.data('submitted', true);
      jQuery('fb\\:login-button a.fb_button').attr('onclick', 'return false;');
    }
  });
});

//fix for simultaneous normal register and fb register
jQuery(window).load(function () {
  jQuery('fb\\:login-button a.fb_button').click(function(){
    jQuery('form[data-dup-safe=true]').each(function(){
      jQuery(this).data('submitted', true);
    });
  });
});

jQuery(document).ready(function() {
  jQuery('a[data-dup-safe=true]').click(function(e) {
    var link = jQuery(this);
    if (link.data('protected')) {
      return false;
    } else {
      link.data('protected', true);
    }
  });
});

</script>






