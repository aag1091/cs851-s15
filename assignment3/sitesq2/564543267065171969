<script type="text/javascript">
	(function() {
		var user_info = decodeURI(cbs_lib.get_cookie('sso-userdata'));
		
		//Make sure the cookie exists and make sure it is not set to empty after loggin out.
		if (typeof user_info !== 'undefined' && typeof user_info !== null && user_info) {
			var user_data_obj = JSON.parse(user_info);
		}
		var user_data_string = '';

		if (user_data_obj) {
			if (user_data_obj.hasOwnProperty('ZipCode')) {
				user_data_string += '&zip=' + encodeURIComponent(user_data_obj.ZipCode);
			}
			if (user_data_obj.hasOwnProperty('Gender')) {
				user_data_string += '&gd=' + encodeURIComponent(user_data_obj.Gender);
			}
			if (user_data_obj.hasOwnProperty('Age')) {
				user_data_string += '&ag=' + encodeURIComponent(user_data_obj.Age);
			}
		}
	
		var xl8 = document.createElement('script');
		xl8.type = "text/javascript";
		xl8.src = "http://loadus.exelator.com/load/?j=d" +
			"&p=279" +
			"&g=019" +
			"&ctg1=animals&subctg1=environment&ctg2=everyday-earth&subctg2=local&ctg3=news&subctg3=tech" +
			user_data_string;
		
		var node = document.getElementsByTagName('script')[0];
		node.parentNode.insertBefore(xl8, node);
	})();
</script>
<!-- End eXelate Tag -->

