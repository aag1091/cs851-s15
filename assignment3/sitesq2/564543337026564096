		jQuery('#tab-news').click(function(){
			jQuery('#tab-news').addClass('current');
			jQuery('#tab-popular').removeClass('current');
			jQuery('#tab-reviews').removeClass('current');
			jQuery('#nav-news').show();
			jQuery('#nav-popular').hide();
			jQuery('#nav-reviews').hide();
		});
		jQuery('#tab-popular').click(function(){
			jQuery('#tab-news').removeClass('current');
			jQuery('#tab-popular').addClass('current');
			jQuery('#tab-reviews').removeClass('current');
			jQuery('#nav-news').hide();
			jQuery('#nav-popular').show();
			jQuery('#nav-reviews').hide();
		});
		jQuery('#tab-reviews').click(function(){
			jQuery('#tab-news').removeClass('current');
			jQuery('#tab-popular').removeClass('current');
			jQuery('#tab-reviews').addClass('current');
			jQuery('#nav-news').hide();
			jQuery('#nav-popular').hide();
			jQuery('#nav-reviews').show();
		});
	</script>



