/\/content.clearchannel.com\/cc-common\/gallery\/thumbCrop.php?src=\/export\/home\/jacor-common\/mlib\/2135\/02\/1422991502112.jpg&wmax=75&hmax=75&quality=80"}];
    $("#galleryContent").galleria({
        debug: false,
        width: 670,
        height: 795,
        show: photo_index,
        thumbnails: show_thumbnails,
        history: true,
        transition: "flash",
        carouselSteps: 7,
        dataSource: galleryData
    });

    // Initialize google share button
    gapi.plus.render(document.getElementById('gplus'), getParamBag(gallery_url));

    Galleria.ready(function() {
        this.bind('image', function(e) {
        var data = this.getData();
        var shareUrl = gallery_url + data.mediaid;

        if(this.getIndex() > 0) {

            //Reinitialize all social buttons
            // Facebook
            var fblike_optn = document.getElementById("fblike");
            if(typeof fblike_optn !== 'undefined' && fblike_optn !== null)
            {
                $('#fbshare').html('<fb:like href='+shareUrl+' send="false" layout="button_count" show_faces="false" width="100" action="recommend" send="true" colorscheme="light" style="position: absolute; top: 0px; left:0px;" id="fblike"></fb:like>');
                if (typeof FB !== 'undefined') {
                    FB.XFBML.parse(document.getElementById('fbshare'));
                }
            }

            var fbframe_optn = document.getElementById("fbframe");
            if(typeof fbframe_optn !== 'undefined' && fbframe_optn !== null)
            {
                new_src = "http://www.facebook.com/plugins/like.php?href="+shareUrl+"&layout=button_count&show_faces=false&width=100&action=recommend&send=true&colorscheme=light&height=21";
                document.getElementById("fbframe").setAttribute('src', new_src);
            }

            //Google share
            if(typeof(gapi) !== 'undefined') {
                    gapi.plus.render(document.getElementById('gplus'), getParamBag(shareUrl));
            }

            //Twitter tweet button
            if(typeof(twttr) !== 'undefined') {
                var data_via = '';
                var data_text = '';
                if(typeof($('.twitter-share-button').attr('data-url')) !== 'undefined') {
                    data_via = 'data-via="'+$('.twitter-share-button').attr('data-url')+'"';
                }
                if(typeof($('.twitter-share-button').attr('data-text')) !== 'undefined') {
                    data_text = 'data-text="'+$('.twitter-share-button').attr('data-text')+'"';
                }
                var tweet_button = '<a class="twitter-share-button" href="http://twitter.com/share" data-url="'+shareUrl+'" data-count="horizontal" '+data_text+' '+data_via+'>Tweet</a>';
                $("iframe.twitter-share-button").replaceWith(tweet_button);
                twttr.widgets.load();
            }

            // addthis social button
            if((typeof(addthis_conf) != 'undefined')){
                addthis.ost = 0;
                addthis.update('share', 'url', shareUrl); // new url
                addthis.update('share', 'title', window.document.title); // new title.
                addthis.ready(); // this will re-render the buttons.
                addthis.init()
            }
        }
    });  });

    var sponsorShowed   = 0;
    var sponsorBanner   = {
        showBanner: function(){
            if(sponsorShowed==0 && show_splash){
                if (show_thumbnails === false) {
                    $(".galleria-container").append("<div id='sponsorBanner' style='position:absolute; height: 450px; width: 600px; z-index: 1001; top: 4px;'></div>");
                } else {
                    $(".galleria-container").append("<div id='sponsorBanner' style='position:absolute; height: 450px; width: 600px; z-index: 1001; top: 70px;'></div>");
                }
                $("#sponsorBanner").append("<div id='sponsorBannerContent' style=\"color: #fff; height: 600px;\"></div>").delay(3200).fadeOut();
                if (typeof(dfp_active) != 'undefined' && dfp_active != "") {
                    DFP.pushAd({div: "sponsorBannerContent", position: "7012", size: "600x450"});
                } else {
                    launchCCTempAd("sponsorBannerContent","7012","600","450",tile300x250,counter300x250,"iframe","");
                }
                sponsorShowed = 1;
            }
        }
    };
function getParamBag(url){
    return {
          action: "share",
          href: url,
          annotation:'bubble'
    };
}
</script>
