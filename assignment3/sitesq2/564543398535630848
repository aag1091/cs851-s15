					$("#videoswiper-thumbs-rating-down").empty();
					$("#videoswiper-thumbs-rating-down").append(data.down);
					
					$('#vote-popup-msg').empty();
					$('#vote-popup-msg').append('<div class="success">Success! your vote has been added</div>');
				}else{
					$('#vote-popup-msg').empty();
					$('#vote-popup-msg').append('<div class="error">Sorry! you have already voted on this</div>');
				}
  			}
  		});
  		return false;
  	});
  	
