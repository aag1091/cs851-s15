  			data: {
  			action: 'MyAjaxFunctionDown', id: '87920'
  			},
  			dataType: 'json',
  			success: function(data, textStatus, XMLHttpRequest){
				if(data.status == 1){
					$("#videoswiper-thumbs-rating-total").empty();
					$("#videoswiper-thumbs-rating-total").append(data.total);
