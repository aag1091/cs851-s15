    <h1 class="cN-headingPage" itemprop="name">Former commissioner says disability cuts breach UN convention</h1>
    <!-- google_ad_section_end -->
    <div class="meta hnews">
        <dl>
            <dt class="hiddenVisually">Date</dt>
            <dd class="updated dtstamp">
                <time itemprop="datePublished" datetime="February 7, 2015">
                February 7, 2015</time>
            </dd>
        </dl>
        <!--todo: on UGC-->
        <ul class="tools">
            <li class="reading collectorHidden" style="display:none">(29)</li>
            <li class="clipping" data-assetid="d-137qo4" data-assettype="ARTICLE" data-asseturl="http://www.smh.com.au/federal-politics/political-news/former-commissioner-says-disability-cuts-breach-un-convention-20150206-137qo4.html">
                    <button class="btnClipping btnFauxLink" type="button">Read later</button>
                </li>
            </ul>
        <!-- Display author details -->
        <!-- class:cT-headshot -->
                    <div class="author optionA">
                        <div class="cT-headshot">
                            <h3 class="authorName">Julia May</h3>
                        </div>
                        <!-- class:cT-headshot -->
                    </div>
                <!-- Display author details
        <div class="zoom hiddenVisually">
