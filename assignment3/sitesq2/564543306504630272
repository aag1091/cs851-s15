	<script type="text/javascript" src="http://www.platzshare.com/application/views/platzshare/web-content/lib/scripts/jquery-ui-1.8.21.core_datepicker.min.js"></script>
	<script type="text/javascript" src="http://www.platzshare.com/application/views/platzshare/web-content/lib/scripts/mCustomScrollbar/jquery.mCustomScrollbar.min.js"></script>
	<script type="text/javascript">
		jQuery(document).ready(function() {
			 attach_search_datepickers();
			 attach_inquiry_datepickers();
			 jQuery('#booking_search_keyword').click(function() {
			 	jQuery("#booking_search_check_in, #booking_search_check_out").datepicker();
			 });
	         
	         if(jQuery('#landing').length > 0)
	            jQuery('#landing').removeClass('loading').delay(1500).fadeOut('medium', function() { ps_init_page(); });
	         else
	            ps_init_page();
	         
	         // do_scrollbars();
	
			// Add datepickers for Request Info inputs
			jQuery('input[name="date-of-exp-to"], input[name="date-of-exp-from"]').datepicker();
		
		});
		function attach_search_datepickers() {
			if(typeof(jQuery("#booking_search_check_in, #booking_search_check_out").datepicker) != 'function') {
				setTimeout("attach_search_datepickers();", 2000);
				return;
			} else
				jQuery("#booking_search_check_in, #booking_search_check_out").datepicker();
		}
		function attach_inquiry_datepickers() {
			if(typeof(jQuery(".inquiry_datepicker").datepicker) != 'function') {
				setTimeout("attach_inquiry_datepickers();", 2000);
				return;
			} else
				jQuery(".inquiry_datepicker").datepicker();
		}
	    
	    function do_scrollbars(undo) {
	    	undo = typeof(undo) == 'undefined' ? false : true;
	    	
	    	if(undo == true) {
	    		jQuery('.mCSBscrollbars').mCustomScrollbar('destroy');
	    		return;
	    	}
	    	
	        jQuery('.mCSBscrollbars').mCustomScrollbar({
	            theme:"dark",
	            scrollButtons: {
	                enable:true
	            },
	            advanced:{
	                updateOnBrowserResize: true,
	                updateOnContentResize: true
	            }
	        });
	    }
	    
	    var gallery_image_loading = false;
	    var gallery_thumb_width,
	        gallery_thumb_margin,
	        gallery_thumb_border_width,
	        gallery_thumbs_ul_width_computed,
	        gallery_step_size,
	        gallery_viewport_width,
	        gallery_thumbs_sliding = false,
	        gallery_slide_thumb_step = 7;
	    var archive_rental_fullimage_loading = false;
	    function ps_init_page() {
	        ps_animate_load_bgs();
	        setTimeout('jQuery("#post_landing .slide_set.reservation").animate({opacity:"1"});', 1000);
	        
	        ps_adjust_elements_on_resize('#post_landing, #single_rental, #archive_rental');
	        ps_adjust_fontsizes_on_resize();
	        ps_adjust_other_elements();
	        jQuery(window).resize(function() {
	            ps_adjust_elements_on_resize('#post_landing, #single_rental, #archive_rental');
	            ps_adjust_fontsizes_on_resize();
	            ps_adjust_other_elements();
	        });
	        
	        ps_datepicker('.ps_datepicker');
	        
	        // Single page events
	        jQuery('.tab[data-targettabpanel]').click(function() {
	        	jQuery(this).parent().find('.tab').removeClass('active');
	        	jQuery(this).addClass('active');
	        	jQuery(this).parent().parent().find('.tab_panel').css({display:'none'});
	        	jQuery('.tab_panel[data-tabpanel="'+jQuery(this).attr('data-targettabpanel')+'"]').css({display:'block'});
	        });
	        setTimeout('jQuery(".rental_overpanel").removeClass("closed"); jQuery(jQuery(".rental_overpanel h2 .tab")[0]).click();', 800);
	         
	        jQuery('.rental_overpanel a.close').click(function() {
	            jQuery('.rental_overpanel').height(jQuery('.rental_overpanel').height()).addClass('closed');
	        });
	        jQuery('.rental_overpanel').click(function(e) {
	            if(jQuery(e.target).hasClass('close')) return;
	            if(jQuery(this).hasClass('closed'))
	                jQuery(this).removeClass('closed');
	        });
	        // Single page events - gallery thumbs
	        if(typeof(gallery_viewport_width) == 'undefined') gallery_viewport_width = jQuery('#single_rental .sr_right .gallery_thumbs').width();
	        if(typeof(gallery_thumb_width) == 'undefined') gallery_thumb_width = jQuery('#single_rental .sr_right .gallery_thumbs ul li:first-child').width();
	        if(typeof(gallery_thumb_margin) == 'undefined') gallery_thumb_margin = parseInt(jQuery('#single_rental .sr_right .gallery_thumbs ul li:first-child').css('margin-right'));
	        if(typeof(gallery_thumb_border_width) == 'undefined') gallery_thumb_border_width = parseInt(jQuery('#single_rental .sr_right .gallery_thumbs ul li:first-child').css('border-left-width')) * 2;
	        gallery_thumbs_ul_width_computed = (gallery_thumb_width + gallery_thumb_margin + gallery_thumb_border_width) * jQuery('#single_rental .sr_right .gallery_thumbs ul li').length;
	        jQuery('#single_rental .sr_right .gallery_thumbs a.next').css({ display: ((gallery_thumbs_ul_width_computed <= gallery_viewport_width) ? 'none' : 'block') });
	        jQuery('#single_rental .sr_right .gallery_thumbs ul').width(gallery_thumbs_ul_width_computed);
	        gallery_step_size = (gallery_thumb_width + gallery_thumb_margin + gallery_thumb_border_width) * gallery_slide_thumb_step;
	        jQuery('#single_rental .sr_right .gallery_thumbs a.prev, #single_rental .sr_right .gallery_thumbs a.next').click(function() {
	            var direction = jQuery(this).hasClass('prev') ? 'prev' : 'next';
	            var current_left_position = parseInt(jQuery('#single_rental .sr_right .gallery_thumbs ul').css('left'));
	            var final_left_position;
	            if(direction == 'prev')
	                final_left_position = (current_left_position + gallery_step_size);
	                
	            else
	                final_left_position = (current_left_position - gallery_step_size);
	            jQuery('#single_rental .sr_right .gallery_thumbs ul').animate({ left: final_left_position+'px' }, 'medium');
	            
	            final_left_position = (final_left_position < 0) ? (final_left_position * -1) : final_left_position;
	            var no_more_prev = false, no_more_next = false;
	            if((gallery_thumbs_ul_width_computed - final_left_position) < gallery_viewport_width)
	                no_more_next = true;
	            if(final_left_position == 0)
	                no_more_prev = true;
	            
	            jQuery('#single_rental .sr_right .gallery_thumbs a.prev').css({ display: (no_more_prev ? 'none' : 'block') });
	            jQuery('#single_rental .sr_right .gallery_thumbs a.next').css({ display: (no_more_next ? 'none' : 'block') });
	        });
	
	
	        jQuery('#single_rental .sr_right .gallery_thumbs ul li a').click(function(e) {
	            e.preventDefault();
	            var triggering_element = jQuery(this);
	            if(jQuery(triggering_element).parent().hasClass('active') || gallery_image_loading == true) return false;
	            else {
	                gallery_image_loading = true;
	                jQuery('#single_rental .sr_right .large_photo').addClass('loading');
	                var imagefull = jQuery(this).attr('data-imagefull');
	                var img = new Image();
	                img.src = imagefull;
	
	                img.onload = function() {
	                    jQuery('#single_rental .sr_right .gallery_thumbs ul li').removeClass('active');
	                    jQuery('#single_rental .sr_right .large_photo').removeClass('loading').fadeOut('fast', function() {
	                        jQuery('#single_rental .sr_right .large_photo').css({ backgroundImage: 'url('+ imagefull +')' }).fadeIn('fast');
	                        jQuery(triggering_element).parent().addClass('active');
	                        gallery_image_loading = false;
	
	console.log(imagefull);
	
	
	                    });
	                };
	            }
	        });
	
	        setTimeout("jQuery('#single_rental .sr_right, #archive_rental .ar_right').addClass('loaded');", 10000);
	 
			
	        // Archive page events - mode switch
	        jQuery('.rental_overpanel .buttons_tabbed .button, #single_rental .rental_location .map').click(function() {
	        	var mode = jQuery(this).attr('data-fulldisplaytype');
	        	jQuery(this).attr('data-fulldisplaytype', (mode == 'image' ? 'map' : 'image'));
	        	jQuery('#archive_rental, #single_rental').removeClass('viewmode-image').removeClass('viewmode-map').addClass('viewmode-'+mode);
	        	jQuery(this).parent().find('.button').removeClass('active');
	        	if(jQuery(this).hasClass('toggle-button'))
					jQuery(this).removeClass('state-image').removeClass('state-map').addClass('state-'+mode);
				else
	        		jQuery(this).addClass('active');
	        	var transition_speed = 'fast';
	        	if(mode == 'image') {
	        		jQuery('#hvhr_searched_map_container').animate({ opacity: '0' }, transition_speed, function() {
	        			jQuery('#single_rental .sr_right .large_photo, #single_rental .sr_right .gallery_thumbs, #archive_rental .ar_right .large_photo').fadeIn(transition_speed);
	        		});
	        	} else {
	        		jQuery('#single_rental .sr_right .large_photo, #single_rental .sr_right .gallery_thumbs, #archive_rental .ar_right .large_photo').fadeOut('fast', function() {
	        			jQuery('#hvhr_searched_map_container').animate({ opacity: '1' }, 'fast');
	        		});
	        	}
	        });
	    }
	    
	    function ps_archive_listing_events(unbind) {
	    	unbind = typeof(unbind) == 'undefined' ? false : true;
	    	
	    	if(unbind == true) {
	    		jQuery('#archive_rental .rental_listing ul li').unbind('click');
	    		return;
	    	}
	    	
	        jQuery('#archive_rental .rental_listing ul li').click(function(e) {
	            e.preventDefault();
	            if(jQuery(e.target).hasClass('permalink') && jQuery(e.target).attr('href').toString().indexOf('http://') > -1) {
	            	window.location = jQuery(e.target).attr('href');
	            	return;
	            }
	            jQuery(this).parent().find('li').removeClass('active');
	            jQuery(this).addClass('active');
	            var imagefull = jQuery(this).find('.thumb').attr('data-imagefull');
	            if(archive_rental_fullimage_loading == true || typeof(imagefull) == 'undefined' || imagefull == '') return false;
	            jQuery('#archive_rental .ar_right .large_photo').addClass('loading');
	            archive_rental_fullimage_loading = true;
	            var img = new Image();
	            img.src = imagefull;
	            img.onload = function() {
	                jQuery('#archive_rental .ar_right .large_photo').removeClass('loading').fadeOut('fast', function() {
	                    jQuery('#archive_rental .ar_right .large_photo').css({ backgroundImage: 'url('+img.src+')' }).fadeIn('fast');
	                    archive_rental_fullimage_loading = false;
	                });
	            };            
	        });
	    }
	    
	    function ps_datepicker(selector) {
	        if(typeof(jQuery(selector).datepicker) != 'function') {
				setTimeout("ps_datepicker('"+selector+"');", 2000);
				return;
			} else
				jQuery(selector).datepicker({ dateFormat: 'mm-dd-yy', minDate: 0, maxDate: "+12M ", });
	    }
	    
	    function ps_animate_load_bgs() {
	        var elements = jQuery('*[data-animateloadbg]');
	        for(var x = 0; x < elements.length; x++) {
	            jQuery(elements[x]).addClass('loading').addClass('albd-'+x); // albd = animateloadbgdestination
	            jQuery('<img/>').attr('src', jQuery(elements[x]).attr('data-animateloadbg')).attr('data-destination', 'albd-'+x).load(function() {
	                jQuery('.'+jQuery(this).attr('data-destination'))
	                    .removeAttr('data-animateloadbg')
	                    .removeClass('loading')
	                    .css({ backgroundImage: 'url("'+jQuery(this).attr('src')+'")', opacity: '0' })
	                    .animate({ opacity: '1' }, 'fast');
	
	// console.log(jQuery(this).attr('src'));
	
	            });
	        }
	    }
	    
	    function ps_adjust_elements_on_resize(elements_selector) {
	        var header_height = jQuery('#ps_nav').height();
	        var footer_height = jQuery('#colophon').height();
	        var window_height = jQuery(window).height();
	        jQuery(elements_selector).css({ marginTop: header_height+'px' }).height(window_height - (header_height + footer_height));
	    }
	    
	    function ps_adjust_other_elements() {
	    	// height adjustment
	    	var w_height = jQuery(window).height();
	    	var h_adj_elements = jQuery('*[data-adjheightperc]'), this_h;
	    	for(var x = 0; x < h_adj_elements.length; x++) {
	    		this_h = parseFloat(jQuery(h_adj_elements[x]).attr('data-adjheightperc').toString());
	    		jQuery(h_adj_elements[x]).css({ height: ((this_h * w_height)/100)+'px' });
	    	}
	    }
	    
	    // Fluid font-size
	    var ps_adjust_fontsizes_ref_size = 12.5; // px
	    var ps_adjust_fontsizes_ref_width = 1920; // px
	    var ps_adjust_fontsizes_ref_height = 952;//1080; // px
	    function ps_adjust_fontsizes_on_resize() {
	        var elements_for_proportional_font_size = jQuery('.proportional_font_size');
	        var ps_this_fontsizes_ref_size;
	        
	        var window_width = jQuery(window).width();
	        var window_height = jQuery(window).height();
	        
	        var prop_factor_by_width = window_width / ps_adjust_fontsizes_ref_width;
	        var prop_factor_by_height = window_height / ps_adjust_fontsizes_ref_height;
	                    
	        for(var x = 0; x < elements_for_proportional_font_size.length; x++) {
	            ps_this_fontsizes_ref_size = jQuery(elements_for_proportional_font_size[x]).attr('data-reffs');
	            if(typeof(ps_this_fontsizes_ref_size) == 'undefined' || ps_this_fontsizes_ref_size == '')
	                ps_this_fontsizes_ref_size = ps_adjust_fontsizes_ref_size;
	            else
	                ps_this_fontsizes_ref_size = parseFloat(ps_this_fontsizes_ref_size);                            
	            
	            jQuery(elements_for_proportional_font_size[x]).css({ fontSize: (ps_this_fontsizes_ref_size * Math.min(prop_factor_by_width, prop_factor_by_height))+'px' });
	            
	        }
	    }
	</script>

