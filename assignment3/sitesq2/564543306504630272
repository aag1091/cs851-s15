<script>
	$ = jQuery.noConflict();
	
	jQuery( function() {        
   
		jQuery('a.more_info').click( function () {
 			
			var title = jQuery(this).attr('data-auto');
			
			if(title.length > 25) title = title.substring(0,25);

			console.log( title );

			$('#purchase-notice').html('');
			jQuery.fancybox.close( true );
			jQuery.fancybox.open( jQuery('#form-purchase'), {});
			
			jQuery("#inquiry-form .listing").val( title );
			
			if(title ) jQuery('#form-purchase #title').html('for <br><strong>'+title+'</strong>');
			
			return false;
		});
		
		jQuery('#form-purchase').submit( function() {
			
			// ini
			errors = '';
			$('#inquiry-form #purchase-notice').html('');
			
			// get form values
			var listings_id = jQuery("#inquiry-form .listing").val(),
				purchase_first_name = jQuery("#inquiry-form .purchase-fname").val(),
				purchase_last_name = jQuery("#inquiry-form .purchase-lname").val(),
				purchase_email = jQuery("#inquiry-form .purchase-email").val(),		
				purchase_phone = jQuery("#inquiry-form .purchase-phone").val(),
				purchase_type = jQuery("#inquiry-form #type-of-experience").val(),
				// purchase_from = jQuery('#inquiry-form input[name="date-of-exp-from"]').val(),
				purchase_to = jQuery('#inquiry-form input[name="date-of-exp-to"]').val();
				// purchase_comment = jQuery("#inquiry-form .purchase-comment").val();
			
			// Validate form inputs
			if(purchase_first_name =='') errors += "First Name is required. <br>";
			if(purchase_last_name =='') errors += "Last Name is required. <br>";;
			if(purchase_email =='' || ! isValidEmailAddress(purchase_email) ) errors += "Valid Email is required. <br>";
 
			
			// Check for errors
			if(errors ) {
				console.log(errors);
				
				$('#inquiry-form #purchase-notice').html('<p class="error">Please check your form again. <br>' + errors + '</p>');
			 
			} else {
			
			// Ok, let's continue
			jQuery("#inquiry-form").hide();
			jQuery("#purchase-wait").show('fast');
			
			// do AJAX 
			var form_data = {
				listings_id: listings_id,
				first_name: purchase_first_name,
				last_name: purchase_last_name,
				email: purchase_email,
				phone: purchase_phone,
				purchase_from: '',
				purchase_to: purchase_to,
				purchase_type: purchase_type,
				comment: '', // purchase_comment,
				ajax: 1
			};
			
			// console.log(form_data);
			 
			
			/*** ***/
			jQuery.ajax({ url: "http://www.platzshare.com/listings/request-info/",
					type: 'POST',
					data: form_data,
					// dataType: 'json',
					cache: false,
					success: function(msg) {
						if( msg.match(/SUCCESS/i) ) {
							// alert('Success');
							// response
							jQuery("#purchase-wait").hide()
							.after('<p id="success" class="success">Thank you. Your request has been submitted.</p>');
							
							// direct to login
							setTimeout( function () {
								jQuery.fancybox.close( true );
								jQuery("#inquiry-form").show('fast');
								jQuery("#success").text('');
							}, 3000);
							 
						} else if( msg.match(/error/i) ) {
 							 
							// errors
							jQuery("#inquiry-form").show('fast');
							jQuery("#purchase-wait").hide();
							$('#inquiry-form #purchase-notice').html('<p class="errors">' +msg+'</p>');
							
 						}
					} // success	
				});	// jQuery.ajax	
					
			}
			
			return false;
		});
		

	});
</script>		
        

    </div>
    
    
	<? /* /end listings */ ?>
	
	
    
    

