        <script type="text/javascript">
        // equalize the height of "subscribe" tiles
        var paidServicesItems = $("#subscription-notice-manager-pop #subscribe-area-manager-pop li.service");
        var paidServicesLinks = $("#subscription-notice-manager-pop #subscribe-area-manager-pop a.service-link");
        equalHeight(paidServicesItems,paidServicesLinks);

        // equalize the height of "already a print subscriber?" tiles
        var otherServiceItems = $("#subscription-notice-manager-pop #already-area-manager-pop li.service");
        var otherServicesLinks = $("#subscription-notice-manager-pop #already-area-manager-pop a.service-link");
        equalHeight(otherServiceItems,otherServicesLinks);

        $("#subscription-notice-manager-pop .service-action-area").hide();
        t_dur = 0;
        // show login area
            $("#notice-login-manager-pop").click(function(){
                run_noticeActn_manager_pop();
                $("#login-area-manager-pop").show(t_dur);
                $("#notice-header-manager-pop").html('<span class="ui-icon ui-icon-person float-left"></span> Login with your subscriber account');
            });
        // show subscribe area
            $("#notice-subscribe-manager-pop, #already-alt-manager-pop").click(function(){
                run_noticeActn_manager_pop();
                $("#subscribe-area-manager-pop, #subscribe-alt-manager-pop").show(t_dur);
                $("#notice-header-manager-pop").html('<span class="ui-icon ui-icon-cart float-left"></span> <span class="step">Step 1</span>: Select a site service.');
            });
        // show already subscriber area
            $("#notice-already-manager-pop, #subscribe-alt-manager-pop").click(function(){
                run_noticeActn_manager_pop();
                $("#already-area-manager-pop, #already-alt-manager-pop").show(t_dur);
                $("#notice-header-manager-pop").html('<span class="ui-icon ui-icon-home float-left"></span> <span class="step">Step 1</span>: Select the service that applies to you.');
            });
        // show button set when a selection is canceled
            $("#notice-cancel-manager-pop").click(function(){
                run_noticeActn_manager_pop();
                $("#subscription-notice-manager-pop .button-set,#subscription-notice-manager-pop p.explaination").show(t_dur);
                $("#notice-header-manager-pop").html('<span class="ui-icon ui-icon-locked float-left"></span> Subscription Required');
                $(".notice-alt, #subscribe-area-manager-pop, #already-area-manager-pop, .alt-area, #login-area-manager-pop").hide(t_dur);
            });
        // common actions
        function run_noticeActn_manager_pop(){
            $("#subscribe-area-manager-pop, #already-area-manager-pop, .notice-alt, #subscription-notice-manager-pop .button-set, #subscription-notice-manager-pop p.explaination").hide(t_dur);
            $(".alt-area").show()
        }
        // create buttons
        $(".jquery-ui-button").button();

        // handle authorized, recognized and unathorized dispaly

        // show only service options if user is logged in
        var notice_tncms_authtoken = $.cookie("tncms-authtoken");
        //var notice_tncms_screenname = $.cookie("tncms-screenname");
        if(notice_tncms_authtoken){
            $(".colspan.already").css("clear","none");
            $("#subscription-notice-manager-pop .column").addClass("col-shrink");
            $("#subscription-notice-manager-pop .colspan").addClass("colspan-shrink");
            $("#subscription-notice-manager-pop .button-set p").hide();
            $(".unauthorized-user").hide();
        }

        else{
            $(".authorized-user").hide();
            $(".unauthorized-user").show();
            $(".authorized-user").hide();
        }

        $("#subscription-notice-manager-pop .button-set p.force").show();

        // equalize the service list heights
        function equalHeight(serviceListItem, serviceListLink){
            var bestHeight = 0;
            var curHeight = 0;
            // find best height
            serviceListItem.each(function(index){
                curHeight = $(this).height();
                if (curHeight > bestHeight){
                    bestHeight = curHeight;
                }
            });
            if (bestHeight > 0){
                bestHeight = bestHeight+'px';
                // apply best heights
                serviceListItem.css("height",bestHeight);
                serviceListLink.css("height",bestHeight);
            }
        }
        // add styles to service boxes on hover
        $("#subscription-notice-manager-pop li.service").hover(function(){
            $(this).addClass("ui-state-hover");
          },function(){
            $(this).removeClass("ui-state-hover");
          }
        );
        // when a service is selected
        $("#subscription-notice-manager-pop a.service-link").click(function(){
            $(".service-action-area,.explaination,.alt-area").hide();
            $("#subscription-notice-manager-pop").css("height","200px").addClass("blox-loading");
        });
        </script>
    

        </div>
        



    


</div>
