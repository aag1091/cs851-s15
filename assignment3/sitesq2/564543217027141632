<script type="text/javascript">
var $container = $('.threadSliderContent');
$container.imagesLoaded(function(){
    $container.masonry({
    itemSelector: '.threadSliderItem',
   isAnimated:true,
   isFitWidth: true,
   columnWidth: 200
    });
    $('.threadSliderItem img').lazyload({
        effect: 'fadeIn'
    });
    $('.threadSliderItem img').trigger('scroll');
    $('.threadSliderContent').css({opacity: 0.0, visibility: "visible"}).animate({opacity: 1.0});
    
});
</script>

