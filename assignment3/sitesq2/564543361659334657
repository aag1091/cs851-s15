    jQuery(function($) {

    	$.ajax({
            url:"http://www.podbean.com/site/user/loginInfo",
            type:"GET",
            dataType:"jsonp",
            context:$("#iframe-block"),
            success:function(data){ 
            	            		var showFrame = 1;
            	
                if(data.code=='200' && data.isGuest == '0')
                { 	
                	$(this).attr('src',"/blogtoolbar.php?username="+data.username+"&logo="+data.logo);
                	$('body').data('isLogin',1);
                	showFrame = 1;
				}
                else
                {
                	$('body').data('isLogin',0);
               	}

               	if(showFrame)
               		$(this).parent('div').toggle();
            }
        });
    });
</script>

