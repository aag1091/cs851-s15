		<script type="text/javascript">
			jQuery(function($) {
				
				$.ajax({
		            url:"http://www.podbean.com/site/userCenter/getEpisodeLikeStatus",
		            type:"GET",
					data:{"episodes":["5481816"]},       
		            dataType:"jsonp",
		            success:function(response){
					    $.each(response.data, function(i, n){
						  var linkText = "";
						  var idText = "episode-like-" +n.id;
						  
						  if(n.status == 1)
						  {
						  	linkText  = "Liked";
						  	likeClass = "liked";
						  }
						  else
						  {
						  	linkText  = "Like";
						  	likeClass = "unlike";
						  }	
							

						  var content = "<a href=\"http://www.podbean.com/site/user/login?return=http%3A%2F%2Fthewellwatchers.com%2Fe%2F4-white-guys-sitting-on-the-mics%2F\" id=" + idText + "  class=\"post_toolbar_like "+ likeClass +"\"  data=" + n.count + " >"+ linkText +"</a><span class=divider> | </span>";

						  $("#postbar_"+n.id).prepend(content);
						
						  if(!response.isGuest)
						  {
						  	$("#episode-like-"+ n.id).click(function(){
			          		  if($(this).hasClass("liked"))
					          { 
					              $.ajax({
					                  url: "http://www.podbean.com/site/userCenter/like",
					                  type:"GET",
					                  data:"id="+ n.id +"&type=pb&action=dislike",
					                  context:$(this),
					                  dataType:"jsonp",
					                  success: function(data){
					                    if(data.code == 200)
					                    { 
					                        $(this).removeClass("liked");
					                        $(this).addClass("unlike");
											var count = parseInt($(this).attr("data"))-1;

											if(count == 0)
												$(this).text("Like");
											else
												$(this).text("Like("+count+")");
											
											$(this).attr("data",count);
					                    }
					                  }
					              });
					          }   
					          else
					          {
					              $.ajax({
					                  url: "http://www.podbean.com/site/userCenter/like",
					                  type:"GET",
					                  data:"id="+ n.id +"&type=pb&action=like",
					                  context:$(this),
					                  dataType:"jsonp",
					                  success: function(data){
					                    if(data.code == 200)
					                    {
					                        $(this).removeClass("unlike");
					                        $(this).addClass("liked");
					                        var count = parseInt($(this).attr("data"))+1;
					                        $(this).text("Liked("+count+")");
					                    	$(this).attr("data",count);
					                    	
										}
					                  }
					              });
					          }
							  
							  return false;

					        });
						  }
						});
		            }
			    });

				$.ajax({
					url: "http://www.podbean.com/api2/public/filesPlays",
					type:"GET",
					data:{"blogId":"597397","query":null}, 
					dataType:"json",      
		            success: function(result) {
						if(result.code =="200")
						{
							$.each(result.data, function(id,count){
								id = id.replace(/\./g,"_");
								if(count >0)
								{
									$(".hits_"+id).text(count);	
								}
								else
								{
									$(".hits_"+id).parent("a").prev("span").remove();
									$(".hits_"+id).parent("a").remove();	
								}	
							});							
						}
					}
				});
			});
		</script>
