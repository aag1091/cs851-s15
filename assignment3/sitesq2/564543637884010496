function cookieTest () {
	msite.cookie.write('cookietest', '1');
	var p, e, c = msite.cookie.read('cookietest');
	if (c === '1') {
		console.log('sdfsdf');
		e = document.createElement("div");
		e.innerHTML = "Please note: We have detected cookies are enabled on your browser. You may of arrived here by mistake.";
		e.style.cssText = "padding: 20px; border: 1px dashed #339933; background:#ffdddd; margin:20px 0;";
		msite.cookie.remove('cookietest');
		p = document.getElementById('content-1');
		p.insertBefore(e, p.childNodes[0]);
	}
};
document.addEventListener("DOMContentLoaded", cookieTest, false);
