        <div id="main_content" class="grid-8">
         <!-- /content_top -->

          <div id="content">
          
                                        
                  <div class="header_title">
            <h1>D.C. United unveil new secondary jersey for 2015 MLS season with a nod to supporters chant</h1>                          <div class="article-info">
              <div class="field-date">
                February 8, 2015              </div>
              <div class="field-author">
                <a href="http://www.mlssoccer.com/author/MLSsoccer-staff">MLSsoccer staff</a>              </div>
              <div class="field-articlemedia">
                MLSsoccer.com              </div>
              </div><!-- END: article-info -->
                      </div><!-- END: header_title -->
        
                              <div id="node-279266" class="node">



  
  <div class="content clear-block">
          <div class="share_bar">
                <script type='text/javascript'>
          var userAction = new gigya.socialize.UserAction();
          var image = {type: 'image', src: 'http://www.mlssoccer.com/sites/league/files/imagecache/620x350/image_nodes/2015/02/jersey.png', href: 'http://www.mlssoccer.com/news/article/2015/02/06/dc-united-unveil-new-secondary-jersey-2015-mls-season'};

          userAction.addMediaItem(image);
          userAction.addActionLink('More', 'http://www.mlssoccer.com/news/article/2015/02/06/dc-united-unveil-new-secondary-jersey-2015-mls-season');
          userAction.setDescription('');
          userAction.setLinkBack('http://www.mlssoccer.com/news/article/2015/02/06/dc-united-unveil-new-secondary-jersey-2015-mls-season');
          userAction.setTitle('D.C. United unveil new secondary jersey for 2015 MLS season with a nod to supporters chant');

          function OnShareButtonClickedHandler(eventObj) {
            if (eventObj.shareItem.provider == 'comments') {
              window.location.hash='';
              window.location.hash='commentsDiv';
            }
          };

          var showShareBarUI_params = {
              containerID: 'sharebarDiv',
              layout:'horizontal',
              shareButtons: [{provider: 'facebook-like', tooltip: 'Like this on Facebook', action: 'like', font: 'arial'},{provider: 'twitter-tweet', defaultText: 'D.C. United unveil new secondary jersey for 2015 MLS season with a nod to supporters chant #MLS', tooltip: 'Share on twitter', related:'mls'},{provider: 'google-plusone', tooltip: '+1 on Google+'},{provider: 'Reddit', iconOnly:'false', tooltip: 'Share this on Reddit'},{provider: 'email', iconOnly:'false', tooltip: 'Email this'},{provider: 'comments', categoryID: 'article', iconOnly:'false', streamID: '279266', tooltip: 'View comments'}],
              showCounts: 'top',
