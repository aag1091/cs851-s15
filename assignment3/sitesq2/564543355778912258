   $("div#ctl00_MainContent_SaveAgentsTable_datatable").css({"margin-top":"2px"});
  }else{
   $(".PAGEtitle:eq(0)").parent().attr({"align":"left"});
   $(".PAGEtitle:eq(0)").parent().prev().css({"display":"none"});
  } 

