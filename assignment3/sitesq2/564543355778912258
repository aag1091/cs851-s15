  if(curl.indexOf("resumemanager.aspx")>=0){
   $("h1.PAGEtitle:eq(1)").parent().parent().parent().parent().css({"width":"100%"});
   $("h1.PAGEtitle:eq(1)").parent().parent().find("td:eq(2)").attr({"width":"100%"}); 
