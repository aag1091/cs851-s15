<script>
require([
    'shared/scripts/bam/social/socialplugins/socialPoll'
], function(socialPoll){
var instanceName = "";
var types = ",";
var renderResult = function (percentagesObj) {
    for (p in percentagesObj) {
       if (percentagesObj.hasOwnProperty(p)) {
          $("#" + p).text(percentagesObj[p] + "%");
       }
    }
}
 $.when(socialPoll.getData({types:types, vender: "facebook",instance: instanceName,url: "/gdcross/components/game/mlb/facebook/fb_tracked_poll_mlbn.json"})).then(function () {
    var totalCounts = socialPoll.getTotalCounts.call(null, arguments);
    var total = 0;

    $.ajax({
       dataType: "json",
       url: "/gdcross/components/game/mlb/twitter/tw_mlbn_poll.json",
       success: function(response) {
         var countObj  = response.data;
         var totalLength = countObj.length;
         var entry;
         var twCounts = {
           '' : 0,
           "" : 0
         };

         var totalPercentages = {
           '' : 0,
           "" : 0
         }
         var typesArr = types.split(",");
         var typesLength = typesArr.length;
         for (var i=0; i < totalLength; i++) {
             entry = countObj[i];

             for (var j = 0; j < typesLength; j++) {
                if (entry['GNIP_RULE'].indexOf(typesArr[j]) > -1) {
                  totalCounts[typesArr[j]] += entry['qualifying_tweet_count']*1;
                  console.log(typesArr[j] + "/twitter "  + entry['qualifying_tweet_count']);
                }
             }

         }

         // get percentages  - last min heeck due to data change
         var countSofar = 0;
         var test;
         var totalCount = 0;
         
         //RESET DATA
         totalCounts['wright'] = totalCounts['wright'] + 52012;
         totalCounts['Posey'] = totalCounts['Posey'] + 47549;
         //RESET DATA

          for (var k in totalCounts) {
              if (totalCounts.hasOwnProperty(k)) {

                 totalCount = totalCount + totalCounts[k];

              }
          }
          
          

          for (var k in totalCounts) {
              if (totalCounts.hasOwnProperty(k)) {

                 test = Math.ceil(totalCounts[k]/ totalCount * 1 * 100);
                 if ((countSofar + test) > 100) {
                   totalPercentages[k] = 100 - countSofar;
                 } else {
                   totalPercentages[k] = Math.ceil(totalCounts[k]/ totalCount * 1 * 100);
                 }
                 countSofar +=  totalPercentages[k];
              }
          }

       renderResult(totalPercentages);

       }
    });


 });
});
</script>

