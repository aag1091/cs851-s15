function categorias()
{
var cadena_categorias="#Tom Hiddleston, #Loki, #Ninja Challenge, #Ninja Hug";
var categorias_array=cadena_categorias.split(",");
$.each(categorias_array, function(index, value) {
$('ul#categories').append('<li><a id="cat-' + value.replace(/^\s*|\s*$/g,'').replace(' ','X') + '" title="Show posts in ' + value + '" href="/tagged/' + (value).replace(/^\s*|\s*$/g,'') + '">' + (value).replace(/^\s*|\s*$/g,'') + '</a></li>');
});


$("#categories a").hover(function() {
$(this).stop().animate({ color: "#000"}, 500);
},function() {
$(this).stop().animate({ color: "#888" }, 500);
});


}

function selectcat(categoria) {$('#cat-' + categoria.replace(' ','X')).addClass('active')}




categorias();

 
function loadnotes() {jQuery('.n-notes-0.pending, .n-notes-1.pending, .n-notes-2.pending, .n-notes-3.pending, .n-notes-4.pending, .n-notes-5.pending, .n-notes-6.pending').click()
}
