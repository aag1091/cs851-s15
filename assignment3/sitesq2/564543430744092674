                        callback = function ()
                        {
                            sflIcon.transition({
                                duration: 0.6,
                                opacity: (repeats + 1) % 2
                            });
                            repeats++;
                            if (repeats < 4)
                                Y.later(600, this, callback); // the animation duration for each is 600ms
