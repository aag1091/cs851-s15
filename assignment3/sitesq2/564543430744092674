    Y.later(10, this, function() {(function(Y) {
                                var videoEmbedPlayer = new Y.Media.VideoEmbedHandler(Y,Y.Media.pageChrome,
                                {
                                   modId       :'mediacontentstory',
                                   contentId   :'3e86717e-84ae-3747-a288-b2f0579f76c2',
                                   site        :'news',
                                   lang        :'en-US',
                                   region      :'US',
                                   spaceid     :'7664811',
                                   autoPlay    :'0',
                                   ybang       :false
                                });
                            })(Y);
    });
});
});
        
        </script>

	
	
