        <tr>
            <td width="50" rowspan="2" class="newsimg"><a name="144"></a><img width="50" height="50" src="images/cnews/S.png" alt="Latest News" /></td>
            <td width="10" rowspan="2" class="news10space">&nbsp;</td>
            <td width="530" class="newstitle">Community Activity Update</td>
