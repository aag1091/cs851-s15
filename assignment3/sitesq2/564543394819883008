	   jQuery('ip\\:products, [data-component=products]').each(function() {
					var target = jQuery(this);              
					var template = jQuery(this).children('script').html();					
	
					jQuery.ajax({
						url: 'http://www.imagine-publishing.co.uk/api/v1/products.json',
						data: { magazine: jQuery(this).attr('magazine') },
						dataType: 'jsonp',
						success: function(data) {
							target.html(Mustache.render(template, data));
						}
					});
				});
	 });
</script>

