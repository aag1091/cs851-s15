    <script>
    $(document).ready(function(){
    	$('.feedyourmind img').click(function(){
    		$this = $(this).parents().eq(1);
    		if($this.css('marginTop')!='-150px'){
    			$this.animate({marginTop:'-150px'});
    			$this.find('.site img').each(function(){$(this).animate({marginTop:'-20px'});});
    		} else{
    			$this.animate({marginTop:'0'});
    			$this.find('.site img').each(function(){$(this).animate({marginTop:'0'});});
    		}
    	});
    	$(document).click(function(e){
    		if(!$(e.target).closest('#feedyourmind').length){
    			$('#feedyourmind').animate({marginTop:'-150px'});
    			$('#feedyourmind .site img').animate({marginTop:'-20px'});
    		}
    	})
    })
    </script>

