<script type="text/javascript">
	function searchKeyPress(e)
    {
            // look for window.event in case event isn't passed in
            if (window.event) { e = window.event; }
            if (e.keyCode == 13)
            {
                    document.getElementById('checkButton').click();
            }
    }
</script>
