<script>

	function processAds() {
		if (OAS_version < 11) {
			OAS_process();
		} else {
			$.getScript(OAS_url + 'adstream_mjx.ads/' + OAS_sitepage + '/1'
				+ OAS_rns + '@'	+ OAS_listpos + '?' + OAS_query, OAS_process);
		}
	}

	function prepareAdArray() {
		for (var id in OAS_ads) {
			var ad = OAS_ads[id],
			// get the defined screenSizes properties in displayAds widget
			adScreenSizes = ad[2].split(',');

			// for each of these screenSizes, check if the current window matches
			for (var i = 0; i < adScreenSizes.length; i++) {
				if (STV.client.screenSize[ jQuery.trim(adScreenSizes[i]) ]) {
					// if the current window matches the screenSize then add it to the ad call
					OAS_listpos.push(ad[0]);
					OAS_fds[id] = ad.concat([$('#' + id)]);
					break;
				}
			}
		}
		OAS_listpos = STV.helpers.uniqueArray(OAS_listpos).join(',');
	}

	jQuery(window).ready(function(){
		var $ = jQuery;

		if (! STV.helpers.count(OAS_ads)) {
			return;
		}

		if ($('.oas-keyword').length > 0) { // if there is an element containing an OAS keyword on the page
			if ($('.oas-keyword').first().attr('data-oas-keyword')) {
				OAS_query = "keyword=" + $('.oas-keyword').first().attr('data-oas-keyword'); // set the OAS_query to the data attribute value
	        } else {
	        	OAS_query = '';
	        }
        }

		prepareAdArray();

		OAS_process = function() {
			if (!(OAS_version > 10 && !OAS_RICH)) {
				for (var id in OAS_fds) {
					(function(ad){
						setTimeout(function(){
							var thisID = $(ad[3]).attr('id');
							postscribe('#' + thisID, '<script>OAS_AD("' + ad[0] + '");<\/script>');
						}, 1);
					})(OAS_fds[id]);
				}
			}
		};

		processAds();
	});

</script>


