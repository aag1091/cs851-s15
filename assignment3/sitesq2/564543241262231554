        <script type="text/javascript">
        // <![CDATA[
            // A/B Autoplay
        var STVvideo = window.STVvideo || {},
        autoplayParam = '',
        swh = screen.width+'x'+screen.height;

        function getAnalytixParams(){
            //set default channel analytics parameters(empty values)
            var anaParams = {
                ns_campaign: 'ns_campaign=',
                ns_mchannel: 'ns_mchannel=',
                ns_source: 'ns_source=',
                ns_linkname: 'ns_linkname=',
                ns_fee: 'ns_fee=0'
            },
            //get each query string parameter in url. Returned as array queryParams
            queryParams = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&'),
            i = queryParams.length,
            params = '',
            noParams = true;

            //loop the queryParams and override the default channel analytics parameters (anaParams) if they exist
            while(i--){
                switch(queryParams[i].split('=')[0]){
                    case 'ns_campaign':
                    anaParams.ns_campaign = queryParams[i];
                    noParams = false;
                    break;
                    case 'ns_mchannel':
                    anaParams.ns_mchannel = queryParams[i];
                    noParams = false;
                    break;
                    case 'ns_source':
                    anaParams.ns_source = queryParams[i];
                    noParams = false;
                    break;
                    case 'ns_linkname':
                    anaParams.ns_linkname = queryParams[i];
                    noParams = false;
                    break;
                    //make sure ns_fee value is always integer
                    case 'ns_fee':
                    anaParams.ns_fee = (queryParams[i] == 'ns_fee=' ? queryParams[i] + '0' : queryParams[i]);
                    noParams = false;
                    break;
                    default : '';
                }
            }
            if(noParams){
                return '';
            } else {
                //loop the anaParams and return them as a string
                for(var key in anaParams){
                    params += '&' + anaParams[key];
                }
                return params;
            }
        }

        function getABPreRollGroup() {
            var cookieMatch = document.cookie.match(/stv_pre_roll_testing_group=([^;]+)/),
                preRollTestGroup = '';

            if (cookieMatch) preRollTestGroup = cookieMatch[1];

            if (typeof preRollTestGroup === 'string' && preRollTestGroup.match(/^(A|B)$/i)) {
                return '&stv_abtest_preroll=' + preRollTestGroup;
            } else {
                return '';
            }
        }

        if (document.querySelectorAll('[data-livestream="true"]').length > 0) {
            window.ns_pixelUrl = '';
        } else {
            window.ns_pixelUrl = 'http://uk.sitestat.com/stv/aberdeen/s?';
        }

         function ns_onclick(i, b, c, h, j) {
             'use strict';

             var followLink,
                 ns_pixelUrl = window.ns_pixelUrl,
                 ns_softclick_timeout;

             if (Array.isArray(c)) {
                 followLink = c[1];
                 c = c[0];
             }

             //log(c);
             //log(followLink);

             var e = '';
             if (typeof ns_pixelUrl === 'string') {
                 e = ns_pixelUrl.substring(0, ns_pixelUrl.indexOf('?') + 1);
             }
             e += c;
             e += '&ns_type=' + h +
                 '&ns_action=view';
             e += '&ns__t=' + (new Date()).getTime();
             if (!b) {
                 b = i.href;
             }
             var d = document.referrer;
             if (d.lastIndexOf('/') === d.length  - 1) {
                 d = d.substring(d.lastIndexOf('/'), 0);
             }
             if (d.length > 0) {
                 e += '&ns_referrer=' + escape(d);
             }
             j = j || '';
             var f = (i && i.target && i.target !== '') ? (i.target.substring(0, 1) === '_') ? i.target.substring(1) : i.target : 'self';
             var g = new Image();
             if (f && b) {
                 if (window[f]) {
                     window.ns_softclick_timer = (function (k, a) {
                         return function () {
                             if (window.ns_softclick_timeout) {
                                 window.clearTimeout(window.ns_softclick_timeout);
                             }
                             g.onload = g.onerror = function () {
                                 return;
                             };
                             if (followLink === true) {
                                 window[((window[k]) ? k : 'self')].location.href = a;
                             }
                         };
                     }(f, b));

                     ns_softclick_timeout = window.setTimeout(function () {
                         window.ns_softclick_timer();
                     }, 5000);

                     g.onload = g.onerror = window.ns_softclick_timer;

                 } else {
                     window.open(b, f, j);
                 }
             }
             //log(e);
             //comment this line to stop clicks registering in DAX
             g.src = e;
             return false;
         }

         STV.helpers.userDetails(function (data) {
                    var t_url = 'http://uk.sitestat.com/stv/aberdeen/s?name=articles.309369-the-campaign-against-marischal-square-plan-continues-on-february-14.page&stv_site=aberdeen&stv_breadcrumb1=aberdeen&stv_breadcrumb2=none&stv_breadcrumb3=none&stv_breadcrumb4=none&stv_breadcrumb5=none&stv_content_id=309369&stv_content_type=article&stv_author_id=253135&stv_byline_id=253135&stv_from_itv=false&stv_local_region=aberdeen&stv_postdate=20150206&stv_posttype=STV';
                    t_url = t_url + '&stv_gigya_id=' + data.UID;
                    function sitestat(u){var d=document,l=d.location;ns_pixelUrl=u+"&ns__t="+(new Date().getTime());u=ns_pixelUrl+"&ns_c="+((d.characterSet)?d.characterSet:d.defaultCharset)+"&ns_ti="+escape(d.title)+"&ns_jspageurl="+escape(l&&l.href?l.href:d.URL)+"&ns_st_cs="+escape(swh)+"&ns_referrer="+escape(d.referrer);(d.images)?new Image().src=u:d.write('<'+'p><img src="'+u+'" height="1" width="1" alt="*"/><'+'/p>');};
                    if (document.querySelectorAll('[data-livestream="true"]').length === 0) {

                        sitestat(t_url + getABPreRollGroup() + autoplayParam + getAnalytixParams());
                    }

                    $('body').append('<noscript><p><img src="' + t_url + '" height="1" width="1" alt="*"/></p></noscript>');
            });
