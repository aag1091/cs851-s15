<script>
//configuration
	OAS_url = 'http://oas.stv.tv/RealMedia/ads/';
	OAS_sitepage = 'aberdeen.stv.tv/articles/309369-the-campaign-against-marischal-square-plan-continues-on-february-14';
	OAS_listpos = [];
	OAS_query = '';
	OAS_target = '_top';

//end of configuration
	OAS_rn = new String (Math.random());
	OAS_rns = OAS_rn.substring (2, 11);
	OAS_ads = {};
	OAS_fds = {};

// version number detection
	OAS_version = 11; // default version
	if ((navigator.userAgent.indexOf('Mozilla/3') != -1) || (navigator.userAgent.indexOf('Mozilla/4.0 WebTV') != -1)) {
		OAS_version = 10; // drop version to 10 if browser match
	}



// helpers
	function OAS_NORMAL(pos) {
		document.write('<a href="' + OAS_url + 'click_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" target=' + OAS_target + '>');
		document.write('<img src="' + OAS_url + 'adstream_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" border=0 /></a>');
	}

	function OAS_AD(pos) {
		return (OAS_version >= 11 ? OAS_RICH(pos) : OAS_NORMAL(pos));
	}

	function OAS_AD_DEFER(pos, classes, screenSize) {
		var id = 'ad-' + pos + '-' + Math.round(Math.random() * 1000);
		if (document.getElementById(id)) {
			return OAS_AD_DEFER(pos);
		}

		//document.write('<b id="' + id + '" style="display:none;"></b>');
		document.write('<b id="' + id + '"></b>');
		OAS_ads[id] = [pos, classes, screenSize];
	}



	
	// KM added - copy of original function but returns the markup rather than document.write
	// This is required for ad containers that are returned from AJAX calls rather than static pages
	function OAS_AD_DEFER_LIVE(pos, classes, screenSize) {

		var id = 'ad-' + pos + '-' + Math.round(Math.random() * 1000);
		if (document.getElementById(id)) {
			return OAS_AD_DEFER_LIVE(pos);
		}
		OAS_ads[id] = [pos, classes, screenSize];
		return('<b id="' + id + '"></b>');

		prepareAdArray();
	}


</script>
