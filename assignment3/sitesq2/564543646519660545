<script>
function GetXmlHttpObject2()
	{
		if (window.XMLHttpRequest)
		  {
			  // code for IE7+, Firefox, Chrome, Opera, Safari
			  return new XMLHttpRequest();
		  }
		if (window.ActiveXObject)
		  {
			  // code for IE6, IE5
			  return new ActiveXObject("Microsoft.XMLHTTP");
		  }
		return null;
	}

function processEmailOptin2()
{

        var xmlhttp = GetXmlHttpObject2();
        var lfckv = document.getElementById("agreement-check2").checked

        //alert(xmlhttp);
        
	if(lfckv)
	{


		var url;
		var email = document.getElementById("email_input2").value;

		if( email == "")
		{
			document.getElementById("email_results2").innerHTML = "PLEASE ENTER YOUR EMAIL ADDRESS.";
		}
		else
		{
			url = "/inc/process_email_optin.php?email="+ email +"&ip=10.180.30.34";
			//alert(url);
			document.getElementById('signup_form2').innerHTML = "<img src='http://www.etonline.com/inc/2010/08/23823617/ajax-loader.gif' />";

			xmlhttp.onreadystatechange=function()
				{
					if(xmlhttp.readyState==4)
					  {
						if(xmlhttp.status == 200)
						{
							var res = xmlhttp.responseText;
							//alert("here"+res);
//MWH FOR TESTING IN DEV ONLY
//res = "Success";
							if(res.indexOf("Success") > -1)                                               
							{
     						           document.getElementById('df_thanks2').style.display = "block";							   
							   document.getElementById('email_results2').innerHTML = "";
              				}
							else
							{
							   document.getElementById('df_error2').style.display = "block";
							}
						}
					}
				}
				xmlhttp.open("GET",url,true);
				xmlhttp.send();
		
			


		}
	

	}
	else
	{
		document.getElementById('email_results2').innerHTML = "Please Accept The Terms and Conditions before submitting the request";
	}
}
</script>

