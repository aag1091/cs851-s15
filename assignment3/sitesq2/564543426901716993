<script>

var isMobile = {
    Android: function() {
        return navigator.userAgent.match(/Android/i);
    },
    BlackBerry: function() {
        return navigator.userAgent.match(/BlackBerry/i);
    },
    iOS: function() {
        return navigator.userAgent.match(/iPhone|iPad|iPod/i);
    },
    Opera: function() {
        return navigator.userAgent.match(/Opera Mini/i);
    },
    Windows: function() {
        return navigator.userAgent.match(/IEMobile/i);
    },
    Kindle: function() {
        return navigator.userAgent.match(/Silk/i);
    },
    pho1: function() {
        return navigator.userAgent.match(/webOS/i);
    },
    pho2: function() {
        return navigator.userAgent.match(/Windows Phone/i);
    },
    any: function() {
        return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows() || isMobile.Kindle() || isMobile.pho1() || isMobile.pho2());
    }
};



if( isMobile.any() ) {

window.top.location.href = "http://www.giveawaytab.com/giveaway2/mobile.php?pageid=216486181730645";
}
else{

window.top.location.href = "https://www.facebook.com/RockyRoadPrinting?sk=app_289264554441128";



}
</script>
