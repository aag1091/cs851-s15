    $('a.del_status').live('click',function(e){//replay delete
        var del_what=$(this).closest('.box_comment');
        e.preventDefault();
    	if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            if(user_id!=""){
                var choose=confirm("Do you want to delete it?");
                if(choose){
                    $.post("/article_reply.php",{reply_data:$(this).attr('data-value'),mode:'delete'},function(data){
                        if(data[0]==1){
                            $('.comment_count').text((Number($('.comment_count').text().replace(',',''))-Number(data[1])).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
