<script>
$(document).ready(function(){
    $('.theshop_img1').on('click',function(e){
        e.preventDefault();
        $.post("/article_reply.php",{mode:'theshop_track',refer_from:'article',page:current_page_url},function(data){
            if(data[0]==1){
                window.open("https://shop.allkpop.com","_blank");        
            }
        },"json"); 
    });
    $('.theshop_img2').on('click',function(e){
        e.preventDefault();
        $.post("/article_reply.php",{mode:'theshop_track',refer_from:'tournament',page:current_page_url},function(data){
            if(data[0]==1){
                window.open("https://shop.allkpop.com","_blank");        
            }
        },"json");
    });
});
</script>
