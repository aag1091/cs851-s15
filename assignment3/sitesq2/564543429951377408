        setTimeout(function(){$('.box_friend_comment').removeClass('comment_focus')},5000);
    }
    if(img_id!=""){setTimeout(function(){scroll_to_target_id('img_id_'+img_id);},1000);}
    $('.entry em').live('click',function(e){//reply report
