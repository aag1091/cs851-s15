        $('#comment_link .modal-body .page_num').val($('#comment_link .modal-body .page_num').val().substring(0,$('#comment_link .modal-body .page_num').val().lastIndexOf("/")+1));
        var position=$(this);
        $('#comment_link').modal('show');
        $('#comment_link .modal-body .page_num').val($('#comment_link .modal-body .page_num').val()+$(this).attr('data-value'));
        }
