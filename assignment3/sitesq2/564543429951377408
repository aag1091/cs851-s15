    $('a.report_send_re').live('click',function(e){
         e.preventDefault();
         if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
             var report_content_re=$(this).closest('.report_screen_re').children('.modal-body').find('input[name="report_value_re"]:checked');
             var report_option_re=$(this).closest('.report_screen_re').children('.modal-body').find('.report_option_re');
             if(report_content_re.val()==undefined){
                alert("choose one of your reason.");return;
             }else if(report_option_re.val()==""){
                alert(cmt_report);return;
             }else{
                 $.post("/article_reply.php",{rereply_data:rereport_re.attr('data-value'),report_content_re:report_content_re.val(),report_option_re:report_option_re.val(),mode:'rerepply_report'},function(data){
                    if(data==1){
                        if(Number(rereport_re.children('.rereport_screen_re').text())<4){rereport_re.children('.rereport_screen_re').html(Number(rereport_re.children('.rereport_screen_re').text())+1).show();    
                        }else{rereport_re.children('.rereport_screen_re').html(Number(rereport_re.children('.rereport_screen_re').text())+1).show();
                        }
                        report_content_re.prop('checked',false);
                        $('#report_screen_re').modal('hide');
                    }else if(data=="onetime"){alert(one_report);return;
                    }else if(data=="login"){alert(login_alert);return;
                    }else{/*alert(data);return;*/}
                });
            }
	    }else{}
    });
    if (typeof comment_loadData=='function'){}else{
        function comment_loadData(lastmsg,orderby,anchor){
            $.post("/common/comment.php",{lastmsg:lastmsg,article_idx:view_row_idx,orderby:orderby},function(data){
                $("#comment_container").html(data);if(anchor=="y"){scroll_to_target_id('goto_box_comment');}
            });
        }
    }
    $('.morebox_btn').live("click",function(){$('.more').click();});
    $('.more').on("click",function(){
        var ID = $(this).attr("id");
        var num=$(this).attr("data-value");
        if(ID){
            $("#more"+ID).html('<img src="http://static1.allkpop.com/img/loading.gif" />');
            $.ajax({
                type: "POST",
                url: "/common/comment.php",
                data: "lastmsg="+ ID + "&article_idx="+view_row_idx+"&num="+num+"&orderby="+orderby, 
                cache: false,
                success: function(html){
                    $("#comment_container").append(html);
                    $("#more"+ID).remove(); // removing old more button
                }
            });
        }else{$(".morebox.end").html('The End');}
        return false;
    });
    $('.content-show').hide();
    $('.show-icon').on('click',function (e) {e.preventDefault();if (status == 0) {$('.content-show').slideUp('fast');$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22.png");status = 1;}else{$('.image_link_screen').hide();cam_status=1;$('.youtube_link_screen').hide();youtube_status=1;$('.content-show').slideDown('fast');$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22_active.png");status = 0;}});

$('.youtube-icon').on('click',function(e){e.preventDefault();position_array=pasteHtmlAtCaret1("");$('.image_link_screen').hide();cam_status=1;$('.content-show').hide();$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22.png");status=1;if(youtube_status==0){$('.youtube_link_screen').hide();youtube_status=1;}else{$('.youtube_link_screen').show();youtube_status=0;}});
$('a.youtube_cancel').on('click',function(e){e.preventDefault();$('.youtube_link_screen').hide();youtube_status=1;});
$('a.youtube_send').on('click',function(e){e.preventDefault();if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);var youtube_link=$(this).closest('.youtube_link_screen').children('.modal-body').find('.youtube_link');var youtube_link_value=youtube_link.val();youtube_link_value=youtube_link_value.replace(/[^a-zA-Z0-9-_]/g,'');var sHTML2 = "[MEDIA=youtube]"+youtube_link_value+"[/MEDIA]";pasteHtmlAtCaret2(sHTML2,position_array[0],position_array[1]);youtube_link.val('');$('.youtube_link_screen').hide();youtube_status=1;}});

$('.camera-icon').on('click',function(e){e.preventDefault();position_array=pasteHtmlAtCaret1("");$('.youtube_link_screen').hide();youtube_status=1;$('.content-show').hide();$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22.png");status = 1;if(cam_status==0){$('.image_link_screen').hide();cam_status=1;}else{$('.image_link_screen').show();cam_status=0;}});
$('a.image_cancel').on('click',function(e){e.preventDefault();$('.image_link_screen').hide();cam_status=1;});
$('a.image_send').on('click',function(e){e.preventDefault();if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);var image_link=$(this).closest('.image_link_screen').children('.modal-body').find('.image_link');var image_link_value=image_link.val();image_link_value=image_link_value.replace(/(<([^>]+)>)/ig,"");if(checkURL(image_link_value)){testImage(image_link_value,record);}else{alert("Invaild Url.");image_link.val('');}}});
});
function checkURL(url){return(url.match(/\.(jpeg|jpg|gif|png)$/) != null);}
function record(url, result) {if(result=="success"){var sHTML1 = "<img src='"+url+"' alt='' style='max-width:506px!important;'>";pasteHtmlAtCaret2(sHTML1,position_array[0],position_array[1]);$('.image_link').val('');$('.image_link_screen').hide();cam_status=1;}else if(result=="error"){alert("Invaild Url.");$('.image_link').val('');}else if(result=="timeout"){alert("Timeout.");$('.image_link').val('');}}  
function testImage(url, callback, timeout){if(url.substr(0,4) != 'http'){url = 'http://' + url;}timeout = timeout || 5000;var timedOut = false, timer;var img = new Image();img.onerror = img.onabort = function() {if (!timedOut) {clearTimeout(timer);callback(url, "error");}};img.onload = function() {if (!timedOut) {clearTimeout(timer);callback(url, "success");}};img.src = url;timer = setTimeout(function() {timedOut = true;callback(url, "timeout");}, timeout);}


</script>
