    if(comment_flag==1){
        var idleInterval = setInterval("comment_update()",20000);$(this).mousemove(function(e){idleTime=0;});//comment_update();
        set_cookie("comment_interval", idleInterval, 1, "allkpop.com"); 
    }
    $(".update_screen").on('click',function(e){
        e.preventDefault();
        var nr=$('.update_result').attr('data-nr');
        var nrr=$('.update_result').attr('data-nrr');
        var tc=$('.update_result').attr('data-tc');
        $(".update_screen").fadeOut('fast',function(){
            $(".update_screen").attr("data-status","0");
            if(nr!=null && Number($('.comment_update').attr('data-click-num1'))<Number(nr)){$('.comment_update').attr('data-click-num1',nr);//change to last reply_idx
            }
            if(nrr!=null && Number($('.comment_update').attr('data-click-num2'))<Number(nrr)){$('.comment_update').attr('data-click-num2',nrr);//change to last rereply_idx
            }
            if(tc!=null){$('.comment_count').text((Number($('.comment_count').text().replace(",",""))+Number(tc)).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));//update final total comment number
            }
            $('.new_screen').prepend($('.update_result').html());$('.new_screen_box').show();
        });
    });
    $(".redactor_akp_posts_reply").on('click',function(e){//reply textarea box wider 1
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);if(user_id!=""){$('.akp_posts_cancel').show();}else{$('.alert').html('<a class="close">x</a><strong>Error!</strong> '+login_alert).show();$('.close').click(function(){$('.alert').hide();});return;}}
    });
    $('.akp_posts_btn').on('click',function(e){//reply insert send 2
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        $('.alert').hide();
    	if(user_id!=""){
            var co = $.cookie("write_"+user_id);
            if(co==null){
                var akp_posts_reply=$('.redactor_akp_posts_reply').html();
                if(check_link(akp_posts_reply)=="102"){alert(comment_link_alert);return;}
                if(akp_posts_reply!=""&&akp_posts_reply!="<p><br></p>"){
                    $.post("/article_reply.php",{parent_idx:view_row_idx,mode:'write',akp_posts_reply:akp_posts_reply},function(data){
                        console.log(data);
                        if(data[0]==1){
                            if (status==0){
                                $('.content-show').slideUp('fast');
                    			$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22.png");
                                icon_status = 1;
                            }
                            $('.comment_count').text((Number($('.comment_count').text().replace(',',''))+1).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
                            $('.akp_posts_reply').val("");
                            $('.redactor_akp_posts_reply').html("<p><br></p>");
                            $('.list-comments').prepend(data[1]);
                            setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
                        }else if(data[0]=="200"){
                            if (status==0){
                                $('.content-show').slideUp('fast');
                    			$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22.png");
                                icon_status = 1;
                            }
                            $('.comment_count').text((Number($('.comment_count').text().replace(',',''))+1).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
                            $('.akp_posts_reply').val("");
                            $('.redactor_akp_posts_reply').html("<p><br></p>");
                            setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
                            alert("You've reached the maximum of 200 comments and posts.");return;
                        }else if(data[0]=="login"){
                            alert(login_alert);return;
                        }else if(data[0]=="ban"){
                            alert("You can't post another comment until "+data[1]+" EST.");
                            return;
                        }else if(data[0]=="ip_ban"){
                            alert(data[1]);
                            return;
                        }else if(data[0]=="warning"){
                        }else{/*alert(data);return;*/}
                    },"json"); 
                }else{
                    $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+empty_alert).show();$('.close').click(function(){$('.alert').hide();});return;
                }
            }else{
                $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+wait_alert).show();$('.close').click(function(){$('.alert').hide();});return;
            }
        }else{
            $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+login_alert).show();$('.close').click(function(){$('.alert').hide();});return;
        }
	}else{}
    });
    $('.akp_posts_cancel').on('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            if(user_id!=""){
                var choose=confirm("Do you want to cancel it?");
                if(choose){
                    if (status==0){$('.content-show').slideUp('fast');$('.show-icon img').attr("src","http://static2.allkpop.com/img/btn22.png");status = 1;}
                    $('.redactor_akp_posts_reply').html("<p><br></p>");$('.akp_posts_reply').val('');setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
                    if(ref!=""){scroll_to_target_id('cmt_id_'+ref);ref="";}$('.akp_posts_edit').hide();$('.akp_posts_cancel').hide();$('.akp_posts_btn').show();
                }
            }
        }
    });
    $('a.reedit').live('click',function(e){//reply edit
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        reedit=$(this).closest('.box-cm').find('.reply_content');
        ref=$(this).attr('data-ref');
        
        if(reedit.find('div.video-wrap').length){
            $(this).closest('.box-cm').find('.reply_content').find('.video-wrap').each(function(){
                if($(this).find("iframe").attr('src').indexOf("youtube.com/embed")>-1){
                    var regex = /\<.+?embed\/(.+?)\".+\>/;
                    var youtube_src = regex.exec($(this).html())[1];
                    $(this).replaceWith("[MEDIA=youtube]"+youtube_src+"[/MEDIA]");
                }
            });
        }else if(reedit.find('iframe').length){
            $(this).closest('.box-cm').find('.reply_content').find('iframe').each(function(){
                if($(this).attr('src').indexOf("youtube.com/embed")>-1){
                    var regex = /\<.+?embed\/(.+?)\".+\>/;
                    var youtube_src = regex.exec($("<div />").append($(this).clone()).html())[1];
                    $(this).replaceWith("[MEDIA=youtube]"+youtube_src+"[/MEDIA]");
                }
            });
        }
        var reedit_html=reedit.html();
        $(".redactor_akp_posts_reply").html(reedit_html);
        $('.akp_posts_reply').val(reedit_html);
        setEndOfContenteditable($('.redactor_akp_posts_reply').get(0));
        $('.akp_posts_btn').hide();
        $('.akp_posts_edit').attr('data-value',$(this).attr('data-value')).show();
        $('.akp_posts_cancel').show();
        scroll_to_target_id('goto_box_comment');
        }
    });
