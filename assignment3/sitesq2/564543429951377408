        $.post("/article_reply.php",{article_data:article_data,category:'article',mode:'love_it'},function(data){
            if(data==1){
                $('.btn-love-scrap').css('display',"none");$('.btn-love-cancel').css('display',"block");
                var current_love_it=$('.love_it_cnt');current_love_it.text(Number(current_love_it.text())+1);var total_share=$('.customShare');total_share.text(Number(total_share.text())+1);
            }else if(data=="login"){alert(login_alert);return;
            }else{/*alert(data);return;*/}
        });
        }
    });
    $('.btn-love-cancel').on('click',function(e){//scrap del
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        $.post("/article_reply.php",{article_data:article_data,category:'article',mode:'cancel_love_it'},function(data){
            if(data==1){
                $('.btn-love-scrap').css('display',"block");$('.btn-love-cancel').css('display',"none");
                var current_love_cancel=$('.love_it_cnt');
                if(current_love_cancel.text()<=0){current_love_cancel.text("0");}else{current_love_cancel.text(Number(current_love_cancel.text())-1);}   
                var total_share=$('.customShare');if(total_share.text()<=0){total_share.text("0");}else{total_share.text(Number(total_share.text())-1);} 
            }else if(data=="login"){
                alert(login_alert);return;
            }else{/*alert(data);return;*/}
        });
        }
    });
    $('.btn-thumbs-up').on('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            var btn_thumbs_up=$(this),thumbs_up_cnt=$('.thumbs_up_cnt'),thumbs_down_cnt=$('.thumbs_down_cnt'),thumbs_up_cnt1=$('.thumbs_up_cnt1'),thumbs_down_cnt1=$('.thumbs_down_cnt1');
            if(user_id!=""){
                if(user_id==btn_thumbs_up.attr("data-usr")){alert("You cannot vote on your own article.");return;
                }else{
                    $.post("/article_reply.php",{article_data:article_data,mode:'thumbs_up'},function(data){
                        if(data==1){
                            thumbs_up_cnt.text(Number(thumbs_up_cnt.text())+1);
                            thumbs_up_cnt1.text(Number(thumbs_up_cnt1.text())+1);
                        }else if(data=="login"){
                            alert(login_alert);return;
                        }else if(data=="vote_five"){
                            alert("Maximum 5 votes for same person per 24hours.");return;
                        }else if(data=="vote_one"){
                            alert("Maximum 1 vote for same content per 24hours.");return;
                        }else if(data=="vote_minus_cancel"){
                            //thumbs_up_cnt.text(Number(thumbs_up_cnt.text())-1);
                            thumbs_down_cnt.text(Number(thumbs_down_cnt.text())-1);
                            thumbs_down_cnt1.text(Number(thumbs_down_cnt1.text())-1);
                        }else if(data=="ip_ban"){
                            alert("Banned for multiple trolling accounts.");return;
                        }else{/*alert(data);return;*/}
                    });    
                }
            }else{alert(login_alert);return;}  
        }
    });
    $('.btn-thumbs-down').on('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            var btn_thumbs_down=$(this),thumbs_up_cnt=$('.thumbs_up_cnt'),thumbs_down_cnt=$('.thumbs_down_cnt'),thumbs_up_cnt1=$('.thumbs_up_cnt1'),thumbs_down_cnt1=$('.thumbs_down_cnt1');
            if(user_id!=""){
                if(user_id==btn_thumbs_down.attr("data-usr")){alert("You cannot vote on your own article.");return;
                }else{
                    $.post("/article_reply.php",{article_data:article_data,mode:'thumbs_down'},function(data){
                        if(data==1){
                            thumbs_down_cnt.text(Number(thumbs_down_cnt.text())+1);
                            thumbs_down_cnt1.text(Number(thumbs_down_cnt1.text())+1);
                        }else if(data=="login"){
                            alert(login_alert);return;
                        }else if(data=="vote_five"){
                            alert("Maximum 5 votes for same person per 24hours.");return;
                        }else if(data=="vote_one"){
                            alert("Maximum 1 vote for same content per 24hours.");return;
                        }else if(data=="vote_plus_cancel"){
                            thumbs_up_cnt.text(Number(thumbs_up_cnt.text())-1);
                            thumbs_up_cnt1.text(Number(thumbs_up_cnt1.text())-1);
                        }else if(data=="ip_ban"){
                            alert("Banned for multiple trolling accounts.");return;
                        }else{/*alert(data);return;*/}
                    });
                }
            }else{alert(login_alert);return;}
        }
    });
