<script>
var user_id='';var user_name='';var data_from='/article.php';var new_tab='1';var notice_flag='1';
var start_time='1427694420';var current_time='1428103123';var end_time='1428292740';
function notice_tracking(data_from,data_to,data_value,user_id,user_name,start_time,end_time){
    if(user_id==""){user_id="0";user_name="guest";}
    $.ajax({
      type: 'POST',url: "/common/notice_tracking.php",data: {mode:'click',data_from:data_from,data_to:data_to,data_value:data_value,user_id:user_id,user_name:user_name,start_time:start_time,end_time:end_time},dataType: "html",async:false
    }).done(function( data ) {
        if(new_tab==1){
            window.open(data_to,'_blank');
        }else{
            window.open(data_to,'_self');    
        }
        
    });
}
function notice_impression(data_from,data_value,user_id,user_name,start_time,end_time){
    if(user_id==""){user_id="0";user_name="guest";}
    $.post("/common/notice_tracking.php",{mode:'impression',data_from:data_from,data_value:data_value,user_id:user_id,user_name:user_name,start_time:start_time,end_time:end_time},function(data){
        //console.log(data);
    },"json");
}
$(document).ready(function(){
    if(notice_flag==1&&start_time<=current_time&&end_time>=current_time){
        var notice_idx=$('.notice-close').attr("data-value");
        if(notice_idx!=undefined){
            setTimeout(function() {notice_impression(data_from,notice_idx,user_id,user_name,start_time,end_time);},500);
        }
        
        $('.notice-close').on('click',function(){
            var valid_domain='allkpop.com';
            var domain_string = valid_domain ?("; domain=" + valid_domain) : '' 
            var notice_idx=$(this).attr("data-value");//3600*24*30*12
            document.cookie = "notice="+encodeURIComponent(notice_idx)+"; max-age=" + 3600*24 +"; path=/" + domain_string;
            $('#notice-container-placeholder').hide();
        });
        $('.notice-content').on('click',function(){
            $('.notice_button').click();
        });
        $('.notice_button').on('click',function(){
            var data_to=$(this).attr('data-href');
            var data_value=$(this).attr('data-value');
            notice_tracking(data_from,data_to,data_value,user_id,user_name,start_time,end_time);
        });
    }
    
});
</script>
