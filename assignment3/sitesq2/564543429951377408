<script>
function check_link(str){
    str=str.replace(/(<img([^>]+)>)/ig,"");
    if(str.indexOf("．Co­m­")!=-1||str.indexOf("．Com")!=-1||str.indexOf("ᶜᴼᴹ")!=-1||str.indexOf("．c­o­m")!=-1||str.indexOf("Ｃ­ Ｏ­ Ｍ­")!=-1||str.indexOf("ＣＯＭ")!=-1||str.indexOf("ℂℴℳ")!=-1||str.indexOf("ℭ­­Ｏℳ")!=-1||str.indexOf("ℭＯℳ")!=-1||str.indexOf("ℭℴℳ")!=-1){return "102";}
    str=str.toLowerCase().replace(/(&nbsp;)*/g,"").replace(/[^\w\s .$]/gi,'').replace(/\s{2,}/g, ' ');
    console.log("before: "+str);
    var exp = /(((http(s?)(:\/\/))?([w]{3}\.)?)([a-z|0-9])+\.(com(\.au)?|do|sh|nu|lt|pl|it|de|in|im|vu|as|ie|tw|ru|gs|gd|gp|ag|es|us|biz|org|me|net|ly|be|gl|info|(co(\.))?uk|ca|nz|tv)(\/[^\s]+)*)+/g;
    var result=str.match(exp);
    console.log("result: "+result);
    if(result!=null){
        var flag=0;var allow_domain=["allkpop.com","tokyohive.com","youtube.com","twitter.com","facebook.com"];
        for(var i=0,len=result.length;i<len;i++){if(result[i].indexOf(allow_domain[0])==-1&&result[i].indexOf(allow_domain[1])==-1&&result[i].indexOf(allow_domain[2])==-1&&result[i].indexOf(allow_domain[3])==-1&&result[i].indexOf(allow_domain[4])==-1){flag=1;break;}}
        if(flag==1){return "102";}else{return "101";}
    }
    var exp1=/([0-9])+(jobs)/g;var result1=str.match(exp1);if(result1!=null){return "102";}str=str.replace(/<\/?[^>]+(>|$)/g, "");
    console.log("after: "+str);
    if(str.indexOf("netjob10")!=-1||str.indexOf("makes $")!=-1||str.indexOf(".c")!=-1||str.indexOf(".o")!=-1||str.indexOf(".m")!=-1||str.indexOf(".om")!=-1||str.indexOf(".cm")!=-1||str.indexOf(".co")!=-1){return "102";}
    
    var result=str.match(exp);
    if(result!=null){
        var flag=0;var allow_domain=["allkpop.com","tokyohive.com","youtube.com","twitter.com","facebook.com"];
        for(var i=0,len=result.length;i<len;i++){if(result[i].indexOf(allow_domain[0])==-1&&result[i].indexOf(allow_domain[1])==-1&&result[i].indexOf(allow_domain[2])==-1&&result[i].indexOf(allow_domain[3])==-1&&result[i].indexOf(allow_domain[4])==-1){flag=1;break;}}
        if(flag==1){return "102";}else{return "101";}
    }
    return "101";
}
function set_cookie(cookie_name, cookie_value,lifespan_in_days, valid_domain){
    var domain_string = valid_domain ?("; domain=" + valid_domain) : '' ;
    document.cookie = cookie_name+"="+encodeURIComponent(cookie_value)+"; max-age=" + 60 * 60 *24 * lifespan_in_days +"; path=/" + domain_string;
}
var comment_update=function(){
    idleTime=idleTime+1;
    if(idleTime<2){
        $.post("/article_reply.php",{parent_idx:view_row_idx,mode:'update',click1:$('.comment_update').attr('data-click-num1'),click2:$('.comment_update').attr('data-click-num2')},function(update_data){
            if(update_data[0]>0){
                $.post("/common/show_comment.php",{reply_idxs:update_data[1],rereply_idxs:update_data[2]},function(data){//prepare new comment list
                    $('.update_result').html(data[0]);//updated content
                    $('.update_result').attr('data-nr',data[1]);//updated last reply idx
                    $('.update_result').attr('data-nrr',data[2]);//updated last rereply idx
                    $('.update_result').attr('data-tc',update_data[0]);//updated total new comment number
                },"json");
                $(".update_screen").html('Load '+Number(update_data[0])+' New Comment(s)').show('fast');
                if($(".update_screen").attr("data-status")==0){}
                $(".update_screen").attr("data-status","1");
            }else if(update_data[0]=="login"){
                alert(login_alert);return;
            }else if(update_data[0]==0){//no changed
            }else{alert(update_data);}//error
        },"json");
    }else{/*console.log("Move your mouse to activate.");*/}
}
if (typeof scroll_to_target_id=='function'){}else{
function scroll_to_target_id(id_name){
    if($('#'+id_name).attr('id')!=undefined){
        if (navigator.userAgent.search("Chrome") >= 0){
            parent.document.body.scrollTop=$('#'+id_name).offset().top;
        }else{
            document.getElementById(id_name).scrollIntoView();
        }
        if(id_name!="goto_box_comment"){
            if(id_name=="article-nav"){
                $('#cmt_screen').css({'opacity':'0'}).animate({opacity:1},2000);
            }else{
                $('#'+id_name).css('opacity','0').animate({opacity:1},2000);
            }
        }    
    }
}
}
if (typeof get_emoticon_set=='function'){}else{
function get_emoticon_set(data){
    $.post("/article_reply.php",{article_data:data,mode:'emoticon'},function(data){
        if(data[0]==1){$('.editer-content').html(data[1]);if(data[2]=="unlocked")$('.toolbar1 .btn-unlock').hide();else if(data[2]=="locked")$('.toolbar1 .btn-unlock').show('slow');}else{alert(data[0]);return;}
    },"json");   
}
}
if (typeof check_emoticon_use_cnt=='function'){}else{function check_emoticon_use_cnt(){var temp=$('.redactor_akp_posts_reply').html();var emoticon_use_cnt=temp.match(/\/emoticon\//g);if(emoticon_use_cnt==null){return 1;}else{return emoticon_use_cnt.length+1;}}}
if (typeof pasteHtmlAtCaret=='function'){}else{function pasteHtmlAtCaret(html){document.getElementById('redactor_akp_posts_reply').focus();var sel, range;if(window.getSelection){sel = window.getSelection();if (sel.getRangeAt && sel.rangeCount) {range = sel.getRangeAt(0);range.deleteContents();var el = document.createElement("div");el.innerHTML = html;var frag = document.createDocumentFragment(), node, lastNode;while ( (node = el.firstChild) ) {lastNode = frag.appendChild(node);}range.insertNode(frag);if(lastNode){range = range.cloneRange();range.setStartAfter(lastNode);range.collapse(true);sel.removeAllRanges();sel.addRange(range);}}}else if(document.selection && document.selection.type != "Control") {document.selection.createRange().pasteHTML(html);}}}
if (typeof pasteHtmlAtCaret1=='function'){}else{function pasteHtmlAtCaret1(html){document.getElementById('redactor_akp_posts_reply').focus();var array=[];var sel, range;if (window.getSelection) {sel = window.getSelection();array[0]=sel;if (sel.getRangeAt && sel.rangeCount) {range = sel.getRangeAt(0);array[1]=range;}}return array;}}
if (typeof pasteHtmlAtCaret2=='function'){}else{function pasteHtmlAtCaret2(html,sel,range) {document.getElementById('redactor_akp_posts_reply').focus();if (window.getSelection) {if (sel.getRangeAt && sel.rangeCount) {range.deleteContents();var el = document.createElement("div");el.innerHTML = html;var frag = document.createDocumentFragment(), node, lastNode;while ( (node = el.firstChild) ) {lastNode = frag.appendChild(node);}range.insertNode(frag);if(lastNode){range = range.cloneRange();range.setStartAfter(lastNode);range.collapse(true);sel.removeAllRanges();sel.addRange(range);}}}else if (document.selection && document.selection.type != "Control") {document.selection.createRange().pasteHTML(html);}}}
if (typeof setEndOfContenteditable=='function'){}else{
function setEndOfContenteditable(contentEditableElement){
    var range,selection;
    if(document.createRange){//Firefox, Chrome, Opera, Safari, IE 9+
        range = document.createRange();//Create a range (a range is a like the selection but invisible)
        range.selectNodeContents(contentEditableElement);//Select the entire contents of the element with the range
        range.collapse(false);//collapse the range to the end point. false means collapse to end rather than the start
        selection = window.getSelection();//get the selection object (allows you to change selection)
        selection.removeAllRanges();//remove any selections already made
        selection.addRange(range);//make the range you have just created the visible selection
    }else if(document.selection){//IE 8 and lower
        range = document.body.createTextRange();//Create a range (a range is a like the selection but invisible)
        range.moveToElementText(contentEditableElement);//Select the entire contents of the element with the range
        range.collapse(false);//collapse the range to the end point. false means collapse to end rather than the start
        range.select();//Select the range (make it the visible selection
    }
}
}
$(document).ready(function(){
    if(annotation_flag==1){
        $("#toAnnotate").annotateImage({
            getUrl: "/plugin/annotation/get.php",
    		saveUrl: "/plugin/annotation/save.php",
    		deleteUrl: "/plugin/annotation/delete.php",
            useAjax: true,
    		editable: true
        });
    }
    $('.btn-cmt').on('click',function(){window.parent.$('#login-wrapper').click();});
    $('.akp_category').on('click',function(){var slug = $(this).attr('data-value');var url="/category/"+slug+"/page/1";location.href=url;return;});
    get_emoticon_set($('.toolbar1 .selected').attr('data-value'));
    $('.toolbar1 .btnsticker').on('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
            if($(this).hasClass("selected")){}else{$('.toolbar1 .btnsticker').removeClass('selected');$(this).addClass('selected');get_emoticon_set($('.toolbar1 .selected').attr('data-value'));}
        }
    });
    $('.toolbar1 .btn-unlock').on('click',function(e){
        e.preventDefault();
        if(!$(this).data('isClicked')){var link=$(this);link.data('isClicked',true);setTimeout(function(){link.removeData('isClicked')},1500);
        if(user_id!=""){
            var choose=confirm("Do you want to unlock now?");if(choose){var url="/shop/stickers/"+$('.toolbar1 .selected').attr('data-idx');location.href=url;return;}else{return;}
        }else{
            $('.alert').html('<a class="close">x</a><strong>Error!</strong> '+login_alert).show();$('.close').click(function(){$('.alert').hide();});return;
        }
        }
    });
    $('.cmt_screen_close').on('click',function(){$('.cmt_screen_box').remove();});
    $('.new_screen_close').on('click',function(){$('.new_screen').html('');$('.new_screen_box').hide();});
