var social_share_flag='0';var social_share_cnt='39912';$('.total_share_info').mouseover(function(){$('.total_share_info_screen').show();});$('.total_share_info').mouseout(function(){$('.total_share_info_screen').hide();});
function get_social_count(){var scrap_cnt='10';var total_share=$('.customShare');var total_share1=$('.love_it_cnt');var url=$('.fb_article').attr('data-social');var total_share_cnt=Number(scrap_cnt);$('.ak_share').html(scrap_cnt);
    if(total_share.text()=="0"){
        $.getJSON("http://graph.facebook.com/?id="+url, function(data){if(data.shares>=0){total_share_cnt+=Number(data.shares);$('.fb_share').html(data.shares);}data='';});
        $.getJSON("http://urls.api.twitter.com/1/urls/count.json?url="+url+"&callback=?",function(data){if(data.count>=0){total_share_cnt+=Number(data.count);$('.tw_share').html(data.count);}data='';});
        setTimeout(function(){total_share.text(total_share_cnt);total_share1.text(total_share_cnt);
            if(social_share_flag==1&&social_share_cnt<total_share_cnt){$.post("/article_reply.php",{mode:'social_share',idx:'101914',social_share:total_share_cnt},function(data){if(data==1){}else{console.log(data);}});}
        },3000);
    }
}
get_social_count();
$(document).ready(function(){$('.editor_pick_off').live('click',function(e){e.preventDefault();var editor_pick_off=$(this);$.post("/meme_reply.php",{meme_data:$(this).attr('data-value'),mode:'editor_pick_off'},function(data){if(data==1){editor_pick_off.parent().children('.icon-star').removeClass('editor_pick_off').addClass('editor_pick_on').css({'color':'#999'});alert("Editor's pick is off");}});});$('.editor_pick_on').live('click',function(e){e.preventDefault();var editor_pick_on=$(this);$.post("/meme_reply.php",{meme_data:$(this).attr('data-value'),mode:'editor_pick_on'},function(data){if(data==1){editor_pick_on.parent().children('.icon-star').removeClass('editor_pick_on').addClass('editor_pick_off').css({'color':'#FFCC00'});alert("Editor's pick is on");}});});});
</script>                	<div class="entry">
                        Recently, <a href="http://www.allkpop.com/artisttag/sooyoung" target="_blank">Sooyoung</a> confirmed that <a href="http://www.allkpop.com/artisttag/girls-generation" target="_blank">Girls' Generation</a> was in the middle of preparing for a comeback at a Hong Kong brand event. <p><font face="arial"><span style="font-size: 16px; line-height: 24px;"><br></span></font><p style='font-size:12px;font-weight:bold;'>SEE ALSO: <a style='font-size:12px;' href='http://www.allkpop.com/article/2015/04/girls-generation-top-oricon-chart-with-tokyo-dome-live-dvd' data-link='http://www.allkpop.com/article/2015/04/girls-generation-top-oricon-chart-with-tokyo-dome-live-dvd' class='see_also' data-idx='101914'>Girls' Generation top Oricon chart with Tokyo dome live DVD!</a></p>
</p><p><br></p><p>During the event, Sooyoung stated, "<i>We are working to release a Girls' Generation album this year.</i>" She continued, "<i>In order to make an album that both Girls' Generation and fans can be satisfied with, we are worrying over the music and performance.</i>" </p><p><br></p><p>Sooyoung continued to express her desire to continue acting as she stated, "<i>If there's a good script, I would like to greet the public as an actress.</i>" <br><br>Following <a href="http://www.allkpop.com/artisttag/yuri" target="_blank">Yuri</a>'s announcement that there would be a Girls' Generation album <a href="http://www.allkpop.com/article/2015/01/yuri-says-fans-should-expect-girls-generation-comeback-this-year" target="_blank">by the end of the year</a>, Sooyoung is now the second member to confirm to the public that the girls are indeed working on a project! Fans are excited to see what the popular girl group will turn out for their next comeback after having won a Bonsang during the <b>'Golden Disk Awards.'</b></p>
<p></p>
