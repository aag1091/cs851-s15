    <!-- Javascript -->
    <script type="text/javascript">
      $(function(){

      setLanguage("title12");

      var pcJson = {"ja":["so25501243","b18170b9a05f4b5201234faf18a3b1e853782a19f6681ce6678d7f2f4c9a589c",1428103135],"en":["so25501240","68682acc84621a1337192e2ac26b0422d5f0eba90bf92e1ce53a64da8bace125",1428103135]};
      var spJson = {"ja":["so25501241","9faf5b3ea3106f9bfdf82800fc1f55e9d1f0fc7df8c2636de5cae01bb0924ebc",1428103135],"en":["so25501237","fcfd2df77afbeb1504bf8702829d36e55fdca80bcdfa61d07c28daf7533d4f21",1428103135]};
      var player = new NAMPlayer;
      var poster = '/img/title_12/12_poster.jpg';
      //currentLangはNAMLocalize.jsで定義

      player.createBilingualPlayer(pcJson,spJson,currentLang,poster);

      $('.en-subtitles').on('click',function(){
      if(player.current_lang == 'ja'){
      player.createBilingualPlayer(pcJson,spJson,'en',poster);
      }
      });

      $('.no-subtitles').on('click',function(){
      if(player.current_lang == 'en'){
      player.createBilingualPlayer(pcJson,spJson,'ja',poster);
      }
      });

      // opening skip
      $('.movie-skip').on('click',function(){
      player.skipOpening(49);
      });

      // repeat movie
      $('.videoRepeat').on('click',function(){
      player.toggleFadeMovieRepeat();
      });

      });
    </script>
