<div class="dd-block clearfix"><div class="dd-blockcontent"><!-- CDUB - CountDown-Up Big - Kubik-Rubik Joomla! Extensions by Viktor Vogel --><div class="cdub_main">
    <div class="cdub_color208">
                    <h2 class="cdub_color208">Sbrigati !!!</h2>
                            <span>15.04.2015</span>
                            -
                            <span>20:00 o'clock</span>
                                            <br /><br />
                        <span>Manca davvero poco alla fine del primo concorso di "English no problem"...sbrigati, basta davvero poco per poter vincere!!!</span>
                                            <span id="bigbefore208">
                                                                        <br /><br />
                                                <span class="cdub_bold">Time remaining</span>
                                                                <br /><br />
                                        <span id="bigtime208"></span></span>
                <span id="bigafter208" class="cdub_none">
                                            <br /><br />
                                                                <span class="cdub_bold">Countdown<br />expired</span>
                                        </span>
                                                        <br /><br /><span>Link:<br /><a target="_self" title="Come funziona?" href="http://www.englishnoproblem.org/top-i-tuoi-punti/come-funziona.html"><strong>Come funziona?</strong></a></span>
                                        <br /><br /><span class="small"><a  href="http://www.englishnoproblem.org/i-tuoi-punti/come-si-accumulano-punti.html" target="_top" title="CountDown-Up Big - Kubik-Rubik Joomla! Extensions">Come si accumulano i punti</a></span>
            </div>
</div><script type="text/javascript">// <![CDATA[
            var bigcountdown_now208 = 1428103127;
            var bigcountdown_to208 = 1429120800;

            bigcountdown_timebetween208 = bigcountdown_to208 - bigcountdown_now208;

            var up = 0;
            if (bigcountdown_timebetween208 < 0) {
                bigcountdown_timebetween208 = bigcountdown_timebetween208 * -1;
                var up = 1;
            }

            var bigcountdown_daysremain208 = 0;
            var bigcountdown_hoursremain208 = 0;
            var bigcountdown_minutesremain208 = 0;
            var bigcountdown_secondsremain208 = bigcountdown_timebetween208;

            if (bigcountdown_timebetween208 >= 60) {
                bigcountdown_secondsremain208 = bigcountdown_timebetween208 % 60;
                bigcountdown_minutesremain208 = (bigcountdown_timebetween208 - bigcountdown_secondsremain208) / 60;
            }

            if (bigcountdown_minutesremain208 >= 60) {
                bigcountdown_timebetween208 = bigcountdown_minutesremain208;
                bigcountdown_minutesremain208 = bigcountdown_timebetween208 % 60;
                bigcountdown_hoursremain208 = (bigcountdown_timebetween208 - bigcountdown_minutesremain208) / 60;
            }

            if (bigcountdown_hoursremain208 >= 24) {
                bigcountdown_timebetween208 = bigcountdown_hoursremain208;
                bigcountdown_hoursremain208 = bigcountdown_timebetween208 % 24;
                bigcountdown_daysremain208 = (bigcountdown_timebetween208 - bigcountdown_hoursremain208) / 24;
            }

            var bigtime208 = document.getElementById("bigtime208");
            var bigtimetext208 = "";

            if (up == 0) {
                var bigcountdown_timer208 = setInterval(bigCountDownTimer208, 1000);
                var bigtime208 = document.getElementById("bigtime208");
                var bigtimetext208 = "";
            } else {
                var bigcountdown_timer208 = setInterval(bigCountUpTimer208, 1000);
                document.getElementById("bigafter208").style.display = "inline";
                var bigtime208 = document.getElementById("bigtime_up208");
                var bigtimetext208 = "";
            }

            function bigRewriteCountDownSpan208() {
                bigtimetext208 = "";
                if (up == 0) {
                    bigtimetext208 += (bigcountdown_daysremain208) ? "<span class=\"cdub_font_dyn\">" + bigcountdown_daysremain208 + " " + (bigcountdown_daysremain208 <= 1 ? "Day</span>" : "Days</span>") : "";
                } else {
                    bigtimetext208 += (bigcountdown_daysremain208) ? "<span class=\"cdub_font_dyn\">" + bigcountdown_daysremain208 + " " + (bigcountdown_daysremain208 <= 1 ? "Day</span>" : "Days</span>") : "";
                }
                if (bigcountdown_daysremain208 > 0) {
                    bigtimetext208 += "<br /><br /><span class=\"cdub_font2\">"
                } else {
                    bigtimetext208 += "<span class=\"cdub_font3\">"
                }
                bigtimetext208 += (bigcountdown_hoursremain208 || bigcountdown_daysremain208) ? bigcountdown_hoursremain208 + (bigcountdown_hoursremain208 <= 1 ? " h : " : " h : ") : "";
                bigtimetext208 += (bigcountdown_minutesremain208 || bigcountdown_hoursremain208 || bigcountdown_daysremain208) ? bigcountdown_minutesremain208 + (bigcountdown_minutesremain208 <= 1 ? " m : " : " m : ") : "";
                bigtimetext208 += bigcountdown_secondsremain208 + (bigcountdown_secondsremain208 <= 1 ? " s " : " s ");
                bigtimetext208 += "</span>"
                bigtime208.innerHTML = bigtimetext208;
            }

            function bigCountDownTimer208() {
                if (bigcountdown_secondsremain208 == 0 && bigcountdown_minutesremain208 == 0 && bigcountdown_hoursremain208 == 0 && bigcountdown_daysremain208 == 0) {
                    clearInterval(bigcountdown_timer208);
                    document.getElementById("bigbefore208").style.display = "none";
                    document.getElementById("bigafter208").style.display = "inline";
                    return;
                }

                if (bigcountdown_secondsremain208 > 0) bigcountdown_secondsremain208--;
                else {
                    bigcountdown_secondsremain208 = (bigcountdown_minutesremain208 || bigcountdown_hoursremain208 || bigcountdown_daysremain208) ? 59 : 0;
                    if (bigcountdown_minutesremain208 > 0) bigcountdown_minutesremain208--;
                    else {
                        bigcountdown_minutesremain208 = (bigcountdown_hoursremain208 || bigcountdown_daysremain208) ? 59 : 0;
                        if (bigcountdown_hoursremain208 > 0) bigcountdown_hoursremain208--;
                        else {
                            bigcountdown_hoursremain208 = (bigcountdown_daysremain208) ? 23 : 0;
                            if (bigcountdown_daysremain208) bigcountdown_daysremain208--;
                        }
                    }
                }

                bigRewriteCountDownSpan208();
            }

            function bigCountUpTimer208() {

                if (bigcountdown_secondsremain208 < 59) bigcountdown_secondsremain208++;
                else {
                    bigcountdown_secondsremain208 = 0;
                    if (bigcountdown_minutesremain208 < 59) bigcountdown_minutesremain208++;
                    else {
                        bigcountdown_minutesremain208 = 0;
                        if (bigcountdown_hoursremain208 < 23) bigcountdown_hoursremain208++;
                        else {
                            bigcountdown_hoursremain208 = 0;
                            bigcountdown_daysremain208++;
                        }
                    }
                }
                bigRewriteCountDownSpan208();
            }
