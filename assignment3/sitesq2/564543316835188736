    <script type='text/javascript'>
    
        var openReviewForm = function()
        {
            var user_review_form_table = document.getElementById('user_review_form_table');
            user_review_form_table.style.display = 'block';
            
            var user_review_form_button_container = document.getElementById('user_review_form_button_container');
            user_review_form_button_container.style.top = '0px';
            user_review_form_button_container.style.left = '0px';
        }
           
        var starImages = new Array();
        starImages[0] = new Array();
        starImages[1] = new Array();
        starImages[0][0] = 'http://shop.crackberry.com/store_images/v3/star-lg-off.png';
        starImages[0][1] = 'http://shop.crackberry.com/store_images/v3/star-lg-on.png';
        starImages[1][0] = 'http://shop.crackberry.com/store_images/androidcentral/star-rating-lg-off.png';
        starImages[1][1] = 'http://shop.crackberry.com/store_images/androidcentral/star-rating-lg-on.png';
        var ratingRetainer = 0;
        
        
        var changeRating = function(newRating,starVersion)
        {
            ratingRetainer = newRating;
            
            if(document.getElementById('rating_retainer'))
            {
                document.user_review.rating.value = newRating;
            }
            
            if(newRating > 5 || newRating < 0)
            {
                return false;
            }
            
            currentRating = 1;
            while(currentRating <= 5)
            {
                if(currentRating <= newRating)
                {
                    document.getElementById('rate_star_' + currentRating).src = starImages[starVersion][1];
                }
                else
                {
                    document.getElementById('rate_star_' + currentRating).src = starImages[starVersion][0];
                }
                currentRating++;
            }
        }
        
        var ratingOnMouseOver = function(rating,starVersion)
        {
            if(rating > 5 || rating < 0)
            {
                return false;
            }
            
            currentRating = 1;
            while(currentRating <= 5)
            {
                if(currentRating <= rating)
                {
                    document.getElementById('rate_star_' + currentRating).src = starImages[starVersion][1];
                }
                else
                {
                    document.getElementById('rate_star_' + currentRating).src = starImages[starVersion][0];
                }
                currentRating++;
            }
        }
        
        var ratingOnMouseOut = function(starVersion)
        {
            if(document.getElementById('rating_retainer'))
            {
                var newRating = document.user_review.rating.value;
            }
            else
            {
                var newRating = ratingRetainer;
            }
            changeRating(newRating,starVersion);
        }
    </script>
    </form>
