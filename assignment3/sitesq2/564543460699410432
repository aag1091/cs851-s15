    function ShareOnTwitter() {
        
      openWindow('https://twitter.com/share?text=My+amazing+friend+Kevin+Wood+is+%23fundraising+for+%40TheBHF.+Donate+to+Kevin%27s+%40JustGiving+page+&url=http%3a%2f%2fwww.justgiving.com%2fKevinWood2015%2f%3futm_source%3dTwitter%26utm_medium%3dfundraisingpage%26utm_content%3dKevinWood2015%26utm_campaign%3dpfp-tweet');
