<script type="text/javascript">
    (function ($) {

        $(function () {
            $("#inlinepublish-page-to-facebook").unbind();

            $("#inlinepublish-page-to-facebook").click(function share() {

                JustGiving.Analytics.SocialTracking.ExtendSettings({ target: 'fundraising_page_owner' });

                
                var obj = {
                    method: 'share_open_graph',
                    action_type: 'jgdonation:support',
                    action_properties: JSON.stringify({
                        fundraising_page: 'http://www.justgiving.com/KevinWood2015/',
                        ref: 'pfp-share-facebook-test-C-control'
                    })
                };
                

                    var callback = function (response) {
                    var socialData = { network: 'Facebook' };

                    if (response && response.post_id) {
                        socialData.socialAction = 'FacebookShareSuccess';
                        if (window.JustGiving.Analytics.SocialTrackingTarget) {
                            socialData.target = window.JustGiving.Analytics.SocialTrackingTarget;
                        }
                        JustGiving.Analytics.SocialTracking.ExtendSettings(socialData);
                        JustGiving.Analytics.SocialTracking.Track();
                        
                        //KM Event
                        var properties = {};
                        properties['context'] = 'Facebook';
                        properties['action'] = 'Shared';
                        amplify.publish("kmq", ['record', 'Shared', properties]);
                    } else {
                        socialData.socialAction = 'FacebookShareFail';
                        if (window.JustGiving.Analytics.SocialTrackingTarget) {
                            socialData.target = window.JustGiving.Analytics.SocialTrackingTarget;
                        }
                        JustGiving.Analytics.SocialTracking.ExtendSettings(socialData);
                        JustGiving.Analytics.SocialTracking.Track();
                        
                        //KM Event
                        var properties = {};
                        properties['context'] = 'Facebook';
                        properties['action'] = 'FacebookShareFail';
                        amplify.publish("kmq", ['record', 'Shared', properties]);
                    }
                };

                FB.ui(obj, callback);

                return false;
            });
        });
    }(jQuery));
</script>



