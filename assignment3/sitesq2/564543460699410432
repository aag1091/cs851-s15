        <script id="tear-off-template" type="text/x-jQuery-tmpl">
            <div class="tear-off-strip">
                <p>
                    Donate to ${Name}<br>
                    page at <strong>justgiving.com<br>
                        /${pageURL}</strong>
                </p>
            </div>
        </script>

        <script type="text/javascript" src="/scripts/beta1.jquery.tmpl.min.js"></script>
        <script type="text/javascript">
            function newFilledArray(length, val) {
                var array = [];
                for (var i = 0; i < length; i++) {
                    array[i] = val;
                }
                return array;
            }

            var tearOffDetails = newFilledArray(10, { Name: "Kevin Wood's", pageURL: "KevinWood2015" });
            $('#tear-off-template').tmpl(tearOffDetails).appendTo('#tear-off-strip-container');

        </script>

