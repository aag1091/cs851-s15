          if (gapi.iframes && gapi.iframes.getContext) {
            gapi.iframes.getContext().openChild({
                url: 'https://www.blogger.com/navbar.g?targetBlogID\0753294062227707013876\46blogName\75The+Patriot+Factor\46publishMode\75PUBLISH_MODE_BLOGSPOT\46navbarType\75BLACK\46layoutType\75LAYOUTS\46searchRoot\75http://thepatriotfactor.blogspot.com/search\46blogLocale\75en\46v\0752\46homepageUrl\75http://thepatriotfactor.blogspot.com/\46targetPostID\0757264801645973189555\46blogPostOrPageUrl\75http://thepatriotfactor.blogspot.com/2015/02/op-ed-tickling-surface-of-those-out-to.html\46vt\0756240183374158186062',
