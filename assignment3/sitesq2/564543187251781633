<script type="text/javascript">
  $(document).ready(function () {
    if ('False' == 'True') {
      $('.rss').show();
    }
    if ($.browser.msie) {
      $("#FooterBar-Wrapper ul").css("margin-top", "1px");
      $('#aSeoText').css("margin-top", "-20px");
      $("#FooterBar-Drawers").css("padding", "2px 0px 0px 2px");
    }

    $("#FooterBar-Drawers").click(function (e) {
      if ($(e.target).attr("drawer") == undefined) { return; }
      CloseDrawer($(this).attr("drawer"));
      $("#FooterBar-Drawers").attr("drawer", "none");
      CheckActive();
    });

    $("#FooterBar-Drawers div.drawer").hide();

    $("#FooterBar-Wrapper a.ilink[drawer]").click(function (e) {
      e.preventDefault();

      var link = $(this);

      var drawer = $(link).attr("drawer");

      var active = CheckActive();

      if (drawer == "rss") {
        if ($("#rssfeed").html() == null) {
          $(this).addClass("loading").removeClass(drawer);
          $("#FooterBar-Drawers div.drawer." + drawer).html("<iframe id='rssfeed' src='/rss.aspx' style='border:none;width:100%;height:100%;margin:0px;padding:0px;'></iframe>");
        }
        else {
          OpenDrawer(this, drawer, "30px");
        }
        $("#rssfeed").load(function () {
          OpenDrawer(this, drawer, "30px");
          if (active != "rss") {
            link.addClass(drawer).removeClass("loading");
          }
        });
      }
      if (drawer == "facebook") {
        if ($("#facebookfeed").html() == null) {
          $(this).addClass("loading").removeClass(drawer);
          $("#FooterBar-Drawers div.drawer." + drawer).html("<iframe id='facebookfeed' src='/facebook.aspx' style='border:none;width:100%;height:100%;margin:0px;padding:0px;'></iframe>");
        }
        else {
          OpenDrawer(this, drawer, "31px");
        }
        $("#facebookfeed").load(function () {
          OpenDrawer(this, drawer, "31px");
          if (active != "facebook") {
            link.addClass(drawer).removeClass("loading");
          }
        });
      }
      if (drawer == "seotext") {
        OpenDrawer(link, drawer, "30px");
      }
      if (drawer == "ebay") {
        OpenDrawer(link, drawer, "30px");
      }
      if (drawer == "twitter") {
        if ($("#twitterfeed").html() == null) {
          $(this).addClass("loading").removeClass(drawer);
          $("#FooterBar-Drawers div.drawer." + drawer).html("<iframe id='twitterfeed' src='/twitter.aspx' style='border:none;width:100%;height:100%;margin:-6px 0px 0px;padding:0px;'></iframe>");
        }
        else {
          OpenDrawer(this, drawer, "25px");
        }
        $("#twitterfeed").load(function () {
          OpenDrawer(this, drawer, "25px");
          if (active != "twitter") {
            link.addClass(drawer).removeClass("loading");
          }
        });
      }
      if (drawer == "youtube") {
        OpenDrawer(link, drawer, "30px");
      }
      if (drawer == "googleplus") {
        OpenDrawer(link, drawer, "30px");
      }
    });

  });

  function FacebookAlert() {
    alert("This website does not have a facebook page or profile, If you are the owner of this website and wish to add one, contact our customer service department @ 1-866-388-9778");
    return false;
  }
  function TwitterAlert() {
    alert("This website does not have a twitter account associated with it, If you are the owner of this website and wish to add one, contact our customer service department @ 1-866-388-9778");
    return false;
  }
  function GooglePlusAlert() {
    alert("This website does not have a Google+ profile, If you are the owner of this website and wish to add one, contact our customer service department @ 1-866-388-9778");
    return false;
  }
  function YouTubeAlert() {
    alert("This website does not have a YouTube profile, If you are the owner of this website and wish to add one, contact our customer service department @ 1-866-388-9778");
    return false;
  }

  function CheckActive() {
    var active = $("#FooterBar-Drawers").attr("drawer");

    if (active == "seotext") {
      $("#FooterBar-Wrapper a.ilink.text").addClass("minus").removeClass("plus");
    }
    else {
      $("#FooterBar-Wrapper a.ilink.text").addClass("plus").removeClass("minus");
    }

    return active;
  }

  function OpenDrawer(link, drawer, location) {
    var active = CheckActive();

    if (active == drawer) {
      $("#FooterBar-Drawers").attr("drawer", "none");
      CheckActive();
      //$("#FooterBar-Wrapper a.")
      CloseDrawer(drawer);
      return;
    }
    else if (active != "none") {
      CloseDrawer(active);
      setTimeout(function () {
        $("#FooterBar-Drawers").attr("drawer", drawer);
        CheckActive();
        $("#FooterBar-Drawers div.drawer." + drawer).show();
        $("#FooterBar-Drawers").animate({
          bottom: location
        }, 500, "swing");

      }, 500);
    }
    else {
      $("#FooterBar-Drawers").attr("drawer", drawer);
      CheckActive();
      $("#FooterBar-Drawers div.drawer." + drawer).show();
      $("#FooterBar-Drawers").animate({
        bottom: location
      }, 500, "swing");
    }
  }
  function CloseDrawer(drawer, toopen) {
    $("#FooterBar-Drawers").animate({
      bottom: "-500px"
    }, 500, "swing", function () {
      $("#FooterBar-Drawers .drawer." + drawer).hide();
    });
  }

</script>

