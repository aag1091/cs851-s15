data-pattern="http://graphics8.nytimes.com/newsgraphics/2015/01/08/places-to-go/assets/images/quebeccity-{{size}}.jpg"
data-widths="[240, 320, 640, 720, 900, 1254, 1600, 2200, 2600]"
>
          <figcaption class="g-caption" itemprop="caption description">
