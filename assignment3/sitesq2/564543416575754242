                  <input type="text" name="artist" value="Artist Name" onBlur="if(this.value=='') this.value='Artist Name'" onFocus="if(this.value =='Artist Name' ) this.value=''">
                  <input type="text" name="yourName" value="Your Name" onBlur="if(this.value=='') this.value='Your Name'" onFocus="if(this.value =='Your Name' ) this.value=''">
                  <input type="text" name="dedicatedTo" value="Dedicated To" onBlur="if(this.value=='') this.value='Dedicated To'" onFocus="if(this.value =='Dedicated To' ) this.value=''">
